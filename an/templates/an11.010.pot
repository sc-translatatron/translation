#. <section class="sutta" id="an11.10"><article><div class="hgroup"><p class="division">
msgctxt "an11.10:0.1"
msgid "Aṅguttara Nikāya 11"
msgstr ""

#. </p><p>
msgctxt "an11.10:0.2"
msgid "1. Nissayavagga"
msgstr ""

#. </p><h1>
msgctxt "an11.10:0.3"
msgid "10. Moranivāpasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an11.10:1.1"
msgid "Ekaṃ samayaṃ bhagavā rājagahe viharati moranivāpe paribbājakārāme."
msgstr ""

msgctxt "an11.10:1.2"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "an11.10:1.3"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "an11.10:1.4"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "an11.10:1.5"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.327"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an11.10:2.1"
msgid ""
"“Tīhi, bhikkhave, dhammehi samannāgato bhikkhu accantaniṭṭho hoti "
"accantayogakkhemī accantabrahmacārī accantapariyosāno seṭṭho devamanussānaṃ."
msgstr ""

msgctxt "an11.10:2.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an11.10:2.3"
msgid ""
"Asekhena sīlakkhandhena, asekhena samādhikkhandhena, asekhena "
"paññākkhandhena—"
msgstr ""

msgctxt "an11.10:2.4"
msgid ""
"imehi, kho, bhikkhave, tīhi dhammehi samannāgato bhikkhu accantaniṭṭho hoti "
"accantayogakkhemī accantabrahmacārī accantapariyosāno seṭṭho devamanussānaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an11.10:3.1"
msgid ""
"Aparehipi, bhikkhave, tīhi dhammehi samannāgato bhikkhu accantaniṭṭho hoti "
"accantayogakkhemī accantabrahmacārī accantapariyosāno seṭṭho devamanussānaṃ."
msgstr ""

msgctxt "an11.10:3.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an11.10:3.3"
msgid "Iddhipāṭihāriyena, ādesanāpāṭihāriyena, anusāsanīpāṭihāriyena—"
msgstr ""

msgctxt "an11.10:3.4"
msgid ""
"imehi kho, bhikkhave, tīhi dhammehi samannāgato bhikkhu accantaniṭṭho hoti, "
"accantayogakkhemī accantabrahmacārī accantapariyosāno seṭṭho devamanussānaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an11.10:4.1"
msgid ""
"Aparehipi, bhikkhave, tīhi dhammehi samannāgato bhikkhu accantaniṭṭho hoti "
"accantayogakkhemī accantabrahmacārī accantapariyosāno seṭṭho devamanussānaṃ."
msgstr ""

msgctxt "an11.10:4.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an11.10:4.3"
msgid "Sammādiṭṭhiyā, sammāñāṇena, sammāvimuttiyā—"
msgstr ""

msgctxt "an11.10:4.4"
msgid ""
"imehi kho, bhikkhave, tīhi dhammehi samannāgato bhikkhu accantaniṭṭho hoti "
"accantayogakkhemī accantabrahmacārī accantapariyosāno seṭṭho devamanussānaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an11.10:5.1"
msgid ""
"Dvīhi, bhikkhave, dhammehi samannāgato bhikkhu accantaniṭṭho hoti "
"accantayogakkhemī accantabrahmacārī accantapariyosāno seṭṭho devamanussānaṃ."
msgstr ""

msgctxt "an11.10:5.2"
msgid "Katamehi dvīhi?"
msgstr ""

msgctxt "an11.10:5.3"
msgid "Vijjāya, caraṇena—"
msgstr ""

msgctxt "an11.10:5.4"
msgid ""
"imehi kho, bhikkhave, dvīhi dhammehi samannāgato bhikkhu accantaniṭṭho hoti "
"accantayogakkhemī accantabrahmacārī accantapariyosāno seṭṭho devamanussānaṃ."
msgstr ""

msgctxt "an11.10:5.5"
msgid "Brahmunā pesā, bhikkhave, sanaṅkumārena gāthā bhāsitā:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an11.10:6.1"
msgid "‘Khattiyo seṭṭho janetasmiṃ,"
msgstr ""

#. <br>
msgctxt "an11.10:6.2"
msgid "ye gottapaṭisārino;"
msgstr ""

#. <br>
msgctxt "an11.10:6.3"
msgid "Vijjācaraṇasampanno,"
msgstr ""

#. <br>
msgctxt "an11.10:6.4"
msgid "so seṭṭho devamānuse’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.328"></a>
#. <a class="sc" id="sc7"></a>
msgctxt "an11.10:7.1"
msgid ""
"Sā kho panesā, bhikkhave, sanaṅkumārena gāthā bhāsitā subhāsitā, no "
"dubbhāsitā; atthasaṃhitā, no anatthasaṃhitā; anumatā mayā."
msgstr ""

msgctxt "an11.10:7.2"
msgid "Ahampi, bhikkhave, evaṃ vadāmi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an11.10:8.1"
msgid "‘Khattiyo seṭṭho janetasmiṃ,"
msgstr ""

#. <br>
msgctxt "an11.10:8.2"
msgid "ye gottapaṭisārino;"
msgstr ""

#. <br>
msgctxt "an11.10:8.3"
msgid "Vijjācaraṇasampanno,"
msgstr ""

#. <br>
msgctxt "an11.10:8.4"
msgid "so seṭṭho devamānuse’”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an11.10:8.5"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an11.10:8.6"
msgid "Nissayavaggo paṭhamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an11.10:9.1"
msgid "Kimatthiyā cetanā tayo,"
msgstr ""

#. <br>
msgctxt "an11.10:9.2"
msgid "upanisā byasanena ca;"
msgstr ""

#. <br>
msgctxt "an11.10:9.3"
msgid "Dve saññā manasikāro,"
msgstr ""

#. <br>
msgctxt "an11.10:9.4"
msgid "saddho moranivāpakanti."
msgstr ""
