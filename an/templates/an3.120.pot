#. <section class="sutta" id="an3.120"><article><div class="hgroup"><p class="division">
msgctxt "an3.120:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.120:0.2"
msgid "12. Āpāyikavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.120:0.3"
msgid "120. Paṭhamasoceyyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.120:1.1"
msgid "“Tīṇimāni, bhikkhave, soceyyāni."
msgstr ""

msgctxt "an3.120:1.2"
msgid "Katamāni tīṇi?"
msgstr ""

msgctxt "an3.120:1.3"
msgid "Kāyasoceyyaṃ, vacīsoceyyaṃ, manosoceyyaṃ."
msgstr ""

msgctxt "an3.120:1.4"
msgid "Katamañca, bhikkhave, kāyasoceyyaṃ?"
msgstr ""

msgctxt "an3.120:1.5"
msgid ""
"Idha, bhikkhave, ekacco pāṇātipātā paṭivirato hoti, adinnādānā paṭivirato "
"hoti, kāmesumicchācārā paṭivirato hoti."
msgstr ""

msgctxt "an3.120:1.6"
msgid "Idaṃ vuccati, bhikkhave, kāyasoceyyaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.120:2.1"
msgid "Katamañca, bhikkhave, vacīsoceyyaṃ?"
msgstr ""

msgctxt "an3.120:2.2"
msgid ""
"Idha, bhikkhave, ekacco musāvādā paṭivirato hoti, pisuṇāya vācāya paṭivirato "
"hoti, pharusāya vācāya paṭivirato hoti, samphappalāpā paṭivirato hoti."
msgstr ""

msgctxt "an3.120:2.3"
msgid "Idaṃ vuccati, bhikkhave, vacīsoceyyaṃ."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.272"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "an3.120:3.1"
msgid "Katamañca, bhikkhave, manosoceyyaṃ?"
msgstr ""

msgctxt "an3.120:3.2"
msgid "Idha, bhikkhave, ekacco anabhijjhālu hoti abyāpannacitto sammādiṭṭhiko."
msgstr ""

msgctxt "an3.120:3.3"
msgid "Idaṃ vuccati, bhikkhave, manosoceyyaṃ."
msgstr ""

msgctxt "an3.120:3.4"
msgid "Imāni kho, bhikkhave, tīṇi soceyyānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.120:3.5"
msgid "Aṭṭhamaṃ."
msgstr ""
