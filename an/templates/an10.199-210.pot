#. <section class="sutta" id="an10.199-210"><article><div class="hgroup"><p class="division">
msgctxt "an10.199-210:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
#. VAR: 20. Aparapuggalavagga → sevitabbāsevitabbavaggo (s1-3)
msgctxt "an10.199-210:0.2"
msgid "20. Aparapuggalavagga"
msgstr ""

#. </p><h1>
#. VAR: 199–​210. Nasevitabbādisutta → sevitabbādidvādasasuttāni (bj)
msgctxt "an10.199-210:0.3"
msgid "199–​210. Nasevitabbādisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.199-210:1.1"
msgid "“Dasahi, bhikkhave, dhammehi samannāgato puggalo na sevitabbo."
msgstr ""

msgctxt "an10.199-210:1.2"
msgid "Katamehi dasahi?"
msgstr ""

msgctxt "an10.199-210:1.3"
msgid ""
"Pāṇātipātī hoti, adinnādāyī hoti, kāmesumicchācārī hoti, musāvādī hoti, "
"pisuṇavāco hoti, pharusavāco hoti, samphappalāpī hoti, abhijjhālu hoti, "
"byāpannacitto hoti, micchādiṭṭhiko hoti—"
msgstr ""

msgctxt "an10.199-210:1.4"
msgid "imehi kho, bhikkhave, dasahi dhammehi samannāgato puggalo na sevitabbo."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.282"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.199-210:2.1"
msgid "Dasahi, bhikkhave, dhammehi samannāgato puggalo sevitabbo."
msgstr ""

msgctxt "an10.199-210:2.2"
msgid "Katamehi dasahi?"
msgstr ""

msgctxt "an10.199-210:2.3"
msgid ""
"Pāṇātipātā paṭivirato hoti, adinnādānā paṭivirato hoti, kāmesumicchācārā "
"paṭivirato hoti, musāvādā paṭivirato hoti, pisuṇāya vācāya paṭivirato hoti, "
"pharusāya vācāya paṭivirato hoti, samphappalāpā paṭivirato hoti, "
"anabhijjhālu hoti, abyāpannacitto hoti, sammādiṭṭhiko hoti—"
msgstr ""

msgctxt "an10.199-210:2.4"
msgid ""
"imehi kho, bhikkhave, dasahi dhammehi samannāgato puggalo sevitabbo. (1)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an10.199-210:3.1"
msgid "Dasahi, bhikkhave, dhammehi samannāgato puggalo na bhajitabbo … pe …"
msgstr ""

msgctxt "an10.199-210:3.2"
msgid "bhajitabbo …"
msgstr ""

msgctxt "an10.199-210:3.3"
msgid "na payirupāsitabbo …"
msgstr ""

msgctxt "an10.199-210:3.4"
msgid "payirupāsitabbo …"
msgstr ""

msgctxt "an10.199-210:3.5"
msgid "na pujjo hoti …"
msgstr ""

msgctxt "an10.199-210:3.6"
msgid "pujjo hoti …"
msgstr ""

msgctxt "an10.199-210:3.7"
msgid "na pāsaṃso hoti …"
msgstr ""

msgctxt "an10.199-210:3.8"
msgid "pāsaṃso hoti …"
msgstr ""

msgctxt "an10.199-210:3.9"
msgid "agāravo hoti …"
msgstr ""

msgctxt "an10.199-210:3.10"
msgid "gāravo hoti …"
msgstr ""

msgctxt "an10.199-210:3.11"
msgid "appatisso hoti …"
msgstr ""

msgctxt "an10.199-210:3.12"
msgid "sappatisso hoti …"
msgstr ""

msgctxt "an10.199-210:3.13"
msgid "na ārādhako hoti …"
msgstr ""

msgctxt "an10.199-210:3.14"
msgid "ārādhako hoti …"
msgstr ""

msgctxt "an10.199-210:3.15"
msgid "na visujjhati …"
msgstr ""

msgctxt "an10.199-210:3.16"
msgid "visujjhati …"
msgstr ""

#. VAR: nādhibhoti → nābhibhoti (bj)
msgctxt "an10.199-210:3.17"
msgid "mānaṃ nādhibhoti …"
msgstr ""

msgctxt "an10.199-210:3.18"
msgid "mānaṃ adhibhoti …"
msgstr ""

msgctxt "an10.199-210:3.19"
msgid "paññāya na vaḍḍhati …"
msgstr ""

msgctxt "an10.199-210:3.20"
msgid "paññāya vaḍḍhati … pe …. (2–11.)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an10.199-210:4.1"
msgid ""
"Dasahi, bhikkhave, dhammehi samannāgato puggalo bahuṃ apuññaṃ pasavati …"
msgstr ""

msgctxt "an10.199-210:4.2"
msgid "bahuṃ puññaṃ pasavati."
msgstr ""

msgctxt "an10.199-210:4.3"
msgid "Katamehi dasahi?"
msgstr ""

msgctxt "an10.199-210:4.4"
msgid ""
"Pāṇātipātā paṭivirato hoti, adinnādānā paṭivirato hoti, kāmesumicchācārā "
"paṭivirato hoti, musāvādā paṭivirato hoti, pisuṇāya vācāya paṭivirato hoti, "
"pharusāya vācāya paṭivirato hoti, samphappalāpā paṭivirato hoti, "
"anabhijjhālu hoti, abyāpannacitto hoti, sammādiṭṭhiko hoti—"
msgstr ""

msgctxt "an10.199-210:4.5"
msgid ""
"imehi kho, bhikkhave, dasahi dhammehi samannāgato puggalo bahuṃ puññaṃ "
"pasavatī”ti. (12)"
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.199-210:4.6"
msgid "Aparapuggalavaggo pañcamo."
msgstr ""

#. </p><p class="endsutta">
msgctxt "an10.199-210:4.7"
msgid "Catuttho paṇṇāsako samatto."
msgstr ""
