#. <section class="sutta" id="an4.120"><article><div class="hgroup"><p class="division">
msgctxt "an4.120:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.120:0.2"
msgid "12. Kesivagga"
msgstr ""

#. </p><h1>
msgctxt "an4.120:0.3"
msgid "120. Dutiyabhayasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.120:1.1"
msgid "“Cattārimāni, bhikkhave, bhayāni."
msgstr ""

msgctxt "an4.120:1.2"
msgid "Katamāni cattāri?"
msgstr ""

msgctxt "an4.120:1.3"
msgid "Aggibhayaṃ, udakabhayaṃ, rājabhayaṃ, corabhayaṃ—"
msgstr ""

msgctxt "an4.120:1.4"
msgid "imāni kho, bhikkhave, cattāri bhayānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.120:1.5"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.120:1.6"
msgid "Kesivaggo dutiyo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.120:2.1"
msgid "Kesi javo patodo ca,"
msgstr ""

#. <br>
msgctxt "an4.120:2.2"
msgid "Nāgo ṭhānena pañcamaṃ;"
msgstr ""

#. <br>
msgctxt "an4.120:2.3"
msgid "Appamādo ca ārakkho,"
msgstr ""

#. <br>
msgctxt "an4.120:2.4"
msgid "Saṃvejanīyañca dve bhayāti."
msgstr ""
