#. <section class="sutta" id="an4.175"><article><div class="hgroup"><p class="division">
msgctxt "an4.175:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.175:0.2"
msgid "18. Sañcetaniyavagga"
msgstr ""

#. </p><h1>
#. VAR: upavānasuttaṃ (bj)
msgctxt "an4.175:0.3"
msgid "175. Upavāṇasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. VAR: upavāṇo → upavāno (bj)
msgctxt "an4.175:1.1"
msgid ""
"Atha kho āyasmā upavāṇo yenāyasmā sāriputto tenupasaṅkami; upasaṅkamitvā "
"āyasmatā sāriputtena saddhiṃ sammodi."
msgstr ""

msgctxt "an4.175:1.2"
msgid ""
"Sammodanīyaṃ kathaṃ sāraṇīyaṃ vītisāretvā ekamantaṃ nisīdi. Ekamantaṃ "
"nisinno kho āyasmā upavāṇo āyasmantaṃ sāriputtaṃ etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.175:2.1"
msgid "“Kiṃ nu kho, āvuso sāriputta, vijjāyantakaro hotī”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.175:3.1"
msgid "“No hidaṃ, āvuso”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.175:4.1"
msgid "“Kiṃ panāvuso sāriputta, caraṇenantakaro hotī”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.175:5.1"
msgid "“No hidaṃ, āvuso”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an4.175:6.1"
msgid "“Kiṃ panāvuso sāriputta, vijjācaraṇenantakaro hotī”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an4.175:7.1"
msgid "“No hidaṃ, āvuso”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an4.175:8.1"
msgid "“Kiṃ panāvuso sāriputta, aññatra vijjācaraṇenantakaro hotī”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an4.175:9.1"
msgid "“No hidaṃ, āvuso”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "an4.175:10.1"
msgid ""
"“‘Kiṃ nu kho, āvuso sāriputta, vijjāyantakaro hotī’ti, iti puṭṭho samāno:"
msgstr ""

msgctxt "an4.175:10.2"
msgid "‘no hidaṃ, āvuso’ti vadesi."
msgstr ""

msgctxt "an4.175:10.3"
msgid "‘Kiṃ panāvuso sāriputta, caraṇenantakaro hotī’ti, iti puṭṭho samāno:"
msgstr ""

msgctxt "an4.175:10.4"
msgid "‘no hidaṃ, āvuso’ti vadesi."
msgstr ""

msgctxt "an4.175:10.5"
msgid ""
"‘Kiṃ panāvuso sāriputta, vijjācaraṇenantakaro hotī’ti, iti puṭṭho samāno:"
msgstr ""

msgctxt "an4.175:10.6"
msgid "‘no hidaṃ, āvuso’ti vadesi."
msgstr ""

msgctxt "an4.175:10.7"
msgid ""
"‘Kiṃ panāvuso sāriputta, aññatra vijjācaraṇenantakaro hotī’ti, iti puṭṭho "
"samāno:"
msgstr ""

msgctxt "an4.175:10.8"
msgid "‘no hidaṃ, āvuso’ti vadesi."
msgstr ""

msgctxt "an4.175:10.9"
msgid "Yathā kathaṃ panāvuso, antakaro hotī”ti?"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.164"></a>
#. <a class="sc" id="sc11"></a>
msgctxt "an4.175:11.1"
msgid ""
"“Vijjāya ce, āvuso, antakaro abhavissa, saupādānova samāno antakaro "
"abhavissa."
msgstr ""

msgctxt "an4.175:11.2"
msgid ""
"Caraṇena ce, āvuso, antakaro abhavissa, saupādānova samāno antakaro "
"abhavissa."
msgstr ""

msgctxt "an4.175:11.3"
msgid ""
"Vijjācaraṇena ce, āvuso, antakaro abhavissa, saupādānova samāno antakaro "
"abhavissa."
msgstr ""

msgctxt "an4.175:11.4"
msgid ""
"Aññatra vijjācaraṇena ce, āvuso, antakaro abhavissa, puthujjano antakaro "
"abhavissa."
msgstr ""

msgctxt "an4.175:11.5"
msgid "Puthujjano hi, āvuso, aññatra vijjācaraṇena."
msgstr ""

msgctxt "an4.175:11.6"
msgid "Caraṇavipanno kho, āvuso, yathābhūtaṃ na jānāti na passati."
msgstr ""

msgctxt "an4.175:11.7"
msgid "Caraṇasampanno yathābhūtaṃ jānāti passati."
msgstr ""

msgctxt "an4.175:11.8"
msgid "Yathābhūtaṃ jānaṃ passaṃ antakaro hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.175:11.9"
msgid "Pañcamaṃ."
msgstr ""
