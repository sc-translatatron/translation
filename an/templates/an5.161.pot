#. <section class="sutta" id="an5.161"><article><div class="hgroup"><p class="division">
msgctxt "an5.161:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.161:0.2"
msgid "17. Āghātavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.161:0.3"
msgid "161. Paṭhamaāghātapaṭivinayasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.186"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.161:1.1"
msgid ""
"“Pañcime, bhikkhave, āghātapaṭivinayā yattha bhikkhuno uppanno āghāto "
"sabbaso paṭivinetabbo."
msgstr ""

msgctxt "an5.161:1.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.161:1.3"
msgid ""
"Yasmiṃ, bhikkhave, puggale āghāto jāyetha, mettā tasmiṃ puggale bhāvetabbā;"
msgstr ""

msgctxt "an5.161:1.4"
msgid "evaṃ tasmiṃ puggale āghāto paṭivinetabbo."
msgstr ""

msgctxt "an5.161:1.5"
msgid ""
"Yasmiṃ, bhikkhave, puggale āghāto jāyetha, karuṇā tasmiṃ puggale bhāvetabbā;"
msgstr ""

msgctxt "an5.161:1.6"
msgid "evaṃ tasmiṃ puggale āghāto paṭivinetabbo."
msgstr ""

msgctxt "an5.161:1.7"
msgid ""
"Yasmiṃ, bhikkhave, puggale āghāto jāyetha, upekkhā tasmiṃ puggale bhāvetabbā;"
msgstr ""

msgctxt "an5.161:1.8"
msgid "evaṃ tasmiṃ puggale āghāto paṭivinetabbo."
msgstr ""

msgctxt "an5.161:1.9"
msgid ""
"Yasmiṃ, bhikkhave, puggale āghāto jāyetha, asatiamanasikāro tasmiṃ puggale "
"āpajjitabbo;"
msgstr ""

msgctxt "an5.161:1.10"
msgid "evaṃ tasmiṃ puggale āghāto paṭivinetabbo."
msgstr ""

msgctxt "an5.161:1.11"
msgid ""
"Yasmiṃ, bhikkhave, puggale āghāto jāyetha, kammassakatā tasmiṃ puggale "
"adhiṭṭhātabbā:"
msgstr ""

msgctxt "an5.161:1.12"
msgid ""
"‘kammassako ayamāyasmā kammadāyādo kammayoni kammabandhu kammapaṭisaraṇo, "
msgstr ""

msgctxt "an5.161:1.13"
msgid "yaṃ kammaṃ karissati kalyāṇaṃ vā pāpakaṃ vā tassa dāyādo bhavissatī’ti;"
msgstr ""

msgctxt "an5.161:1.14"
msgid "evaṃ tasmiṃ puggale āghāto paṭivinetabbo."
msgstr ""

msgctxt "an5.161:1.15"
msgid ""
"Ime kho, bhikkhave, pañca āghātapaṭivinayā, yattha bhikkhuno uppanno āghāto "
"sabbaso paṭivinetabbo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.161:1.16"
msgid "Paṭhamaṃ."
msgstr ""
