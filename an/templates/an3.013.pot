#. <section class="sutta" id="an3.13"><article><div class="hgroup"><p class="division">
msgctxt "an3.13:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.13:0.2"
msgid "2. Rathakāravagga"
msgstr ""

#. </p><h1>
msgctxt "an3.13:0.3"
msgid "13. Āsaṃsasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.13:1.1"
msgid "“Tayome, bhikkhave, puggalā santo saṃvijjamānā lokasmiṃ."
msgstr ""

msgctxt "an3.13:1.2"
msgid "Katame tayo?"
msgstr ""

msgctxt "an3.13:1.3"
msgid "Nirāso, āsaṃso, vigatāso."
msgstr ""

msgctxt "an3.13:1.4"
msgid "Katamo ca, bhikkhave puggalo nirāso?"
msgstr ""

#. VAR: venakule vā nesādakule vā → nesādakule vā veṇakule vā (sī, pā1), veṇakule vā nesādakule vā (syā1-3, kaṃ)
msgctxt "an3.13:1.5"
msgid ""
"Idha, bhikkhave, ekacco puggalo nīce kule paccājāto hoti, caṇḍālakule vā "
"venakule vā nesādakule vā rathakārakule vā pukkusakule vā dalidde "
"appannapānabhojane kasiravuttike, yattha kasirena ghāsacchādo labbhati."
msgstr ""

#. VAR: bavhābādho → bahvābādho (s1-3, km, pts1, mr)
msgctxt "an3.13:1.6"
msgid ""
"So ca hoti dubbaṇṇo duddasiko okoṭimako bavhābādho kāṇo vā kuṇī vā khañjo vā "
"pakkhahato vā, na lābhī annassa pānassa vatthassa yānassa "
"mālāgandhavilepanassa seyyāvasathapadīpeyyassa."
msgstr ""

msgctxt "an3.13:1.7"
msgid "So suṇāti:"
msgstr ""

msgctxt "an3.13:1.8"
msgid "‘itthannāmo kira khattiyo khattiyehi khattiyābhisekena abhisitto’ti."
msgstr ""

msgctxt "an3.13:1.9"
msgid "Tassa na evaṃ hoti:"
msgstr ""

msgctxt "an3.13:1.10"
msgid "‘kudāssu nāma mampi khattiyā khattiyābhisekena abhisiñcissantī’ti."
msgstr ""

msgctxt "an3.13:1.11"
msgid "Ayaṃ vuccati, bhikkhave, puggalo nirāso."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.108"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.13:2.1"
msgid "Katamo ca, bhikkhave, puggalo āsaṃso?"
msgstr ""

#. VAR: ābhiseko → abhisekena (s1-3) | abhiseko (pts1)
#. VAR: acalappatto → macalappatto (bj, s1-3) | m-acalapatto (pts1)
msgctxt "an3.13:2.2"
msgid ""
"Idha, bhikkhave, rañño khattiyassa muddhāvasittassa jeṭṭho putto hoti "
"ābhiseko anabhisitto acalappatto."
msgstr ""

msgctxt "an3.13:2.3"
msgid "So suṇāti:"
msgstr ""

msgctxt "an3.13:2.4"
msgid "‘itthannāmo kira khattiyo khattiyehi khattiyābhisekena abhisitto’ti."
msgstr ""

msgctxt "an3.13:2.5"
msgid "Tassa evaṃ hoti:"
msgstr ""

msgctxt "an3.13:2.6"
msgid "‘kudāssu nāma mampi khattiyā khattiyābhisekena abhisiñcissantī’ti."
msgstr ""

msgctxt "an3.13:2.7"
msgid "Ayaṃ vuccati, bhikkhave, puggalo āsaṃso."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an3.13:3.1"
msgid "Katamo ca, bhikkhave, puggalo vigatāso?"
msgstr ""

msgctxt "an3.13:3.2"
msgid "Idha, bhikkhave, rājā hoti khattiyo muddhāvasitto."
msgstr ""

msgctxt "an3.13:3.3"
msgid "So suṇāti:"
msgstr ""

msgctxt "an3.13:3.4"
msgid "‘itthannāmo kira khattiyo khattiyehi khattiyābhisekena abhisitto’ti."
msgstr ""

msgctxt "an3.13:3.5"
msgid "Tassa na evaṃ hoti:"
msgstr ""

msgctxt "an3.13:3.6"
msgid "‘kudāssu nāma mampi khattiyā khattiyābhisekena abhisiñcissantī’ti."
msgstr ""

msgctxt "an3.13:3.7"
msgid "Taṃ kissa hetu?"
msgstr ""

#. VAR: sā → sāssa (bj, s1-3, km, pts1)
msgctxt "an3.13:3.8"
msgid "Yā hissa, bhikkhave, pubbe anabhisittassa abhisekāsā sā paṭippassaddhā."
msgstr ""

msgctxt "an3.13:3.9"
msgid "Ayaṃ vuccati, bhikkhave, puggalo vigatāso."
msgstr ""

msgctxt "an3.13:3.10"
msgid "Ime kho, bhikkhave, tayo puggalā santo saṃvijjamānā lokasmiṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an3.13:4.1"
msgid "Evamevaṃ kho, bhikkhave, tayo puggalā santo saṃvijjamānā bhikkhūsu."
msgstr ""

msgctxt "an3.13:4.2"
msgid "Katame tayo?"
msgstr ""

msgctxt "an3.13:4.3"
msgid "Nirāso, āsaṃso, vigatāso."
msgstr ""

msgctxt "an3.13:4.4"
msgid "Katamo ca, bhikkhave, puggalo nirāso?"
msgstr ""

msgctxt "an3.13:4.5"
msgid ""
"Idha, bhikkhave, ekacco puggalo dussīlo hoti pāpadhammo asuci "
"saṅkassarasamācāro paṭicchannakammanto assamaṇo samaṇapaṭiñño abrahmacārī "
"brahmacāripaṭiñño antopūti avassuto kasambujāto."
msgstr ""

msgctxt "an3.13:4.6"
msgid "So suṇāti:"
msgstr ""

msgctxt "an3.13:4.7"
msgid ""
"‘itthannāmo kira bhikkhu āsavānaṃ khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ "
"diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharatī’ti."
msgstr ""

msgctxt "an3.13:4.8"
msgid "Tassa na evaṃ hoti:"
msgstr ""

msgctxt "an3.13:4.9"
msgid ""
"‘kudāssu nāma ahampi āsavānaṃ khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ "
"diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharissāmī’ti."
msgstr ""

msgctxt "an3.13:4.10"
msgid "Ayaṃ vuccati, bhikkhave, puggalo nirāso."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.109"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "an3.13:5.1"
msgid "Katamo ca, bhikkhave, puggalo āsaṃso?"
msgstr ""

msgctxt "an3.13:5.2"
msgid "Idha, bhikkhave, bhikkhu sīlavā hoti kalyāṇadhammo."
msgstr ""

msgctxt "an3.13:5.3"
msgid "So suṇāti:"
msgstr ""

msgctxt "an3.13:5.4"
msgid ""
"‘itthannāmo kira bhikkhu āsavānaṃ khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ "
"diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharatī’ti."
msgstr ""

msgctxt "an3.13:5.5"
msgid "Tassa evaṃ hoti:"
msgstr ""

msgctxt "an3.13:5.6"
msgid ""
"‘kudāssu nāma ahampi āsavānaṃ khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ "
"diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharissāmī’ti."
msgstr ""

msgctxt "an3.13:5.7"
msgid "Ayaṃ vuccati, bhikkhave, puggalo āsaṃso."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an3.13:6.1"
msgid "Katamo ca, bhikkhave, puggalo vigatāso?"
msgstr ""

msgctxt "an3.13:6.2"
msgid "Idha, bhikkhave, bhikkhu arahaṃ hoti khīṇāsavo."
msgstr ""

msgctxt "an3.13:6.3"
msgid "So suṇāti:"
msgstr ""

msgctxt "an3.13:6.4"
msgid ""
"‘itthannāmo kira bhikkhu āsavānaṃ khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ "
"diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharatī’ti."
msgstr ""

msgctxt "an3.13:6.5"
msgid "Tassa na evaṃ hoti:"
msgstr ""

msgctxt "an3.13:6.6"
msgid ""
"‘kudāssu nāma ahampi āsavānaṃ khayā … pe … sacchikatvā upasampajja "
"viharissāmī’ti."
msgstr ""

msgctxt "an3.13:6.7"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "an3.13:6.8"
msgid "Yā hissa, bhikkhave, pubbe avimuttassa vimuttāsā sā paṭippassaddhā."
msgstr ""

msgctxt "an3.13:6.9"
msgid "Ayaṃ vuccati, bhikkhave, puggalo vigatāso."
msgstr ""

msgctxt "an3.13:6.10"
msgid "Ime kho, bhikkhave, tayo puggalā santo saṃvijjamānā bhikkhūsū”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.13:6.11"
msgid "Tatiyaṃ."
msgstr ""
