#. <section class="sutta" id="an5.150"><article><div class="hgroup"><p class="division">
msgctxt "an5.150:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.150:0.2"
msgid "15. Tikaṇḍakīvagga"
msgstr ""

#. </p><h1>
msgctxt "an5.150:0.3"
msgid "150. Dutiyasamayavimuttasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.150:1.1"
msgid ""
"“Pañcime, bhikkhave, dhammā samayavimuttassa bhikkhuno parihānāya "
"saṃvattanti."
msgstr ""

msgctxt "an5.150:1.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.150:1.3"
msgid ""
"Kammārāmatā, bhassārāmatā, niddārāmatā, indriyesu aguttadvāratā, bhojane "
"amattaññutā."
msgstr ""

msgctxt "an5.150:1.4"
msgid ""
"Ime kho, bhikkhave, pañca dhammā samayavimuttassa bhikkhuno parihānāya "
"saṃvattanti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.174"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.150:2.1"
msgid ""
"Pañcime, bhikkhave, dhammā samayavimuttassa bhikkhuno aparihānāya "
"saṃvattanti."
msgstr ""

msgctxt "an5.150:2.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.150:2.3"
msgid ""
"Na kammārāmatā, na bhassārāmatā, na niddārāmatā, indriyesu guttadvāratā, "
"bhojane mattaññutā."
msgstr ""

msgctxt "an5.150:2.4"
msgid ""
"Ime kho, bhikkhave, pañca dhammā samayavimuttassa bhikkhuno aparihānāya "
"saṃvattantī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.150:2.5"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.150:2.6"
msgid "Tikaṇḍakīvaggo pañcamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.150:3.1"
msgid "Datvā avajānāti ārabhati ca,"
msgstr ""

#. <br>
msgctxt "an5.150:3.2"
msgid "Sārandada tikaṇḍa nirayena ca;"
msgstr ""

#. <br>
msgctxt "an5.150:3.3"
msgid "Mitto asappurisasappurisena,"
msgstr ""

#. <br>
msgctxt "an5.150:3.4"
msgid "Samayavimuttaṃ apare dveti."
msgstr ""

#. </p></blockquote><p class="endsutta">
msgctxt "an5.150:3.5"
msgid "Tatiyo paṇṇāsako samatto."
msgstr ""
