#. <section class="sutta" id="an5.171"><article><div class="hgroup"><p class="division">
msgctxt "an5.171:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.171:0.2"
msgid "18. Upāsakavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.171:0.3"
msgid "171. Sārajjasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "an5.171:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "an5.171:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "an5.171:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "an5.171:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "an5.171:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "an5.171:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.171:2.1"
msgid ""
"“Pañcahi, bhikkhave, dhammehi samannāgato upāsako sārajjaṃ okkanto hoti."
msgstr ""

msgctxt "an5.171:2.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.171:2.3"
msgid ""
"Pāṇātipātī hoti, adinnādāyī hoti, kāmesumicchācārī hoti, musāvādī hoti, "
"surāmerayamajjapamādaṭṭhāyī hoti."
msgstr ""

msgctxt "an5.171:2.4"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgato upāsako sārajjaṃ okkanto "
"hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.171:3.1"
msgid "Pañcahi, bhikkhave, dhammehi samannāgato upāsako visārado hoti."
msgstr ""

msgctxt "an5.171:3.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.171:3.3"
msgid ""
"Pāṇātipātā paṭivirato hoti, adinnādānā paṭivirato hoti, kāmesumicchācārā "
"paṭivirato hoti, musāvādā paṭivirato hoti, surāmerayamajjapamādaṭṭhānā "
"paṭivirato hoti."
msgstr ""

msgctxt "an5.171:3.4"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgato upāsako visārado hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.171:3.5"
msgid "Paṭhamaṃ."
msgstr ""
