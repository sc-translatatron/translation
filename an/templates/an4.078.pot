#. <section class="sutta" id="an4.78"><article><div class="hgroup"><p class="division">
msgctxt "an4.78:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.78:0.2"
msgid "8. Apaṇṇakavagga"
msgstr ""

#. </p><h1>
#. VAR: dakkhiṇāvisuddhisuttaṃ (bj)
msgctxt "an4.78:0.3"
msgid "78. Dakkhiṇasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.78:1.1"
msgid "“Catasso imā, bhikkhave, dakkhiṇā visuddhiyo."
msgstr ""

msgctxt "an4.78:1.2"
msgid "Katamā catasso?"
msgstr ""

msgctxt "an4.78:1.3"
msgid "Atthi, bhikkhave, dakkhiṇā dāyakato visujjhati, no paṭiggāhakato;"
msgstr ""

msgctxt "an4.78:1.4"
msgid "atthi, bhikkhave, dakkhiṇā paṭiggāhakato visujjhati, no dāyakato;"
msgstr ""

msgctxt "an4.78:1.5"
msgid "atthi, bhikkhave, dakkhiṇā neva dāyakato visujjhati, no paṭiggāhakato;"
msgstr ""

msgctxt "an4.78:1.6"
msgid "atthi, bhikkhave, dakkhiṇā dāyakato ceva visujjhati paṭiggāhakato ca."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.81"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.78:2.1"
msgid "Kathañca, bhikkhave, dakkhiṇā dāyakato visujjhati, no paṭiggāhakato?"
msgstr ""

#. VAR: paṭiggāhakā honti dussīlā pāpadhammā → paṭiggāhako hoti dussīlo pāpadhammo (s1-3, km, mr)
msgctxt "an4.78:2.2"
msgid ""
"Idha, bhikkhave, dāyako hoti sīlavā kalyāṇadhammo; paṭiggāhakā honti dussīlā "
"pāpadhammā."
msgstr ""

msgctxt "an4.78:2.3"
msgid "Evaṃ kho, bhikkhave, dakkhiṇā dāyakato visujjhati, no paṭiggāhakato."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.78:3.1"
msgid "Kathañca, bhikkhave, dakkhiṇā paṭiggāhakato visujjhati, no dāyakato?"
msgstr ""

msgctxt "an4.78:3.2"
msgid ""
"Idha, bhikkhave, dāyako hoti dussīlo pāpadhammo; paṭiggāhakā honti sīlavanto "
"kalyāṇadhammā."
msgstr ""

msgctxt "an4.78:3.3"
msgid "Evaṃ kho, bhikkhave, dakkhiṇā paṭiggāhakato visujjhati, no dāyakato."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.78:4.1"
msgid ""
"Kathañca, bhikkhave, dakkhiṇā neva dāyakato visujjhati, no paṭiggāhakato?"
msgstr ""

msgctxt "an4.78:4.2"
msgid ""
"Idha, bhikkhave, dāyako hoti dussīlo pāpadhammo; paṭiggāhakāpi honti dussīlā "
"pāpadhammā."
msgstr ""

msgctxt "an4.78:4.3"
msgid ""
"Evaṃ kho, bhikkhave, dakkhiṇā neva dāyakato visujjhati, no paṭiggāhakato."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.78:5.1"
msgid ""
"Kathañca, bhikkhave, dakkhiṇā dāyakato ceva visujjhati paṭiggāhakato ca?"
msgstr ""

msgctxt "an4.78:5.2"
msgid ""
"Idha, bhikkhave, dāyako hoti sīlavā kalyāṇadhammo; paṭiggāhakāpi honti "
"sīlavanto kalyāṇadhammā."
msgstr ""

msgctxt "an4.78:5.3"
msgid ""
"Evaṃ kho, bhikkhave, dakkhiṇā dāyakato ceva visujjhati paṭiggāhakato ca."
msgstr ""

msgctxt "an4.78:5.4"
msgid "Imā kho, bhikkhave, catasso dakkhiṇā visuddhiyo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.78:5.5"
msgid "Aṭṭhamaṃ."
msgstr ""
