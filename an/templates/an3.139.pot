#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.139:1.1"
msgid "“Tisso imā, bhikkhave, vuddhiyo."
msgstr ""

msgctxt "an3.139:1.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "an3.139:1.3"
msgid "Saddhāvuddhi, sīlavuddhi, paññāvuddhi—"
msgstr ""

msgctxt "an3.139:1.4"
msgid "imā kho, bhikkhave, tisso vuddhiyo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.139:1.5"
msgid "Sattamaṃ."
msgstr ""
