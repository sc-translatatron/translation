msgid "Evamevaṃ kho, āvuso, ñāṇavādañca bhikkhu vadamāno bhāvanāvādañca:"
msgstr ""

msgctxt "an10.24:11.2"
msgid ""
"‘jānāmimaṃ dhammaṃ, passāmimaṃ dhammaṃ, bhāvitakāyomhi bhāvitasīlo "
"bhāvitacitto bhāvitapañño’ti."
msgstr ""

msgctxt "an10.24:11.3"
msgid "Tañce, āvuso, bhikkhuṃ lobho nābhibhuyya tiṭṭhati, "
msgstr ""

msgctxt "an10.24:11.4"
msgid "doso …"
msgstr ""

msgctxt "an10.24:11.5"
msgid "moho …"
msgstr ""

msgctxt "an10.24:11.6"
msgid "kodho …"
msgstr ""

msgctxt "an10.24:11.7"
msgid "upanāho …"
msgstr ""

msgctxt "an10.24:11.8"
msgid "makkho …"
msgstr ""

msgctxt "an10.24:11.9"
msgid "paḷāso …"
msgstr ""

msgctxt "an10.24:11.10"
msgid "macchariyaṃ …"
msgstr ""

msgctxt "an10.24:11.11"
msgid "pāpikā issā …"
msgstr ""

msgctxt "an10.24:11.12"
msgid "pāpikā icchā nābhibhuyya tiṭṭhati, so evamassa veditabbo:"
msgstr ""

msgctxt "an10.24:11.13"
msgid ""
"‘tathā ayamāyasmā pajānāti yathā pajānato lobho na hoti, tathāhimaṃ "
"āyasmantaṃ lobho nābhibhuyya tiṭṭhati;"
msgstr ""

msgctxt "an10.24:11.14"
msgid "tathā ayamāyasmā pajānāti yathā pajānato doso na hoti …"
msgstr ""

msgctxt "an10.24:11.15"
msgid "moho …"
msgstr ""

msgctxt "an10.24:11.16"
msgid "kodho …"
msgstr ""

msgctxt "an10.24:11.17"
msgid "upanāho …"
msgstr ""

msgctxt "an10.24:11.18"
msgid "makkho …"
msgstr ""

msgctxt "an10.24:11.19"
msgid "paḷāso …"
msgstr ""

msgctxt "an10.24:11.20"
msgid "macchariyaṃ …"
msgstr ""

msgctxt "an10.24:11.21"
msgid "pāpikā issā …"
msgstr ""

msgctxt "an10.24:11.22"
msgid ""
"pāpikā icchā na hoti, tathāhimaṃ āyasmantaṃ pāpikā icchā nābhibhuyya "
"tiṭṭhatī’”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.24:11.23"
msgid "Catutthaṃ."
msgstr ""
