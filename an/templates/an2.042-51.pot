#. <section id="vagga"><section class="sutta" data-uid="an2.42" id="42"><article><div class="hgroup"><p class="division">
msgctxt "an2.42-51:0.1"
msgid "Aṅguttara Nikāya 2"
msgstr ""

#. </p><p>
#. VAR: parisāvagga (pts1)
msgctxt "an2.42-51:0.2"
msgid "5. Parisavagga"
msgstr ""

#. </p><h1>
msgctxt "an2.42-51:0.3"
msgid "42"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.70"></a>
#. <a class="sc" id="sc42.1"></a>
msgctxt "an2.42-51:42.1.1"
msgid "“Dvemā, bhikkhave, parisā."
msgstr ""

msgctxt "an2.42-51:42.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.42-51:42.1.3"
msgid "Uttānā ca parisā gambhīrā ca parisā."
msgstr ""

msgctxt "an2.42-51:42.1.4"
msgid "Katamā ca, bhikkhave, uttānā parisā?"
msgstr ""

msgctxt "an2.42-51:42.1.5"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū uddhatā honti unnaḷā capalā "
"mukharā vikiṇṇavācā muṭṭhassatī asampajānā asamāhitā vibbhantacittā "
"pākatindriyā."
msgstr ""

msgctxt "an2.42-51:42.1.6"
msgid "Ayaṃ vuccati, bhikkhave, uttānā parisā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc42.2"></a>
msgctxt "an2.42-51:42.2.1"
msgid "Katamā ca, bhikkhave, gambhīrā parisā?"
msgstr ""

msgctxt "an2.42-51:42.2.2"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū anuddhatā honti anunnaḷā acapalā "
"amukharā avikiṇṇavācā upaṭṭhitassatī sampajānā samāhitā ekaggacittā "
"saṃvutindriyā."
msgstr ""

msgctxt "an2.42-51:42.2.3"
msgid "Ayaṃ vuccati, bhikkhave, gambhīrā parisā."
msgstr ""

msgctxt "an2.42-51:42.2.4"
msgid "Imā kho, bhikkhave, dve parisā."
msgstr ""

msgctxt "an2.42-51:42.2.5"
msgid ""
"Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ parisānaṃ yadidaṃ gambhīrā parisā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.43" id="43"><article><div class="hgroup"><h1>
msgctxt "an2.42-51:42.2.6"
msgid "43"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc43.1"></a>
msgctxt "an2.42-51:43.1.1"
msgid "“Dvemā, bhikkhave, parisā."
msgstr ""

msgctxt "an2.42-51:43.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.42-51:43.1.3"
msgid "Vaggā ca parisā samaggā ca parisā."
msgstr ""

msgctxt "an2.42-51:43.1.4"
msgid "Katamā ca, bhikkhave, vaggā parisā?"
msgstr ""

msgctxt "an2.42-51:43.1.5"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū bhaṇḍanajātā kalahajātā "
"vivādāpannā aññamaññaṃ mukhasattīhi vitudantā viharanti."
msgstr ""

msgctxt "an2.42-51:43.1.6"
msgid "Ayaṃ vuccati, bhikkhave, vaggā parisā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc43.2"></a>
msgctxt "an2.42-51:43.2.1"
msgid "Katamā ca, bhikkhave, samaggā parisā?"
msgstr ""

msgctxt "an2.42-51:43.2.2"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū samaggā sammodamānā avivadamānā "
"khīrodakībhūtā aññamaññaṃ piyacakkhūhi sampassantā viharanti."
msgstr ""

msgctxt "an2.42-51:43.2.3"
msgid "Ayaṃ vuccati, bhikkhave, samaggā parisā."
msgstr ""

msgctxt "an2.42-51:43.2.4"
msgid "Imā kho, bhikkhave, dve parisā."
msgstr ""

msgctxt "an2.42-51:43.2.5"
msgid ""
"Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ parisānaṃ yadidaṃ samaggā parisā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.44" id="44"><article><div class="hgroup"><h1>
msgctxt "an2.42-51:43.2.6"
msgid "44"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.71"></a>
#. <a class="sc" id="sc44.1"></a>
msgctxt "an2.42-51:44.1.1"
msgid "“Dvemā, bhikkhave, parisā."
msgstr ""

msgctxt "an2.42-51:44.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.42-51:44.1.3"
msgid "Anaggavatī ca parisā aggavatī ca parisā."
msgstr ""

msgctxt "an2.42-51:44.1.4"
msgid "Katamā ca, bhikkhave, anaggavatī parisā?"
msgstr ""

#. VAR: bāhulikā → bāhullikā (s1-3, km, mr)
msgctxt "an2.42-51:44.1.5"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ therā bhikkhū bāhulikā honti sāthalikā, "
"okkamane pubbaṅgamā, paviveke nikkhittadhurā, na vīriyaṃ ārabhanti "
"appattassa pattiyā anadhigatassa adhigamāya asacchikatassa sacchikiriyāya."
msgstr ""

msgctxt "an2.42-51:44.1.6"
msgid "Tesaṃ pacchimā janatā diṭṭhānugatiṃ āpajjati."
msgstr ""

msgctxt "an2.42-51:44.1.7"
msgid ""
"Sāpi hoti bāhulikā sāthalikā, okkamane pubbaṅgamā, paviveke nikkhittadhurā, "
"na vīriyaṃ ārabhati appattassa pattiyā anadhigatassa adhigamāya "
"asacchikatassa sacchikiriyāya."
msgstr ""

msgctxt "an2.42-51:44.1.8"
msgid "Ayaṃ vuccati, bhikkhave, anaggavatī parisā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc44.2"></a>
msgctxt "an2.42-51:44.2.1"
msgid "Katamā ca, bhikkhave, aggavatī parisā?"
msgstr ""

msgctxt "an2.42-51:44.2.2"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ therā bhikkhū na bāhulikā honti na "
"sāthalikā, okkamane nikkhittadhurā, paviveke pubbaṅgamā, vīriyaṃ ārabhanti "
"appattassa pattiyā anadhigatassa adhigamāya asacchikatassa sacchikiriyāya."
msgstr ""

msgctxt "an2.42-51:44.2.3"
msgid "Tesaṃ pacchimā janatā diṭṭhānugatiṃ āpajjati."
msgstr ""

msgctxt "an2.42-51:44.2.4"
msgid ""
"Sāpi hoti na bāhulikā na sāthalikā, okkamane nikkhittadhurā, paviveke "
"pubbaṅgamā, vīriyaṃ ārabhati appattassa pattiyā anadhigatassa adhigamāya "
"asacchikatassa sacchikiriyāya."
msgstr ""

msgctxt "an2.42-51:44.2.5"
msgid "Ayaṃ vuccati, bhikkhave, aggavatī parisā."
msgstr ""

msgctxt "an2.42-51:44.2.6"
msgid "Imā kho, bhikkhave, dve parisā."
msgstr ""

msgctxt "an2.42-51:44.2.7"
msgid ""
"Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ parisānaṃ yadidaṃ aggavatī parisā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.45" id="45"><article><div class="hgroup"><h1>
msgctxt "an2.42-51:44.2.8"
msgid "45"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc45.1"></a>
msgctxt "an2.42-51:45.1.1"
msgid "“Dvemā, bhikkhave, parisā."
msgstr ""

msgctxt "an2.42-51:45.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.42-51:45.1.3"
msgid "Anariyā ca parisā ariyā ca parisā."
msgstr ""

msgctxt "an2.42-51:45.1.4"
msgid "Katamā ca, bhikkhave, anariyā parisā?"
msgstr ""

msgctxt "an2.42-51:45.1.5"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū ‘idaṃ dukkhan’ti yathābhūtaṃ "
"nappajānanti, ‘ayaṃ dukkhasamudayo’ti yathābhūtaṃ nappajānanti, ‘ayaṃ "
"dukkhanirodho’ti yathābhūtaṃ nappajānanti, ‘ayaṃ dukkhanirodhagāminī "
"paṭipadā’ti yathābhūtaṃ nappajānanti."
msgstr ""

msgctxt "an2.42-51:45.1.6"
msgid "Ayaṃ vuccati, bhikkhave, anariyā parisā."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.72"></a>
#. <a class="sc" id="sc45.2"></a>
msgctxt "an2.42-51:45.2.1"
msgid "Katamā ca, bhikkhave, ariyā parisā?"
msgstr ""

msgctxt "an2.42-51:45.2.2"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū ‘idaṃ dukkhan’ti yathābhūtaṃ "
"pajānanti, ‘ayaṃ dukkhasamudayo’ti yathābhūtaṃ pajānanti, ‘ayaṃ "
"dukkhanirodho’ti yathābhūtaṃ pajānanti, ‘ayaṃ dukkhanirodhagāminī paṭipadā’"
"ti yathābhūtaṃ pajānanti."
msgstr ""

msgctxt "an2.42-51:45.2.3"
msgid "Ayaṃ vuccati, bhikkhave, ariyā parisā."
msgstr ""

msgctxt "an2.42-51:45.2.4"
msgid "Imā kho, bhikkhave, dve parisā."
msgstr ""

msgctxt "an2.42-51:45.2.5"
msgid "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ parisānaṃ yadidaṃ ariyā parisā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.46" id="46"><article><div class="hgroup"><h1>
msgctxt "an2.42-51:45.2.6"
msgid "46"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc46.1"></a>
msgctxt "an2.42-51:46.1.1"
msgid "“Dvemā, bhikkhave, parisā."
msgstr ""

msgctxt "an2.42-51:46.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.42-51:46.1.3"
msgid "Parisākasaṭo ca parisāmaṇḍo ca."
msgstr ""

msgctxt "an2.42-51:46.1.4"
msgid "Katamo ca, bhikkhave, parisākasaṭo?"
msgstr ""

msgctxt "an2.42-51:46.1.5"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū chandāgatiṃ gacchanti, dosāgatiṃ "
"gacchanti, mohāgatiṃ gacchanti, bhayāgatiṃ gacchanti."
msgstr ""

msgctxt "an2.42-51:46.1.6"
msgid "Ayaṃ vuccati, bhikkhave, parisākasaṭo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc46.2"></a>
msgctxt "an2.42-51:46.2.1"
msgid "Katamo ca, bhikkhave, parisāmaṇḍo?"
msgstr ""

msgctxt "an2.42-51:46.2.2"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū na chandāgatiṃ gacchanti, na "
"dosāgatiṃ gacchanti, na mohāgatiṃ gacchanti, na bhayāgatiṃ gacchanti."
msgstr ""

msgctxt "an2.42-51:46.2.3"
msgid "Ayaṃ vuccati, bhikkhave, parisāmaṇḍo."
msgstr ""

msgctxt "an2.42-51:46.2.4"
msgid "Imā kho, bhikkhave, dve parisā."
msgstr ""

msgctxt "an2.42-51:46.2.5"
msgid "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ parisānaṃ yadidaṃ parisāmaṇḍo”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.47" id="47"><article><div class="hgroup"><h1>
msgctxt "an2.42-51:46.2.6"
msgid "47"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.73"></a>
#. <a class="sc" id="sc47.1"></a>
msgctxt "an2.42-51:47.1.1"
msgid "“Dvemā, bhikkhave, parisā."
msgstr ""

msgctxt "an2.42-51:47.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.42-51:47.1.3"
msgid ""
"Okkācitavinītā parisā nopaṭipucchāvinītā, paṭipucchāvinītā parisā "
"nookkācitavinītā."
msgstr ""

msgctxt "an2.42-51:47.1.4"
msgid "Katamā ca, bhikkhave, okkācitavinītā parisā nopaṭipucchāvinītā?"
msgstr ""

msgctxt "an2.42-51:47.1.5"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū ye te suttantā tathāgatabhāsitā "
"gambhīrā gambhīratthā lokuttarā suññatāpaṭisaṃyuttā tesu bhaññamānesu na "
"sussūsanti na sotaṃ odahanti na aññā cittaṃ upaṭṭhapenti na ca te dhamme "
"uggahetabbaṃ pariyāpuṇitabbaṃ maññanti."
msgstr ""

#. VAR: kavitā → kavikatā (sabbattha)
msgctxt "an2.42-51:47.1.6"
msgid ""
"Ye pana te suttantā kavitā kāveyyā cittakkharā cittabyañjanā bāhirakā "
"sāvakabhāsitā tesu bhaññamānesu sussūsanti sotaṃ odahanti aññā cittaṃ "
"upaṭṭhapenti, te dhamme uggahetabbaṃ pariyāpuṇitabbaṃ maññanti, te ca taṃ "
"dhammaṃ pariyāpuṇitvā na ceva aññamaññaṃ paṭipucchanti na ca paṭivicaranti:"
msgstr ""

msgctxt "an2.42-51:47.1.7"
msgid "‘idaṃ kathaṃ, imassa ko attho’ti?"
msgstr ""

msgctxt "an2.42-51:47.1.8"
msgid ""
"Te avivaṭañceva na vivaranti, anuttānīkatañca na uttānīkaronti, "
"anekavihitesu ca kaṅkhāṭhāniyesu dhammesu kaṅkhaṃ na paṭivinodenti."
msgstr ""

msgctxt "an2.42-51:47.1.9"
msgid "Ayaṃ vuccati, bhikkhave, okkācitavinītā parisā no paṭipucchāvinītā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc47.2"></a>
msgctxt "an2.42-51:47.2.1"
msgid "Katamā ca, bhikkhave, paṭipucchāvinītā parisā nookkācitavinītā?"
msgstr ""

msgctxt "an2.42-51:47.2.2"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū ye te suttantā kavitā kāveyyā "
"cittakkharā cittabyañjanā bāhirakā sāvakabhāsitā tesu bhaññamānesu na "
"sussūsanti na sotaṃ odahanti na aññā cittaṃ upaṭṭhapenti, na ca te dhamme "
"uggahetabbaṃ pariyāpuṇitabbaṃ maññanti."
msgstr ""

msgctxt "an2.42-51:47.2.3"
msgid ""
"Ye pana te suttantā tathāgatabhāsitā gambhīrā gambhīratthā lokuttarā "
"suññatāpaṭisaṃyuttā tesu bhaññamānesu sussūsanti sotaṃ odahanti aññā cittaṃ "
"upaṭṭhapenti, te ca dhamme uggahetabbaṃ pariyāpuṇitabbaṃ maññanti."
msgstr ""

msgctxt "an2.42-51:47.2.4"
msgid "Te taṃ dhammaṃ pariyāpuṇitvā aññamaññaṃ paṭipucchanti paṭivicaranti:"
msgstr ""

msgctxt "an2.42-51:47.2.5"
msgid "‘idaṃ kathaṃ, imassa ko attho’ti?"
msgstr ""

msgctxt "an2.42-51:47.2.6"
msgid ""
"Te avivaṭañceva vivaranti, anuttānīkatañca uttānīkaronti, anekavihitesu ca "
"kaṅkhāṭhāniyesu dhammesu kaṅkhaṃ paṭivinodenti."
msgstr ""

msgctxt "an2.42-51:47.2.7"
msgid "Ayaṃ vuccati, bhikkhave, paṭipucchāvinītā parisā nookkācitavinītā."
msgstr ""

msgctxt "an2.42-51:47.2.8"
msgid "Imā kho, bhikkhave, dve parisā."
msgstr ""

msgctxt "an2.42-51:47.2.9"
msgid ""
"Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ parisānaṃ yadidaṃ paṭipucchāvinītā "
"parisā nookkācitavinītā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.48" id="48"><article><div class="hgroup"><h1>
msgctxt "an2.42-51:47.2.10"
msgid "48"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.74"></a>
#. <a class="sc" id="sc48.1"></a>
msgctxt "an2.42-51:48.1.1"
msgid "“Dvemā, bhikkhave, parisā."
msgstr ""

msgctxt "an2.42-51:48.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.42-51:48.1.3"
msgid "Āmisagaru parisā no saddhammagaru, saddhammagaru parisā no āmisagaru."
msgstr ""

msgctxt "an2.42-51:48.1.4"
msgid "Katamā ca, bhikkhave, āmisagaru parisā no saddhammagaru?"
msgstr ""

msgctxt "an2.42-51:48.1.5"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū gihīnaṃ odātavasanānaṃ sammukhā "
"aññamaññassa vaṇṇaṃ bhāsanti:"
msgstr ""

msgctxt "an2.42-51:48.1.6"
msgid ""
"‘asuko bhikkhu ubhatobhāgavimutto, asuko paññāvimutto, asuko kāyasakkhī, "
"asuko diṭṭhippatto, asuko saddhāvimutto, asuko dhammānusārī, asuko "
"saddhānusārī, asuko sīlavā kalyāṇadhammo, asuko dussīlo pāpadhammo’ti."
msgstr ""

msgctxt "an2.42-51:48.1.7"
msgid "Te tena lābhaṃ labhanti."
msgstr ""

#. VAR: gathitā → gadhitā (s1-3, mr)
#. VAR: ajjhopannā → ajjhosānā (mr)
msgctxt "an2.42-51:48.1.8"
msgid ""
"Te taṃ lābhaṃ labhitvā gathitā mucchitā ajjhopannā anādīnavadassāvino "
"anissaraṇapaññā paribhuñjanti."
msgstr ""

msgctxt "an2.42-51:48.1.9"
msgid "Ayaṃ vuccati, bhikkhave, āmisagaru parisā no saddhammagaru."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc48.2"></a>
msgctxt "an2.42-51:48.2.1"
msgid "Katamā ca, bhikkhave, saddhammagaru parisā noāmisagaru?"
msgstr ""

msgctxt "an2.42-51:48.2.2"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū gihīnaṃ odātavasanānaṃ sammukhā "
"aññamaññassa vaṇṇaṃ na bhāsanti:"
msgstr ""

msgctxt "an2.42-51:48.2.3"
msgid ""
"‘asuko bhikkhu ubhatobhāgavimutto, asuko paññāvimutto, asuko kāyasakkhī, "
"asuko diṭṭhippatto, asuko saddhāvimutto, asuko dhammānusārī, asuko "
"saddhānusārī, asuko sīlavā kalyāṇadhammo, asuko dussīlo pāpadhammo’ti."
msgstr ""

msgctxt "an2.42-51:48.2.4"
msgid "Te tena lābhaṃ labhanti."
msgstr ""

msgctxt "an2.42-51:48.2.5"
msgid ""
"Te taṃ lābhaṃ labhitvā agathitā amucchitā anajjhosannā ādīnavadassāvino "
"nissaraṇapaññā paribhuñjanti."
msgstr ""

msgctxt "an2.42-51:48.2.6"
msgid "Ayaṃ vuccati, bhikkhave, saddhammagaru parisā noāmisagaru."
msgstr ""

msgctxt "an2.42-51:48.2.7"
msgid "Imā kho, bhikkhave, dve parisā."
msgstr ""

msgctxt "an2.42-51:48.2.8"
msgid ""
"Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ parisānaṃ yadidaṃ saddhammagaru parisā "
"noāmisagarū”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.49" id="49"><article><div class="hgroup"><h1>
msgctxt "an2.42-51:48.2.9"
msgid "49"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.75"></a>
#. <a class="sc" id="sc49.1"></a>
msgctxt "an2.42-51:49.1.1"
msgid "“Dvemā, bhikkhave, parisā."
msgstr ""

msgctxt "an2.42-51:49.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.42-51:49.1.3"
msgid "Visamā ca parisā samā ca parisā."
msgstr ""

msgctxt "an2.42-51:49.1.4"
msgid "Katamā ca, bhikkhave, visamā parisā?"
msgstr ""

msgctxt "an2.42-51:49.1.5"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ adhammakammāni pavattanti dhammakammāni "
"nappavattanti, avinayakammāni pavattanti vinayakammāni nappavattanti, "
"adhammakammāni dippanti dhammakammāni na dippanti, avinayakammāni dippanti "
"vinayakammāni na dippanti."
msgstr ""

#. VAR: () → (visamattā bhikkhave parisāya adhammakammāni pavattanti … vinayakammāni na dippanti.) (bj, s1-3, pts1)
msgctxt "an2.42-51:49.1.6"
msgid "Ayaṃ vuccati, bhikkhave, visamā parisā. ()"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc49.2"></a>
msgctxt "an2.42-51:49.2.1"
msgid "Katamā ca, bhikkhave, samā parisā?"
msgstr ""

msgctxt "an2.42-51:49.2.2"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ dhammakammāni pavattanti adhammakammāni "
"nappavattanti, vinayakammāni pavattanti avinayakammāni nappavattanti, "
"dhammakammāni dippanti adhammakammāni na dippanti, vinayakammāni dippanti "
"avinayakammāni na dippanti."
msgstr ""

#. VAR: () → (samattā bhikkhave parisāya dhammakammāni pavattanti … avinayakammāni na dippanti.) (bj, s1-3, pts1)
msgctxt "an2.42-51:49.2.3"
msgid "Ayaṃ vuccati, bhikkhave, samā parisā. ()"
msgstr ""

msgctxt "an2.42-51:49.2.4"
msgid "Imā kho, bhikkhave, dve parisā."
msgstr ""

msgctxt "an2.42-51:49.2.5"
msgid "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ parisānaṃ yadidaṃ samā parisā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.50" id="50"><article><div class="hgroup"><h1>
msgctxt "an2.42-51:49.2.6"
msgid "50"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc50.1"></a>
msgctxt "an2.42-51:50.1.1"
msgid "“Dvemā, bhikkhave, parisā."
msgstr ""

msgctxt "an2.42-51:50.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.42-51:50.1.3"
msgid ""
"Adhammikā ca parisā dhammikā ca parisā … pe … imā kho, bhikkhave, dve parisā."
msgstr ""

msgctxt "an2.42-51:50.1.4"
msgid ""
"Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ parisānaṃ yadidaṃ dhammikā parisā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.51" id="51"><article><div class="hgroup"><h1>
msgctxt "an2.42-51:50.1.5"
msgid "51"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.76"></a>
#. <a class="sc" id="sc51.1"></a>
msgctxt "an2.42-51:51.1.1"
msgid "“Dvemā, bhikkhave, parisā."
msgstr ""

msgctxt "an2.42-51:51.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.42-51:51.1.3"
msgid "Adhammavādinī ca parisā dhammavādinī ca parisā."
msgstr ""

msgctxt "an2.42-51:51.1.4"
msgid "Katamā ca, bhikkhave, adhammavādinī parisā?"
msgstr ""

msgctxt "an2.42-51:51.1.5"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū adhikaraṇaṃ ādiyanti dhammikaṃ vā "
"adhammikaṃ vā."
msgstr ""

msgctxt "an2.42-51:51.1.6"
msgid ""
"Te taṃ adhikaraṇaṃ ādiyitvā na ceva aññamaññaṃ saññāpenti na ca saññattiṃ "
"upagacchanti, na ca nijjhāpenti na ca nijjhattiṃ upagacchanti."
msgstr ""

#. VAR: parāmāsā → parāmassa (bj, s1-3, pts1)
msgctxt "an2.42-51:51.1.7"
msgid ""
"Te asaññattibalā anijjhattibalā appaṭinissaggamantino tameva adhikaraṇaṃ "
"thāmasā parāmāsā abhinivissa voharanti:"
msgstr ""

msgctxt "an2.42-51:51.1.8"
msgid "‘idameva saccaṃ moghamaññan’ti."
msgstr ""

msgctxt "an2.42-51:51.1.9"
msgid "Ayaṃ vuccati, bhikkhave, adhammavādinī parisā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc51.2"></a>
msgctxt "an2.42-51:51.2.1"
msgid "Katamā ca, bhikkhave, dhammavādinī parisā?"
msgstr ""

msgctxt "an2.42-51:51.2.2"
msgid ""
"Idha, bhikkhave, yassaṃ parisāyaṃ bhikkhū adhikaraṇaṃ ādiyanti dhammikaṃ vā "
"adhammikaṃ vā."
msgstr ""

msgctxt "an2.42-51:51.2.3"
msgid ""
"Te taṃ adhikaraṇaṃ ādiyitvā aññamaññaṃ saññāpenti ceva saññattiñca "
"upagacchanti, nijjhāpenti ceva nijjhattiñca upagacchanti."
msgstr ""

msgctxt "an2.42-51:51.2.4"
msgid ""
"Te saññattibalā nijjhattibalā paṭinissaggamantino, na tameva adhikaraṇaṃ "
"thāmasā parāmāsā abhinivissa voharanti:"
msgstr ""

msgctxt "an2.42-51:51.2.5"
msgid "‘idameva saccaṃ moghamaññan’ti."
msgstr ""

msgctxt "an2.42-51:51.2.6"
msgid "Ayaṃ vuccati, bhikkhave, dhammavādinī parisā."
msgstr ""

msgctxt "an2.42-51:51.2.7"
msgid "Imā kho, bhikkhave, dve parisā."
msgstr ""

msgctxt "an2.42-51:51.2.8"
msgid ""
"Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ parisānaṃ yadidaṃ dhammavādinī "
"parisā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an2.42-51:51.2.9"
msgid "Parisavaggo pañcamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc51.3"></a>
msgctxt "an2.42-51:51.3.1"
msgid "Uttānā vaggā aggavatī,"
msgstr ""

#. <br>
msgctxt "an2.42-51:51.3.2"
msgid "Ariyā kasaṭo ca pañcamo;"
msgstr ""

#. <br>
msgctxt "an2.42-51:51.3.3"
msgid "Okkācitaāmisañceva,"
msgstr ""

#. <br>
msgctxt "an2.42-51:51.3.4"
msgid "Visamā adhammādhammiyena cāti."
msgstr ""

#. </p></blockquote><p class="endsutta">
msgctxt "an2.42-51:51.3.5"
msgid "Paṭhamo paṇṇāsako samatto."
msgstr ""
