#. <section class="sutta" id="an4.248"><article><div class="hgroup"><p class="division">
msgctxt "an4.248:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.248:0.2"
msgid "25. Āpattibhayavagga"
msgstr ""

#. </p><h1>
msgctxt "an4.248:0.3"
msgid "248. Paññāvuddhisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.248:1.1"
msgid "“Cattārome, bhikkhave, dhammā paññāvuddhiyā saṃvattanti."
msgstr ""

msgctxt "an4.248:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.248:1.3"
msgid ""
"Sappurisasaṃsevo, saddhammasavanaṃ, yonisomanasikāro, "
"dhammānudhammappaṭipatti—"
msgstr ""

msgctxt "an4.248:1.4"
msgid "ime kho, bhikkhave, cattāro dhammā paññāvuddhiyā saṃvattantī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.248:1.5"
msgid "Chaṭṭhaṃ."
msgstr ""
