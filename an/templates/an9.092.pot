#. <section class="sutta" id="an9.92"><article><div class="hgroup"><p class="division">
msgctxt "an9.92:0.1"
msgid "Aṅguttara Nikāya 9"
msgstr ""

#. </p><p>
msgctxt "an9.92:0.2"
msgid "9. Iddhipādavagga"
msgstr ""

#. </p><h1>
#. VAR: 92. Cetasovinibandhasutta → vinibandhaiddhipādasuttaṃ (bj)
msgctxt "an9.92:0.3"
msgid "92. Cetasovinibandhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an9.92:1.1"
msgid "“Pañcime, bhikkhave, cetasovinibandhā."
msgstr ""

msgctxt "an9.92:1.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an9.92:1.3"
msgid "Idha, bhikkhave, bhikkhu kāmesu avītarāgo hoti … pe …"
msgstr ""

msgctxt "an9.92:1.4"
msgid "ime kho, bhikkhave, pañca cetasovinibandhā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an9.92:2.1"
msgid ""
"Imesaṃ kho, bhikkhave, pañcannaṃ cetasovinibandhānaṃ pahānāya ime cattāro "
"iddhipādā bhāvetabbā."
msgstr ""

msgctxt "an9.92:2.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an9.92:2.3"
msgid ""
"Idha, bhikkhave, bhikkhu chandasamādhipadhānasaṅkhārasamannāgataṃ iddhipādaṃ "
"bhāveti, "
msgstr ""

msgctxt "an9.92:2.4"
msgid "vīriyasamādhi …"
msgstr ""

msgctxt "an9.92:2.5"
msgid "cittasamādhi …"
msgstr ""

msgctxt "an9.92:2.6"
msgid "vīmaṃsāsamādhipadhānasaṅkhārasamannāgataṃ iddhipādaṃ bhāveti."
msgstr ""

msgctxt "an9.92:2.7"
msgid ""
"Imesaṃ kho, bhikkhave, pañcannaṃ cetasovinibandhānaṃ pahānāya ime cattāro "
"iddhipādā bhāvetabbā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an9.92:2.8"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an9.92:2.9"
msgid "Iddhipādavaggo catuttho."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an9.92:3.1"
msgid "Yatheva satipaṭṭhānā,"
msgstr ""

#. <br>
msgctxt "an9.92:3.2"
msgid "padhānā caturopi ca;"
msgstr ""

#. <br>
msgctxt "an9.92:3.3"
msgid "Cattāro iddhipādā ca,"
msgstr ""

#. <br>
msgctxt "an9.92:3.4"
msgid "tatheva sampayojayeti."
msgstr ""
