#. <section class="sutta" id="an7.46"><article><div class="hgroup"><p class="division">
msgctxt "an7.46:0.1"
msgid "Aṅguttara Nikāya 7"
msgstr ""

#. </p><p>
msgctxt "an7.46:0.2"
msgid "5. Mahāyaññavagga"
msgstr ""

#. </p><h1>
msgctxt "an7.46:0.3"
msgid "46. Paṭhamaaggisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.41"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an7.46:1.1"
msgid "“Sattime, bhikkhave, aggī."
msgstr ""

msgctxt "an7.46:1.2"
msgid "Katame satta?"
msgstr ""

msgctxt "an7.46:1.3"
msgid ""
"Rāgaggi, dosaggi, mohaggi, āhuneyyaggi, gahapataggi, dakkhiṇeyyaggi, "
"kaṭṭhaggi—"
msgstr ""

msgctxt "an7.46:1.4"
msgid "ime kho, bhikkhave, satta aggī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an7.46:1.5"
msgid "Tatiyaṃ."
msgstr ""
