#. <section class="sutta" id="an10.86"><article><div class="hgroup"><p class="division">
msgctxt "an10.86:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.86:0.2"
msgid "9. Theravagga"
msgstr ""

#. </p><h1>
msgctxt "an10.86:0.3"
msgid "86. Adhimānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.162"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.86:1.1"
msgid ""
"Ekaṃ samayaṃ āyasmā mahākassapo rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

msgctxt "an10.86:1.2"
msgid "Tatra kho āyasmā mahākassapo bhikkhū āmantesi:"
msgstr ""

msgctxt "an10.86:1.3"
msgid "“āvuso bhikkhave”ti."
msgstr ""

msgctxt "an10.86:1.4"
msgid "“Āvuso”ti kho te bhikkhū āyasmato mahākassapassa paccassosuṃ."
msgstr ""

msgctxt "an10.86:1.5"
msgid "Āyasmā mahākassapo etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.86:2.1"
msgid "“Idhāvuso, bhikkhu aññaṃ byākaroti:"
msgstr ""

msgctxt "an10.86:2.2"
msgid ""
"‘khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyāti "
"pajānāmī’ti."
msgstr ""

msgctxt "an10.86:2.3"
msgid ""
"Tamenaṃ tathāgato vā tathāgatasāvako vā jhāyī samāpattikusalo "
"paracittakusalo paracittapariyāyakusalo samanuyuñjati samanuggāhati "
"samanubhāsati."
msgstr ""

msgctxt "an10.86:2.4"
msgid ""
"So tathāgatena vā tathāgatasāvakena vā jhāyinā samāpattikusalena "
"paracittakusalena paracittapariyāyakusalena samanuyuñjiyamāno "
"samanuggāhiyamāno samanubhāsiyamāno irīṇaṃ āpajjati vicinaṃ āpajjati anayaṃ "
"āpajjati byasanaṃ āpajjati anayabyasanaṃ āpajjati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an10.86:3.1"
msgid ""
"Tamenaṃ tathāgato vā tathāgatasāvako vā jhāyī samāpattikusalo "
"paracittakusalo paracittapariyāyakusalo evaṃ cetasā ceto paricca manasi "
"karoti:"
msgstr ""

msgctxt "an10.86:3.2"
msgid "‘kiṃ nu kho ayamāyasmā aññaṃ byākaroti—"
msgstr ""

msgctxt "an10.86:3.3"
msgid ""
"khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyāti "
"pajānāmī’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an10.86:4.1"
msgid ""
"Tamenaṃ tathāgato vā tathāgatasāvako vā jhāyī samāpattikusalo "
"paracittakusalo paracittapariyāyakusalo evaṃ cetasā ceto paricca pajānāti:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an10.86:5.1"
msgid ""
"‘Adhimāniko kho ayamāyasmā adhimānasacco, appatte pattasaññī, akate "
"katasaññī, anadhigate adhigatasaññī."
msgstr ""

msgctxt "an10.86:5.2"
msgid "Adhimānena aññaṃ byākaroti—"
msgstr ""

msgctxt "an10.86:5.3"
msgid ""
"khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyāti "
"pajānāmī’ti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.163"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "an10.86:6.1"
msgid ""
"Tamenaṃ tathāgato vā tathāgatasāvako vā jhāyī samāpattikusalo "
"paracittakusalo paracittapariyāyakusalo evaṃ cetasā ceto paricca manasi "
"karoti:"
msgstr ""

msgctxt "an10.86:6.2"
msgid ""
"‘kiṃ nu kho ayamāyasmā nissāya adhimāniko adhimānasacco, appatte pattasaññī, "
"akate katasaññī, anadhigate adhigatasaññī."
msgstr ""

msgctxt "an10.86:6.3"
msgid "Adhimānena aññaṃ byākaroti—"
msgstr ""

msgctxt "an10.86:6.4"
msgid ""
"khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyāti "
"pajānāmī’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an10.86:7.1"
msgid ""
"Tamenaṃ tathāgato vā tathāgatasāvako vā jhāyī samāpattikusalo "
"paracittakusalo paracittapariyāyakusalo evaṃ cetasā ceto paricca pajānāti:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an10.86:8.1"
msgid ""
"‘Bahussuto kho pana ayamāyasmā sutadharo sutasannicayo, ye te dhammā "
"ādikalyāṇā majjhekalyāṇā pariyosānakalyāṇā sātthaṃ sabyañjanaṃ "
"kevalaparipuṇṇaṃ parisuddhaṃ brahmacariyaṃ abhivadanti, tathārūpāssa dhammā "
"bahussutā honti dhātā vacasā paricitā manasānupekkhitā diṭṭhiyā "
"suppaṭividdhā."
msgstr ""

msgctxt "an10.86:8.2"
msgid ""
"Tasmā ayamāyasmā adhimāniko adhimānasacco, appatte pattasaññī, akate "
"katasaññī, anadhigate adhigatasaññī."
msgstr ""

msgctxt "an10.86:8.3"
msgid "Adhimānena aññaṃ byākaroti—"
msgstr ""

msgctxt "an10.86:8.4"
msgid ""
"khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyāti "
"pajānāmī’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an10.86:9.1"
msgid ""
"Tamenaṃ tathāgato vā tathāgatasāvako vā jhāyī samāpattikusalo "
"paracittakusalo paracittapariyāyakusalo evaṃ cetasā ceto paricca pajānāti:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "an10.86:10.1"
msgid "‘Abhijjhālu kho pana ayamāyasmā;"
msgstr ""

msgctxt "an10.86:10.2"
msgid "abhijjhāpariyuṭṭhitena cetasā bahulaṃ viharati."
msgstr ""

msgctxt "an10.86:10.3"
msgid ""
"Abhijjhāpariyuṭṭhānaṃ kho pana tathāgatappavedite dhammavinaye "
"parihānametaṃ. (1)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "an10.86:11.1"
msgid "Byāpanno kho pana ayamāyasmā;"
msgstr ""

msgctxt "an10.86:11.2"
msgid "byāpādapariyuṭṭhitena cetasā bahulaṃ viharati."
msgstr ""

msgctxt "an10.86:11.3"
msgid ""
"Byāpādapariyuṭṭhānaṃ kho pana tathāgatappavedite dhammavinaye parihānametaṃ. "
"(2)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "an10.86:12.1"
msgid "Thinamiddho kho pana ayamāyasmā;"
msgstr ""

msgctxt "an10.86:12.2"
msgid "thinamiddhapariyuṭṭhitena cetasā bahulaṃ viharati."
msgstr ""

msgctxt "an10.86:12.3"
msgid ""
"Thinamiddhapariyuṭṭhānaṃ kho pana tathāgatappavedite dhammavinaye "
"parihānametaṃ. (3)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc13"></a>
msgctxt "an10.86:13.1"
msgid "Uddhato kho pana ayamāyasmā;"
msgstr ""

msgctxt "an10.86:13.2"
msgid "uddhaccapariyuṭṭhitena cetasā bahulaṃ viharati."
msgstr ""

msgctxt "an10.86:13.3"
msgid ""
"Uddhaccapariyuṭṭhānaṃ kho pana tathāgatappavedite dhammavinaye "
"parihānametaṃ. (4)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc14"></a>
msgctxt "an10.86:14.1"
msgid "Vicikiccho kho pana ayamāyasmā;"
msgstr ""

msgctxt "an10.86:14.2"
msgid "vicikicchāpariyuṭṭhitena cetasā bahulaṃ viharati."
msgstr ""

msgctxt "an10.86:14.3"
msgid ""
"Vicikicchāpariyuṭṭhānaṃ kho pana tathāgatappavedite dhammavinaye "
"parihānametaṃ. (5)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.164"></a>
#. <a class="sc" id="sc15"></a>
msgctxt "an10.86:15.1"
msgid "Kammārāmo kho pana ayamāyasmā kammarato kammārāmataṃ anuyutto."
msgstr ""

msgctxt "an10.86:15.2"
msgid "Kammārāmatā kho pana tathāgatappavedite dhammavinaye parihānametaṃ. (6)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc16"></a>
msgctxt "an10.86:16.1"
msgid "Bhassārāmo kho pana ayamāyasmā bhassarato bhassārāmataṃ anuyutto."
msgstr ""

msgctxt "an10.86:16.2"
msgid ""
"Bhassārāmatā kho pana tathāgatappavedite dhammavinaye parihānametaṃ. (7)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc17"></a>
msgctxt "an10.86:17.1"
msgid "Niddārāmo kho pana ayamāyasmā niddārato niddārāmataṃ anuyutto."
msgstr ""

msgctxt "an10.86:17.2"
msgid "Niddārāmatā kho pana tathāgatappavedite dhammavinaye parihānametaṃ. (8)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc18"></a>
msgctxt "an10.86:18.1"
msgid ""
"Saṅgaṇikārāmo kho pana ayamāyasmā saṅgaṇikarato saṅgaṇikārāmataṃ anuyutto."
msgstr ""

msgctxt "an10.86:18.2"
msgid ""
"Saṅgaṇikārāmatā kho pana tathāgatappavedite dhammavinaye parihānametaṃ. (9)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc19"></a>
msgctxt "an10.86:19.1"
msgid ""
"Sati kho pana ayamāyasmā uttari karaṇīye oramattakena visesādhigamena antarā "
"vosānaṃ āpanno."
msgstr ""

msgctxt "an10.86:19.2"
msgid ""
"Antarā vosānagamanaṃ kho pana tathāgatappavedite dhammavinaye parihānametaṃ’"
". (10)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc20"></a>
msgctxt "an10.86:20.1"
msgid ""
"So vatāvuso, bhikkhu ‘ime dasa dhamme appahāya imasmiṃ dhammavinaye vuddhiṃ "
"virūḷhiṃ vepullaṃ āpajjissatī’ti netaṃ ṭhānaṃ vijjati."
msgstr ""

msgctxt "an10.86:20.2"
msgid ""
"So vatāvuso, bhikkhu ‘ime dasa dhamme pahāya imasmiṃ dhammavinaye vuddhiṃ "
"virūḷhiṃ vepullaṃ āpajjissatī’ti ṭhānametaṃ vijjatī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.86:20.3"
msgid "Chaṭṭhaṃ."
msgstr ""
