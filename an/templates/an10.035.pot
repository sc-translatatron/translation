#. <section class="sutta" id="an10.35"><article><div class="hgroup"><p class="division">
msgctxt "an10.35:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.35:0.2"
msgid "4. Upālivagga"
msgstr ""

#. </p><h1>
msgctxt "an10.35:0.3"
msgid "35. Nissayasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.73"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.35:1.1"
msgid ""
"“Katihi nu kho, bhante, dhammehi samannāgatena bhikkhunā nissayo dātabbo”ti?"
msgstr ""

msgctxt "an10.35:1.2"
msgid "“Dasahi kho, upāli, dhammehi samannāgatena bhikkhunā nissayo dātabbo."
msgstr ""

msgctxt "an10.35:1.3"
msgid "Katamehi dasahi?"
msgstr ""

msgctxt "an10.35:1.4"
msgid "Idhupāli, bhikkhu sīlavā hoti … pe … samādāya sikkhati sikkhāpadesu;"
msgstr ""

msgctxt "an10.35:1.5"
msgid "bahussuto hoti … pe … diṭṭhiyā suppaṭividdhā;"
msgstr ""

msgctxt "an10.35:1.6"
msgid ""
"pātimokkhaṃ kho panassa vitthārena svāgataṃ hoti suvibhattaṃ suppavattaṃ "
"suvinicchitaṃ suttaso anubyañjanaso;"
msgstr ""

msgctxt "an10.35:1.7"
msgid "paṭibalo hoti gilānaṃ upaṭṭhātuṃ vā upaṭṭhāpetuṃ vā;"
msgstr ""

msgctxt "an10.35:1.8"
msgid "paṭibalo hoti anabhiratiṃ vūpakāsetuṃ vā vūpakāsāpetuṃ vā;"
msgstr ""

msgctxt "an10.35:1.9"
msgid "paṭibalo hoti uppannaṃ kukkuccaṃ dhammato vinodetuṃ;"
msgstr ""

msgctxt "an10.35:1.10"
msgid "paṭibalo hoti uppannaṃ diṭṭhigataṃ dhammato vivecetuṃ;"
msgstr ""

msgctxt "an10.35:1.11"
msgid "paṭibalo hoti adhisīle … pe …"
msgstr ""

msgctxt "an10.35:1.12"
msgid "adhicitte …"
msgstr ""

msgctxt "an10.35:1.13"
msgid "adhipaññāya samādapetuṃ."
msgstr ""

msgctxt "an10.35:1.14"
msgid ""
"Imehi kho, upāli, dasahi dhammehi samannāgatena bhikkhunā nissayo dātabbo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.35:1.15"
msgid "Pañcamaṃ."
msgstr ""
