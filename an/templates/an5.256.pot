#. <section class="sutta" id="an5.256"><article><div class="hgroup"><p class="division">
msgctxt "an5.256:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.256:0.2"
msgid "26. Upasampadāvagga"
msgstr ""

#. </p><h1>
msgctxt "an5.256:0.3"
msgid "256. Paṭhamajhānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.256:1.1"
msgid ""
"“Pañcime, bhikkhave, dhamme appahāya abhabbo paṭhamaṃ jhānaṃ upasampajja "
"viharituṃ."
msgstr ""

msgctxt "an5.256:1.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.256:1.3"
msgid ""
"Āvāsamacchariyaṃ, kulamacchariyaṃ, lābhamacchariyaṃ, vaṇṇamacchariyaṃ, "
"dhammamacchariyaṃ."
msgstr ""

msgctxt "an5.256:1.4"
msgid ""
"Ime kho, bhikkhave, pañca dhamme appahāya abhabbo paṭhamaṃ jhānaṃ "
"upasampajja viharituṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.256:2.1"
msgid ""
"Pañcime, bhikkhave, dhamme pahāya bhabbo paṭhamaṃ jhānaṃ upasampajja "
"viharituṃ."
msgstr ""

msgctxt "an5.256:2.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.256:2.3"
msgid ""
"Āvāsamacchariyaṃ, kulamacchariyaṃ, lābhamacchariyaṃ, vaṇṇamacchariyaṃ, "
"dhammamacchariyaṃ."
msgstr ""

msgctxt "an5.256:2.4"
msgid ""
"Ime kho, bhikkhave, pañca dhamme pahāya bhabbo paṭhamaṃ jhānaṃ upasampajja "
"viharitun”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.256:2.5"
msgid "Chaṭṭhaṃ."
msgstr ""
