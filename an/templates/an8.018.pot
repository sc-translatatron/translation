#. <section class="sutta" id="an8.18"><article><div class="hgroup"><p class="division">
msgctxt "an8.18:0.1"
msgid "Aṅguttara Nikāya 8"
msgstr ""

#. </p><p>
msgctxt "an8.18:0.2"
msgid "2. Mahāvagga"
msgstr ""

#. </p><h1>
#. VAR: 18. Dutiyabandhanasutta → itthibandhanasuttaṃ (bj)
msgctxt "an8.18:0.3"
msgid "18. Dutiyabandhanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an8.18:1.1"
msgid "“Aṭṭhahi, bhikkhave, ākārehi puriso itthiṃ bandhati."
msgstr ""

msgctxt "an8.18:1.2"
msgid "Katamehi aṭṭhahi?"
msgstr ""

msgctxt "an8.18:1.3"
msgid "Ruṇṇena, bhikkhave, puriso itthiṃ bandhati;"
msgstr ""

msgctxt "an8.18:1.4"
msgid "hasitena, bhikkhave, puriso itthiṃ bandhati;"
msgstr ""

msgctxt "an8.18:1.5"
msgid "bhaṇitena, bhikkhave, puriso itthiṃ bandhati;"
msgstr ""

msgctxt "an8.18:1.6"
msgid "ākappena, bhikkhave, puriso itthiṃ bandhati;"
msgstr ""

msgctxt "an8.18:1.7"
msgid "vanabhaṅgena, bhikkhave, puriso itthiṃ bandhati;"
msgstr ""

msgctxt "an8.18:1.8"
msgid "gandhena, bhikkhave, puriso itthiṃ bandhati;"
msgstr ""

msgctxt "an8.18:1.9"
msgid "rasena, bhikkhave, puriso itthiṃ bandhati;"
msgstr ""

msgctxt "an8.18:1.10"
msgid "phassena, bhikkhave, puriso itthiṃ bandhati."
msgstr ""

msgctxt "an8.18:1.11"
msgid "Imehi kho, bhikkhave, aṭṭhahākārehi puriso itthiṃ bandhati."
msgstr ""

msgctxt "an8.18:1.12"
msgid "Te, bhikkhave, sattā subaddhā, ye phassena baddhā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an8.18:1.13"
msgid "Aṭṭhamaṃ."
msgstr ""
