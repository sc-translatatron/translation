#. <section class="sutta" id="an5.239"><article><div class="hgroup"><p class="division">
msgctxt "an5.239:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.239:0.2"
msgid "24. Āvāsikavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.239:0.3"
msgid "239. Paṭhamamacchariyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.239:1.1"
msgid ""
"“Pañcahi, bhikkhave, dhammehi samannāgato āvāsiko bhikkhu yathābhataṃ "
"nikkhitto evaṃ niraye."
msgstr ""

msgctxt "an5.239:1.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.239:1.3"
msgid "Āvāsamaccharī hoti;"
msgstr ""

msgctxt "an5.239:1.4"
msgid "kulamaccharī hoti;"
msgstr ""

msgctxt "an5.239:1.5"
msgid "lābhamaccharī hoti;"
msgstr ""

#. VAR: vaṇṇamaccharī → dhammamaccharī (mr)
msgctxt "an5.239:1.6"
msgid "vaṇṇamaccharī hoti;"
msgstr ""

msgctxt "an5.239:1.7"
msgid "saddhādeyyaṃ vinipāteti."
msgstr ""

msgctxt "an5.239:1.8"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgato āvāsiko bhikkhu "
"yathābhataṃ nikkhitto evaṃ niraye."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.239:2.1"
msgid ""
"Pañcahi, bhikkhave, dhammehi samannāgato āvāsiko bhikkhu yathābhataṃ "
"nikkhitto evaṃ sagge."
msgstr ""

msgctxt "an5.239:2.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.239:2.3"
msgid "Na āvāsamaccharī hoti;"
msgstr ""

msgctxt "an5.239:2.4"
msgid "na kulamaccharī hoti;"
msgstr ""

msgctxt "an5.239:2.5"
msgid "na lābhamaccharī hoti;"
msgstr ""

msgctxt "an5.239:2.6"
msgid "na vaṇṇamaccharī hoti;"
msgstr ""

msgctxt "an5.239:2.7"
msgid "saddhādeyyaṃ na vinipāteti."
msgstr ""

msgctxt "an5.239:2.8"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgato āvāsiko bhikkhu "
"yathābhataṃ nikkhitto evaṃ sagge”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.239:2.9"
msgid "Navamaṃ."
msgstr ""
