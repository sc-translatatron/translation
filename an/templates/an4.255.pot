#. <section class="sutta" id="an4.255"><article><div class="hgroup"><p class="division">
msgctxt "an4.255:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.255:0.2"
msgid "26. Abhiññāvagga"
msgstr ""

#. </p><h1>
msgctxt "an4.255:0.3"
msgid "255. Pariyesanāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.255:1.1"
msgid "“Catasso imā, bhikkhave, anariyapariyesanā."
msgstr ""

msgctxt "an4.255:1.2"
msgid "Katamā catasso?"
msgstr ""

msgctxt "an4.255:1.3"
msgid ""
"Idha, bhikkhave, ekacco attanā jarādhammo samāno jarādhammaṃyeva pariyesati;"
msgstr ""

msgctxt "an4.255:1.4"
msgid "attanā byādhidhammo samāno byādhidhammaṃyeva pariyesati;"
msgstr ""

msgctxt "an4.255:1.5"
msgid "attanā maraṇadhammo samāno maraṇadhammaṃyeva pariyesati;"
msgstr ""

msgctxt "an4.255:1.6"
msgid "attanā saṅkilesadhammo samāno saṅkilesadhammaṃyeva pariyesati."
msgstr ""

msgctxt "an4.255:1.7"
msgid "Imā kho, bhikkhave, catasso anariyapariyesanā."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.248"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.255:2.1"
msgid "Catasso imā, bhikkhave, ariyapariyesanā."
msgstr ""

msgctxt "an4.255:2.2"
msgid "Katamā catasso?"
msgstr ""

msgctxt "an4.255:2.3"
msgid ""
"Idha, bhikkhave, ekacco attanā jarādhammo samāno jarādhamme ādīnavaṃ viditvā "
"ajaraṃ anuttaraṃ yogakkhemaṃ nibbānaṃ pariyesati;"
msgstr ""

msgctxt "an4.255:2.4"
msgid ""
"attanā byādhidhammo samāno byādhidhamme ādīnavaṃ viditvā abyādhiṃ anuttaraṃ "
"yogakkhemaṃ nibbānaṃ pariyesati;"
msgstr ""

msgctxt "an4.255:2.5"
msgid ""
"attanā maraṇadhammo samāno maraṇadhamme ādīnavaṃ viditvā amataṃ anuttaraṃ "
"yogakkhemaṃ nibbānaṃ pariyesati;"
msgstr ""

msgctxt "an4.255:2.6"
msgid ""
"attanā saṅkilesadhammo samāno saṅkilesadhamme ādīnavaṃ viditvā asaṅkiliṭṭhaṃ "
"anuttaraṃ yogakkhemaṃ nibbānaṃ pariyesati."
msgstr ""

msgctxt "an4.255:2.7"
msgid "Imā kho, bhikkhave, catasso ariyapariyesanā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.255:2.8"
msgid "Dutiyaṃ."
msgstr ""
