#. <section class="sutta" id="an6.1"><article><div class="hgroup"><p class="division">
msgctxt "an6.1:0.1"
msgid "Aṅguttara Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "an6.1:0.2"
msgid "1. Āhuneyyavagga"
msgstr ""

#. </p><h1>
msgctxt "an6.1:0.3"
msgid "1. Paṭhamaāhuneyyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.279"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "an6.1:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "an6.1:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "an6.1:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "an6.1:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "an6.1:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "an6.1:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an6.1:2.1"
msgid ""
"“Chahi, bhikkhave, dhammehi samannāgato bhikkhu āhuneyyo hoti pāhuneyyo "
"dakkhiṇeyyo añjalikaraṇīyo anuttaraṃ puññakkhettaṃ lokassa."
msgstr ""

msgctxt "an6.1:2.2"
msgid "Katamehi chahi?"
msgstr ""

msgctxt "an6.1:2.3"
msgid ""
"Idha, bhikkhave, bhikkhu cakkhunā rūpaṃ disvā neva sumano hoti na dummano, "
"upekkhako viharati sato sampajāno."
msgstr ""

msgctxt "an6.1:2.4"
msgid "Sotena saddaṃ sutvā …"
msgstr ""

msgctxt "an6.1:2.5"
msgid "ghānena gandhaṃ ghāyitvā …"
msgstr ""

msgctxt "an6.1:2.6"
msgid "jivhāya rasaṃ sāyitvā …"
msgstr ""

msgctxt "an6.1:2.7"
msgid "kāyena phoṭṭhabbaṃ phusitvā …"
msgstr ""

msgctxt "an6.1:2.8"
msgid ""
"manasā dhammaṃ viññāya neva sumano hoti na dummano, upekkhako viharati sato "
"sampajāno."
msgstr ""

msgctxt "an6.1:2.9"
msgid ""
"Imehi kho, bhikkhave, chahi dhammehi samannāgato bhikkhu āhuneyyo hoti "
"pāhuneyyo dakkhiṇeyyo añjalikaraṇīyo anuttaraṃ puññakkhettaṃ lokassā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an6.1:3.1"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "an6.1:3.2"
msgid "Attamanā te bhikkhū bhagavato bhāsitaṃ abhinandunti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.1:3.3"
msgid "Paṭhamaṃ."
msgstr ""
