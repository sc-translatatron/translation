#. <section class="sutta" id="an4.139"><article><div class="hgroup"><p class="division">
msgctxt "an4.139:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.139:0.2"
msgid "14. Puggalavagga"
msgstr ""

#. </p><h1>
msgctxt "an4.139:0.3"
msgid "139. Dhammakathikasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.139:1.1"
msgid "“Cattārome, bhikkhave, dhammakathikā."
msgstr ""

msgctxt "an4.139:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.139:1.3"
msgid "Idha, bhikkhave, ekacco dhammakathiko appañca bhāsati asahitañca;"
msgstr ""

#. VAR: parisā cassa → parisā ca (bj, s1-3, km, pts1)
msgctxt "an4.139:1.4"
msgid "parisā cassa na kusalā hoti sahitāsahitassa."
msgstr ""

msgctxt "an4.139:1.5"
msgid ""
"Evarūpo, bhikkhave, dhammakathiko evarūpāya parisāya dhammakathikotveva "
"saṅkhaṃ gacchati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.139:2.1"
msgid "Idha pana, bhikkhave, ekacco dhammakathiko appañca bhāsati sahitañca;"
msgstr ""

msgctxt "an4.139:2.2"
msgid "parisā cassa kusalā hoti sahitāsahitassa."
msgstr ""

msgctxt "an4.139:2.3"
msgid ""
"Evarūpo, bhikkhave, dhammakathiko evarūpāya parisāya dhammakathikotveva "
"saṅkhaṃ gacchati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.139:3.1"
msgid "Idha pana, bhikkhave, ekacco dhammakathiko bahuñca bhāsati asahitañca;"
msgstr ""

msgctxt "an4.139:3.2"
msgid "parisā cassa na kusalā hoti sahitāsahitassa."
msgstr ""

msgctxt "an4.139:3.3"
msgid ""
"Evarūpo, bhikkhave, dhammakathiko evarūpāya parisāya dhammakathikotveva "
"saṅkhaṃ gacchati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.139:4.1"
msgid "Idha pana, bhikkhave, ekacco dhammakathiko bahuñca bhāsati sahitañca;"
msgstr ""

msgctxt "an4.139:4.2"
msgid "parisā cassa kusalā hoti sahitāsahitassa."
msgstr ""

msgctxt "an4.139:4.3"
msgid ""
"Evarūpo, bhikkhave, dhammakathiko evarūpāya parisāya dhammakathikotveva "
"saṅkhaṃ gacchati."
msgstr ""

msgctxt "an4.139:4.4"
msgid "Ime kho, bhikkhave, cattāro dhammakathikā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.139:4.5"
msgid "Navamaṃ."
msgstr ""
