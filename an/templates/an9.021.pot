#. <section class="sutta" id="an9.21"><article><div class="hgroup"><p class="division">
msgctxt "an9.21:0.1"
msgid "Aṅguttara Nikāya 9"
msgstr ""

#. </p><p>
msgctxt "an9.21:0.2"
msgid "3. Sattāvāsavagga"
msgstr ""

#. </p><h1>
#. VAR: 21. Tiṭhānasutta → ṭhānasuttaṃ (bj)
msgctxt "an9.21:0.3"
msgid "21. Tiṭhānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an9.21:1.1"
msgid ""
"“Tīhi, bhikkhave, ṭhānehi uttarakurukā manussā deve ca tāvatiṃse "
"adhiggaṇhanti jambudīpake ca manusse."
msgstr ""

msgctxt "an9.21:1.2"
msgid "Katamehi tīhi?"
msgstr ""

# See BB’s not on visesaguna/-bhuna.
#. VAR: visesaguṇā → visesabhuno (bj, s1-3, pts1)
msgctxt "an9.21:1.3"
msgid "Amamā, apariggahā, niyatāyukā, visesaguṇā—"
msgstr ""

msgctxt "an9.21:1.4"
msgid ""
"imehi kho, bhikkhave, tīhi ṭhānehi uttarakurukā manussā deve ca tāvatiṃse "
"adhiggaṇhanti jambudīpake ca manusse."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an9.21:2.1"
msgid ""
"Tīhi, bhikkhave, ṭhānehi devā tāvatiṃsā uttarakuruke ca manusse "
"adhiggaṇhanti jambudīpake ca manusse."
msgstr ""

msgctxt "an9.21:2.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an9.21:2.3"
msgid "Dibbena āyunā, dibbena vaṇṇena, dibbena sukhena—"
msgstr ""

msgctxt "an9.21:2.4"
msgid ""
"imehi kho, bhikkhave, tīhi ṭhānehi devā tāvatiṃsā uttarakuruke ca manusse "
"adhiggaṇhanti jambudīpake ca manusse."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an9.21:3.1"
msgid ""
"Tīhi, bhikkhave, ṭhānehi jambudīpakā manussā uttarakuruke ca manusse "
"adhiggaṇhanti deve ca tāvatiṃse."
msgstr ""

msgctxt "an9.21:3.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an9.21:3.3"
msgid "Sūrā, satimanto, idha brahmacariyavāso—"
msgstr ""

msgctxt "an9.21:3.4"
msgid ""
"imehi kho, bhikkhave, tīhi ṭhānehi jambudīpakā manussā uttarakuruke ca "
"manusse adhiggaṇhanti deve ca tāvatiṃse”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an9.21:3.5"
msgid "Paṭhamaṃ."
msgstr ""
