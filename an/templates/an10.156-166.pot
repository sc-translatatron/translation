#. <section class="sutta" id="an10.156"><article><div class="hgroup"><p class="division">
msgctxt "an10.156-166:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.156-166:0.2"
msgid "16. Puggalavagga"
msgstr ""

#. </p><h1>
msgctxt "an10.156-166:0.3"
msgid "156–​166. Bhajitabbādisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.249"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.156-166:1.1"
msgid "“Dasahi, bhikkhave, dhammehi samannāgato puggalo na bhajitabbo … pe …"
msgstr ""

msgctxt "an10.156-166:1.2"
msgid "bhajitabbo … pe …"
msgstr ""

msgctxt "an10.156-166:1.3"
msgid "na payirupāsitabbo …"
msgstr ""

msgctxt "an10.156-166:1.4"
msgid "payirupāsitabbo … pe …"
msgstr ""

msgctxt "an10.156-166:1.5"
msgid "na pujjo hoti …"
msgstr ""

msgctxt "an10.156-166:1.6"
msgid "pujjo hoti … pe …"
msgstr ""

msgctxt "an10.156-166:1.7"
msgid "na pāsaṃso hoti …"
msgstr ""

msgctxt "an10.156-166:1.8"
msgid "pāsaṃso hoti … pe …"
msgstr ""

msgctxt "an10.156-166:1.9"
msgid "agāravo hoti …"
msgstr ""

msgctxt "an10.156-166:1.10"
msgid "sagāravo hoti … pe …"
msgstr ""

msgctxt "an10.156-166:1.11"
msgid "appatisso hoti …"
msgstr ""

msgctxt "an10.156-166:1.12"
msgid "sappatisso hoti … pe …"
msgstr ""

msgctxt "an10.156-166:1.13"
msgid "na ārādhako hoti …"
msgstr ""

msgctxt "an10.156-166:1.14"
msgid "ārādhako hoti … pe …"
msgstr ""

msgctxt "an10.156-166:1.15"
msgid "na visujjhati …"
msgstr ""

msgctxt "an10.156-166:1.16"
msgid "visujjhati … pe …"
msgstr ""

msgctxt "an10.156-166:1.17"
msgid "mānaṃ nādhibhoti …"
msgstr ""

msgctxt "an10.156-166:1.18"
msgid "mānaṃ adhibhoti … pe …"
msgstr ""

msgctxt "an10.156-166:1.19"
msgid "paññāya na vaḍḍhati …"
msgstr ""

msgctxt "an10.156-166:1.20"
msgid "paññāya vaḍḍhati … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.156-166:2.1"
msgid "Bahuṃ apuññaṃ pasavati …"
msgstr ""

msgctxt "an10.156-166:2.2"
msgid "bahuṃ puññaṃ pasavati."
msgstr ""

msgctxt "an10.156-166:2.3"
msgid "Katamehi dasahi?"
msgstr ""

msgctxt "an10.156-166:2.4"
msgid ""
"Sammādiṭṭhiko hoti, sammāsaṅkappo hoti, sammāvāco hoti, sammākammanto hoti, "
"sammāājīvo hoti, sammāvāyāmo hoti, sammāsati hoti, sammāsamādhi hoti, "
"sammāñāṇī hoti, sammāvimutti hoti—"
msgstr ""

msgctxt "an10.156-166:2.5"
msgid ""
"imehi kho, bhikkhave, dasahi dhammehi samannāgato puggalo bahuṃ puññaṃ "
"pasavatī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.156-166:2.6"
msgid "Puggalavaggo paṭhamo."
msgstr ""
