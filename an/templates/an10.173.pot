#. <section class="sutta" id="an10.173"><article><div class="hgroup"><p class="division">
msgctxt "an10.173:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.173:0.2"
msgid "17. Jāṇussoṇivagga"
msgstr ""

#. </p><h1>
msgctxt "an10.173:0.3"
msgid "173. Tatiyaadhammasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.173:1.1"
msgid "“Adhammo ca, bhikkhave, veditabbo dhammo ca;"
msgstr ""

msgctxt "an10.173:1.2"
msgid "anattho ca veditabbo attho ca."
msgstr ""

msgctxt "an10.173:1.3"
msgid ""
"Adhammañca viditvā dhammañca, anatthañca viditvā atthañca yathā dhammo yathā "
"attho tathā paṭipajjitabbaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.173:2.1"
msgid "Katamo ca, bhikkhave, adhammo, katamo ca dhammo;"
msgstr ""

msgctxt "an10.173:2.2"
msgid "katamo ca anattho, katamo ca attho?"
msgstr ""

msgctxt "an10.173:2.3"
msgid "Pāṇātipāto, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.173:2.4"
msgid "pāṇātipātā veramaṇī dhammo;"
msgstr ""

msgctxt "an10.173:2.5"
msgid ""
"ye ca pāṇātipātapaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.173:2.6"
msgid ""
"pāṇātipātā veramaṇipaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ "
"gacchanti, ayaṃ attho."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.261"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "an10.173:3.1"
msgid "Adinnādānaṃ, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.173:3.2"
msgid "adinnādānā veramaṇī dhammo …"
msgstr ""

msgctxt "an10.173:3.3"
msgid "kāmesumicchācāro, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.173:3.4"
msgid "kāmesumicchācārā veramaṇī dhammo …"
msgstr ""

msgctxt "an10.173:3.5"
msgid "musāvādo, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.173:3.6"
msgid "musāvādā veramaṇī dhammo …"
msgstr ""

msgctxt "an10.173:3.7"
msgid "pisuṇā vācā, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.173:3.8"
msgid "pisuṇāya vācāya veramaṇī dhammo …"
msgstr ""

msgctxt "an10.173:3.9"
msgid "pharusā vācā, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.173:3.10"
msgid "pharusāya vācāya veramaṇī dhammo …"
msgstr ""

msgctxt "an10.173:3.11"
msgid "samphappalāpo, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.173:3.12"
msgid "samphappalāpā veramaṇī dhammo …"
msgstr ""

msgctxt "an10.173:3.13"
msgid "abhijjhā, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.173:3.14"
msgid "anabhijjhā dhammo …"
msgstr ""

msgctxt "an10.173:3.15"
msgid "byāpādo, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.173:3.16"
msgid "abyāpādo dhammo …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an10.173:4.1"
msgid "Micchādiṭṭhi, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.173:4.2"
msgid "sammādiṭṭhi dhammo;"
msgstr ""

msgctxt "an10.173:4.3"
msgid ""
"ye ca micchādiṭṭhipaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.173:4.4"
msgid ""
"sammādiṭṭhipaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an10.173:5.1"
msgid "‘Adhammo ca, bhikkhave, veditabbo dhammo ca;"
msgstr ""

msgctxt "an10.173:5.2"
msgid "anattho ca veditabbo attho ca."
msgstr ""

msgctxt "an10.173:5.3"
msgid ""
"Adhammañca viditvā dhammañca, anatthañca viditvā atthañca yathā dhammo yathā "
"attho tathā paṭipajjitabban’ti, "
msgstr ""

msgctxt "an10.173:5.4"
msgid "iti yaṃ taṃ vuttaṃ, idametaṃ paṭicca vuttan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.173:5.5"
msgid "Sattamaṃ."
msgstr ""
