#. <section class="sutta" id="an5.237"><article><div class="hgroup"><p class="division">
msgctxt "an5.237:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.237:0.2"
msgid "24. Āvāsikavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.237:0.3"
msgid "237. Dutiyaavaṇṇārahasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.265"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.237:1.1"
msgid ""
"“Pañcahi, bhikkhave, dhammehi samannāgato āvāsiko bhikkhu yathābhataṃ "
"nikkhitto evaṃ niraye."
msgstr ""

msgctxt "an5.237:1.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.237:1.3"
msgid "Ananuvicca apariyogāhetvā avaṇṇārahassa vaṇṇaṃ bhāsati;"
msgstr ""

msgctxt "an5.237:1.4"
msgid "ananuvicca apariyogāhetvā vaṇṇārahassa avaṇṇaṃ bhāsati;"
msgstr ""

msgctxt "an5.237:1.5"
msgid "āvāsamaccharī hoti āvāsapaligedhī;"
msgstr ""

msgctxt "an5.237:1.6"
msgid "kulamaccharī hoti kulapaligedhī;"
msgstr ""

msgctxt "an5.237:1.7"
msgid "saddhādeyyaṃ vinipāteti."
msgstr ""

msgctxt "an5.237:1.8"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgato āvāsiko bhikkhu "
"yathābhataṃ nikkhitto evaṃ niraye."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.237:2.1"
msgid ""
"Pañcahi, bhikkhave, dhammehi samannāgato āvāsiko bhikkhu yathābhataṃ "
"nikkhitto evaṃ sagge."
msgstr ""

msgctxt "an5.237:2.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.237:2.3"
msgid "Anuvicca pariyogāhetvā avaṇṇārahassa avaṇṇaṃ bhāsati;"
msgstr ""

msgctxt "an5.237:2.4"
msgid "anuvicca pariyogāhetvā vaṇṇārahassa vaṇṇaṃ bhāsati;"
msgstr ""

msgctxt "an5.237:2.5"
msgid "na āvāsamaccharī hoti na āvāsapaligedhī;"
msgstr ""

msgctxt "an5.237:2.6"
msgid "na kulamaccharī hoti na kulapaligedhī;"
msgstr ""

msgctxt "an5.237:2.7"
msgid "saddhādeyyaṃ na vinipāteti."
msgstr ""

msgctxt "an5.237:2.8"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgato āvāsiko bhikkhu "
"yathābhataṃ nikkhitto evaṃ sagge”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.237:2.9"
msgid "Sattamaṃ."
msgstr ""
