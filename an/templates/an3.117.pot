#. <section class="sutta" id="an3.117"><article><div class="hgroup"><p class="division">
msgctxt "an3.117:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.117:0.2"
msgid "12. Āpāyikavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.117:0.3"
msgid "117. Vipattisampadāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.117:1.1"
msgid "“Tisso imā, bhikkhave, vipattiyo."
msgstr ""

msgctxt "an3.117:1.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "an3.117:1.3"
msgid "Sīlavipatti, cittavipatti, diṭṭhivipatti."
msgstr ""

msgctxt "an3.117:1.4"
msgid "Katamā ca, bhikkhave, sīlavipatti?"
msgstr ""

msgctxt "an3.117:1.5"
msgid ""
"Idha, bhikkhave, ekacco pāṇātipātī hoti, adinnādāyī hoti, kāmesumicchācārī "
"hoti, musāvādī hoti, pisuṇavāco hoti, pharusavāco hoti, samphappalāpī hoti."
msgstr ""

msgctxt "an3.117:1.6"
msgid "Ayaṃ vuccati, bhikkhave, sīlavipatti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.117:2.1"
msgid "Katamā ca, bhikkhave, cittavipatti?"
msgstr ""

msgctxt "an3.117:2.2"
msgid "Idha, bhikkhave, ekacco abhijjhālu hoti byāpannacitto."
msgstr ""

msgctxt "an3.117:2.3"
msgid "Ayaṃ vuccati, bhikkhave, cittavipatti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.269"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "an3.117:3.1"
msgid "Katamā ca, bhikkhave, diṭṭhivipatti?"
msgstr ""

msgctxt "an3.117:3.2"
msgid "Idha, bhikkhave, ekacco micchādiṭṭhiko hoti viparītadassano:"
msgstr ""

msgctxt "an3.117:3.3"
msgid ""
"‘natthi dinnaṃ, natthi yiṭṭhaṃ, natthi hutaṃ, natthi sukatadukkaṭānaṃ, "
"kammānaṃ phalaṃ vipāko, natthi ayaṃ loko, natthi paro loko, natthi mātā, "
"natthi pitā, natthi sattā opapātikā, natthi loke samaṇabrāhmaṇā sammaggatā "
"sammāpaṭipannā ye imañca lokaṃ parañca lokaṃ sayaṃ abhiññā sacchikatvā "
"pavedentī’ti."
msgstr ""

msgctxt "an3.117:3.4"
msgid "Ayaṃ vuccati, bhikkhave, diṭṭhivipatti."
msgstr ""

msgctxt "an3.117:3.5"
msgid ""
"Sīlavipattihetu vā, bhikkhave, sattā kāyassa bhedā paraṃ maraṇā apāyaṃ "
"duggatiṃ vinipātaṃ nirayaṃ upapajjanti;"
msgstr ""

msgctxt "an3.117:3.6"
msgid ""
"cittavipattihetu vā, bhikkhave, sattā kāyassa bhedā paraṃ maraṇā apāyaṃ "
"duggatiṃ vinipātaṃ nirayaṃ upapajjanti;"
msgstr ""

msgctxt "an3.117:3.7"
msgid ""
"diṭṭhivipattihetu vā, bhikkhave, sattā kāyassa bhedā paraṃ maraṇā apāyaṃ "
"duggatiṃ vinipātaṃ nirayaṃ upapajjanti."
msgstr ""

msgctxt "an3.117:3.8"
msgid "Imā kho, bhikkhave, tisso vipattiyoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an3.117:4.1"
msgid "Tisso imā, bhikkhave, sampadā."
msgstr ""

msgctxt "an3.117:4.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "an3.117:4.3"
msgid "Sīlasampadā, cittasampadā, diṭṭhisampadā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an3.117:5.1"
msgid "Katamā ca, bhikkhave, sīlasampadā?"
msgstr ""

msgctxt "an3.117:5.2"
msgid ""
"Idha, bhikkhave, ekacco pāṇātipātā paṭivirato hoti, adinnādānā paṭivirato "
"hoti, kāmesumicchācārā paṭivirato hoti, musāvādā paṭivirato hoti, pisuṇāya "
"vācāya paṭivirato hoti, pharusāya vācāya paṭivirato hoti, samphappalāpā "
"paṭivirato hoti."
msgstr ""

msgctxt "an3.117:5.3"
msgid "Ayaṃ vuccati, bhikkhave, sīlasampadā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an3.117:6.1"
msgid "Katamā ca, bhikkhave, cittasampadā?"
msgstr ""

msgctxt "an3.117:6.2"
msgid "Idha, bhikkhave, ekacco anabhijjhālu hoti abyāpannacitto."
msgstr ""

msgctxt "an3.117:6.3"
msgid "Ayaṃ vuccati, bhikkhave, cittasampadā."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.270"></a>
#. <a class="sc" id="sc7"></a>
msgctxt "an3.117:7.1"
msgid "Katamā ca, bhikkhave, diṭṭhisampadā?"
msgstr ""

msgctxt "an3.117:7.2"
msgid "Idha, bhikkhave, ekacco sammādiṭṭhiko hoti aviparītadassano:"
msgstr ""

msgctxt "an3.117:7.3"
msgid ""
"‘atthi dinnaṃ, atthi yiṭṭhaṃ, atthi hutaṃ, atthi sukatadukkaṭānaṃ kammānaṃ "
"phalaṃ vipāko, atthi ayaṃ loko, atthi paro loko, atthi mātā, atthi pitā, "
"atthi sattā opapātikā, atthi loke samaṇabrāhmaṇā sammaggatā sammāpaṭipannā "
"ye imañca lokaṃ parañca lokaṃ sayaṃ abhiññā sacchikatvā pavedentī’ti."
msgstr ""

msgctxt "an3.117:7.4"
msgid "Ayaṃ vuccati, bhikkhave, diṭṭhisampadā."
msgstr ""

msgctxt "an3.117:7.5"
msgid ""
"Sīlasampadāhetu vā, bhikkhave, sattā kāyassa bhedā paraṃ maraṇā sugatiṃ "
"saggaṃ lokaṃ upapajjanti;"
msgstr ""

msgctxt "an3.117:7.6"
msgid ""
"cittasampadāhetu vā, bhikkhave, sattā kāyassa bhedā paraṃ maraṇā sugatiṃ "
"saggaṃ lokaṃ upapajjanti;"
msgstr ""

msgctxt "an3.117:7.7"
msgid ""
"diṭṭhisampadāhetu vā, bhikkhave, sattā kāyassa bhedā paraṃ maraṇā sugatiṃ "
"saggaṃ lokaṃ upapajjanti."
msgstr ""

msgctxt "an3.117:7.8"
msgid "Imā kho, bhikkhave, tisso sampadā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.117:7.9"
msgid "Pañcamaṃ."
msgstr ""
