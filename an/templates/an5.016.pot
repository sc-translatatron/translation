#. <section class="sutta" id="an5.16"><article><div class="hgroup"><p class="division">
msgctxt "an5.16:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.16:0.2"
msgid "2. Balavagga"
msgstr ""

#. </p><h1>
#. VAR: balaaggasuttaṃ (bj)
msgctxt "an5.16:0.3"
msgid "16. Punakūṭasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.16:1.1"
msgid "“Pañcimāni, bhikkhave, balāni."
msgstr ""

msgctxt "an5.16:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "an5.16:1.3"
msgid "Saddhābalaṃ, vīriyabalaṃ, satibalaṃ, samādhibalaṃ, paññābalaṃ—"
msgstr ""

msgctxt "an5.16:1.4"
msgid "imāni kho, bhikkhave, pañca balāni."
msgstr ""

msgctxt "an5.16:1.5"
msgid ""
"Imesaṃ kho, bhikkhave, pañcannaṃ balānaṃ etaṃ aggaṃ etaṃ saṅgāhikaṃ etaṃ "
"saṅghātaniyaṃ, yadidaṃ paññābalaṃ."
msgstr ""

msgctxt "an5.16:1.6"
msgid ""
"Seyyathāpi, bhikkhave, kūṭāgārassa etaṃ aggaṃ etaṃ saṅgāhikaṃ etaṃ "
"saṅghātaniyaṃ, yadidaṃ kūṭaṃ."
msgstr ""

msgctxt "an5.16:1.7"
msgid ""
"Evamevaṃ kho, bhikkhave, imesaṃ pañcannaṃ balānaṃ etaṃ aggaṃ etaṃ saṅgāhikaṃ "
"etaṃ saṅghātaniyaṃ, yadidaṃ paññābalan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.16:1.8"
msgid "Chaṭṭhaṃ."
msgstr ""
