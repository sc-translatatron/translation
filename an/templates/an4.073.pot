#. <section class="sutta" id="an4.73"><article><div class="hgroup"><p class="division">
msgctxt "an4.73:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.73:0.2"
msgid "8. Apaṇṇakavagga"
msgstr ""

#. </p><h1>
#. VAR: asappurisavadhukāsuttaṃ (bj)
msgctxt "an4.73:0.3"
msgid "73. Sappurisasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.73:1.1"
msgid "“Catūhi, bhikkhave, dhammehi samannāgato asappuriso veditabbo."
msgstr ""

msgctxt "an4.73:1.2"
msgid "Katamehi catūhi?"
msgstr ""

msgctxt "an4.73:1.3"
msgid ""
"Idha, bhikkhave, asappuriso yo hoti parassa avaṇṇo taṃ apuṭṭhopi pātu "
"karoti, ko pana vādo puṭṭhassa."
msgstr ""

msgctxt "an4.73:1.4"
msgid ""
"Puṭṭho kho pana pañhābhinīto ahāpetvā alambitvā paripūraṃ vitthārena parassa "
"avaṇṇaṃ bhāsitā hoti."
msgstr ""

msgctxt "an4.73:1.5"
msgid "Veditabbametaṃ, bhikkhave, asappuriso ayaṃ bhavanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.73:2.1"
msgid ""
"Puna caparaṃ, bhikkhave, asappuriso yo hoti parassa vaṇṇo taṃ puṭṭhopi na "
"pātu karoti, ko pana vādo apuṭṭhassa."
msgstr ""

msgctxt "an4.73:2.2"
msgid ""
"Puṭṭho kho pana pañhābhinīto hāpetvā lambitvā aparipūraṃ avitthārena parassa "
"vaṇṇaṃ bhāsitā hoti."
msgstr ""

msgctxt "an4.73:2.3"
msgid "Veditabbametaṃ, bhikkhave, asappuriso ayaṃ bhavanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.73:3.1"
msgid ""
"Puna caparaṃ, bhikkhave, asappuriso yo hoti attano avaṇṇo taṃ puṭṭhopi na "
"pātu karoti, ko pana vādo apuṭṭhassa."
msgstr ""

msgctxt "an4.73:3.2"
msgid ""
"Puṭṭho kho pana pañhābhinīto hāpetvā lambitvā aparipūraṃ avitthārena attano "
"avaṇṇaṃ bhāsitā hoti."
msgstr ""

msgctxt "an4.73:3.3"
msgid "Veditabbametaṃ, bhikkhave, asappuriso ayaṃ bhavanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.73:4.1"
msgid ""
"Puna caparaṃ, bhikkhave, asappuriso yo hoti attano vaṇṇo taṃ apuṭṭhopi pātu "
"karoti, ko pana vādo puṭṭhassa."
msgstr ""

msgctxt "an4.73:4.2"
msgid ""
"Puṭṭho kho pana pañhābhinīto ahāpetvā alambitvā paripūraṃ vitthārena attano "
"vaṇṇaṃ bhāsitā hoti."
msgstr ""

msgctxt "an4.73:4.3"
msgid "Veditabbametaṃ, bhikkhave, asappuriso ayaṃ bhavanti."
msgstr ""

msgctxt "an4.73:4.4"
msgid "Imehi kho, bhikkhave, catūhi dhammehi samannāgato asappuriso veditabbo."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.78"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.73:5.1"
msgid "Catūhi, bhikkhave, dhammehi samannāgato sappuriso veditabbo."
msgstr ""

msgctxt "an4.73:5.2"
msgid "Katamehi catūhi?"
msgstr ""

msgctxt "an4.73:5.3"
msgid ""
"Idha, bhikkhave, sappuriso yo hoti parassa avaṇṇo taṃ puṭṭhopi na pātu "
"karoti, ko pana vādo apuṭṭhassa."
msgstr ""

msgctxt "an4.73:5.4"
msgid ""
"Puṭṭho kho pana pañhābhinīto hāpetvā lambitvā aparipūraṃ avitthārena parassa "
"avaṇṇaṃ bhāsitā hoti."
msgstr ""

msgctxt "an4.73:5.5"
msgid "Veditabbametaṃ, bhikkhave, sappuriso ayaṃ bhavanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an4.73:6.1"
msgid ""
"Puna caparaṃ, bhikkhave, sappuriso yo hoti parassa vaṇṇo taṃ apuṭṭhopi pātu "
"karoti, ko pana vādo puṭṭhassa."
msgstr ""

msgctxt "an4.73:6.2"
msgid ""
"Puṭṭho kho pana pañhābhinīto ahāpetvā alambitvā paripūraṃ vitthārena parassa "
"vaṇṇaṃ bhāsitā hoti."
msgstr ""

msgctxt "an4.73:6.3"
msgid "Veditabbametaṃ, bhikkhave, sappuriso ayaṃ bhavanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an4.73:7.1"
msgid ""
"Puna caparaṃ, bhikkhave, sappuriso yo hoti attano avaṇṇo taṃ apuṭṭhopi pātu "
"karoti, ko pana vādo puṭṭhassa."
msgstr ""

msgctxt "an4.73:7.2"
msgid ""
"Puṭṭho kho pana pañhābhinīto ahāpetvā alambitvā paripūraṃ vitthārena attano "
"avaṇṇaṃ bhāsitā hoti."
msgstr ""

msgctxt "an4.73:7.3"
msgid "Veditabbametaṃ, bhikkhave, sappuriso ayaṃ bhavanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an4.73:8.1"
msgid ""
"Puna caparaṃ, bhikkhave, sappuriso yo hoti attano vaṇṇo taṃ puṭṭhopi na pātu "
"karoti, ko pana vādo apuṭṭhassa."
msgstr ""

msgctxt "an4.73:8.2"
msgid ""
"Puṭṭho kho pana pañhābhinīto hāpetvā lambitvā aparipūraṃ avitthārena attano "
"vaṇṇaṃ bhāsitā hoti."
msgstr ""

msgctxt "an4.73:8.3"
msgid "Veditabbametaṃ, bhikkhave, sappuriso ayaṃ bhavanti."
msgstr ""

msgctxt "an4.73:8.4"
msgid "Imehi kho, bhikkhave, catūhi dhammehi samannāgato sappuriso veditabbo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an4.73:9.1"
msgid ""
"Seyyathāpi, bhikkhave, vadhukā yaññadeva rattiṃ vā divaṃ vā ānītā hoti, "
"tāvadevassā tibbaṃ hirottappaṃ paccupaṭṭhitaṃ hoti sassuyāpi sasurepi "
"sāmikepi antamaso dāsakammakaraporisesu."
msgstr ""

msgctxt "an4.73:9.2"
msgid ""
"Sā aparena samayena saṃvāsamanvāya vissāsamanvāya sassumpi sasurampi "
"sāmikampi evamāha:"
msgstr ""

msgctxt "an4.73:9.3"
msgid "‘apetha, kiṃ pana tumhe jānāthā’ti."
msgstr ""

msgctxt "an4.73:9.4"
msgid ""
"Evamevaṃ kho, bhikkhave, idhekacco bhikkhu yaññadeva rattiṃ vā divaṃ vā "
"agārasmā anagāriyaṃ pabbajito hoti, tāvadevassa tibbaṃ hirottappaṃ "
"paccupaṭṭhitaṃ hoti bhikkhūsu bhikkhunīsu upāsakesu upāsikāsu antamaso "
"ārāmikasamaṇuddesesu."
msgstr ""

msgctxt "an4.73:9.5"
msgid ""
"So aparena samayena saṃvāsamanvāya vissāsamanvāya ācariyampi upajjhāyampi "
"evamāha:"
msgstr ""

msgctxt "an4.73:9.6"
msgid "‘apetha, kiṃ pana tumhe jānāthā’ti."
msgstr ""

msgctxt "an4.73:9.7"
msgid "Tasmātiha, bhikkhave, evaṃ sikkhitabbaṃ:"
msgstr ""

msgctxt "an4.73:9.8"
msgid "‘adhunāgatavadhukāsamena cetasā viharissāmā’ti."
msgstr ""

msgctxt "an4.73:9.9"
msgid "Evañhi vo, bhikkhave, sikkhitabban”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.73:9.10"
msgid "Tatiyaṃ."
msgstr ""
