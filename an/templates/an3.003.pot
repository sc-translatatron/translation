#. <section class="sutta" id="an3.3"><article><div class="hgroup"><p class="division">
msgctxt "an3.3:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.3:0.2"
msgid "1. Bālavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.3:0.3"
msgid "3. Cintīsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.3:1.1"
msgid ""
"“Tīṇimāni, bhikkhave, bālassa bālalakkhaṇāni bālanimittāni bālāpadānāni."
msgstr ""

msgctxt "an3.3:1.2"
msgid "Katamāni tīṇi?"
msgstr ""

msgctxt "an3.3:1.3"
msgid ""
"Idha, bhikkhave, bālo duccintitacintī ca hoti dubbhāsitabhāsī ca "
"dukkaṭakammakārī ca."
msgstr ""

#. VAR: cedaṃ → no cetaṃ (s1-3, km, mr)
#. VAR: kena naṃ paṇḍitā jāneyyuṃ → tena naṃ paṇḍitā na jāneyyuṃ (mr) | na naṃ paṇḍitā jāneyyuṃ (?)
msgctxt "an3.3:1.4"
msgid ""
"No cedaṃ, bhikkhave, bālo duccintitacintī ca abhavissa dubbhāsitabhāsī ca "
"dukkaṭakammakārī ca, kena naṃ paṇḍitā jāneyyuṃ:"
msgstr ""

msgctxt "an3.3:1.5"
msgid "‘bālo ayaṃ bhavaṃ asappuriso’ti?"
msgstr ""

msgctxt "an3.3:1.6"
msgid ""
"Yasmā ca kho, bhikkhave, bālo duccintitacintī ca hoti dubbhāsitabhāsī ca "
"dukkaṭakammakārī ca tasmā naṃ paṇḍitā jānanti:"
msgstr ""

msgctxt "an3.3:1.7"
msgid "‘bālo ayaṃ bhavaṃ asappuriso’ti."
msgstr ""

msgctxt "an3.3:1.8"
msgid ""
"Imāni kho, bhikkhave, tīṇi bālassa bālalakkhaṇāni bālanimittāni bālāpadānāni."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.103"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.3:2.1"
msgid ""
"Tīṇimāni, bhikkhave, paṇḍitassa paṇḍitalakkhaṇāni paṇḍitanimittāni "
"paṇḍitāpadānāni."
msgstr ""

msgctxt "an3.3:2.2"
msgid "Katamāni tīṇi?"
msgstr ""

msgctxt "an3.3:2.3"
msgid ""
"Idha, bhikkhave, paṇḍito sucintitacintī ca hoti subhāsitabhāsī ca "
"sukatakammakārī ca."
msgstr ""

msgctxt "an3.3:2.4"
msgid ""
"No cedaṃ, bhikkhave, paṇḍito sucintitacintī ca abhavissa subhāsitabhāsī ca "
"sukatakammakārī ca, kena naṃ paṇḍitā jāneyyuṃ:"
msgstr ""

msgctxt "an3.3:2.5"
msgid "‘paṇḍito ayaṃ bhavaṃ sappuriso’ti?"
msgstr ""

msgctxt "an3.3:2.6"
msgid ""
"Yasmā ca kho, bhikkhave, paṇḍito sucintitacintī ca hoti subhāsitabhāsī ca "
"sukatakammakārī ca tasmā naṃ paṇḍitā jānanti:"
msgstr ""

msgctxt "an3.3:2.7"
msgid "‘paṇḍito ayaṃ bhavaṃ sappuriso’ti."
msgstr ""

msgctxt "an3.3:2.8"
msgid ""
"Imāni kho, bhikkhave, tīṇi paṇḍitassa paṇḍitalakkhaṇāni paṇḍitanimittāni "
"paṇḍitāpadānāni."
msgstr ""

msgctxt "an3.3:2.9"
msgid "Tasmātiha … pe …."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.3:2.10"
msgid "Tatiyaṃ."
msgstr ""
