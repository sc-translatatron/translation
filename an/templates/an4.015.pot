#. <section class="sutta" id="an4.15"><article><div class="hgroup"><p class="division">
msgctxt "an4.15:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.15:0.2"
msgid "2. Caravagga"
msgstr ""

#. </p><h1>
#. VAR: aggapaññattisuttaṃ (bj)
msgctxt "an4.15:0.3"
msgid "15. Paññattisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.15:1.1"
msgid "“Catasso imā, bhikkhave, aggapaññattiyo."
msgstr ""

msgctxt "an4.15:1.2"
msgid "Katamā catasso?"
msgstr ""

msgctxt "an4.15:1.3"
msgid "Etadaggaṃ, bhikkhave, attabhāvīnaṃ yadidaṃ—"
msgstr ""

msgctxt "an4.15:1.4"
msgid "rāhu asurindo."
msgstr ""

msgctxt "an4.15:1.5"
msgid "Etadaggaṃ, bhikkhave, kāmabhogīnaṃ yadidaṃ—"
msgstr ""

msgctxt "an4.15:1.6"
msgid "rājā mandhātā."
msgstr ""

msgctxt "an4.15:1.7"
msgid "Etadaggaṃ, bhikkhave, ādhipateyyānaṃ yadidaṃ—"
msgstr ""

msgctxt "an4.15:1.8"
msgid "māro pāpimā."
msgstr ""

msgctxt "an4.15:1.9"
msgid ""
"Sadevake, bhikkhave, loke samārake sabrahmake sassamaṇabrāhmaṇiyā pajāya "
"sadevamanussāya tathāgato aggamakkhāyati arahaṃ sammāsambuddho."
msgstr ""

msgctxt "an4.15:1.10"
msgid "Imā kho, bhikkhave, catasso aggapaññattiyoti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
#. VAR: Rāhuggaṃ → rāhaggaṃ (bj, pts1)
msgctxt "an4.15:2.1"
msgid "Rāhuggaṃ attabhāvīnaṃ,"
msgstr ""

#. <br>
msgctxt "an4.15:2.2"
msgid "mandhātā kāmabhoginaṃ;"
msgstr ""

#. <br>
msgctxt "an4.15:2.3"
msgid "Māro ādhipateyyānaṃ,"
msgstr ""

#. <br>
msgctxt "an4.15:2.4"
msgid "iddhiyā yasasā jalaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.15:3.1"
msgid "Uddhaṃ tiriyaṃ apācīnaṃ,"
msgstr ""

#. <br>
msgctxt "an4.15:3.2"
msgid "yāvatājagatogati;"
msgstr ""

#. <br>
msgctxt "an4.15:3.3"
msgid "Sadevakassa lokassa,"
msgstr ""

#. <br>
msgctxt "an4.15:3.4"
msgid "buddho aggo pavuccatī”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an4.15:3.5"
msgid "Pañcamaṃ."
msgstr ""
