#. <section class="sutta" id="an10.155"><article><div class="hgroup"><p class="division">
msgctxt "an10.155:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.155:0.2"
msgid "16. Puggalavagga"
msgstr ""

#. </p><h1>
msgctxt "an10.155:0.3"
msgid "155. Sevitabbasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.248"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.155:1.1"
msgid "“Dasahi, bhikkhave, dhammehi samannāgato puggalo na sevitabbo."
msgstr ""

msgctxt "an10.155:1.2"
msgid "Katamehi dasahi?"
msgstr ""

msgctxt "an10.155:1.3"
msgid ""
"Micchādiṭṭhiko hoti, micchāsaṅkappo hoti, micchāvāco hoti, micchākammanto "
"hoti, micchāājīvo hoti, micchāvāyāmo hoti, micchāsati hoti, micchāsamādhi "
"hoti, micchāñāṇī hoti, micchāvimutti hoti—"
msgstr ""

msgctxt "an10.155:1.4"
msgid "imehi kho, bhikkhave, dasahi dhammehi samannāgato puggalo na sevitabbo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.155:2.1"
msgid "Dasahi, bhikkhave, dhammehi samannāgato puggalo sevitabbo."
msgstr ""

msgctxt "an10.155:2.2"
msgid "Katamehi dasahi?"
msgstr ""

msgctxt "an10.155:2.3"
msgid ""
"Sammādiṭṭhiko hoti, sammāsaṅkappo hoti, sammāvāco hoti, sammākammanto hoti, "
"sammāājīvo hoti, sammāvāyāmo hoti, sammāsati hoti, sammāsamādhi hoti, "
"sammāñāṇī hoti, sammāvimutti hoti—"
msgstr ""

msgctxt "an10.155:2.4"
msgid "imehi kho, bhikkhave, dasahi dhammehi samannāgato puggalo sevitabbo”ti."
msgstr ""
