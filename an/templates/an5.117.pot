#. <section class="sutta" id="an5.117"><article><div class="hgroup"><p class="division">
msgctxt "an5.117:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.117:0.2"
msgid "12. Andhakavindavagga"
msgstr ""

#. </p><h1>
#. VAR: issukīsuttaṃ (bj)
msgctxt "an5.117:0.3"
msgid "117. Issukinīsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.117:1.1"
msgid ""
"“Pañcahi, bhikkhave, dhammehi samannāgatā bhikkhunī yathābhataṃ nikkhittā "
"evaṃ niraye."
msgstr ""

msgctxt "an5.117:1.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.117:1.3"
msgid "Ananuvicca apariyogāhetvā avaṇṇārahassa vaṇṇaṃ bhāsati, "
msgstr ""

msgctxt "an5.117:1.4"
msgid "ananuvicca apariyogāhetvā vaṇṇārahassa avaṇṇaṃ bhāsati, "
msgstr ""

msgctxt "an5.117:1.5"
msgid "issukinī ca hoti, "
msgstr ""

msgctxt "an5.117:1.6"
msgid "maccharinī ca, "
msgstr ""

#. VAR: saddhādeyyaṃ → saddhādeyyañca (s1-3)
msgctxt "an5.117:1.7"
msgid "saddhādeyyaṃ vinipāteti."
msgstr ""

msgctxt "an5.117:1.8"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgatā bhikkhunī yathābhataṃ "
"nikkhittā evaṃ niraye."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.117:2.1"
msgid ""
"Pañcahi, bhikkhave, dhammehi samannāgatā bhikkhunī yathābhataṃ nikkhittā "
"evaṃ sagge."
msgstr ""

msgctxt "an5.117:2.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.117:2.3"
msgid "Anuvicca pariyogāhetvā avaṇṇārahassa avaṇṇaṃ bhāsati, "
msgstr ""

msgctxt "an5.117:2.4"
msgid "anuvicca pariyogāhetvā vaṇṇārahassa vaṇṇaṃ bhāsati, "
msgstr ""

msgctxt "an5.117:2.5"
msgid "anissukinī ca hoti, "
msgstr ""

msgctxt "an5.117:2.6"
msgid "amaccharinī ca, "
msgstr ""

msgctxt "an5.117:2.7"
msgid "saddhādeyyaṃ na vinipāteti."
msgstr ""

msgctxt "an5.117:2.8"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgatā bhikkhunī yathābhataṃ "
"nikkhittā evaṃ sagge”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.117:2.9"
msgid "Sattamaṃ."
msgstr ""
