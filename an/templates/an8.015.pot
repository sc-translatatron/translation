#. <section class="sutta" id="an8.15"><article><div class="hgroup"><p class="division">
msgctxt "an8.15:0.1"
msgid "Aṅguttara Nikāya 8"
msgstr ""

#. </p><p>
msgctxt "an8.15:0.2"
msgid "2. Mahāvagga"
msgstr ""

#. </p><h1>
msgctxt "an8.15:0.3"
msgid "15. Malasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an8.15:1.1"
msgid "“Aṭṭhimāni, bhikkhave, malāni."
msgstr ""

msgctxt "an8.15:1.2"
msgid "Katamāni aṭṭha?"
msgstr ""

msgctxt "an8.15:1.3"
msgid "Asajjhāyamalā, bhikkhave, mantā;"
msgstr ""

msgctxt "an8.15:1.4"
msgid "anuṭṭhānamalā, bhikkhave, gharā;"
msgstr ""

msgctxt "an8.15:1.5"
msgid "malaṃ, bhikkhave, vaṇṇassa kosajjaṃ;"
msgstr ""

msgctxt "an8.15:1.6"
msgid "pamādo, bhikkhave, rakkhato malaṃ;"
msgstr ""

msgctxt "an8.15:1.7"
msgid "malaṃ, bhikkhave, itthiyā duccaritaṃ;"
msgstr ""

msgctxt "an8.15:1.8"
msgid "maccheraṃ, bhikkhave, dadato malaṃ;"
msgstr ""

msgctxt "an8.15:1.9"
msgid "malā, bhikkhave, pāpakā akusalā dhammā asmiṃ loke paramhi ca;"
msgstr ""

#. VAR: tato → tato ca (s1-3, pts1)
msgctxt "an8.15:1.10"
msgid "tato, bhikkhave, malā malataraṃ avijjā paramaṃ malaṃ."
msgstr ""

msgctxt "an8.15:1.11"
msgid "Imāni kho, bhikkhave, aṭṭha malānīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an8.15:2.1"
msgid "Asajjhāyamalā mantā,"
msgstr ""

#. <br>
msgctxt "an8.15:2.2"
msgid "anuṭṭhānamalā gharā;"
msgstr ""

#. <br>
msgctxt "an8.15:2.3"
msgid "Malaṃ vaṇṇassa kosajjaṃ,"
msgstr ""

#. <br>
msgctxt "an8.15:2.4"
msgid "pamādo rakkhato malaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an8.15:3.1"
msgid "Malitthiyā duccaritaṃ,"
msgstr ""

#. <br>
msgctxt "an8.15:3.2"
msgid "maccheraṃ dadato malaṃ;"
msgstr ""

#. <br>
msgctxt "an8.15:3.3"
msgid "Malā ve pāpakā dhammā,"
msgstr ""

#. <br>
msgctxt "an8.15:3.4"
msgid "asmiṃ loke paramhi ca;"
msgstr ""

#. <br>
msgctxt "an8.15:3.5"
msgid "Tato malā malataraṃ,"
msgstr ""

#. <br>
msgctxt "an8.15:3.6"
msgid "avijjā paramaṃ malan”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an8.15:3.7"
msgid "Pañcamaṃ."
msgstr ""
