#. <section id="vagga"><section class="sutta" data-uid="an1.616" id="616"><article><div class="hgroup"><p class="division">
msgctxt "an1.616-627:0.1"
msgid "Aṅguttara Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "an1.616-627:0.2"
msgid "31. Amatavagga"
msgstr ""

#. </p><h1>
msgctxt "an1.616-627:0.3"
msgid "616"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc616.1"></a>
msgctxt "an1.616-627:616.1.1"
msgid ""
"“Amataṃ te, bhikkhave, na paribhuñjanti ye kāyagatāsatiṃ na paribhuñjanti."
msgstr ""

msgctxt "an1.616-627:616.1.2"
msgid "Amataṃ te, bhikkhave, paribhuñjanti ye kāyagatāsatiṃ paribhuñjantī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.617" id="617"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:616.1.3"
msgid "617"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc617.1"></a>
msgctxt "an1.616-627:617.1.1"
msgid "“Amataṃ tesaṃ, bhikkhave, aparibhuttaṃ yesaṃ kāyagatāsati aparibhuttā."
msgstr ""

msgctxt "an1.616-627:617.1.2"
msgid "Amataṃ tesaṃ, bhikkhave, paribhuttaṃ yesaṃ kāyagatāsati paribhuttā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.618" id="618"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:617.1.3"
msgid "618"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc618.1"></a>
msgctxt "an1.616-627:618.1.1"
msgid "“Amataṃ tesaṃ, bhikkhave, parihīnaṃ yesaṃ kāyagatāsati parihīnā."
msgstr ""

msgctxt "an1.616-627:618.1.2"
msgid "Amataṃ tesaṃ, bhikkhave, aparihīnaṃ yesaṃ kāyagatāsati aparihīnā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.619" id="619"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:618.1.3"
msgid "619"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.46"></a>
#. <a class="sc" id="sc619.1"></a>
msgctxt "an1.616-627:619.1.1"
msgid "“Amataṃ tesaṃ, bhikkhave, viraddhaṃ yesaṃ kāyagatāsati viraddhā."
msgstr ""

#. VAR: āraddhaṃ → aviraddhaṃ (mr)
msgctxt "an1.616-627:619.1.2"
msgid "Amataṃ tesaṃ, bhikkhave, āraddhaṃ yesaṃ kāyagatāsati āraddhā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.620" id="620"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:619.1.3"
msgid "620"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc620.1"></a>
msgctxt "an1.616-627:620.1.1"
msgid "“Amataṃ te, bhikkhave, pamādiṃsu ye kāyagatāsatiṃ pamādiṃsu."
msgstr ""

msgctxt "an1.616-627:620.1.2"
msgid "Amataṃ te, bhikkhave, na pamādiṃsu ye kāyagatāsatiṃ na pamādiṃsu”."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.621" id="621"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:620.1.3"
msgid "621"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc621.1"></a>
msgctxt "an1.616-627:621.1.1"
msgid "“Amataṃ tesaṃ, bhikkhave, pamuṭṭhaṃ yesaṃ kāyagatāsati pamuṭṭhā."
msgstr ""

msgctxt "an1.616-627:621.1.2"
msgid "Amataṃ tesaṃ, bhikkhave, appamuṭṭhaṃ yesaṃ kāyagatāsati appamuṭṭhā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.622" id="622"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:621.1.3"
msgid "622"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc622.1"></a>
msgctxt "an1.616-627:622.1.1"
msgid "“Amataṃ tesaṃ, bhikkhave, anāsevitaṃ yesaṃ kāyagatāsati anāsevitā."
msgstr ""

msgctxt "an1.616-627:622.1.2"
msgid "Amataṃ tesaṃ, bhikkhave, āsevitaṃ yesaṃ kāyagatāsati āsevitā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.623" id="623"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:622.1.3"
msgid "623"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc623.1"></a>
msgctxt "an1.616-627:623.1.1"
msgid "“Amataṃ tesaṃ, bhikkhave, abhāvitaṃ yesaṃ kāyagatāsati abhāvitā."
msgstr ""

msgctxt "an1.616-627:623.1.2"
msgid "Amataṃ tesaṃ, bhikkhave, bhāvitaṃ yesaṃ kāyagatāsati bhāvitā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.624" id="624"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:623.1.3"
msgid "624"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc624.1"></a>
msgctxt "an1.616-627:624.1.1"
msgid "“Amataṃ tesaṃ, bhikkhave, abahulīkataṃ yesaṃ kāyagatāsati abahulīkatā."
msgstr ""

msgctxt "an1.616-627:624.1.2"
msgid "Amataṃ tesaṃ, bhikkhave, bahulīkataṃ yesaṃ kāyagatāsati bahulīkatā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.625" id="625"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:624.1.3"
msgid "625"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc625.1"></a>
msgctxt "an1.616-627:625.1.1"
msgid "“Amataṃ tesaṃ, bhikkhave, anabhiññātaṃ yesaṃ kāyagatāsati anabhiññātā."
msgstr ""

msgctxt "an1.616-627:625.1.2"
msgid "Amataṃ tesaṃ, bhikkhave, abhiññātaṃ yesaṃ kāyagatāsati abhiññātā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.626" id="626"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:625.1.3"
msgid "626"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc626.1"></a>
msgctxt "an1.616-627:626.1.1"
msgid "“Amataṃ tesaṃ, bhikkhave, apariññātaṃ yesaṃ kāyagatāsati apariññātā."
msgstr ""

msgctxt "an1.616-627:626.1.2"
msgid "Amataṃ tesaṃ, bhikkhave, pariññātaṃ yesaṃ kāyagatāsati pariññātā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an1.627" id="627"><article><div class="hgroup"><h1>
msgctxt "an1.616-627:626.1.3"
msgid "627"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc627.1"></a>
msgctxt "an1.616-627:627.1.1"
msgid "“Amataṃ tesaṃ, bhikkhave, asacchikataṃ yesaṃ kāyagatāsati asacchikatā."
msgstr ""

msgctxt "an1.616-627:627.1.2"
msgid "Amataṃ tesaṃ, bhikkhave, sacchikataṃ yesaṃ kāyagatāsati sacchikatā”ti."
msgstr ""

#. VAR: ( … ) → (ekakanipātassa suttasahassaṃ samattaṃ.) (bj) | (ekanipātassa suttasahassaṃ samattaṃ.) (s1-3, km, pts1)
#, fuzzy
msgctxt "an1.616-627:627.1.3"
msgid "( … )"
msgstr "( … )"

#. </p><p>
#. <a class="sc" id="sc627.2"></a>
#. VAR: etthantare pāṭho sī, syā1-3, kaṃ, pā1 potthakesu
msgctxt "an1.616-627:627.2.1"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "an1.616-627:627.2.2"
msgid "Attamanā te bhikkhū bhagavato bhāsitaṃ abhinandunti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an1.616-627:627.2.3"
msgid "(Amatavaggo.)"
msgstr ""

#. </p><p class="endbook">
msgctxt "an1.616-627:627.2.4"
msgid "Ekakanipātapāḷi niṭṭhitā."
msgstr ""
