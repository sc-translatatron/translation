#. <section class="sutta" id="an6.105"><article><div class="hgroup"><p class="division">
msgctxt "an6.105:0.1"
msgid "Aṅguttara Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "an6.105:0.2"
msgid "10. Ānisaṃsavagga"
msgstr ""

#. </p><h1>
msgctxt "an6.105:0.3"
msgid "105. Bhavasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an6.105:1.1"
msgid "“Tayome, bhikkhave, bhavā pahātabbā, tīsu sikkhāsu sikkhitabbaṃ."
msgstr ""

msgctxt "an6.105:1.2"
msgid "Katame tayo bhavā pahātabbā?"
msgstr ""

msgctxt "an6.105:1.3"
msgid "Kāmabhavo, rūpabhavo, arūpabhavo—"
msgstr ""

msgctxt "an6.105:1.4"
msgid "ime tayo bhavā pahātabbā."
msgstr ""

msgctxt "an6.105:1.5"
msgid "Katamāsu tīsu sikkhāsu sikkhitabbaṃ?"
msgstr ""

msgctxt "an6.105:1.6"
msgid "Adhisīlasikkhāya, adhicittasikkhāya, adhipaññāsikkhāya—"
msgstr ""

msgctxt "an6.105:1.7"
msgid "imāsu tīsu sikkhāsu sikkhitabbaṃ."
msgstr ""

msgctxt "an6.105:1.8"
msgid ""
"Yato kho, bhikkhave, bhikkhuno ime tayo bhavā pahīnā honti, imāsu ca tīsu "
"sikkhāsu sikkhitasikkho hoti—"
msgstr ""

msgctxt "an6.105:1.9"
msgid ""
"ayaṃ vuccati, bhikkhave, bhikkhu acchecchi taṇhaṃ, vivattayi saṃyojanaṃ, "
"sammā mānābhisamayā antamakāsi dukkhassā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.105:1.10"
msgid "Dasamaṃ."
msgstr ""
