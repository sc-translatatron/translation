#. <section class="sutta" id="an9.64"><article><div class="hgroup"><p class="division">
msgctxt "an9.64:0.1"
msgid "Aṅguttara Nikāya 9"
msgstr ""

#. </p><p>
msgctxt "an9.64:0.2"
msgid "7. Satipaṭṭhānavagga"
msgstr ""

#. </p><h1>
msgctxt "an9.64:0.3"
msgid "64. Nīvaraṇasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.458"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an9.64:1.1"
msgid "“Pañcimāni, bhikkhave, nīvaraṇāni."
msgstr ""

msgctxt "an9.64:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "an9.64:1.3"
msgid ""
"Kāmacchandanīvaraṇaṃ, byāpādanīvaraṇaṃ, thinamiddhanīvaraṇaṃ, "
"uddhaccakukkuccanīvaraṇaṃ, vicikicchānīvaraṇaṃ—"
msgstr ""

msgctxt "an9.64:1.4"
msgid "imāni kho, bhikkhave, pañca nīvaraṇāni."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an9.64:2.1"
msgid ""
"Imesaṃ kho, bhikkhave, pañcannaṃ nīvaraṇānaṃ pahānāya cattāro satipaṭṭhānā "
"bhāvetabbā."
msgstr ""

msgctxt "an9.64:2.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an9.64:2.3"
msgid ""
"Idha, bhikkhave, bhikkhu kāye kāyānupassī viharati ātāpī sampajāno satimā "
"vineyya loke abhijjhādomanassaṃ;"
msgstr ""

msgctxt "an9.64:2.4"
msgid "vedanāsu … pe …"
msgstr ""

msgctxt "an9.64:2.5"
msgid "citte …"
msgstr ""

msgctxt "an9.64:2.6"
msgid ""
"dhammesu dhammānupassī viharati ātāpī sampajāno satimā vineyya loke "
"abhijjhādomanassaṃ."
msgstr ""

msgctxt "an9.64:2.7"
msgid ""
"Imesaṃ kho, bhikkhave, pañcannaṃ nīvaraṇānaṃ pahānāya ime cattāro "
"satipaṭṭhānā bhāvetabbā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an9.64:2.8"
msgid "Dutiyaṃ."
msgstr ""
