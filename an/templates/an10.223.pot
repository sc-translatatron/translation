#. <section class="sutta" id="an10.223"><article><div class="hgroup"><p class="division">
msgctxt "an10.223:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.223:0.2"
msgid "22. Sāmaññavagga"
msgstr ""

#. </p><h1>
msgctxt "an10.223:0.3"
msgid "223"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.223:1.1"
msgid ""
"“Tiṃsāya, bhikkhave, dhammehi samannāgato yathābhataṃ nikkhitto evaṃ niraye."
msgstr ""

msgctxt "an10.223:1.2"
msgid "Katamehi tiṃsāya?"
msgstr ""

msgctxt "an10.223:1.3"
msgid ""
"Attanā ca pāṇātipātī hoti, parañca pāṇātipāte samādapeti, pāṇātipāte ca "
"samanuñño hoti;"
msgstr ""

msgctxt "an10.223:1.4"
msgid ""
"attanā ca adinnādāyī hoti, parañca adinnādāne samādapeti, adinnādāne ca "
"samanuñño hoti;"
msgstr ""

msgctxt "an10.223:1.5"
msgid ""
"attanā ca kāmesumicchācārī hoti, parañca kāmesumicchācāre samādapeti, "
"kāmesumicchācāre ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:1.6"
msgid ""
"attanā ca musāvādī hoti, parañca musāvāde samādapeti, musāvāde ca samanuñño "
"hoti;"
msgstr ""

msgctxt "an10.223:1.7"
msgid ""
"attanā ca pisuṇavāco hoti, parañca pisuṇāya vācāya samādapeti, pisuṇāya "
"vācāya ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:1.8"
msgid ""
"attanā ca pharusavāco hoti, parañca pharusāya vācāya samādapeti, pharusāya "
"vācāya ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:1.9"
msgid ""
"attanā ca samphappalāpī hoti, parañca samphappalāpe samādapeti, "
"samphappalāpe ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:1.10"
msgid ""
"attanā ca abhijjhālu hoti, parañca abhijjhāya samādapeti, abhijjhāya ca "
"samanuñño hoti;"
msgstr ""

msgctxt "an10.223:1.11"
msgid ""
"attanā ca byāpannacitto hoti, parañca byāpāde samādapeti, byāpāde ca "
"samanuñño hoti;"
msgstr ""

msgctxt "an10.223:1.12"
msgid ""
"attanā ca micchādiṭṭhiko hoti, parañca micchādiṭṭhiyā samādapeti, "
"micchādiṭṭhiyā ca samanuñño hoti—"
msgstr ""

msgctxt "an10.223:1.13"
msgid ""
"imehi kho, bhikkhave, tiṃsāya dhammehi samannāgato yathābhataṃ nikkhitto "
"evaṃ niraye."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.306"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.223:2.1"
msgid ""
"Tiṃsāya, bhikkhave, dhammehi samannāgato yathābhataṃ nikkhitto evaṃ sagge."
msgstr ""

msgctxt "an10.223:2.2"
msgid "Katamehi tiṃsāya?"
msgstr ""

msgctxt "an10.223:2.3"
msgid ""
"Attanā ca pāṇātipātā paṭivirato hoti, parañca pāṇātipātā veramaṇiyā "
"samādapeti, pāṇātipātā veramaṇiyā ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:2.4"
msgid ""
"attanā ca adinnādānā paṭivirato hoti, parañca adinnādānā veramaṇiyā "
"samādapeti, adinnādānā veramaṇiyā ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:2.5"
msgid ""
"attanā ca kāmesumicchācārā paṭivirato hoti, parañca kāmesumicchācārā "
"veramaṇiyā samādapeti, kāmesumicchācārā veramaṇiyā ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:2.6"
msgid ""
"attanā ca musāvādā paṭivirato hoti, parañca musāvādā veramaṇiyā samādapeti, "
"musāvādā veramaṇiyā ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:2.7"
msgid ""
"attanā ca pisuṇāya vācāya paṭivirato hoti, parañca pisuṇāya vācāya "
"veramaṇiyā samādapeti, pisuṇāya vācāya veramaṇiyā ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:2.8"
msgid ""
"attanā ca pharusāya vācāya paṭivirato hoti, parañca pharusāya vācāya "
"veramaṇiyā samādapeti, pharusāya vācāya veramaṇiyā ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:2.9"
msgid ""
"attanā ca samphappalāpā paṭivirato hoti, parañca samphappalāpā veramaṇiyā "
"samādapeti, samphappalāpā veramaṇiyā ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:2.10"
msgid ""
"attanā ca anabhijjhālu hoti, parañca anabhijjhāya samādapeti, anabhijjhāya "
"ca samanuñño hoti;"
msgstr ""

msgctxt "an10.223:2.11"
msgid ""
"attanā ca abyāpannacitto hoti, parañca abyāpāde samādapeti, abyāpāde ca "
"samanuñño hoti;"
msgstr ""

msgctxt "an10.223:2.12"
msgid ""
"attanā ca sammādiṭṭhiko hoti, parañca sammādiṭṭhiyā samādapeti, "
"sammādiṭṭhiyā ca samanuñño hoti—"
msgstr ""

msgctxt "an10.223:2.13"
msgid ""
"imehi kho, bhikkhave, tiṃsāya dhammehi samannāgato yathābhataṃ nikkhitto "
"evaṃ sagge”ti."
msgstr ""
