#. <section class="sutta" id="an6.73"><article><div class="hgroup"><p class="division">
msgctxt "an6.73:0.1"
msgid "Aṅguttara Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "an6.73:0.2"
msgid "7. Devatāvagga"
msgstr ""

#. </p><h1>
msgctxt "an6.73:0.3"
msgid "73. Paṭhamatajjhānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an6.73:1.1"
msgid ""
"“Cha, bhikkhave, dhamme appahāya abhabbo paṭhamaṃ jhānaṃ upasampajja "
"viharituṃ."
msgstr ""

msgctxt "an6.73:1.2"
msgid "Katame cha?"
msgstr ""

msgctxt "an6.73:1.3"
msgid "Kāmacchandaṃ, byāpādaṃ, thinamiddhaṃ, uddhaccakukkuccaṃ, vicikicchaṃ."
msgstr ""

msgctxt "an6.73:1.4"
msgid "Kāmesu kho panassa ādīnavo na yathābhūtaṃ sammappaññāya sudiṭṭho hoti."
msgstr ""

msgctxt "an6.73:1.5"
msgid ""
"Ime kho, bhikkhave, cha dhamme appahāya abhabbo paṭhamaṃ jhānaṃ upasampajja "
"viharituṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an6.73:2.1"
msgid ""
"Cha, bhikkhave, dhamme pahāya bhabbo paṭhamaṃ jhānaṃ upasampajja viharituṃ."
msgstr ""

msgctxt "an6.73:2.2"
msgid "Katame cha?"
msgstr ""

msgctxt "an6.73:2.3"
msgid "Kāmacchandaṃ, byāpādaṃ, thinamiddhaṃ, uddhaccakukkuccaṃ, vicikicchaṃ, "
msgstr ""

msgctxt "an6.73:2.4"
msgid "kāmesu kho panassa ādīnavo na yathābhūtaṃ sammappaññāya sudiṭṭho hoti."
msgstr ""

msgctxt "an6.73:2.5"
msgid ""
"Ime kho, bhikkhave, cha dhamme pahāya bhabbo paṭhamaṃ jhānaṃ upasampajja "
"viharitun”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.73:2.6"
msgid "Navamaṃ."
msgstr ""
