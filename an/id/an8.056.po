#. <section class="sutta" id="an8.56"><article><div class="hgroup"><p class="division">
msgctxt "an8.56:0.1"
msgid "Aṅguttara Nikāya 8"
msgstr ""

#. </p><p>
msgctxt "an8.56:0.2"
msgid "6. Gotamīvagga"
msgstr ""

#. </p><h1>
#. VAR: 56. Bhayasutta → kāmādhivacanasuttaṃ (bj)
msgctxt "an8.56:0.3"
msgid "56. Bhayasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.290"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an8.56:1.1"
msgid "“‘Bhayan’ti, bhikkhave, kāmānametaṃ adhivacanaṃ."
msgstr ""

msgctxt "an8.56:1.2"
msgid "‘Dukkhan’ti, bhikkhave, kāmānametaṃ adhivacanaṃ."
msgstr ""

msgctxt "an8.56:1.3"
msgid "‘Rogo’ti, bhikkhave, kāmānametaṃ adhivacanaṃ."
msgstr ""

msgctxt "an8.56:1.4"
msgid "‘Gaṇḍo’ti, bhikkhave, kāmānametaṃ adhivacanaṃ."
msgstr ""

msgctxt "an8.56:1.5"
msgid "‘Sallan’ti, bhikkhave, kāmānametaṃ adhivacanaṃ."
msgstr ""

msgctxt "an8.56:1.6"
msgid "‘Saṅgo’ti, bhikkhave, kāmānametaṃ adhivacanaṃ."
msgstr ""

msgctxt "an8.56:1.7"
msgid "‘Paṅko’ti, bhikkhave, kāmānametaṃ adhivacanaṃ."
msgstr ""

msgctxt "an8.56:1.8"
msgid "‘Gabbho’ti, bhikkhave, kāmānametaṃ adhivacanaṃ."
msgstr ""

msgctxt "an8.56:1.9"
msgid "Kasmā ca, bhikkhave, ‘bhayan’ti kāmānametaṃ adhivacanaṃ?"
msgstr ""

msgctxt "an8.56:1.10"
msgid ""
"Yasmā ca kāmarāgarattāyaṃ, bhikkhave, chandarāgavinibaddho diṭṭhadhammikāpi "
"bhayā na parimuccati, samparāyikāpi bhayā na parimuccati, tasmā ‘bhayan’ti "
"kāmānametaṃ adhivacanaṃ."
msgstr ""

msgctxt "an8.56:1.11"
msgid "Kasmā ca, bhikkhave, ‘dukkhan’ti … pe …"
msgstr ""

msgctxt "an8.56:1.12"
msgid "‘rogo’ti …"
msgstr ""

msgctxt "an8.56:1.13"
msgid "‘gaṇḍo’ti …"
msgstr ""

msgctxt "an8.56:1.14"
msgid "‘sallan’ti …"
msgstr ""

msgctxt "an8.56:1.15"
msgid "‘saṅgo’ti …"
msgstr ""

msgctxt "an8.56:1.16"
msgid "‘paṅko’ti …"
msgstr ""

msgctxt "an8.56:1.17"
msgid "‘gabbho’ti kāmānametaṃ adhivacanaṃ?"
msgstr ""

msgctxt "an8.56:1.18"
msgid ""
"Yasmā ca kāmarāgarattāyaṃ, bhikkhave, chandarāgavinibaddho diṭṭhadhammikāpi "
"gabbhā na parimuccati, samparāyikāpi gabbhā na parimuccati, tasmā ‘gabbho’ti "
"kāmānametaṃ adhivacanaṃ."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an8.56:2.1"
msgid "Bhayaṃ dukkhañca rogo ca,"
msgstr ""

#. <br>
msgctxt "an8.56:2.2"
msgid "gaṇḍo sallañca saṅgo ca;"
msgstr ""

#. <br>
msgctxt "an8.56:2.3"
msgid "Paṅko gabbho ca ubhayaṃ,"
msgstr ""

#. <br>
msgctxt "an8.56:2.4"
msgid "ete kāmā pavuccanti;"
msgstr ""

#. <br>
msgctxt "an8.56:2.5"
msgid "Yattha satto puthujjano."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an8.56:3.1"
msgid "Otiṇṇo sātarūpena,"
msgstr ""

#. <br>
msgctxt "an8.56:3.2"
msgid "puna gabbhāya gacchati;"
msgstr ""

#. <br>
msgctxt "an8.56:3.3"
msgid "Yato ca bhikkhu ātāpī,"
msgstr ""

#. <br>
#. VAR: sampajaññaṃ → sampajaññena (s1, s2) | sampajaññe (s3, mr)
msgctxt "an8.56:3.4"
msgid "sampajaññaṃ na riccati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an8.56:4.1"
msgid "So imaṃ palipathaṃ duggaṃ,"
msgstr ""

# following BB to read palipatha = palipa+tha
#. <br>
msgctxt "an8.56:4.2"
msgid "atikkamma tathāvidho;"
msgstr ""

#. <br>
msgctxt "an8.56:4.3"
msgid "Pajaṃ jātijarūpetaṃ,"
msgstr ""

#. <br>
msgctxt "an8.56:4.4"
msgid "phandamānaṃ avekkhatī”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an8.56:4.5"
msgid "Chaṭṭhaṃ."
msgstr ""
