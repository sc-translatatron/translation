#. <section class="sutta" id="an3.155"><article><div class="hgroup"><p class="division">
msgctxt "an3.155:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.155:0.2"
msgid "15. Maṅgalavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.155:0.3"
msgid "155. Pubbaṇhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. VAR: Ye → ye hi (bj, pts1)
msgctxt "an3.155:1.1"
msgid ""
"“Ye, bhikkhave, sattā pubbaṇhasamayaṃ kāyena sucaritaṃ caranti, vācāya "
"sucaritaṃ caranti, manasā sucaritaṃ caranti, supubbaṇho, bhikkhave, tesaṃ "
"sattānaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.155:2.1"
msgid ""
"Ye, bhikkhave, sattā majjhanhikasamayaṃ kāyena sucaritaṃ caranti, vācāya "
"sucaritaṃ caranti, manasā sucaritaṃ caranti, sumajjhanhiko, bhikkhave, tesaṃ "
"sattānaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an3.155:3.1"
msgid ""
"Ye, bhikkhave, sattā sāyanhasamayaṃ kāyena sucaritaṃ caranti, vācāya "
"sucaritaṃ caranti, manasā sucaritaṃ caranti, susāyanho, bhikkhave, tesaṃ "
"sattānanti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an3.155:4.1"
msgid "Sunakkhattaṃ sumaṅgalaṃ,"
msgstr ""

#. <br>
#. VAR: suhuṭṭhitaṃ → suvuṭṭhitaṃ (bj, pts1)
msgctxt "an3.155:4.2"
msgid "suppabhātaṃ suhuṭṭhitaṃ;"
msgstr ""

#. <br>
msgctxt "an3.155:4.3"
msgid "Sukhaṇo sumuhutto ca,"
msgstr ""

#. <br>
msgctxt "an3.155:4.4"
msgid "suyiṭṭhaṃ brahmacārisu."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an3.155:5.1"
msgid "Padakkhiṇaṃ kāyakammaṃ,"
msgstr ""

#. <br>
msgctxt "an3.155:5.2"
msgid "vācākammaṃ padakkhiṇaṃ;"
msgstr ""

#. <br>
msgctxt "an3.155:5.3"
msgid "Padakkhiṇaṃ manokammaṃ,"
msgstr ""

#. <br>
#. VAR: paṇīdhi te padakkhiṇe → paṇidhiyo padakkhiṇā (bj) | paṇidhi te padakkhiṇā (s1-3, km) | paṇidhīyo padakkhiṇā (pts1)
msgctxt "an3.155:5.4"
msgid "paṇīdhi te padakkhiṇe;"
msgstr ""

#. <br>
msgctxt "an3.155:5.5"
msgid "Padakkhiṇāni katvāna,"
msgstr ""

#. <br>
#. VAR: labhantatthe → labhatatthe (bj, pts1)
msgctxt "an3.155:5.6"
msgid "labhantatthe padakkhiṇe."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an3.155:6.1"
msgid "Te atthaladdhā sukhitā,"
msgstr ""

#. <br>
msgctxt "an3.155:6.2"
msgid "viruḷhā buddhasāsane;"
msgstr ""

#. <br>
msgctxt "an3.155:6.3"
msgid "Arogā sukhitā hotha,"
msgstr ""

#. <br>
msgctxt "an3.155:6.4"
msgid "saha sabbehi ñātibhī”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an3.155:6.5"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.155:6.6"
msgid "Maṅgalavaggo pañcamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an3.155:7.1"
msgid "Akusalañca sāvajjaṃ,"
msgstr ""

#. <br>
msgctxt "an3.155:7.2"
msgid "visamāsucinā saha;"
msgstr ""

#. <br>
msgctxt "an3.155:7.3"
msgid "Caturo khatā vandanā,"
msgstr ""

#. <br>
msgctxt "an3.155:7.4"
msgid "pubbaṇhena ca te dasāti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an3.155:7.5"
msgid "Tatiyo paṇṇāsako samatto."
msgstr ""
