#. <section class="sutta" id="an10.114"><article><div class="hgroup"><p class="division">
msgctxt "an10.114:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.114:0.2"
msgid "12. Paccorohaṇivagga"
msgstr ""

#. </p><h1>
msgctxt "an10.114:0.3"
msgid "114. Dutiyaadhammasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.114:1.1"
msgid "“Adhammo ca, bhikkhave, veditabbo dhammo ca;"
msgstr ""

msgctxt "an10.114:1.2"
msgid "anattho ca veditabbo attho ca."
msgstr ""

msgctxt "an10.114:1.3"
msgid ""
"Adhammañca viditvā dhammañca, anatthañca viditvā atthañca yathā dhammo yathā "
"attho tathā paṭipajjitabbaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.114:2.1"
msgid ""
"Katamo ca, bhikkhave, adhammo, katamo ca dhammo, katamo ca anattho, katamo "
"ca attho?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an10.114:3.1"
msgid "Micchādiṭṭhi, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.114:3.2"
msgid "sammādiṭṭhi dhammo;"
msgstr ""

msgctxt "an10.114:3.3"
msgid ""
"ye ca micchādiṭṭhipaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.114:3.4"
msgid ""
"sammādiṭṭhipaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho. (1)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an10.114:4.1"
msgid "Micchāsaṅkappo, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.114:4.2"
msgid "sammāsaṅkappo dhammo;"
msgstr ""

msgctxt "an10.114:4.3"
msgid ""
"ye ca micchāsaṅkappapaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.114:4.4"
msgid ""
"sammāsaṅkappapaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho. (2)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an10.114:5.1"
msgid "Micchāvācā, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.114:5.2"
msgid "sammāvācā dhammo;"
msgstr ""

msgctxt "an10.114:5.3"
msgid ""
"ye ca micchāvācāpaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.114:5.4"
msgid ""
"sammāvācāpaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho. (3)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.224"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "an10.114:6.1"
msgid "Micchākammanto, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.114:6.2"
msgid "sammākammanto dhammo;"
msgstr ""

msgctxt "an10.114:6.3"
msgid ""
"ye ca micchākammantapaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.114:6.4"
msgid ""
"sammākammantapaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho. (4)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an10.114:7.1"
msgid "Micchāājīvo, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.114:7.2"
msgid "sammāājīvo dhammo;"
msgstr ""

msgctxt "an10.114:7.3"
msgid ""
"ye ca micchāājīvapaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.114:7.4"
msgid ""
"sammāājīvapaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho. (5)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an10.114:8.1"
msgid "Micchāvāyāmo, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.114:8.2"
msgid "sammāvāyāmo dhammo;"
msgstr ""

msgctxt "an10.114:8.3"
msgid ""
"ye ca micchāvāyāmapaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.114:8.4"
msgid ""
"sammāvāyāmapaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho. (6)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an10.114:9.1"
msgid "Micchāsati, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.114:9.2"
msgid "sammāsati dhammo;"
msgstr ""

msgctxt "an10.114:9.3"
msgid ""
"ye ca micchāsatipaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.114:9.4"
msgid ""
"sammāsatipaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho. (7)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "an10.114:10.1"
msgid "Micchāsamādhi, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.114:10.2"
msgid "sammāsamādhi dhammo;"
msgstr ""

msgctxt "an10.114:10.3"
msgid ""
"ye ca micchāsamādhipaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.114:10.4"
msgid ""
"sammāsamādhipaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho. (8)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "an10.114:11.1"
msgid "Micchāñāṇaṃ, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.114:11.2"
msgid "sammāñāṇaṃ dhammo;"
msgstr ""

msgctxt "an10.114:11.3"
msgid ""
"ye ca micchāñāṇapaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.114:11.4"
msgid ""
"sammāñāṇapaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho. (9)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "an10.114:12.1"
msgid "Micchāvimutti, bhikkhave, adhammo;"
msgstr ""

msgctxt "an10.114:12.2"
msgid "sammāvimutti dhammo;"
msgstr ""

msgctxt "an10.114:12.3"
msgid ""
"ye ca micchāvimuttipaccayā aneke pāpakā akusalā dhammā sambhavanti, ayaṃ "
"anattho;"
msgstr ""

msgctxt "an10.114:12.4"
msgid ""
"sammāvimuttipaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti, ayaṃ "
"attho. (10)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc13"></a>
msgctxt "an10.114:13.1"
msgid "‘Adhammo ca, bhikkhave, veditabbo dhammo ca;"
msgstr ""

msgctxt "an10.114:13.2"
msgid "anattho ca veditabbo attho ca."
msgstr ""

msgctxt "an10.114:13.3"
msgid ""
"Adhammañca viditvā dhammañca, anatthañca viditvā atthañca yathā dhammo yathā "
"attho tathā paṭipajjitabban’ti, "
msgstr ""

msgctxt "an10.114:13.4"
msgid "iti yaṃ taṃ vuttaṃ, idametaṃ paṭicca vuttan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.114:13.5"
msgid "Dutiyaṃ."
msgstr ""
