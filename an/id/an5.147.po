#. <section class="sutta" id="an5.147"><article><div class="hgroup"><p class="division">
msgctxt "an5.147:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.147:0.2"
msgid "15. Tikaṇḍakīvagga"
msgstr ""

#. </p><h1>
msgctxt "an5.147:0.3"
msgid "147. Asappurisadānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.147:1.1"
msgid "“Pañcimāni, bhikkhave, asappurisadānāni."
msgstr ""

msgctxt "an5.147:1.2"
msgid "Katamāni pañca?"
msgstr ""

#. VAR: acittīkatvā → acitiṃ katvā (s1-3) | acittikatvā (pts1) | acittiṃ katvā (mr)
#. VAR: apaviddhaṃ → apaviṭṭhaṃ (s1-3, km)
msgctxt "an5.147:1.3"
msgid ""
"Asakkaccaṃ deti, acittīkatvā deti, asahatthā deti, apaviddhaṃ deti, "
"anāgamanadiṭṭhiko deti."
msgstr ""

msgctxt "an5.147:1.4"
msgid "Imāni kho, bhikkhave, pañca asappurisadānāni."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.172"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.147:2.1"
msgid "Pañcimāni, bhikkhave, sappurisadānāni."
msgstr ""

msgctxt "an5.147:2.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "an5.147:2.3"
msgid ""
"Sakkaccaṃ deti, cittīkatvā deti, sahatthā deti, anapaviddhaṃ deti, "
"āgamanadiṭṭhiko deti."
msgstr ""

msgctxt "an5.147:2.4"
msgid "Imāni kho, bhikkhave, pañca sappurisadānānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.147:2.5"
msgid "Sattamaṃ."
msgstr ""
