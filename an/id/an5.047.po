#. <section class="sutta" id="an5.47"><article><div class="hgroup"><p class="division">
msgctxt "an5.47:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.47:0.2"
msgid "5. Muṇḍarājavagga"
msgstr ""

#. </p><h1>
#. VAR: pañcadhanasuttaṃ (bj)
msgctxt "an5.47:0.3"
msgid "47. Dhanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.47:1.1"
msgid "“Pañcimāni, bhikkhave, dhanāni."
msgstr ""

msgctxt "an5.47:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "an5.47:1.3"
msgid "Saddhādhanaṃ, sīladhanaṃ, sutadhanaṃ, cāgadhanaṃ, paññādhanaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.47:2.1"
msgid "Katamañca, bhikkhave, saddhādhanaṃ?"
msgstr ""

msgctxt "an5.47:2.2"
msgid ""
"Idha, bhikkhave, ariyasāvako saddho hoti, saddahati tathāgatassa bodhiṃ:"
msgstr ""

msgctxt "an5.47:2.3"
msgid "‘itipi so bhagavā … pe … satthā devamanussānaṃ buddho bhagavā’ti."
msgstr ""

msgctxt "an5.47:2.4"
msgid "Idaṃ vuccati, bhikkhave, saddhādhanaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.47:3.1"
msgid "Katamañca, bhikkhave, sīladhanaṃ?"
msgstr ""

msgctxt "an5.47:3.2"
msgid ""
"Idha, bhikkhave, ariyasāvako pāṇātipātā paṭivirato hoti … pe … "
"surāmerayamajjapamādaṭṭhānā paṭivirato hoti."
msgstr ""

msgctxt "an5.47:3.3"
msgid "Idaṃ vuccati, bhikkhave, sīladhanaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an5.47:4.1"
msgid "Katamañca, bhikkhave, sutadhanaṃ?"
msgstr ""

msgctxt "an5.47:4.2"
msgid ""
"Idha, bhikkhave, ariyasāvako bahussuto hoti … pe … diṭṭhiyā suppaṭividdho."
msgstr ""

msgctxt "an5.47:4.3"
msgid "Idaṃ vuccati, bhikkhave, sutadhanaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an5.47:5.1"
msgid "Katamañca, bhikkhave, cāgadhanaṃ?"
msgstr ""

#. VAR: vosaggarato → vossaggarato (s1-3, pts1, c1)
msgctxt "an5.47:5.2"
msgid ""
"Idha, bhikkhave, ariyasāvako vigatamalamaccherena cetasā agāraṃ ajjhāvasati "
"muttacāgo payatapāṇi vosaggarato yācayogo dānasaṃvibhāgarato."
msgstr ""

msgctxt "an5.47:5.3"
msgid "Idaṃ vuccati, bhikkhave, cāgadhanaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an5.47:6.1"
msgid "Katamañca, bhikkhave, paññādhanaṃ?"
msgstr ""

msgctxt "an5.47:6.2"
msgid ""
"Idha, bhikkhave, ariyasāvako paññavā hoti, udayatthagāminiyā paññāya "
"samannāgato ariyāya nibbedhikāya sammā dukkhakkhayagāminiyā."
msgstr ""

msgctxt "an5.47:6.3"
msgid "Idaṃ vuccati, bhikkhave, paññādhanaṃ."
msgstr ""

msgctxt "an5.47:6.4"
msgid "Imāni kho, bhikkhave, pañca dhanānīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.54"></a>
#. <a class="sc" id="sc7"></a>
msgctxt "an5.47:7.1"
msgid "Yassa saddhā tathāgate,"
msgstr ""

#. <br>
msgctxt "an5.47:7.2"
msgid "acalā suppatiṭṭhitā;"
msgstr ""

#. <br>
msgctxt "an5.47:7.3"
msgid "Sīlañca yassa kalyāṇaṃ,"
msgstr ""

#. <br>
msgctxt "an5.47:7.4"
msgid "ariyakantaṃ pasaṃsitaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an5.47:8.1"
msgid "Saṅghe pasādo yassatthi,"
msgstr ""

#. <br>
msgctxt "an5.47:8.2"
msgid "ujubhūtañca dassanaṃ;"
msgstr ""

#. <br>
msgctxt "an5.47:8.3"
msgid "Adaliddoti taṃ āhu,"
msgstr ""

#. <br>
msgctxt "an5.47:8.4"
msgid "amoghaṃ tassa jīvitaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an5.47:9.1"
msgid "Tasmā saddhañca sīlañca,"
msgstr ""

#. <br>
msgctxt "an5.47:9.2"
msgid "pasādaṃ dhammadassanaṃ;"
msgstr ""

#. <br>
msgctxt "an5.47:9.3"
msgid "Anuyuñjetha medhāvī,"
msgstr ""

#. <br>
msgctxt "an5.47:9.4"
msgid "saraṃ buddhāna sāsanan”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an5.47:9.5"
msgid "Sattamaṃ."
msgstr ""
