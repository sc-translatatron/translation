#. <section class="sutta" id="an8.78"><article><div class="hgroup"><p class="division">
msgctxt "an8.78:0.1"
msgid "Aṅguttara Nikāya 8"
msgstr ""

#. </p><p>
msgctxt "an8.78:0.2"
msgid "8. Yamakavagga"
msgstr ""

#. </p><h1>
msgctxt "an8.78:0.3"
msgid "78. Alaṃsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an8.78:1.1"
msgid "Tatra kho āyasmā sāriputto bhikkhū āmantesi … pe …"
msgstr ""

msgctxt "an8.78:1.2"
msgid "chahāvuso, dhammehi samannāgato bhikkhu alaṃ attano, alaṃ paresaṃ."
msgstr ""

msgctxt "an8.78:1.3"
msgid "Katamehi chahi?"
msgstr ""

msgctxt "an8.78:1.4"
msgid "Idhāvuso, bhikkhu khippanisanti ca hoti kusalesu dhammesu;"
msgstr ""

msgctxt "an8.78:1.5"
msgid "sutānañca dhammānaṃ dhāraṇajātiko hoti;"
msgstr ""

msgctxt "an8.78:1.6"
msgid "dhātānañca dhammānaṃ atthūpaparikkhitā hoti;"
msgstr ""

msgctxt "an8.78:1.7"
msgid "atthamaññāya dhammamaññāya dhammānudhammappaṭipanno ca hoti;"
msgstr ""

msgctxt "an8.78:1.8"
msgid ""
"kalyāṇavāco ca hoti kalyāṇavākkaraṇo, poriyā vācāya samannāgato vissaṭṭhāya "
"anelagaḷāya atthassa viññāpaniyā;"
msgstr ""

msgctxt "an8.78:1.9"
msgid "sandassako ca hoti samādapako samuttejako sampahaṃsako sabrahmacārīnaṃ."
msgstr ""

msgctxt "an8.78:1.10"
msgid ""
"Imehi kho, āvuso, chahi dhammehi samannāgato bhikkhu alaṃ attano, alaṃ "
"paresaṃ."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.329"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an8.78:2.1"
msgid "Pañcahāvuso, dhammehi samannāgato bhikkhu alaṃ attano, alaṃ paresaṃ."
msgstr ""

msgctxt "an8.78:2.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an8.78:2.3"
msgid "Idhāvuso, bhikkhu na heva kho khippanisanti ca hoti kusalesu dhammesu;"
msgstr ""

msgctxt "an8.78:2.4"
msgid "sutānañca dhammānaṃ dhāraṇajātiko hoti;"
msgstr ""

msgctxt "an8.78:2.5"
msgid "dhātānañca dhammānaṃ atthūpaparikkhitā hoti;"
msgstr ""

msgctxt "an8.78:2.6"
msgid "atthamaññāya dhammamaññāya dhammānudhammappaṭipanno ca hoti;"
msgstr ""

msgctxt "an8.78:2.7"
msgid "kalyāṇavāco ca hoti … pe …"
msgstr ""

msgctxt "an8.78:2.8"
msgid "sandassako ca hoti … pe … sabrahmacārīnaṃ."
msgstr ""

msgctxt "an8.78:2.9"
msgid ""
"Imehi kho, āvuso, pañcahi dhammehi samannāgato bhikkhu alaṃ attano, alaṃ "
"paresaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an8.78:3.1"
msgid "Catūhāvuso, dhammehi samannāgato bhikkhu alaṃ attano, nālaṃ paresaṃ."
msgstr ""

msgctxt "an8.78:3.2"
msgid "Katamehi catūhi?"
msgstr ""

msgctxt "an8.78:3.3"
msgid "Idhāvuso, bhikkhu khippanisanti ca hoti kusalesu dhammesu;"
msgstr ""

msgctxt "an8.78:3.4"
msgid "sutānañca dhammānaṃ dhāraṇajātiko hoti;"
msgstr ""

msgctxt "an8.78:3.5"
msgid "dhātānañca dhammānaṃ atthūpaparikkhitā hoti;"
msgstr ""

msgctxt "an8.78:3.6"
msgid "atthamaññāya dhammamaññāya dhammānudhammappaṭipanno ca hoti;"
msgstr ""

msgctxt "an8.78:3.7"
msgid "no ca kalyāṇavāco hoti … pe …"
msgstr ""

msgctxt "an8.78:3.8"
msgid "no ca sandassako hoti … pe … sabrahmacārīnaṃ."
msgstr ""

msgctxt "an8.78:3.9"
msgid ""
"Imehi kho, āvuso, catūhi dhammehi samannāgato bhikkhu alaṃ attano, nālaṃ "
"paresaṃ."
msgstr ""

#. </p><p>
