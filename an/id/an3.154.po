#. <a class="sc" id="sc1"></a>
msgctxt "an3.154:1.1"
msgid "“Tisso imā, bhikkhave, vandanā."
msgstr ""

msgctxt "an3.154:1.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "an3.154:1.3"
msgid "Kāyena, vācāya, manasā—"
msgstr ""

msgctxt "an3.154:1.4"
msgid "imā kho, bhikkhave, tisso vandanā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.154:1.5"
msgid "Navamaṃ."
msgstr ""
