#. <section class="sutta" id="an3.4"><article><div class="hgroup"><p class="division">
msgctxt "an3.4:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.4:0.2"
msgid "1. Bālavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.4:0.3"
msgid "4. Accayasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.4:1.1"
msgid "“Tīhi, bhikkhave, dhammehi samannāgato bālo veditabbo."
msgstr ""

msgctxt "an3.4:1.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an3.4:1.3"
msgid ""
"Accayaṃ accayato na passati, accayaṃ accayato disvā yathādhammaṃ "
"nappaṭikaroti, parassa kho pana accayaṃ desentassa yathādhammaṃ "
"nappaṭiggaṇhāti."
msgstr ""

msgctxt "an3.4:1.4"
msgid "Imehi kho, bhikkhave, tīhi dhammehi samannāgato bālo veditabbo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.4:2.1"
msgid "Tīhi, bhikkhave, dhammehi samannāgato paṇḍito veditabbo."
msgstr ""

msgctxt "an3.4:2.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an3.4:2.3"
msgid ""
"Accayaṃ accayato passati, accayaṃ accayato disvā yathādhammaṃ paṭikaroti, "
"parassa kho pana accayaṃ desentassa yathādhammaṃ paṭiggaṇhāti."
msgstr ""

msgctxt "an3.4:2.4"
msgid "Imehi kho, bhikkhave, tīhi dhammehi samannāgato paṇḍito veditabbo."
msgstr ""

msgctxt "an3.4:2.5"
msgid "Tasmātiha … pe …."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.4:2.6"
msgid "Catutthaṃ."
msgstr ""
