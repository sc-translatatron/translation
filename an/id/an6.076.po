#. <section class="sutta" id="an6.76"><article><div class="hgroup"><p class="division">
msgctxt "an6.76:0.1"
msgid "Aṅguttara Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "an6.76:0.2"
msgid "8. Arahattavagga"
msgstr ""

#. </p><h1>
msgctxt "an6.76:0.3"
msgid "76. Arahattasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.430"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an6.76:1.1"
msgid "“Cha, bhikkhave, dhamme appahāya abhabbo arahattaṃ sacchikātuṃ."
msgstr ""

msgctxt "an6.76:1.2"
msgid "Katame cha?"
msgstr ""

msgctxt "an6.76:1.3"
msgid "Mānaṃ, omānaṃ, atimānaṃ, adhimānaṃ, thambhaṃ, atinipātaṃ."
msgstr ""

msgctxt "an6.76:1.4"
msgid "Ime kho, bhikkhave, cha dhamme appahāya abhabbo arahattaṃ sacchikātuṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an6.76:2.1"
msgid "Cha, bhikkhave, dhamme pahāya bhabbo arahattaṃ sacchikātuṃ."
msgstr ""

msgctxt "an6.76:2.2"
msgid "Katame cha?"
msgstr ""

msgctxt "an6.76:2.3"
msgid "Mānaṃ, omānaṃ, atimānaṃ, adhimānaṃ, thambhaṃ, atinipātaṃ."
msgstr ""

msgctxt "an6.76:2.4"
msgid "Ime kho, bhikkhave, cha dhamme pahāya bhabbo arahattaṃ sacchikātun”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.76:2.5"
msgid "Dutiyaṃ."
msgstr ""
