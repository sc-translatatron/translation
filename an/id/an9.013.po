msgstr ""

msgctxt "an9.13:7.2"
msgid "“No hidaṃ, āvuso”."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.383"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "an9.13:8.1"
msgid ""
