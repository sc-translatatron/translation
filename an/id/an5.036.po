#. <section class="sutta" id="an5.36"><article><div class="hgroup"><p class="division">
msgctxt "an5.36:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.36:0.2"
msgid "4. Sumanavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.36:0.3"
msgid "36. Kāladānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.36:1.1"
msgid "“Pañcimāni, bhikkhave, kāladānāni."
msgstr ""

msgctxt "an5.36:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "an5.36:1.3"
msgid "Āgantukassa dānaṃ deti;"
msgstr ""

msgctxt "an5.36:1.4"
msgid "gamikassa dānaṃ deti;"
msgstr ""

msgctxt "an5.36:1.5"
msgid "gilānassa dānaṃ deti;"
msgstr ""

msgctxt "an5.36:1.6"
msgid "dubbhikkhe dānaṃ deti;"
msgstr ""

msgctxt "an5.36:1.7"
msgid ""
"yāni tāni navasassāni navaphalāni tāni paṭhamaṃ sīlavantesu patiṭṭhāpeti."
msgstr ""

msgctxt "an5.36:1.8"
msgid "Imāni kho, bhikkhave, pañca kāladānānīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.36:2.1"
msgid "Kāle dadanti sappaññā,"
msgstr ""

#. <br>
msgctxt "an5.36:2.2"
msgid "vadaññū vītamaccharā;"
msgstr ""

#. <br>
msgctxt "an5.36:2.3"
msgid "Kālena dinnaṃ ariyesu,"
msgstr ""

#. <br>
msgctxt "an5.36:2.4"
msgid "ujubhūtesu tādisu."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.36:3.1"
msgid "Vippasannamanā tassa,"
msgstr ""

#. <br>
msgctxt "an5.36:3.2"
msgid "vipulā hoti dakkhiṇā;"
msgstr ""

#. <br>
msgctxt "an5.36:3.3"
msgid "Ye tattha anumodanti,"
msgstr ""

#. <br>
msgctxt "an5.36:3.4"
msgid "veyyāvaccaṃ karonti vā;"
msgstr ""

#. <br>
#. VAR: Na tena → na tesaṃ (bj, pts1, mr)
msgctxt "an5.36:3.5"
msgid "Na tena dakkhiṇā ūnā,"
msgstr ""

#. <br>
msgctxt "an5.36:3.6"
msgid "tepi puññassa bhāgino."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an5.36:4.1"
msgid "Tasmā dade appaṭivānacitto,"
msgstr ""

#. <br>
msgctxt "an5.36:4.2"
msgid "Yattha dinnaṃ mahapphalaṃ;"
msgstr ""

#. <br>
msgctxt "an5.36:4.3"
msgid "Puññāni paralokasmiṃ,"
msgstr ""

#. <br>
msgctxt "an5.36:4.4"
msgid "Patiṭṭhā honti pāṇinan”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an5.36:4.5"
msgid "Chaṭṭhaṃ."
msgstr ""
