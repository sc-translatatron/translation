#. <section class="sutta" id="an4.156"><article><div class="hgroup"><p class="division">
msgctxt "an4.156:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.156:0.2"
msgid "16. Indriyavagga"
msgstr ""

#. </p><h1>
#. VAR: asaṅkheyyasuttaṃ (bj)
msgctxt "an4.156:0.3"
msgid "156. Kappasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. VAR: asaṅkhyeyyāni → asaṅkheyyāni (bj, s1-3)
msgctxt "an4.156:1.1"
msgid "“Cattārimāni, bhikkhave, kappassa asaṅkhyeyyāni."
msgstr ""

msgctxt "an4.156:1.2"
msgid "Katamāni cattāri?"
msgstr ""

msgctxt "an4.156:1.3"
msgid "Yadā, bhikkhave, kappo saṃvaṭṭati, taṃ na sukaraṃ saṅkhātuṃ—"
msgstr ""

msgctxt "an4.156:1.4"
msgid ""
"ettakāni vassānīti vā, ettakāni vassasatānīti vā, ettakāni vassasahassānīti "
"vā, ettakāni vassasatasahassānīti vā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.156:2.1"
msgid "Yadā, bhikkhave, kappo saṃvaṭṭo tiṭṭhati, taṃ na sukaraṃ saṅkhātuṃ—"
msgstr ""

msgctxt "an4.156:2.2"
msgid ""
"ettakāni vassānīti vā, ettakāni vassasatānīti vā, ettakāni vassasahassānīti "
"vā, ettakāni vassasatasahassānīti vā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.156:3.1"
msgid "Yadā, bhikkhave, kappo vivaṭṭati, taṃ na sukaraṃ saṅkhātuṃ—"
msgstr ""

msgctxt "an4.156:3.2"
msgid ""
"ettakāni vassānīti vā, ettakāni vassasatānīti vā, ettakāni vassasahassānīti "
"vā, ettakāni vassasatasahassānīti vā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.156:4.1"
msgid "Yadā, bhikkhave, kappo vivaṭṭo tiṭṭhati, taṃ na sukaraṃ saṅkhātuṃ—"
msgstr ""

msgctxt "an4.156:4.2"
msgid ""
"ettakāni vassānīti vā, ettakāni vassasatānīti vā, ettakāni vassasahassānīti "
"vā, ettakāni vassasatasahassānīti vā."
msgstr ""

msgctxt "an4.156:4.3"
msgid "Imāni kho, bhikkhave, cattāri kappassa asaṅkhyeyyānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.156:4.4"
msgid "Chaṭṭhaṃ."
msgstr ""
