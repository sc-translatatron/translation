#. <section class="sutta" id="an10.13"><article><div class="hgroup"><p class="division">
msgctxt "an10.13:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.13:0.2"
msgid "2. Nāthavagga"
msgstr ""

#. </p><h1>
msgctxt "an10.13:0.3"
msgid "13. Saṃyojanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.13:1.1"
msgid "“Dasayimāni, bhikkhave, saṃyojanāni."
msgstr ""

msgctxt "an10.13:1.2"
msgid "Katamāni dasa?"
msgstr ""

msgctxt "an10.13:1.3"
msgid "Pañcorambhāgiyāni saṃyojanāni, pañcuddhambhāgiyāni saṃyojanāni."
msgstr ""

msgctxt "an10.13:1.4"
msgid "Katamāni pañcorambhāgiyāni saṃyojanāni?"
msgstr ""

msgctxt "an10.13:1.5"
msgid "Sakkāyadiṭṭhi, vicikicchā, sīlabbataparāmāso, kāmacchando, byāpādo—"
msgstr ""

msgctxt "an10.13:1.6"
msgid "imāni pañcorambhāgiyāni saṃyojanāni."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.13:2.1"
msgid "Katamāni pañcuddhambhāgiyāni saṃyojanāni?"
msgstr ""

msgctxt "an10.13:2.2"
msgid "Rūparāgo, arūparāgo, māno, uddhaccaṃ, avijjā—"
msgstr ""

msgctxt "an10.13:2.3"
msgid "imāni pañcuddhambhāgiyāni saṃyojanāni."
msgstr ""

msgctxt "an10.13:2.4"
msgid "Imāni kho, bhikkhave, dasa saṃyojanānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.13:2.5"
msgid "Tatiyaṃ."
msgstr ""
