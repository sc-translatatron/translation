#. <section id="vagga"><section class="sutta" data-uid="an2.21" id="21"><article><div class="hgroup"><p class="division">
msgctxt "an2.21-31:0.1"
msgid "Aṅguttara Nikāya 2"
msgstr ""

#. </p><p>
msgctxt "an2.21-31:0.2"
msgid "3. Bālavagga"
msgstr ""

#. </p><h1>
msgctxt "an2.21-31:0.3"
msgid "21"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc21.1"></a>
msgctxt "an2.21-31:21.1.1"
msgid "“Dveme, bhikkhave, bālā."
msgstr ""

msgctxt "an2.21-31:21.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.21-31:21.1.3"
msgid ""
"Yo ca accayaṃ accayato na passati, yo ca accayaṃ desentassa yathādhammaṃ "
"nappaṭiggaṇhāti."
msgstr ""

msgctxt "an2.21-31:21.1.4"
msgid "Ime kho, bhikkhave, dve bālāti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc21.2"></a>
msgctxt "an2.21-31:21.2.1"
msgid "Dveme, bhikkhave, paṇḍitā."
msgstr ""

msgctxt "an2.21-31:21.2.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.21-31:21.2.3"
msgid ""
"Yo ca accayaṃ accayato passati, yo ca accayaṃ desentassa yathādhammaṃ "
"paṭiggaṇhāti."
msgstr ""

msgctxt "an2.21-31:21.2.4"
msgid "Ime kho, bhikkhave, dve paṇḍitā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.22" id="22"><article><div class="hgroup"><h1>
msgctxt "an2.21-31:21.2.5"
msgid "22"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc22.1"></a>
msgctxt "an2.21-31:22.1.1"
msgid "“Dveme, bhikkhave, tathāgataṃ abbhācikkhanti."
msgstr ""

msgctxt "an2.21-31:22.1.2"
msgid "Katame dve?"
msgstr ""

#. VAR: duggahitena → duggahītena (pts1)
msgctxt "an2.21-31:22.1.3"
msgid "Duṭṭho vā dosantaro, saddho vā duggahitena."
msgstr ""

msgctxt "an2.21-31:22.1.4"
msgid "Ime kho, bhikkhave, dve tathāgataṃ abbhācikkhantī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.23" id="23"><article><div class="hgroup"><h1>
msgctxt "an2.21-31:22.1.5"
msgid "23"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.60"></a>
#. <a class="sc" id="sc23.1"></a>
msgctxt "an2.21-31:23.1.1"
msgid "“Dveme, bhikkhave, tathāgataṃ abbhācikkhanti."
msgstr ""

msgctxt "an2.21-31:23.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.21-31:23.1.3"
msgid ""
"Yo ca abhāsitaṃ alapitaṃ tathāgatena bhāsitaṃ lapitaṃ tathāgatenāti dīpeti, "
"yo ca bhāsitaṃ lapitaṃ tathāgatena abhāsitaṃ alapitaṃ tathāgatenāti dīpeti."
msgstr ""

msgctxt "an2.21-31:23.1.4"
msgid "Ime kho, bhikkhave, dve tathāgataṃ abbhācikkhantīti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc23.2"></a>
msgctxt "an2.21-31:23.2.1"
msgid "Dveme, bhikkhave, tathāgataṃ nābbhācikkhanti."
msgstr ""

msgctxt "an2.21-31:23.2.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.21-31:23.2.3"
msgid ""
"Yo ca abhāsitaṃ alapitaṃ tathāgatena abhāsitaṃ alapitaṃ tathāgatenāti "
"dīpeti, yo ca bhāsitaṃ lapitaṃ tathāgatena bhāsitaṃ lapitaṃ tathāgatenāti "
"dīpeti."
msgstr ""

msgctxt "an2.21-31:23.2.4"
msgid "Ime kho, bhikkhave, dve tathāgataṃ nābbhācikkhantī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.24" id="24"><article><div class="hgroup"><h1>
msgctxt "an2.21-31:23.2.5"
msgid "24"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc24.1"></a>
msgctxt "an2.21-31:24.1.1"
msgid "“Dveme, bhikkhave, tathāgataṃ abbhācikkhanti."
msgstr ""

msgctxt "an2.21-31:24.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.21-31:24.1.3"
msgid ""
"Yo ca neyyatthaṃ suttantaṃ nītattho suttantoti dīpeti, yo ca nītatthaṃ "
"suttantaṃ neyyattho suttantoti dīpeti."
msgstr ""

msgctxt "an2.21-31:24.1.4"
msgid "Ime kho, bhikkhave, dve tathāgataṃ abbhācikkhantī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.25" id="25"><article><div class="hgroup"><h1>
msgctxt "an2.21-31:24.1.5"
msgid "25"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc25.1"></a>
msgctxt "an2.21-31:25.1.1"
msgid "“Dveme, bhikkhave, tathāgataṃ nābbhācikkhanti."
msgstr ""

msgctxt "an2.21-31:25.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.21-31:25.1.3"
msgid ""
"Yo ca neyyatthaṃ suttantaṃ neyyattho suttantoti dīpeti, yo ca nītatthaṃ "
"suttantaṃ nītattho suttantoti dīpeti."
msgstr ""

msgctxt "an2.21-31:25.1.4"
msgid "Ime kho, bhikkhave, dve tathāgataṃ nābbhācikkhantī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.26" id="26"><article><div class="hgroup"><h1>
msgctxt "an2.21-31:25.1.5"
msgid "26"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc26.1"></a>
msgctxt "an2.21-31:26.1.1"
msgid ""
"“Paṭicchannakammantassa, bhikkhave, dvinnaṃ gatīnaṃ aññatarā gati pāṭikaṅkhā—"
msgstr ""

msgctxt "an2.21-31:26.1.2"
msgid "nirayo vā tiracchānayoni vāti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc26.2"></a>
msgctxt "an2.21-31:26.2.1"
msgid ""
"Appaṭicchannakammantassa, bhikkhave, dvinnaṃ gatīnaṃ aññatarā gati "
"pāṭikaṅkhā—"
msgstr ""

msgctxt "an2.21-31:26.2.2"
msgid "devā vā manussā vā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.27" id="27"><article><div class="hgroup"><h1>
msgctxt "an2.21-31:26.2.3"
msgid "27"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc27.1"></a>
msgctxt "an2.21-31:27.1.1"
msgid ""
"“Micchādiṭṭhikassa, bhikkhave, dvinnaṃ gatīnaṃ aññatarā gati pāṭikaṅkhā—"
msgstr ""

msgctxt "an2.21-31:27.1.2"
msgid "nirayo vā tiracchānayoni vā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.28" id="28"><article><div class="hgroup"><h1>
msgctxt "an2.21-31:27.1.3"
msgid "28"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc28.1"></a>
msgctxt "an2.21-31:28.1.1"
msgid "“Sammādiṭṭhikassa, bhikkhave, dvinnaṃ gatīnaṃ aññatarā gati pāṭikaṅkhā—"
msgstr ""

msgctxt "an2.21-31:28.1.2"
msgid "devā vā manussā vā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.29" id="29"><article><div class="hgroup"><h1>
msgctxt "an2.21-31:28.1.3"
msgid "29"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc29.1"></a>
msgctxt "an2.21-31:29.1.1"
msgid "“Dussīlassa, bhikkhave, dve paṭiggāhā—"
msgstr ""

msgctxt "an2.21-31:29.1.2"
msgid "nirayo vā tiracchānayoni vā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc29.2"></a>
#. VAR: devo vā manusso vāti (mr)
msgctxt "an2.21-31:29.2.1"
msgid "Sīlavato, bhikkhave, dve paṭiggāhā—"
msgstr ""

msgctxt "an2.21-31:29.2.2"
msgid "devā vā manussā vā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.30" id="30"><article><div class="hgroup"><h1>
msgctxt "an2.21-31:29.2.3"
msgid "30"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.61"></a>
#. <a class="sc" id="sc30.1"></a>
#. VAR: araññavanapatthāni → araññe vanapatthāni (bj, pts1)
msgctxt "an2.21-31:30.1.1"
msgid ""
"“Dvāhaṃ, bhikkhave, atthavase sampassamāno araññavanapatthāni pantāni "
"senāsanāni paṭisevāmi."
msgstr ""

msgctxt "an2.21-31:30.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.21-31:30.1.3"
msgid ""
"Attano ca diṭṭhadhammasukhavihāraṃ sampassamāno, pacchimañca janataṃ "
"anukampamāno."
msgstr ""

msgctxt "an2.21-31:30.1.4"
msgid ""
"Ime kho ahaṃ, bhikkhave, dve atthavase sampassamāno araññavanapatthāni "
"pantāni senāsanāni paṭisevāmī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.31" id="31"><article><div class="hgroup"><h1>
msgctxt "an2.21-31:30.1.5"
msgid "31"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc31.1"></a>
msgctxt "an2.21-31:31.1.1"
msgid "“Dve me, bhikkhave, dhammā vijjābhāgiyā."
msgstr ""

msgctxt "an2.21-31:31.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.21-31:31.1.3"
msgid "Samatho ca vipassanā ca."
msgstr ""

#. VAR: kamatthamanubhoti → kimattha … (s1-3, km) | katamattha … (mr)
msgctxt "an2.21-31:31.1.4"
msgid "Samatho, bhikkhave, bhāvito kamatthamanubhoti?"
msgstr ""

msgctxt "an2.21-31:31.1.5"
msgid "Cittaṃ bhāvīyati."
msgstr ""

msgctxt "an2.21-31:31.1.6"
msgid "Cittaṃ bhāvitaṃ kamatthamanubhoti?"
msgstr ""

msgctxt "an2.21-31:31.1.7"
msgid "Yo rāgo so pahīyati."
msgstr ""

msgctxt "an2.21-31:31.1.8"
msgid "Vipassanā, bhikkhave, bhāvitā kamatthamanubhoti?"
msgstr ""

msgctxt "an2.21-31:31.1.9"
msgid "Paññā bhāvīyati."
msgstr ""

msgctxt "an2.21-31:31.1.10"
msgid "Paññā bhāvitā kamatthamanubhoti?"
msgstr ""

msgctxt "an2.21-31:31.1.11"
msgid "Yā avijjā sā pahīyati."
msgstr ""

msgctxt "an2.21-31:31.1.12"
msgid ""
"Rāgupakkiliṭṭhaṃ vā, bhikkhave, cittaṃ na vimuccati, avijjupakkiliṭṭhā vā "
"paññā na bhāvīyati."
msgstr ""

msgctxt "an2.21-31:31.1.13"
msgid ""
"Iti kho, bhikkhave, rāgavirāgā cetovimutti, avijjāvirāgā paññāvimuttī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an2.21-31:31.1.14"
msgid "Bālavaggo tatiyo."
msgstr ""
