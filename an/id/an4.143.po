#. <section class="sutta" id="an4.143"><article><div class="hgroup"><p class="division">
msgctxt "an4.143:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.143:0.2"
msgid "15. Ābhāvagga"
msgstr ""

#. </p><h1>
msgctxt "an4.143:0.3"
msgid "143. Ālokasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.143:1.1"
msgid "“Cattārome, bhikkhave, ālokā."
msgstr ""

msgctxt "an4.143:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.143:1.3"
msgid "Candāloko, sūriyāloko, aggāloko, paññāloko—"
msgstr ""

msgctxt "an4.143:1.4"
msgid "ime kho, bhikkhave, cattāro ālokā."
msgstr ""

msgctxt "an4.143:1.5"
msgid "Etadaggaṃ, bhikkhave, imesaṃ catunnaṃ ālokānaṃ yadidaṃ paññāloko”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.143:1.6"
msgid "Tatiyaṃ."
msgstr ""
