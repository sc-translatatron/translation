#. <section class="sutta" id="an8.66"><article><div class="hgroup"><p class="division">
msgctxt "an8.66:0.1"
msgid "Aṅguttara Nikāya 8"
msgstr ""

#. </p><p>
msgctxt "an8.66:0.2"
msgid "7. Bhūmicālavagga"
msgstr ""

#. </p><h1>
msgctxt "an8.66:0.3"
msgid "66. Vimokkhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an8.66:1.1"
msgid "“Aṭṭhime, bhikkhave, vimokkhā."
msgstr ""

msgctxt "an8.66:1.2"
msgid "Katame aṭṭha?"
msgstr ""

msgctxt "an8.66:1.3"
msgid "Rūpī rūpāni passati."
msgstr ""

msgctxt "an8.66:1.4"
msgid "Ayaṃ paṭhamo vimokkho. (1)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
#. VAR: arūpasaññī, bahiddhā → arūpasaññī eko bahiddhā (s1, s2, pts1, mr)
msgctxt "an8.66:2.1"
msgid "Ajjhattaṃ arūpasaññī, bahiddhā rūpāni passati."
msgstr ""

msgctxt "an8.66:2.2"
msgid "Ayaṃ dutiyo vimokkho. (2)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an8.66:3.1"
msgid "Subhanteva adhimutto hoti."
msgstr ""

msgctxt "an8.66:3.2"
msgid "Ayaṃ tatiyo vimokkho. (3)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an8.66:4.1"
msgid ""
"Sabbaso rūpasaññānaṃ samatikkamā paṭighasaññānaṃ atthaṅgamā nānattasaññānaṃ "
"amanasikārā ‘ananto ākāso’ti ākāsānañcāyatanaṃ upasampajja viharati."
msgstr ""

msgctxt "an8.66:4.2"
msgid "Ayaṃ catuttho vimokkho. (4)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an8.66:5.1"
msgid ""
"Sabbaso ākāsānañcāyatanaṃ samatikkamma ‘anantaṃ viññāṇan’ti "
"viññāṇañcāyatanaṃ upasampajja viharati."
msgstr ""

msgctxt "an8.66:5.2"
msgid "Ayaṃ pañcamo vimokkho. (5)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an8.66:6.1"
msgid ""
"Sabbaso viññāṇañcāyatanaṃ samatikkamma ‘natthi kiñcī’ti ākiñcaññāyatanaṃ "
"upasampajja viharati."
msgstr ""

msgctxt "an8.66:6.2"
msgid "Ayaṃ chaṭṭho vimokkho. (6)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an8.66:7.1"
msgid ""
"Sabbaso ākiñcaññāyatanaṃ samatikkamma nevasaññānāsaññāyatanaṃ upasampajja "
"viharati."
msgstr ""

msgctxt "an8.66:7.2"
msgid "Ayaṃ sattamo vimokkho. (7)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an8.66:8.1"
msgid ""
"Sabbaso nevasaññānāsaññāyatanaṃ samatikkamma saññāvedayitanirodhaṃ "
"upasampajja viharati."
msgstr ""

msgctxt "an8.66:8.2"
msgid "Ayaṃ aṭṭhamo vimokkho."
msgstr ""

msgctxt "an8.66:8.3"
msgid "Ime kho, bhikkhave, aṭṭha vimokkhā”ti. (8)"
msgstr ""

#. </p><p class="endsection">
msgctxt "an8.66:8.4"
msgid "Chaṭṭhaṃ."
msgstr ""
