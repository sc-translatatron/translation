#. <section class="sutta" id="an4.115"><article><div class="hgroup"><p class="division">
msgctxt "an4.115:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.115:0.2"
msgid "12. Kesivagga"
msgstr ""

#. </p><h1>
msgctxt "an4.115:0.3"
msgid "115. Ṭhānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.115:1.1"
msgid "“Cattārimāni, bhikkhave, ṭhānāni."
msgstr ""

msgctxt "an4.115:1.2"
msgid "Katamāni cattāri?"
msgstr ""

#. </p><ol><li>
msgctxt "an4.115:1.3"
msgid "Atthi, bhikkhave, ṭhānaṃ amanāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:1.4"
msgid "tañca kayiramānaṃ anatthāya saṃvattati."
msgstr ""

#. </li><li>
msgctxt "an4.115:1.5"
msgid "Atthi, bhikkhave, ṭhānaṃ amanāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:1.6"
msgid "tañca kayiramānaṃ atthāya saṃvattati."
msgstr ""

#. </li><li>
msgctxt "an4.115:1.7"
msgid "Atthi, bhikkhave, ṭhānaṃ manāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:1.8"
msgid "tañca kayiramānaṃ anatthāya saṃvattati."
msgstr ""

#. </li><li>
msgctxt "an4.115:1.9"
msgid "Atthi, bhikkhave, ṭhānaṃ manāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:1.10"
msgid "tañca kayiramānaṃ atthāya saṃvattati."
msgstr ""

#. </li></ol><p>
#. <a class="sc" id="sc2"></a>
#. VAR: yamidaṃ → yadidaṃ (s1-3, km, mr)
msgctxt "an4.115:2.1"
msgid "Tatra, bhikkhave, yamidaṃ ṭhānaṃ amanāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:2.2"
msgid "tañca kayiramānaṃ anatthāya saṃvattati—"
msgstr ""

#. VAR: maññati → paññāyati (?)
msgctxt "an4.115:2.3"
msgid "idaṃ, bhikkhave, ṭhānaṃ ubhayeneva na kattabbaṃ maññati."
msgstr ""

#. VAR: Yampidaṃ → yadidaṃ (mr)
msgctxt "an4.115:2.4"
msgid "Yampidaṃ ṭhānaṃ amanāpaṃ kātuṃ;"
msgstr ""

#. VAR: naṃ → taṃ (bj, pts1)
msgctxt "an4.115:2.5"
msgid "imināpi naṃ na kattabbaṃ maññati."
msgstr ""

msgctxt "an4.115:2.6"
msgid "Yampidaṃ ṭhānaṃ kayiramānaṃ anatthāya saṃvattati;"
msgstr ""

#. VAR: naṃ → taṃ (pts1)
msgctxt "an4.115:2.7"
msgid "imināpi naṃ na kattabbaṃ maññati."
msgstr ""

msgctxt "an4.115:2.8"
msgid "Idaṃ, bhikkhave, ṭhānaṃ ubhayeneva na kattabbaṃ maññati."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.119"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.115:3.1"
msgid "Tatra, bhikkhave, yamidaṃ ṭhānaṃ amanāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:3.2"
msgid "tañca kayiramānaṃ atthāya saṃvattati—"
msgstr ""

msgctxt "an4.115:3.3"
msgid ""
"imasmiṃ, bhikkhave, ṭhāne bālo ca paṇḍito ca veditabbo purisathāme "
"purisavīriye purisaparakkame."
msgstr ""

msgctxt "an4.115:3.4"
msgid "Na, bhikkhave, bālo iti paṭisañcikkhati:"
msgstr ""

msgctxt "an4.115:3.5"
msgid "‘kiñcāpi kho idaṃ ṭhānaṃ amanāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:3.6"
msgid "atha carahidaṃ ṭhānaṃ kayiramānaṃ atthāya saṃvattatī’ti."
msgstr ""

msgctxt "an4.115:3.7"
msgid "So taṃ ṭhānaṃ na karoti."
msgstr ""

msgctxt "an4.115:3.8"
msgid "Tassa taṃ ṭhānaṃ akayiramānaṃ anatthāya saṃvattati."
msgstr ""

msgctxt "an4.115:3.9"
msgid "Paṇḍito ca kho, bhikkhave, iti paṭisañcikkhati:"
msgstr ""

msgctxt "an4.115:3.10"
msgid "‘kiñcāpi kho idaṃ ṭhānaṃ amanāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:3.11"
msgid "atha carahidaṃ ṭhānaṃ kayiramānaṃ atthāya saṃvattatī’ti."
msgstr ""

msgctxt "an4.115:3.12"
msgid "So taṃ ṭhānaṃ karoti."
msgstr ""

msgctxt "an4.115:3.13"
msgid "Tassa taṃ ṭhānaṃ kayiramānaṃ atthāya saṃvattati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
#. VAR: yamidaṃ → yadidaṃ (s1-3, km)
msgctxt "an4.115:4.1"
msgid "Tatra, bhikkhave, yamidaṃ ṭhānaṃ manāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:4.2"
msgid "tañca kayiramānaṃ anatthāya saṃvattati—"
msgstr ""

msgctxt "an4.115:4.3"
msgid ""
"imasmimpi, bhikkhave, ṭhāne bālo ca paṇḍito ca veditabbo purisathāme "
"purisavīriye purisaparakkame."
msgstr ""

msgctxt "an4.115:4.4"
msgid "Na, bhikkhave, bālo iti paṭisañcikkhati:"
msgstr ""

msgctxt "an4.115:4.5"
msgid "‘kiñcāpi kho idaṃ ṭhānaṃ manāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:4.6"
msgid "atha carahidaṃ ṭhānaṃ kayiramānaṃ anatthāya saṃvattatī’ti."
msgstr ""

msgctxt "an4.115:4.7"
msgid "So taṃ ṭhānaṃ karoti."
msgstr ""

msgctxt "an4.115:4.8"
msgid "Tassa taṃ ṭhānaṃ kayiramānaṃ anatthāya saṃvattati."
msgstr ""

msgctxt "an4.115:4.9"
msgid "Paṇḍito ca kho, bhikkhave, iti paṭisañcikkhati:"
msgstr ""

msgctxt "an4.115:4.10"
msgid "‘kiñcāpi kho idaṃ ṭhānaṃ manāpaṃ kātuṃ;"
msgstr ""

msgctxt "an4.115:4.11"
msgid "atha carahidaṃ ṭhānaṃ kayiramānaṃ anatthāya saṃvattatī’ti."
msgstr ""

msgctxt "an4.115:4.12"
msgid "So taṃ ṭhānaṃ na karoti."
msgstr ""

msgctxt "an4.115:4.13"
msgid "Tassa taṃ ṭhānaṃ akayiramānaṃ atthāya saṃvattati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.115:5.1"
msgid ""
"Tatra, bhikkhave, yamidaṃ ṭhānaṃ manāpaṃ kātuṃ, tañca kayiramānaṃ atthāya "
"saṃvattati—"
msgstr ""

msgctxt "an4.115:5.2"
msgid "idaṃ, bhikkhave, ṭhānaṃ ubhayeneva kattabbaṃ maññati."
msgstr ""

msgctxt "an4.115:5.3"
msgid "Yampidaṃ ṭhānaṃ manāpaṃ kātuṃ, imināpi naṃ kattabbaṃ maññati;"
msgstr ""

msgctxt "an4.115:5.4"
msgid ""
"yampidaṃ ṭhānaṃ kayiramānaṃ atthāya saṃvattati, imināpi naṃ kattabbaṃ "
"maññati."
msgstr ""

msgctxt "an4.115:5.5"
msgid "Idaṃ, bhikkhave, ṭhānaṃ ubhayeneva kattabbaṃ maññati."
msgstr ""

msgctxt "an4.115:5.6"
msgid "Imāni kho, bhikkhave, cattāri ṭhānānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.115:5.7"
msgid "Pañcamaṃ."
msgstr ""
