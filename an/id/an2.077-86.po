#. <section id="vagga"><section class="sutta" data-uid="an2.77" id="77"><article><div class="hgroup"><p class="division">
msgctxt "an2.77-86:0.1"
msgid "Aṅguttara Nikāya 2"
msgstr ""

#. </p><p>
#. VAR: nimittavagga (pts1)
msgctxt "an2.77-86:0.2"
msgid "8. Sanimittavagga"
msgstr ""

#. </p><h1>
msgctxt "an2.77-86:0.3"
msgid "77"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc77.1"></a>
msgctxt "an2.77-86:77.1.1"
msgid "“Sanimittā, bhikkhave, uppajjanti pāpakā akusalā dhammā, no animittā."
msgstr ""

msgctxt "an2.77-86:77.1.2"
msgid "Tasseva nimittassa pahānā evaṃ te pāpakā akusalā dhammā na hontī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.78" id="78"><article><div class="hgroup"><h1>
msgctxt "an2.77-86:77.1.3"
msgid "78"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc78.1"></a>
msgctxt "an2.77-86:78.1.1"
msgid "“Sanidānā, bhikkhave, uppajjanti pāpakā akusalā dhammā, no anidānā."
msgstr ""

msgctxt "an2.77-86:78.1.2"
msgid "Tasseva nidānassa pahānā evaṃ te pāpakā akusalā dhammā na hontī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.79" id="79"><article><div class="hgroup"><h1>
msgctxt "an2.77-86:78.1.3"
msgid "79"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc79.1"></a>
msgctxt "an2.77-86:79.1.1"
msgid "“Sahetukā, bhikkhave, uppajjanti pāpakā akusalā dhammā, no ahetukā."
msgstr ""

msgctxt "an2.77-86:79.1.2"
msgid "Tasseva hetussa pahānā evaṃ te pāpakā akusalā dhammā na hontī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.80" id="80"><article><div class="hgroup"><h1>
msgctxt "an2.77-86:79.1.3"
msgid "80"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc80.1"></a>
msgctxt "an2.77-86:80.1.1"
msgid "“Sasaṅkhārā, bhikkhave, uppajjanti pāpakā akusalā dhammā, no asaṅkhārā."
msgstr ""

msgctxt "an2.77-86:80.1.2"
msgid "Tesaṃyeva saṅkhārānaṃ pahānā evaṃ te pāpakā akusalā dhammā na hontī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.81" id="81"><article><div class="hgroup"><h1>
msgctxt "an2.77-86:80.1.3"
msgid "81"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc81.1"></a>
msgctxt "an2.77-86:81.1.1"
msgid "“Sappaccayā, bhikkhave, uppajjanti pāpakā akusalā dhammā, no appaccayā."
msgstr ""

msgctxt "an2.77-86:81.1.2"
msgid "Tasseva paccayassa pahānā evaṃ te pāpakā akusalā dhammā na hontī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.82" id="82"><article><div class="hgroup"><h1>
msgctxt "an2.77-86:81.1.3"
msgid "82"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.83"></a>
#. <a class="sc" id="sc82.1"></a>
msgctxt "an2.77-86:82.1.1"
msgid "“Sarūpā, bhikkhave, uppajjanti pāpakā akusalā dhammā, no arūpā."
msgstr ""

msgctxt "an2.77-86:82.1.2"
msgid "Tasseva rūpassa pahānā evaṃ te pāpakā akusalā dhammā na hontī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.83" id="83"><article><div class="hgroup"><h1>
msgctxt "an2.77-86:82.1.3"
msgid "83"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc83.1"></a>
msgctxt "an2.77-86:83.1.1"
msgid "“Savedanā, bhikkhave, uppajjanti pāpakā akusalā dhammā, no avedanā."
msgstr ""

msgctxt "an2.77-86:83.1.2"
msgid "Tassāyeva vedanāya pahānā evaṃ te pāpakā akusalā dhammā na hontī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.84" id="84"><article><div class="hgroup"><h1>
msgctxt "an2.77-86:83.1.3"
msgid "84"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc84.1"></a>
msgctxt "an2.77-86:84.1.1"
msgid "“Sasaññā, bhikkhave, uppajjanti pāpakā akusalā dhammā, no asaññā."
msgstr ""

msgctxt "an2.77-86:84.1.2"
msgid "Tassāyeva saññāya pahānā evaṃ te pāpakā akusalā dhammā na hontī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.85" id="85"><article><div class="hgroup"><h1>
msgctxt "an2.77-86:84.1.3"
msgid "85"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc85.1"></a>
msgctxt "an2.77-86:85.1.1"
msgid "“Saviññāṇā, bhikkhave, uppajjanti pāpakā akusalā dhammā, no aviññāṇā."
msgstr ""

msgctxt "an2.77-86:85.1.2"
msgid "Tasseva viññāṇassa pahānā evaṃ te pāpakā akusalā dhammā na hontī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.86" id="86"><article><div class="hgroup"><h1>
msgctxt "an2.77-86:85.1.3"
msgid "86"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc86.1"></a>
msgctxt "an2.77-86:86.1.1"
msgid ""
"“Saṅkhatārammaṇā, bhikkhave, uppajjanti pāpakā akusalā dhammā, no "
"asaṅkhatārammaṇā."
msgstr ""

msgctxt "an2.77-86:86.1.2"
msgid "Tasseva saṅkhatassa pahānā evaṃ te pāpakā akusalā dhammā na hontī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an2.77-86:86.1.3"
msgid "Sanimittavaggo tatiyo."
msgstr ""
