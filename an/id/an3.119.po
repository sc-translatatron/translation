#. <section class="sutta" id="an3.119"><article><div class="hgroup"><p class="division">
msgctxt "an3.119:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.119:0.2"
msgid "12. Āpāyikavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.119:0.3"
msgid "119. Kammantasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.119:1.1"
msgid "“Tisso imā, bhikkhave, vipattiyo."
msgstr ""

msgctxt "an3.119:1.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "an3.119:1.3"
msgid "Kammantavipatti, ājīvavipatti, diṭṭhivipatti."
msgstr ""

msgctxt "an3.119:1.4"
msgid "Katamā ca, bhikkhave, kammantavipatti?"
msgstr ""

msgctxt "an3.119:1.5"
msgid "Idha, bhikkhave, ekacco pāṇātipātī hoti … pe … samphappalāpī hoti."
msgstr ""

msgctxt "an3.119:1.6"
msgid "Ayaṃ vuccati, bhikkhave, kammantavipatti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.119:2.1"
msgid "Katamā ca, bhikkhave, ājīvavipatti?"
msgstr ""

#. VAR: jīvikaṃ → jīvitaṃ (s1-3, km, mr)
msgctxt "an3.119:2.2"
msgid ""
"Idha, bhikkhave, ekacco micchāājīvo hoti, micchāājīvena jīvikaṃ kappeti."
msgstr ""

msgctxt "an3.119:2.3"
msgid "Ayaṃ vuccati, bhikkhave, ājīvavipatti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.271"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "an3.119:3.1"
msgid "Katamā ca, bhikkhave, diṭṭhivipatti?"
msgstr ""

msgctxt "an3.119:3.2"
msgid "Idha, bhikkhave, ekacco micchādiṭṭhiko hoti viparītadassano:"
msgstr ""

msgctxt "an3.119:3.3"
msgid ""
"‘natthi dinnaṃ, natthi yiṭṭhaṃ … pe … ye imañca lokaṃ parañca lokaṃ sayaṃ "
"abhiññā sacchikatvā pavedentī’ti."
msgstr ""

msgctxt "an3.119:3.4"
msgid "Ayaṃ vuccati, bhikkhave, diṭṭhivipatti."
msgstr ""

msgctxt "an3.119:3.5"
msgid "Imā kho, bhikkhave, tisso vipattiyoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an3.119:4.1"
msgid "Tisso imā, bhikkhave, sampadā."
msgstr ""

msgctxt "an3.119:4.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "an3.119:4.3"
msgid "Kammantasampadā, ājīvasampadā, diṭṭhisampadā."
msgstr ""

msgctxt "an3.119:4.4"
msgid "Katamā ca, bhikkhave, kammantasampadā?"
msgstr ""

msgctxt "an3.119:4.5"
msgid ""
"Idha, bhikkhave, ekacco pāṇātipātā paṭivirato hoti … pe … samphappalāpā "
"paṭivirato hoti."
msgstr ""

msgctxt "an3.119:4.6"
msgid "Ayaṃ vuccati, bhikkhave, kammantasampadā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an3.119:5.1"
msgid "Katamā ca, bhikkhave, ājīvasampadā?"
msgstr ""

msgctxt "an3.119:5.2"
msgid "Idha, bhikkhave, ekacco sammāājīvo hoti, sammāājīvena jīvikaṃ kappeti."
msgstr ""

msgctxt "an3.119:5.3"
msgid "Ayaṃ vuccati, bhikkhave, ājīvasampadā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an3.119:6.1"
msgid "Katamā ca, bhikkhave, diṭṭhisampadā?"
msgstr ""

msgctxt "an3.119:6.2"
msgid "Idha, bhikkhave, ekacco sammādiṭṭhiko hoti aviparītadassano:"
msgstr ""

msgctxt "an3.119:6.3"
msgid ""
"‘atthi dinnaṃ, atthi yiṭṭhaṃ … pe … ye imañca lokaṃ parañca lokaṃ sayaṃ "
"abhiññā sacchikatvā pavedentī’ti."
msgstr ""

msgctxt "an3.119:6.4"
msgid "Ayaṃ vuccati, bhikkhave, diṭṭhisampadā."
msgstr ""

msgctxt "an3.119:6.5"
msgid "Imā kho, bhikkhave, tisso sampadā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.119:6.6"
msgid "Sattamaṃ."
msgstr ""
