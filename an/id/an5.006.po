#. <section class="sutta" id="an5.6"><article><div class="hgroup"><p class="division">
msgctxt "an5.6:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.6:0.2"
msgid "1. Sekhabalavagga"
msgstr ""

#. </p><h1>
#. VAR: akusalasamāpattisuttaṃ (bj)
msgctxt "an5.6:0.3"
msgid "6. Samāpattisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.6:1.1"
msgid ""
"“Na tāva, bhikkhave, akusalassa samāpatti hoti yāva saddhā paccupaṭṭhitā "
"hoti kusalesu dhammesu."
msgstr ""

#. VAR: pariyuṭṭhāya → pariyuddhāya (s1-3)
msgctxt "an5.6:1.2"
msgid ""
"Yato ca kho, bhikkhave, saddhā antarahitā hoti, assaddhiyaṃ pariyuṭṭhāya "
"tiṭṭhati;"
msgstr ""

msgctxt "an5.6:1.3"
msgid "atha akusalassa samāpatti hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.6:2.1"
msgid ""
"Na tāva, bhikkhave, akusalassa samāpatti hoti yāva hirī paccupaṭṭhitā hoti "
"kusalesu dhammesu."
msgstr ""

msgctxt "an5.6:2.2"
msgid ""
"Yato ca kho, bhikkhave, hirī antarahitā hoti, ahirikaṃ pariyuṭṭhāya tiṭṭhati;"
msgstr ""

msgctxt "an5.6:2.3"
msgid "atha akusalassa samāpatti hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.6:3.1"
msgid ""
"Na tāva, bhikkhave, akusalassa samāpatti hoti yāva ottappaṃ paccupaṭṭhitaṃ "
"hoti kusalesu dhammesu."
msgstr ""

msgctxt "an5.6:3.2"
msgid ""
"Yato ca kho, bhikkhave, ottappaṃ antarahitaṃ hoti, anottappaṃ pariyuṭṭhāya "
"tiṭṭhati;"
msgstr ""

msgctxt "an5.6:3.3"
msgid "atha akusalassa samāpatti hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an5.6:4.1"
msgid ""
"Na tāva, bhikkhave, akusalassa samāpatti hoti yāva vīriyaṃ paccupaṭṭhitaṃ "
"hoti kusalesu dhammesu."
msgstr ""

msgctxt "an5.6:4.2"
msgid ""
"Yato ca kho, bhikkhave, vīriyaṃ antarahitaṃ hoti, kosajjaṃ pariyuṭṭhāya "
"tiṭṭhati;"
msgstr ""

msgctxt "an5.6:4.3"
msgid "atha akusalassa samāpatti hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an5.6:5.1"
msgid ""
"Na tāva, bhikkhave, akusalassa samāpatti hoti yāva paññā paccupaṭṭhitā hoti "
"kusalesu dhammesu."
msgstr ""

#. VAR: duppaññā → duppaññaṃ (mr)
msgctxt "an5.6:5.2"
msgid ""
"Yato ca kho, bhikkhave, paññā antarahitā hoti, duppaññā pariyuṭṭhāya "
"tiṭṭhati;"
msgstr ""

msgctxt "an5.6:5.3"
msgid "atha akusalassa samāpatti hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.6:5.4"
msgid "Chaṭṭhaṃ."
msgstr ""
