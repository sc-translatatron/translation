#. <section class="sutta" id="an5.20"><article><div class="hgroup"><p class="division">
msgctxt "an5.20:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.20:0.2"
msgid "2. Balavagga"
msgstr ""

#. </p><h1>
#. VAR: attahita-parahitasuttaṃ (bj)
msgctxt "an5.20:0.3"
msgid "20. Catutthahitasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.20:1.1"
msgid ""
"“Pañcahi, bhikkhave, dhammehi samannāgato bhikkhu attahitāya ca paṭipanno "
"hoti parahitāya ca."
msgstr ""

msgctxt "an5.20:1.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.20:1.3"
msgid ""
"Idha, bhikkhave, bhikkhu attanā ca sīlasampanno hoti, parañca sīlasampadāya "
"samādapeti;"
msgstr ""

msgctxt "an5.20:1.4"
msgid ""
"attanā ca samādhisampanno hoti, parañca samādhisampadāya samādapeti, attanā "
"ca paññāsampanno hoti, parañca paññāsampadāya samādapeti;"
msgstr ""

msgctxt "an5.20:1.5"
msgid "attanā ca vimuttisampanno hoti, parañca vimuttisampadāya samādapeti;"
msgstr ""

msgctxt "an5.20:1.6"
msgid ""
"attanā ca vimuttiñāṇadassanasampanno hoti, parañca "
"vimuttiñāṇadassanasampadāya samādapeti."
msgstr ""

msgctxt "an5.20:1.7"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgato bhikkhu attahitāya ca "
"paṭipanno hoti parahitāya cā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.20:1.8"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.20:1.9"
msgid "Balavaggo dutiyo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.20:2.1"
msgid "Ananussutakūṭañca,"
msgstr ""

#. <br>
msgctxt "an5.20:2.2"
msgid "saṅkhittaṃ vitthatena ca;"
msgstr ""

#. <br>
msgctxt "an5.20:2.3"
msgid "Daṭṭhabbañca puna kūṭaṃ,"
msgstr ""

#. <br>
msgctxt "an5.20:2.4"
msgid "cattāropi hitena cāti."
msgstr ""
