#. <section class="sutta" id="an4.70"><article><div class="hgroup"><p class="division">
msgctxt "an4.70:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.70:0.2"
msgid "7. Pattakammavagga"
msgstr ""

#. </p><h1>
msgctxt "an4.70:0.3"
msgid "70. Adhammikasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.75"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.70:1.1"
msgid ""
"“Yasmiṃ, bhikkhave, samaye rājāno adhammikā honti, rājāyuttāpi tasmiṃ samaye "
"adhammikā honti."
msgstr ""

msgctxt "an4.70:1.2"
msgid ""
"Rājāyuttesu adhammikesu brāhmaṇagahapatikāpi tasmiṃ samaye adhammikā honti."
msgstr ""

msgctxt "an4.70:1.3"
msgid ""
"Brāhmaṇagahapatikesu adhammikesu negamajānapadāpi tasmiṃ samaye adhammikā "
"honti."
msgstr ""

msgctxt "an4.70:1.4"
msgid "Negamajānapadesu adhammikesu visamaṃ candimasūriyā parivattanti."
msgstr ""

msgctxt "an4.70:1.5"
msgid ""
"Visamaṃ candimasūriyesu parivattantesu visamaṃ nakkhattāni tārakarūpāni "
"parivattanti."
msgstr ""

#. VAR: rattindivā → rattidivā (mr)
msgctxt "an4.70:1.6"
msgid ""
"Visamaṃ nakkhattesu tārakarūpesu parivattantesu visamaṃ rattindivā "
"parivattanti."
msgstr ""

msgctxt "an4.70:1.7"
msgid "Visamaṃ rattindivesu parivattantesu visamaṃ māsaddhamāsā parivattanti."
msgstr ""

msgctxt "an4.70:1.8"
msgid ""
"Visamaṃ māsaddhamāsesu parivattantesu visamaṃ utusaṃvaccharā parivattanti."
msgstr ""

msgctxt "an4.70:1.9"
msgid ""
"Visamaṃ utusaṃvaccharesu parivattantesu visamaṃ vātā vāyanti visamā apañjasā."
msgstr ""

msgctxt "an4.70:1.10"
msgid ""
"Visamaṃ vātesu vāyantesu visamesu apañjasesu devatā parikupitā bhavanti."
msgstr ""

msgctxt "an4.70:1.11"
msgid "Devatāsu parikupitāsu devo na sammā dhāraṃ anuppavecchati."
msgstr ""

#. VAR: visamapākāni → visamapākīni (bj, s1-3, km, pts1) | visamaṃ pākāni (mr)
msgctxt "an4.70:1.12"
msgid "Deve na sammā dhāraṃ anuppavecchante visamapākāni sassāni bhavanti."
msgstr ""

# See BB’s note for the absence of dubbala. Curious that this wasn’t corrected here.
#. VAR: bavhābādhā → bahvābādhā (s1-3, mr)
msgctxt "an4.70:1.13"
msgid ""
"Visamapākāni, bhikkhave, sassāni manussā paribhuñjantā appāyukā honti "
"dubbaṇṇā ca bavhābādhā ca."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.70:2.1"
msgid ""
"Yasmiṃ, bhikkhave, samaye rājāno dhammikā honti, rājāyuttāpi tasmiṃ samaye "
"dhammikā honti."
msgstr ""

msgctxt "an4.70:2.2"
msgid ""
"Rājāyuttesu dhammikesu brāhmaṇagahapatikāpi tasmiṃ samaye dhammikā honti."
msgstr ""

msgctxt "an4.70:2.3"
msgid ""
"Brāhmaṇagahapatikesu dhammikesu negamajānapadāpi tasmiṃ samaye dhammikā "
"honti."
msgstr ""

msgctxt "an4.70:2.4"
msgid "Negamajānapadesu dhammikesu samaṃ candimasūriyā parivattanti."
msgstr ""

msgctxt "an4.70:2.5"
msgid ""
"Samaṃ candimasūriyesu parivattantesu samaṃ nakkhattāni tārakarūpāni "
"parivattanti."
msgstr ""

msgctxt "an4.70:2.6"
msgid ""
"Samaṃ nakkhattesu tārakarūpesu parivattantesu samaṃ rattindivā parivattanti."
msgstr ""

msgctxt "an4.70:2.7"
msgid "Samaṃ rattindivesu parivattantesu samaṃ māsaddhamāsā parivattanti."
msgstr ""

msgctxt "an4.70:2.8"
msgid "Samaṃ māsaddhamāsesu parivattantesu samaṃ utusaṃvaccharā parivattanti."
msgstr ""

msgctxt "an4.70:2.9"
msgid "Samaṃ utusaṃvaccharesu parivattantesu samaṃ vātā vāyanti samā pañjasā."
msgstr ""

msgctxt "an4.70:2.10"
msgid "Samaṃ vātesu vāyantesu samesu pañjasesu devatā aparikupitā bhavanti."
msgstr ""

msgctxt "an4.70:2.11"
msgid "Devatāsu aparikupitāsu devo sammā dhāraṃ anuppavecchati."
msgstr ""

msgctxt "an4.70:2.12"
msgid "Deve sammā dhāraṃ anuppavecchante samapākāni sassāni bhavanti."
msgstr ""

msgctxt "an4.70:2.13"
msgid ""
"Samapākāni, bhikkhave, sassāni manussā paribhuñjantā dīghāyukā ca honti "
"vaṇṇavanto ca balavanto ca appābādhā cāti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.70:3.1"
msgid "Gunnañce taramānānaṃ,"
msgstr ""

#. <br>
msgctxt "an4.70:3.2"
msgid "jimhaṃ gacchati puṅgavo;"
msgstr ""

#. <br>
msgctxt "an4.70:3.3"
msgid "Sabbā tā jimhaṃ gacchanti,"
msgstr ""

#. <br>
msgctxt "an4.70:3.4"
msgid "nette jimhaṃ gate sati."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.76"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.70:4.1"
msgid "Evamevaṃ manussesu,"
msgstr ""

#. <br>
msgctxt "an4.70:4.2"
msgid "yo hoti seṭṭhasammato;"
msgstr ""

#. <br>
msgctxt "an4.70:4.3"
msgid "So ce adhammaṃ carati,"
msgstr ""

#. <br>
msgctxt "an4.70:4.4"
msgid "pageva itarā pajā;"
msgstr ""

#. <br>
msgctxt "an4.70:4.5"
msgid "Sabbaṃ raṭṭhaṃ dukkhaṃ seti,"
msgstr ""

#. <br>
msgctxt "an4.70:4.6"
msgid "rājā ce hoti adhammiko."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.70:5.1"
msgid "Gunnañce taramānānaṃ,"
msgstr ""

#. <br>
msgctxt "an4.70:5.2"
msgid "ujuṃ gacchati puṅgavo;"
msgstr ""

#. <br>
msgctxt "an4.70:5.3"
msgid "Sabbā tā ujuṃ gacchanti,"
msgstr ""

#. <br>
msgctxt "an4.70:5.4"
msgid "nette ujuṃ gate sati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an4.70:6.1"
msgid "Evamevaṃ manussesu,"
msgstr ""

#. <br>
msgctxt "an4.70:6.2"
msgid "yo hoti seṭṭhasammato;"
msgstr ""

#. <br>
#. VAR: sace → so ceva (bj, pts1) | so ce (s1-3)
msgctxt "an4.70:6.3"
msgid "So sace dhammaṃ carati,"
msgstr ""

#. <br>
msgctxt "an4.70:6.4"
msgid "pageva itarā pajā;"
msgstr ""

#. <br>
msgctxt "an4.70:6.5"
msgid "Sabbaṃ raṭṭhaṃ sukhaṃ seti,"
msgstr ""

#. <br>
msgctxt "an4.70:6.6"
msgid "rājā ce hoti dhammiko”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an4.70:6.7"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.70:6.8"
msgid "Pattakammavaggo dutiyo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc7"></a>
#. VAR: ānaṇyako → anaṇako (bj, pts1) | annanātho (s1-3) | anaṇyako (mr)
msgctxt "an4.70:7.1"
msgid "Pattakammaṃ ānaṇyako,"
msgstr ""

#. <br>
msgctxt "an4.70:7.2"
msgid "Sabrahmanirayā rūpena pañcamaṃ;"
msgstr ""

#. <br>
msgctxt "an4.70:7.3"
msgid "Sarāgaahirājā devadatto,"
msgstr ""

#. <br>
msgctxt "an4.70:7.4"
msgid "Padhānaṃ adhammikena cāti."
msgstr ""
