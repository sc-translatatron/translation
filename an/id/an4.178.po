#. <section class="sutta" id="an4.178"><article><div class="hgroup"><p class="division">
msgctxt "an4.178:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.178:0.2"
msgid "18. Sañcetaniyavagga"
msgstr ""

#. </p><h1>
#. VAR: cetovimuttisuttaṃ (bj)
msgctxt "an4.178:0.3"
msgid "178. Jambālīsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.178:1.1"
msgid "“Cattārome, bhikkhave, puggalā santo saṃvijjamānā lokasmiṃ."
msgstr ""

msgctxt "an4.178:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.178:1.3"
msgid ""
"Idha, bhikkhave, bhikkhu aññataraṃ santaṃ cetovimuttiṃ upasampajja viharati."
msgstr ""

msgctxt "an4.178:1.4"
msgid "So sakkāyanirodhaṃ manasi karoti."
msgstr ""

msgctxt "an4.178:1.5"
msgid ""
"Tassa sakkāyanirodhaṃ manasi karoto sakkāyanirodhe cittaṃ na pakkhandati "
"nappasīdati na santiṭṭhati nādhimuccati."
msgstr ""

#. VAR: Tassa kho evaṃ → tassa kho etaṃ (bj, s1-3, km, pts1) | evaṃ kho tassa (?)
msgctxt "an4.178:1.6"
msgid "Tassa kho evaṃ, bhikkhave, bhikkhuno na sakkāyanirodho pāṭikaṅkho."
msgstr ""

#. VAR: lepagatena → lasagatena (bj, pts1) | lapagatena (s1-3)
#. VAR: gaṇheyyapi → gaṇheyyāpi (pts1) | gayheyyapi (?)
#. VAR: bajjheyyapi → khajjeyyapi (bj) | bajjheyyāpi (pts1)
msgctxt "an4.178:1.7"
msgid ""
"Seyyathāpi, bhikkhave, puriso lepagatena hatthena sākhaṃ gaṇheyya, tassa so "
"hattho sajjeyyapi gaṇheyyapi bajjheyyapi;"
msgstr ""

msgctxt "an4.178:1.8"
msgid ""
"evamevaṃ kho, bhikkhave, bhikkhu aññataraṃ santaṃ cetovimuttiṃ upasampajja "
"viharati."
msgstr ""

msgctxt "an4.178:1.9"
msgid "So sakkāyanirodhaṃ manasi karoti."
msgstr ""

msgctxt "an4.178:1.10"
msgid ""
"Tassa sakkāyanirodhaṃ manasi karoto sakkāyanirodhe cittaṃ na pakkhandati "
"nappasīdati na santiṭṭhati nādhimuccati."
msgstr ""

#. VAR: Tassa kho evaṃ → tassa kho etaṃ (bj, s1-3, km, pts1) | evaṃ kho tassa (?)
msgctxt "an4.178:1.11"
msgid "Tassa kho evaṃ, bhikkhave, bhikkhuno na sakkāyanirodho pāṭikaṅkho."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.166"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.178:2.1"
msgid ""
"Idha pana, bhikkhave, bhikkhu aññataraṃ santaṃ cetovimuttiṃ upasampajja "
"viharati."
msgstr ""

msgctxt "an4.178:2.2"
msgid "So sakkāyanirodhaṃ manasi karoti."
msgstr ""

msgctxt "an4.178:2.3"
msgid ""
"Tassa sakkāyanirodhaṃ manasi karoto sakkāyanirodhe cittaṃ pakkhandati "
"pasīdati santiṭṭhati adhimuccati."
msgstr ""

msgctxt "an4.178:2.4"
msgid "Tassa kho evaṃ, bhikkhave, bhikkhuno sakkāyanirodho pāṭikaṅkho."
msgstr ""

msgctxt "an4.178:2.5"
msgid ""
"Seyyathāpi, bhikkhave, puriso suddhena hatthena sākhaṃ gaṇheyya, tassa so "
"hattho neva sajjeyya na gaṇheyya na bajjheyya;"
msgstr ""

msgctxt "an4.178:2.6"
msgid ""
"evamevaṃ kho, bhikkhave, bhikkhu aññataraṃ santaṃ cetovimuttiṃ upasampajja "
"viharati."
msgstr ""

msgctxt "an4.178:2.7"
msgid "So sakkāyanirodhaṃ manasi karoti."
msgstr ""

msgctxt "an4.178:2.8"
msgid ""
"Tassa sakkāyanirodhaṃ manasi karoto sakkāyanirodhe cittaṃ pakkhandati "
"pasīdati santiṭṭhati adhimuccati."
msgstr ""

msgctxt "an4.178:2.9"
msgid "Tassa kho evaṃ, bhikkhave, bhikkhuno sakkāyanirodho pāṭikaṅkho."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.178:3.1"
msgid ""
"Idha pana, bhikkhave, bhikkhu aññataraṃ santaṃ cetovimuttiṃ upasampajja "
"viharati."
msgstr ""

msgctxt "an4.178:3.2"
msgid "So avijjāppabhedaṃ manasi karoti."
msgstr ""

msgctxt "an4.178:3.3"
msgid ""
"Tassa avijjāppabhedaṃ manasi karoto avijjāppabhede cittaṃ na pakkhandati "
"nappasīdati na santiṭṭhati nādhimuccati."
msgstr ""

msgctxt "an4.178:3.4"
msgid "Tassa kho evaṃ, bhikkhave, bhikkhuno na avijjāppabhedo pāṭikaṅkho."
msgstr ""

msgctxt "an4.178:3.5"
msgid "Seyyathāpi, bhikkhave, jambālī anekavassagaṇikā."
msgstr ""

msgctxt "an4.178:3.6"
msgid ""
"Tassā puriso yāni ceva āyamukhāni tāni pidaheyya, yāni ca apāyamukhāni tāni "
"vivareyya, devo ca na sammā dhāraṃ anuppaveccheyya."
msgstr ""

msgctxt "an4.178:3.7"
msgid "Evañhi tassā, bhikkhave, jambāliyā na āḷippabhedo pāṭikaṅkho."
msgstr ""

msgctxt "an4.178:3.8"
msgid ""
"Evamevaṃ kho, bhikkhave, bhikkhu aññataraṃ santaṃ cetovimuttiṃ upasampajja "
"viharati."
msgstr ""

msgctxt "an4.178:3.9"
msgid "So avijjāppabhedaṃ manasi karoti."
msgstr ""

msgctxt "an4.178:3.10"
msgid ""
"Tassa avijjāppabhedaṃ manasi karoto avijjāppabhede cittaṃ na pakkhandati "
"nappasīdati na santiṭṭhati nādhimuccati."
msgstr ""

msgctxt "an4.178:3.11"
msgid "Tassa kho evaṃ, bhikkhave, bhikkhuno na avijjāppabhedo pāṭikaṅkho."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.167"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.178:4.1"
msgid ""
"Idha pana, bhikkhave, bhikkhu aññataraṃ santaṃ cetovimuttiṃ upasampajja "
"viharati."
msgstr ""

msgctxt "an4.178:4.2"
msgid "So avijjāppabhedaṃ manasi karoti."
msgstr ""

msgctxt "an4.178:4.3"
msgid ""
"Tassa avijjāppabhedaṃ manasi karoto avijjāppabhede cittaṃ pakkhandati "
"pasīdati santiṭṭhati adhimuccati."
msgstr ""

msgctxt "an4.178:4.4"
msgid "Tassa kho evaṃ, bhikkhave, bhikkhuno avijjāppabhedo pāṭikaṅkho."
msgstr ""

msgctxt "an4.178:4.5"
msgid "Seyyathāpi, bhikkhave, jambālī anekavassagaṇikā."
msgstr ""

msgctxt "an4.178:4.6"
msgid ""
"Tassā puriso yāni ceva āyamukhāni tāni vivareyya, yāni ca apāyamukhāni tāni "
"pidaheyya, devo ca sammā dhāraṃ anuppaveccheyya."
msgstr ""

msgctxt "an4.178:4.7"
msgid "Evañhi tassā, bhikkhave, jambāliyā āḷippabhedo pāṭikaṅkho."
msgstr ""

msgctxt "an4.178:4.8"
msgid ""
"Evamevaṃ kho, bhikkhave, bhikkhu aññataraṃ santaṃ cetovimuttiṃ upasampajja "
"viharati."
msgstr ""

msgctxt "an4.178:4.9"
msgid "So avijjāppabhedaṃ manasi karoti."
msgstr ""

msgctxt "an4.178:4.10"
msgid ""
"Tassa avijjāppabhedaṃ manasi karoto avijjāppabhede cittaṃ pakkhandati "
"pasīdati santiṭṭhati adhimuccati."
msgstr ""

msgctxt "an4.178:4.11"
msgid "Tassa kho evaṃ, bhikkhave, bhikkhuno avijjāppabhedo pāṭikaṅkho."
msgstr ""

msgctxt "an4.178:4.12"
msgid "Ime kho, bhikkhave, cattāro puggalā santo saṃvijjamānā lokasmin”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.178:4.13"
msgid "Aṭṭhamaṃ."
msgstr ""
