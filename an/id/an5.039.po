#. <section class="sutta" id="an5.39"><article><div class="hgroup"><p class="division">
msgctxt "an5.39:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.39:0.2"
msgid "4. Sumanavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.39:0.3"
msgid "39. Puttasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.39:1.1"
msgid ""
"“Pañcimāni, bhikkhave, ṭhānāni sampassantā mātāpitaro puttaṃ icchanti kule "
"jāyamānaṃ."
msgstr ""

msgctxt "an5.39:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "an5.39:1.3"
msgid "Bhato vā no bharissati;"
msgstr ""

msgctxt "an5.39:1.4"
msgid "kiccaṃ vā no karissati;"
msgstr ""

# At AN 7.52, etc. kulavamsa clearly means family tradition rather than family lineage.
msgctxt "an5.39:1.5"
msgid "kulavaṃso ciraṃ ṭhassati;"
msgstr ""

msgctxt "an5.39:1.6"
msgid "dāyajjaṃ paṭipajjissati;"
msgstr ""

msgctxt "an5.39:1.7"
msgid "atha vā pana petānaṃ kālaṅkatānaṃ dakkhiṇaṃ anuppadassatīti."
msgstr ""

msgctxt "an5.39:1.8"
msgid ""
"Imāni kho, bhikkhave, pañca ṭhānāni sampassantā mātāpitaro puttaṃ icchanti "
"kule jāyamānanti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.39:2.1"
msgid "Pañca ṭhānāni sampassaṃ,"
msgstr ""

#. <br>
msgctxt "an5.39:2.2"
msgid "puttaṃ icchanti paṇḍitā;"
msgstr ""

#. <br>
msgctxt "an5.39:2.3"
msgid "Bhato vā no bharissati,"
msgstr ""

#. <br>
msgctxt "an5.39:2.4"
msgid "kiccaṃ vā no karissati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.39:3.1"
msgid "Kulavaṃso ciraṃ tiṭṭhe,"
msgstr ""

#. <br>
msgctxt "an5.39:3.2"
msgid "dāyajjaṃ paṭipajjati;"
msgstr ""

#. <br>
msgctxt "an5.39:3.3"
msgid "Atha vā pana petānaṃ,"
msgstr ""

#. <br>
msgctxt "an5.39:3.4"
msgid "dakkhiṇaṃ anuppadassati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an5.39:4.1"
msgid "Ṭhānānetāni sampassaṃ,"
msgstr ""

#. <br>
msgctxt "an5.39:4.2"
msgid "puttaṃ icchanti paṇḍitā;"
msgstr ""

#. <br>
msgctxt "an5.39:4.3"
msgid "Tasmā santo sappurisā,"
msgstr ""

#. <br>
msgctxt "an5.39:4.4"
msgid "kataññū katavedino."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.44"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "an5.39:5.1"
msgid "Bharanti mātāpitaro,"
msgstr ""

#. <br>
msgctxt "an5.39:5.2"
msgid "pubbe katamanussaraṃ;"
msgstr ""

#. <br>
msgctxt "an5.39:5.3"
msgid "Karonti nesaṃ kiccāni,"
msgstr ""

#. <br>
msgctxt "an5.39:5.4"
msgid "yathā taṃ pubbakārinaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an5.39:6.1"
msgid "Ovādakārī bhataposī,"
msgstr ""

#. <br>
msgctxt "an5.39:6.2"
msgid "kulavaṃsaṃ ahāpayaṃ;"
msgstr ""

#. <br>
msgctxt "an5.39:6.3"
msgid "Saddho sīlena sampanno,"
msgstr ""

#. <br>
msgctxt "an5.39:6.4"
msgid "putto hoti pasaṃsiyo”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an5.39:6.5"
msgid "Navamaṃ."
msgstr ""
