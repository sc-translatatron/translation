#. <section id="vagga"><section class="sutta" data-uid="an2.151" id="151"><article><div class="hgroup"><p class="division">
msgctxt "an2.151-162:0.1"
msgid "Aṅguttara Nikāya 2"
msgstr ""

#. </p><p>
msgctxt "an2.151-162:0.2"
msgid "14. Santhāravagga"
msgstr ""

#. </p><h1>
msgctxt "an2.151-162:0.3"
msgid "151"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.93"></a>
#. <a class="sc" id="sc151.1"></a>
#. VAR: santhārā → sandhārā (mr)
msgctxt "an2.151-162:151.1.1"
msgid "“Dveme, bhikkhave, santhārā."
msgstr ""

msgctxt "an2.151-162:151.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.151-162:151.1.3"
msgid "Āmisasanthāro ca dhammasanthāro ca."
msgstr ""

msgctxt "an2.151-162:151.1.4"
msgid "Ime kho, bhikkhave, dve santhārā."
msgstr ""

msgctxt "an2.151-162:151.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ santhārānaṃ yadidaṃ dhammasanthāro”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.152" id="152"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:151.1.6"
msgid "152"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc152.1"></a>
#. VAR: paṭisanthārā → paṭisandhārā (mr)
msgctxt "an2.151-162:152.1.1"
msgid "“Dveme, bhikkhave, paṭisanthārā."
msgstr ""

msgctxt "an2.151-162:152.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.151-162:152.1.3"
msgid "Āmisapaṭisanthāro ca dhammapaṭisanthāro ca."
msgstr ""

msgctxt "an2.151-162:152.1.4"
msgid "Ime kho, bhikkhave, dve paṭisanthārā."
msgstr ""

msgctxt "an2.151-162:152.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ paṭisanthārānaṃ yadidaṃ "
"dhammapaṭisanthāro”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.153" id="153"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:152.1.6"
msgid "153"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc153.1"></a>
msgctxt "an2.151-162:153.1.1"
msgid "“Dvemā, bhikkhave, esanā."
msgstr ""

msgctxt "an2.151-162:153.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.151-162:153.1.3"
msgid "Āmisesanā ca dhammesanā ca."
msgstr ""

msgctxt "an2.151-162:153.1.4"
msgid "Imā kho, bhikkhave, dve esanā."
msgstr ""

msgctxt "an2.151-162:153.1.5"
msgid "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ esanānaṃ yadidaṃ dhammesanā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.154" id="154"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:153.1.6"
msgid "154"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc154.1"></a>
msgctxt "an2.151-162:154.1.1"
msgid "“Dvemā, bhikkhave, pariyesanā."
msgstr ""

msgctxt "an2.151-162:154.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.151-162:154.1.3"
msgid "Āmisapariyesanā ca dhammapariyesanā ca."
msgstr ""

msgctxt "an2.151-162:154.1.4"
msgid "Imā kho, bhikkhave, dve pariyesanā."
msgstr ""

msgctxt "an2.151-162:154.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ pariyesanānaṃ yadidaṃ "
"dhammapariyesanā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.155" id="155"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:154.1.6"
msgid "155"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc155.1"></a>
msgctxt "an2.151-162:155.1.1"
msgid "“Dvemā, bhikkhave, pariyeṭṭhiyo."
msgstr ""

msgctxt "an2.151-162:155.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.151-162:155.1.3"
msgid "Āmisapariyeṭṭhi ca dhammapariyeṭṭhi ca."
msgstr ""

msgctxt "an2.151-162:155.1.4"
msgid "Imā kho, bhikkhave, dve pariyeṭṭhiyo."
msgstr ""

msgctxt "an2.151-162:155.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ pariyeṭṭhīnaṃ yadidaṃ "
"dhammapariyeṭṭhī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.156" id="156"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:155.1.6"
msgid "156"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc156.1"></a>
msgctxt "an2.151-162:156.1.1"
msgid "“Dvemā, bhikkhave, pūjā."
msgstr ""

msgctxt "an2.151-162:156.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.151-162:156.1.3"
msgid "Āmisapūjā ca dhammapūjā ca."
msgstr ""

msgctxt "an2.151-162:156.1.4"
msgid "Imā kho, bhikkhave, dve pūjā."
msgstr ""

msgctxt "an2.151-162:156.1.5"
msgid "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ pūjānaṃ yadidaṃ dhammapūjā”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.157" id="157"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:156.1.6"
msgid "157"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc157.1"></a>
msgctxt "an2.151-162:157.1.1"
msgid "“Dvemāni, bhikkhave, ātitheyyāni."
msgstr ""

msgctxt "an2.151-162:157.1.2"
msgid "Katamāni dve?"
msgstr ""

msgctxt "an2.151-162:157.1.3"
msgid "Āmisātitheyyañca dhammātitheyyañca."
msgstr ""

msgctxt "an2.151-162:157.1.4"
msgid "Imāni kho, bhikkhave, dve ātitheyyāni."
msgstr ""

msgctxt "an2.151-162:157.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ ātitheyyānaṃ yadidaṃ dhammātitheyyan”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.158" id="158"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:157.1.6"
msgid "158"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc158.1"></a>
msgctxt "an2.151-162:158.1.1"
msgid "“Dvemā, bhikkhave, iddhiyo."
msgstr ""

msgctxt "an2.151-162:158.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.151-162:158.1.3"
msgid "Āmisiddhi ca dhammiddhi ca."
msgstr ""

msgctxt "an2.151-162:158.1.4"
msgid "Imā kho, bhikkhave, dve iddhiyo."
msgstr ""

msgctxt "an2.151-162:158.1.5"
msgid "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ iddhīnaṃ yadidaṃ dhammiddhī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.159" id="159"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:158.1.6"
msgid "159"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.94"></a>
#. <a class="sc" id="sc159.1"></a>
msgctxt "an2.151-162:159.1.1"
msgid "“Dvemā, bhikkhave, vuddhiyo."
msgstr ""

msgctxt "an2.151-162:159.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.151-162:159.1.3"
msgid "Āmisavuddhi ca dhammavuddhi ca."
msgstr ""

msgctxt "an2.151-162:159.1.4"
msgid "Imā kho, bhikkhave, dve vuddhiyo."
msgstr ""

msgctxt "an2.151-162:159.1.5"
msgid "Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ vuddhīnaṃ yadidaṃ dhammavuddhī”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.160" id="160"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:159.1.6"
msgid "160"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc160.1"></a>
msgctxt "an2.151-162:160.1.1"
msgid "“Dvemāni, bhikkhave, ratanāni."
msgstr ""

msgctxt "an2.151-162:160.1.2"
msgid "Katamāni dve?"
msgstr ""

msgctxt "an2.151-162:160.1.3"
msgid "Āmisaratanañca dhammaratanañca."
msgstr ""

msgctxt "an2.151-162:160.1.4"
msgid "Imāni kho, bhikkhave, dve ratanāni."
msgstr ""

msgctxt "an2.151-162:160.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ ratanānaṃ yadidaṃ dhammaratanan”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.161" id="161"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:160.1.6"
msgid "161"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc161.1"></a>
msgctxt "an2.151-162:161.1.1"
msgid "“Dveme, bhikkhave, sannicayā."
msgstr ""

msgctxt "an2.151-162:161.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.151-162:161.1.3"
msgid "Āmisasannicayo ca dhammasannicayo ca."
msgstr ""

msgctxt "an2.151-162:161.1.4"
msgid "Ime kho, bhikkhave, dve sannicayā."
msgstr ""

msgctxt "an2.151-162:161.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sannicayānaṃ yadidaṃ dhammasannicayo”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.162" id="162"><article><div class="hgroup"><h1>
msgctxt "an2.151-162:161.1.6"
msgid "162"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc162.1"></a>
msgctxt "an2.151-162:162.1.1"
msgid "“Dvemāni, bhikkhave, vepullāni."
msgstr ""

msgctxt "an2.151-162:162.1.2"
msgid "Katamāni dve?"
msgstr ""

msgctxt "an2.151-162:162.1.3"
msgid "Āmisavepullañca dhammavepullañca."
msgstr ""

msgctxt "an2.151-162:162.1.4"
msgid "Imāni kho, bhikkhave, dve vepullāni."
msgstr ""

msgctxt "an2.151-162:162.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ vepullānaṃ yadidaṃ dhammavepullan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an2.151-162:162.1.6"
msgid "Santhāravaggo catuttho."
msgstr ""
