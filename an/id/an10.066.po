#. <section class="sutta" id="an10.66"><article><div class="hgroup"><p class="division">
msgctxt "an10.66:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.66:0.2"
msgid "7. Yamakavagga"
msgstr ""

#. </p><h1>
msgctxt "an10.66:0.3"
msgid "66. Dutiyasukhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.122"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.66:1.1"
msgid "Ekaṃ samayaṃ āyasmā sāriputto magadhesu viharati nālakagāmake."
msgstr ""

msgctxt "an10.66:1.2"
msgid ""
"Atha kho sāmaṇḍakāni paribbājako yenāyasmā sāriputto tenupasaṅkami; "
"upasaṅkamitvā āyasmatā sāriputtena saddhiṃ sammodi."
msgstr ""

msgctxt "an10.66:1.3"
msgid ""
"Sammodanīyaṃ kathaṃ sāraṇīyaṃ vītisāretvā ekamantaṃ nisīdi. Ekamantaṃ "
"nisinno kho sāmaṇḍakāni paribbājako āyasmantaṃ sāriputtaṃ etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.66:2.1"
msgid ""
"“Kiṃ nu kho, āvuso, sāriputta, imasmiṃ dhammavinaye sukhaṃ, kiṃ dukkhan”ti?"
msgstr ""

msgctxt "an10.66:2.2"
msgid "“Anabhirati kho, āvuso, imasmiṃ dhammavinaye dukkhā, abhirati sukhā."
msgstr ""

msgctxt "an10.66:2.3"
msgid "Anabhiratiyā, āvuso, sati idaṃ dukkhaṃ pāṭikaṅkhaṃ—"
msgstr ""

msgctxt "an10.66:2.4"
msgid "gacchantopi sukhaṃ sātaṃ nādhigacchati, "
msgstr ""

msgctxt "an10.66:2.5"
msgid "ṭhitopi …"
msgstr ""

msgctxt "an10.66:2.6"
msgid "nisinnopi …"
msgstr ""

msgctxt "an10.66:2.7"
msgid "sayānopi …"
msgstr ""

msgctxt "an10.66:2.8"
msgid "gāmagatopi …"
msgstr ""

msgctxt "an10.66:2.9"
msgid "araññagatopi …"
msgstr ""

msgctxt "an10.66:2.10"
msgid "rukkhamūlagatopi …"
msgstr ""

msgctxt "an10.66:2.11"
msgid "suññāgāragatopi …"
msgstr ""

msgctxt "an10.66:2.12"
msgid "abbhokāsagatopi …"
msgstr ""

msgctxt "an10.66:2.13"
msgid "bhikkhumajjhagatopi sukhaṃ sātaṃ nādhigacchati."
msgstr ""

msgctxt "an10.66:2.14"
msgid "Anabhiratiyā, āvuso, sati idaṃ dukkhaṃ pāṭikaṅkhaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an10.66:3.1"
msgid "Abhiratiyā, āvuso, sati idaṃ sukhaṃ pāṭikaṅkhaṃ—"
msgstr ""

msgctxt "an10.66:3.2"
msgid "gacchantopi sukhaṃ sātaṃ adhigacchati, "
msgstr ""

msgctxt "an10.66:3.3"
msgid "ṭhitopi …"
msgstr ""

msgctxt "an10.66:3.4"
msgid "nisinnopi …"
msgstr ""

msgctxt "an10.66:3.5"
msgid "sayānopi …"
msgstr ""

msgctxt "an10.66:3.6"
msgid "gāmagatopi …"
msgstr ""

msgctxt "an10.66:3.7"
msgid "araññagatopi …"
msgstr ""

msgctxt "an10.66:3.8"
msgid "rukkhamūlagatopi …"
msgstr ""

msgctxt "an10.66:3.9"
msgid "suññāgāragatopi …"
msgstr ""

msgctxt "an10.66:3.10"
msgid "abbhokāsagatopi …"
msgstr ""

msgctxt "an10.66:3.11"
msgid "bhikkhumajjhagatopi sukhaṃ sātaṃ adhigacchati."
msgstr ""

msgctxt "an10.66:3.12"
msgid "Abhiratiyā, āvuso, sati idaṃ sukhaṃ pāṭikaṅkhan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.66:3.13"
msgid "Chaṭṭhaṃ."
msgstr ""
