#. <section class="sutta" id="an5.163"><article><div class="hgroup"><p class="division">
msgctxt "an5.163:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.163:0.2"
msgid "17. Āghātavagga"
msgstr ""

#. </p><h1>
#. VAR: sākacchāsuttaṃ (bj)
msgctxt "an5.163:0.3"
msgid "163. Sākacchasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.163:1.1"
msgid "Tatra kho āyasmā sāriputto bhikkhū āmantesi:"
msgstr ""

msgctxt "an5.163:1.2"
msgid "“āvuso bhikkhave”ti."
msgstr ""

msgctxt "an5.163:1.3"
msgid "“Āvuso”ti kho te bhikkhū āyasmato sāriputtassa paccassosuṃ."
msgstr ""

msgctxt "an5.163:1.4"
msgid "Āyasmā sāriputto etadavoca:"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.191"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.163:2.1"
msgid ""
"“Pañcahāvuso, dhammehi samannāgato bhikkhu alaṃ sākaccho sabrahmacārīnaṃ."
msgstr ""

msgctxt "an5.163:2.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.163:2.3"
msgid ""
"Idhāvuso, bhikkhu attanā ca sīlasampanno hoti, sīlasampadākathāya ca āgataṃ "
"pañhaṃ byākattā hoti;"
msgstr ""

msgctxt "an5.163:2.4"
msgid ""
"attanā ca samādhisampanno hoti, samādhisampadākathāya ca āgataṃ pañhaṃ "
"byākattā hoti;"
msgstr ""

msgctxt "an5.163:2.5"
msgid ""
"attanā ca paññāsampanno hoti, paññāsampadākathāya ca āgataṃ pañhaṃ byākattā "
"hoti;"
msgstr ""

msgctxt "an5.163:2.6"
msgid ""
"attanā ca vimuttisampanno hoti, vimuttisampadākathāya ca āgataṃ pañhaṃ "
"byākattā hoti;"
msgstr ""

msgctxt "an5.163:2.7"
msgid ""
"attanā ca vimuttiñāṇadassanasampanno hoti, vimuttiñāṇadassanasampadākathāya "
"ca āgataṃ pañhaṃ byākattā hoti."
msgstr ""

msgctxt "an5.163:2.8"
msgid ""
"Imehi kho, āvuso, pañcahi dhammehi samannāgato bhikkhu alaṃ sākaccho "
"sabrahmacārīnan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.163:2.9"
msgid "Tatiyaṃ."
msgstr ""
