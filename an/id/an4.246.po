#. <section class="sutta" id="an4.246"><article><div class="hgroup"><p class="division">
msgctxt "an4.246:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.246:0.2"
msgid "25. Āpattibhayavagga"
msgstr ""

#. </p><h1>
msgctxt "an4.246:0.3"
msgid "246. Seyyāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.246:1.1"
msgid "“Catasso imā, bhikkhave, seyyā."
msgstr ""

msgctxt "an4.246:1.2"
msgid "Katamā catasso?"
msgstr ""

msgctxt "an4.246:1.3"
msgid "Petaseyyā, kāmabhogiseyyā, sīhaseyyā, tathāgataseyyā."
msgstr ""

msgctxt "an4.246:1.4"
msgid "Katamā ca, bhikkhave, petaseyyā?"
msgstr ""

msgctxt "an4.246:1.5"
msgid "Yebhuyyena, bhikkhave, petā uttānā senti;"
msgstr ""

# Still called the “corpse posture” in yoga!
msgctxt "an4.246:1.6"
msgid "ayaṃ vuccati, bhikkhave, petaseyyā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.246:2.1"
msgid "Katamā ca, bhikkhave, kāmabhogiseyyā?"
msgstr ""

msgctxt "an4.246:2.2"
msgid "Yebhuyyena, bhikkhave, kāmabhogī vāmena passena senti;"
msgstr ""

msgctxt "an4.246:2.3"
msgid "ayaṃ vuccati, bhikkhave, kāmabhogiseyyā."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.245"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.246:3.1"
msgid "Katamā ca, bhikkhave, sīhaseyyā?"
msgstr ""

msgctxt "an4.246:3.2"
msgid ""
"Sīho, bhikkhave, migarājā dakkhiṇena passena seyyaṃ kappeti, pāde pādaṃ "
"accādhāya, antarasatthimhi naṅguṭṭhaṃ anupakkhipitvā."
msgstr ""

msgctxt "an4.246:3.3"
msgid ""
"So paṭibujjhitvā purimaṃ kāyaṃ abbhunnāmetvā pacchimaṃ kāyaṃ anuviloketi."
msgstr ""

msgctxt "an4.246:3.4"
msgid ""
"Sace, bhikkhave, sīho migarājā kiñci passati kāyassa vikkhittaṃ vā visaṭaṃ "
"vā, tena, bhikkhave, sīho migarājā anattamano hoti."
msgstr ""

msgctxt "an4.246:3.5"
msgid ""
"Sace pana, bhikkhave, sīho migarājā na kiñci passati kāyassa vikkhittaṃ vā "
"visaṭaṃ vā, tena, bhikkhave, sīho migarājā attamano hoti."
msgstr ""

msgctxt "an4.246:3.6"
msgid "Ayaṃ vuccati, bhikkhave, sīhaseyyā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.246:4.1"
msgid "Katamā ca, bhikkhave, tathāgataseyyā?"
msgstr ""

msgctxt "an4.246:4.2"
msgid ""
"Idha, bhikkhave, tathāgato vivicceva kāmehi … pe … catutthaṃ jhānaṃ "
"upasampajja viharati."
msgstr ""

msgctxt "an4.246:4.3"
msgid "Ayaṃ vuccati, bhikkhave, tathāgataseyyā."
msgstr ""

msgctxt "an4.246:4.4"
msgid "Imā kho, bhikkhave, catasso seyyā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.246:4.5"
msgid "Catutthaṃ."
msgstr ""
