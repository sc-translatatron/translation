#. <section class="sutta" id="an3.90"><article><div class="hgroup"><p class="division">
msgctxt "an3.90:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.90:0.2"
msgid "9. Samaṇavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.90:0.3"
msgid "90. Dutiyasikkhattayasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.90:1.1"
msgid "“Tisso imā, bhikkhave, sikkhā."
msgstr ""

msgctxt "an3.90:1.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "an3.90:1.3"
msgid "Adhisīlasikkhā, adhicittasikkhā, adhipaññāsikkhā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.90:2.1"
msgid "Katamā ca, bhikkhave, adhisīlasikkhā?"
msgstr ""

msgctxt "an3.90:2.2"
msgid ""
"Idha, bhikkhave, bhikkhu sīlavā hoti … pe … samādāya sikkhati sikkhāpadesu."
msgstr ""

msgctxt "an3.90:2.3"
msgid "Ayaṃ vuccati, bhikkhave, adhisīlasikkhā."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.236"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "an3.90:3.1"
msgid "Katamā ca, bhikkhave, adhicittasikkhā?"
msgstr ""

msgctxt "an3.90:3.2"
msgid ""
"Idha, bhikkhave, bhikkhu vivicceva kāmehi … pe … catutthaṃ jhānaṃ "
"upasampajja viharati."
msgstr ""

msgctxt "an3.90:3.3"
msgid "Ayaṃ vuccati, bhikkhave, adhicittasikkhā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an3.90:4.1"
msgid "Katamā ca, bhikkhave, adhipaññāsikkhā?"
msgstr ""

msgctxt "an3.90:4.2"
msgid ""
"Idha, bhikkhave, bhikkhu āsavānaṃ khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ "
"diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharati."
msgstr ""

msgctxt "an3.90:4.3"
msgid "Ayaṃ vuccati, bhikkhave, adhipaññāsikkhā."
msgstr ""

msgctxt "an3.90:4.4"
msgid "Imā kho, bhikkhave, tisso sikkhāti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an3.90:5.1"
msgid "Adhisīlaṃ adhicittaṃ,"
msgstr ""

#. <br>
msgctxt "an3.90:5.2"
msgid "adhipaññañca vīriyavā;"
msgstr ""

#. <br>
msgctxt "an3.90:5.3"
msgid "Thāmavā dhitimā jhāyī,"
msgstr ""

#. <br>
#. VAR: guttindriyo → uppattindriyo (mr)
msgctxt "an3.90:5.4"
msgid "sato guttindriyo care."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an3.90:6.1"
msgid "Yathā pure tathā pacchā,"
msgstr ""

#. <br>
msgctxt "an3.90:6.2"
msgid "yathā pacchā tathā pure;"
msgstr ""

#. <br>
msgctxt "an3.90:6.3"
msgid "Yathā adho tathā uddhaṃ,"
msgstr ""

#. <br>
msgctxt "an3.90:6.4"
msgid "yathā uddhaṃ tathā adho."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an3.90:7.1"
msgid "Yathā divā tathā rattiṃ,"
msgstr ""

#. <br>
msgctxt "an3.90:7.2"
msgid "yathā rattiṃ tathā divā;"
msgstr ""

#. <br>
msgctxt "an3.90:7.3"
msgid "Abhibhuyya disā sabbā,"
msgstr ""

#. <br>
msgctxt "an3.90:7.4"
msgid "appamāṇasamādhinā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
#. VAR: paṭipadaṃ → pāṭipadaṃ (?)
msgctxt "an3.90:8.1"
msgid "Tamāhu sekhaṃ paṭipadaṃ,"
msgstr ""

#. <br>
#. VAR: saṃsuddhacāriyaṃ → saṃsuddhacāraṇaṃ (bj, pts1) | saṃsuddhacārinaṃ (s1-3, km)
msgctxt "an3.90:8.2"
msgid "atho saṃsuddhacāriyaṃ;"
msgstr ""

#. <br>
msgctxt "an3.90:8.3"
msgid "Tamāhu loke sambuddhaṃ,"
msgstr ""

#. <br>
msgctxt "an3.90:8.4"
msgid "dhīraṃ paṭipadantaguṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an3.90:9.1"
msgid "Viññāṇassa nirodhena,"
msgstr ""

#. <br>
msgctxt "an3.90:9.2"
msgid "taṇhākkhayavimuttino;"
msgstr ""

#. <br>
msgctxt "an3.90:9.3"
msgid "Pajjotasseva nibbānaṃ,"
msgstr ""

#. <br>
msgctxt "an3.90:9.4"
msgid "vimokkho hoti cetaso”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an3.90:9.5"
msgid "Dasamaṃ."
msgstr ""
