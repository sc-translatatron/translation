#. <section class="sutta" id="an4.108"><article><div class="hgroup"><p class="division">
msgctxt "an4.108:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.108:0.2"
msgid "11. Valāhakavagga"
msgstr ""

#. </p><h1>
#. VAR: balivaddasuttaṃ (bj)
msgctxt "an4.108:0.3"
msgid "108. Balībaddasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.109"></a>
#. <a class="sc" id="sc1"></a>
#. VAR: balībaddā → balivaddā (bj, s1-3, km, pts1) | balibaddhā (mr)
msgctxt "an4.108:1.1"
msgid "“Cattārome, bhikkhave, balībaddā."
msgstr ""

msgctxt "an4.108:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.108:1.3"
msgid ""
"Sagavacaṇḍo no paragavacaṇḍo, paragavacaṇḍo no sagavacaṇḍo, sagavacaṇḍo ca "
"paragavacaṇḍo ca, neva sagavacaṇḍo no paragavacaṇḍo—"
msgstr ""

msgctxt "an4.108:1.4"
msgid "ime kho, bhikkhave, cattāro balībaddā."
msgstr ""

msgctxt "an4.108:1.5"
msgid ""
"Evamevaṃ kho, bhikkhave, cattāro balībaddūpamā puggalā santo saṃvijjamānā "
"lokasmiṃ."
msgstr ""

msgctxt "an4.108:1.6"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.108:1.7"
msgid ""
"Sagavacaṇḍo no paragavacaṇḍo, paragavacaṇḍo no sagavacaṇḍo, sagavacaṇḍo ca "
"paragavacaṇḍo ca, neva sagavacaṇḍo no paragavacaṇḍo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.108:2.1"
msgid "Kathañca, bhikkhave, puggalo sagavacaṇḍo hoti, no paragavacaṇḍo?"
msgstr ""

msgctxt "an4.108:2.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo sakaparisaṃ ubbejetā hoti, no paraparisaṃ."
msgstr ""

msgctxt "an4.108:2.3"
msgid "Evaṃ kho, bhikkhave, puggalo sagavacaṇḍo hoti, no paragavacaṇḍo."
msgstr ""

msgctxt "an4.108:2.4"
msgid "Seyyathāpi so, bhikkhave, balībaddo sagavacaṇḍo, no paragavacaṇḍo;"
msgstr ""

msgctxt "an4.108:2.5"
msgid "tathūpamāhaṃ, bhikkhave, imaṃ puggalaṃ vadāmi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.108:3.1"
msgid "Kathañca, bhikkhave, puggalo paragavacaṇḍo hoti, no sagavacaṇḍo?"
msgstr ""

msgctxt "an4.108:3.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo paraparisaṃ ubbejetā hoti, no sakaparisaṃ."
msgstr ""

msgctxt "an4.108:3.3"
msgid "Evaṃ kho, bhikkhave, puggalo paragavacaṇḍo hoti, no sagavacaṇḍo."
msgstr ""

msgctxt "an4.108:3.4"
msgid "Seyyathāpi so, bhikkhave, balībaddo paragavacaṇḍo, no sagavacaṇḍo;"
msgstr ""

msgctxt "an4.108:3.5"
msgid "tathūpamāhaṃ, bhikkhave, imaṃ puggalaṃ vadāmi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.108:4.1"
msgid "Kathañca, bhikkhave, puggalo sagavacaṇḍo ca hoti paragavacaṇḍo ca?"
msgstr ""

msgctxt "an4.108:4.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo sakaparisaṃ ubbejetā hoti paraparisañca."
msgstr ""

msgctxt "an4.108:4.3"
msgid "Evaṃ kho, bhikkhave, puggalo sagavacaṇḍo ca hoti paragavacaṇḍo ca."
msgstr ""

msgctxt "an4.108:4.4"
msgid "Seyyathāpi so, bhikkhave, balībaddo sagavacaṇḍo ca paragavacaṇḍo ca;"
msgstr ""

msgctxt "an4.108:4.5"
msgid "tathūpamāhaṃ, bhikkhave, imaṃ puggalaṃ vadāmi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.108:5.1"
msgid "Kathañca, bhikkhave, puggalo neva sagavacaṇḍo hoti no paragavacaṇḍo?"
msgstr ""

msgctxt "an4.108:5.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo neva sakaparisaṃ ubbejetā hoti, no "
"paraparisañca."
msgstr ""

msgctxt "an4.108:5.3"
msgid "Evaṃ kho, bhikkhave, puggalo neva sagavacaṇḍo hoti, no paragavacaṇḍo."
msgstr ""

msgctxt "an4.108:5.4"
msgid "Seyyathāpi so, bhikkhave, balībaddo neva sagavacaṇḍo, no paragavacaṇḍo;"
msgstr ""

msgctxt "an4.108:5.5"
msgid "tathūpamāhaṃ, bhikkhave, imaṃ puggalaṃ vadāmi."
msgstr ""

msgctxt "an4.108:5.6"
msgid ""
"Ime kho, bhikkhave, cattāro balībaddūpamā puggalā santo saṃvijjamānā "
"lokasmin”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.108:5.7"
msgid "Aṭṭhamaṃ."
msgstr ""
