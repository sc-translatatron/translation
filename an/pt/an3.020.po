#. <section class="sutta" id="an3.20"><article><div class="hgroup"><p class="division">
msgctxt "an3.20:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.20:0.2"
msgid "2. Rathakāravagga"
msgstr ""

#. </p><h1>
msgctxt "an3.20:0.3"
msgid "20. Dutiyapāpaṇikasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. VAR: mahattaṃ vepullattaṃ → mahantattaṃ vepullattaṃ (bj, s1-3) | mahantattaṃ vā vepullattaṃ vā (pts1, mr)
msgctxt "an3.20:1.1"
msgid ""
"“Tīhi, bhikkhave, aṅgehi samannāgato pāpaṇiko nacirasseva mahattaṃ "
"vepullattaṃ pāpuṇāti bhogesu."
msgstr ""

msgctxt "an3.20:1.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an3.20:1.3"
msgid ""
"Idha, bhikkhave, pāpaṇiko cakkhumā ca hoti vidhuro ca nissayasampanno ca."
msgstr ""

msgctxt "an3.20:1.4"
msgid "Kathañca, bhikkhave, pāpaṇiko cakkhumā hoti?"
msgstr ""

msgctxt "an3.20:1.5"
msgid "Idha, bhikkhave, pāpaṇiko paṇiyaṃ jānāti:"
msgstr ""

#. VAR: vikkayamānaṃ → vikkīyamānaṃ (?)
#. VAR: udayo’ti → uddayoti (si)
msgctxt "an3.20:1.6"
msgid ""
"‘idaṃ paṇiyaṃ evaṃ kītaṃ, evaṃ vikkayamānaṃ, ettakaṃ mūlaṃ bhavissati, "
"ettako udayo’ti."
msgstr ""

msgctxt "an3.20:1.7"
msgid "Evaṃ kho, bhikkhave, pāpaṇiko cakkhumā hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.20:2.1"
msgid "Kathañca, bhikkhave, pāpaṇiko vidhuro hoti?"
msgstr ""

msgctxt "an3.20:2.2"
msgid "Idha, bhikkhave, pāpaṇiko kusalo hoti paṇiyaṃ ketuñca vikketuñca."
msgstr ""

msgctxt "an3.20:2.3"
msgid "Evaṃ kho, bhikkhave, pāpaṇiko vidhuro hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.117"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "an3.20:3.1"
msgid "Kathañca, bhikkhave, pāpaṇiko nissayasampanno hoti?"
msgstr ""

msgctxt "an3.20:3.2"
msgid ""
"Idha, bhikkhave, pāpaṇikaṃ ye te gahapatī vā gahapatiputtā vā aḍḍhā "
"mahaddhanā mahābhogā te evaṃ jānanti:"
msgstr ""

msgctxt "an3.20:3.3"
msgid ""
"‘ayaṃ kho bhavaṃ pāpaṇiko cakkhumā vidhuro ca paṭibalo puttadārañca posetuṃ, "
"amhākañca kālena kālaṃ anuppadātun’ti."
msgstr ""

msgctxt "an3.20:3.4"
msgid "Te naṃ bhogehi nipatanti:"
msgstr ""

#. VAR: karitvā → haritvā (bj, s1-3, km)
msgctxt "an3.20:3.5"
msgid ""
"‘ito, samma pāpaṇika, bhoge karitvā puttadārañca posehi, amhākañca kālena "
"kālaṃ anuppadehī’ti."
msgstr ""

msgctxt "an3.20:3.6"
msgid "Evaṃ kho, bhikkhave, pāpaṇiko nissayasampanno hoti."
msgstr ""

msgctxt "an3.20:3.7"
msgid ""
"Imehi kho, bhikkhave, tīhi aṅgehi samannāgato pāpaṇiko nacirasseva mahattaṃ "
"vepullattaṃ pāpuṇāti bhogesu."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an3.20:4.1"
msgid ""
"Evamevaṃ kho, bhikkhave, tīhi dhammehi samannāgato bhikkhu nacirasseva "
"mahattaṃ vepullattaṃ pāpuṇāti kusalesu dhammesu."
msgstr ""

msgctxt "an3.20:4.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an3.20:4.3"
msgid ""
"Idha, bhikkhave, bhikkhu cakkhumā ca hoti vidhuro ca nissayasampanno ca."
msgstr ""

msgctxt "an3.20:4.4"
msgid "Kathañca, bhikkhave, bhikkhu cakkhumā hoti?"
msgstr ""

msgctxt "an3.20:4.5"
msgid ""
"Idha, bhikkhave, bhikkhu ‘idaṃ dukkhan’ti yathābhūtaṃ pajānāti, ‘ayaṃ "
"dukkhasamudayo’ti yathābhūtaṃ pajānāti, ‘ayaṃ dukkhanirodho’ti yathābhūtaṃ "
"pajānāti, ‘ayaṃ dukkhanirodhagāminī paṭipadā’ti yathābhūtaṃ pajānāti."
msgstr ""

msgctxt "an3.20:4.6"
msgid "Evaṃ kho, bhikkhave, bhikkhu cakkhumā hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an3.20:5.1"
msgid "Kathañca, bhikkhave, bhikkhu vidhuro hoti?"
msgstr ""

msgctxt "an3.20:5.2"
msgid ""
"Idha, bhikkhave, bhikkhu āraddhavīriyo viharati akusalānaṃ dhammānaṃ "
"pahānāya, kusalānaṃ dhammānaṃ upasampadāya, thāmavā daḷhaparakkamo "
"anikkhittadhuro kusalesu dhammesu."
msgstr ""

msgctxt "an3.20:5.3"
msgid "Evaṃ kho, bhikkhave, bhikkhu vidhuro hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.118"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "an3.20:6.1"
msgid "Kathañca, bhikkhave, bhikkhu nissayasampanno hoti?"
msgstr ""

msgctxt "an3.20:6.2"
msgid ""
"Idha, bhikkhave, bhikkhu ye te bhikkhū bahussutā āgatāgamā dhammadharā "
"vinayadharā mātikādharā te kālena kālaṃ upasaṅkamitvā paripucchati "
"paripañhati:"
msgstr ""

msgctxt "an3.20:6.3"
msgid "‘idaṃ, bhante, kathaṃ, imassa ko attho’ti?"
msgstr ""

msgctxt "an3.20:6.4"
msgid ""
"Tassa te āyasmanto avivaṭañceva vivaranti, anuttānīkatañca uttānīkaronti, "
"anekavihitesu ca kaṅkhāṭhāniyesu dhammesu kaṅkhaṃ paṭivinodenti."
msgstr ""

msgctxt "an3.20:6.5"
msgid "Evaṃ kho, bhikkhave, bhikkhu nissayasampanno hoti."
msgstr ""

msgctxt "an3.20:6.6"
msgid ""
"Imehi kho, bhikkhave, tīhi dhammehi samannāgato bhikkhu nacirasseva mahattaṃ "
"vepullattaṃ pāpuṇāti kusalesu dhammesū”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.20:6.7"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.20:6.8"
msgid "Rathakāravaggo dutiyo."
msgstr ""

#. <br>
msgctxt "an3.20:6.9"
msgid "Paṭhamabhāṇavāro niṭṭhito."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc7"></a>
#. VAR: Ñāto → ñātako (s1-3, km)
msgctxt "an3.20:7.1"
msgid "Ñāto sāraṇīyo bhikkhu,"
msgstr ""

#. <br>
msgctxt "an3.20:7.2"
msgid "cakkavattī sacetano;"
msgstr ""

#. <br>
msgctxt "an3.20:7.3"
msgid "Apaṇṇakattā devo ca,"
msgstr ""

#. <br>
msgctxt "an3.20:7.4"
msgid "duve pāpaṇikena cāti."
msgstr ""
