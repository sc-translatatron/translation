#. <section class="sutta" id="an5.207"><article><div class="hgroup"><p class="division">
msgctxt "an5.207:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.207:0.2"
msgid "21. Kimilavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.207:0.3"
msgid "207. Yāgusutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.207:1.1"
msgid "“Pañcime, bhikkhave, ānisaṃsā yāguyā."
msgstr ""

msgctxt "an5.207:1.2"
msgid "Katame pañca?"
msgstr ""

#. VAR: Khuddaṃ → khudaṃ (bj, pts1)
msgctxt "an5.207:1.3"
msgid ""
"Khuddaṃ paṭihanati, pipāsaṃ paṭivineti, vātaṃ anulometi, vatthiṃ sodheti, "
"āmāvasesaṃ pāceti."
msgstr ""

msgctxt "an5.207:1.4"
msgid "Ime kho, bhikkhave, pañca ānisaṃsā yāguyā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.207:1.5"
msgid "Sattamaṃ."
msgstr ""
