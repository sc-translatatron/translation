msgstr ""

#. </h1></div><p><a class="sc" id="1"></a><a class="pts" id="pts5.359"></a>
msgctxt "an11.22-29:5.1"
msgid ""
"“Ekādasahi, bhikkhave, aṅgehi samannāgato gopālako abhabbo gogaṇaṃ "
"pariharituṃ phātiṃ kātuṃ."
msgstr ""

msgctxt "an11.22-29:5.2"
msgid "Katamehi ekādasahi?"
msgstr ""

msgctxt "an11.22-29:5.3"
msgid ""
"Idha, bhikkhave, gopālako na rūpaññū hoti, na lakkhaṇakusalo hoti, na "
"āsāṭikaṃ hāretā hoti, na vaṇaṃ paṭicchādetā hoti, na dhūmaṃ kattā hoti, na "
"titthaṃ jānāti, na pītaṃ jānāti, na vīthiṃ jānāti, na gocarakusalo hoti, "
"anavasesadohī ca hoti, ye te usabhā gopitaro gopariṇāyakā te na "
"atirekapūjāya pūjetā hoti—"
msgstr ""

msgctxt "an11.22-29:5.4"
msgid ""
"imehi kho, bhikkhave, ekādasahi aṅgehi samannāgato gopālako abhabbo gogaṇaṃ "
"pariharituṃ phātiṃ kātuṃ."
msgstr ""

#. </p><p><a class="sc" id="2"></a>
msgctxt "an11.22-29:6.1"
msgid ""
"Evamevaṃ kho, bhikkhave, ekādasahi dhammehi samannāgato bhikkhu abhabbo "
"cakkhusmiṃ aniccānupassī viharituṃ…"
msgstr ""

msgctxt "an11.22-29:6.2"
msgid "pe…"
msgstr ""

msgctxt "an11.22-29:6.3"
msgid "abhabbo cakkhusmiṃ dukkhānupassī viharituṃ…"
msgstr ""

msgctxt "an11.22-29:6.4"
msgid "abhabbo cakkhusmiṃ anattānupassī viharituṃ…"
msgstr ""

msgctxt "an11.22-29:6.5"
msgid "abhabbo cakkhusmiṃ khayānupassī viharituṃ…"
msgstr ""

msgctxt "an11.22-29:6.6"
msgid "abhabbo cakkhusmiṃ vayānupassī viharituṃ…"
msgstr ""

msgctxt "an11.22-29:6.7"
msgid "abhabbo cakkhusmiṃ virāgānupassī viharituṃ…"
msgstr ""

msgctxt "an11.22-29:6.8"
msgid "abhabbo cakkhusmiṃ nirodhānupassī viharituṃ…"
msgstr ""

msgctxt "an11.22-29:6.9"
msgid "abhabbo cakkhusmiṃ paṭinissaggānupassī viharituṃ”."
msgstr ""
