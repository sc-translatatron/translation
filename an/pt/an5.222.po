#. <section class="sutta" id="an5.222"><article><div class="hgroup"><p class="division">
msgctxt "an5.222:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.222:0.2"
msgid "23. Dīghacārikavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.222:0.3"
msgid "222. Dutiyadīghacārikasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.222:1.1"
msgid ""
"“Pañcime, bhikkhave, ādīnavā dīghacārikaṃ anavatthacārikaṃ anuyuttassa "
"viharato."
msgstr ""

msgctxt "an5.222:1.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.222:1.3"
msgid ""
"Anadhigataṃ nādhigacchati, adhigatā parihāyati, adhigatenekaccena avisārado "
"hoti, gāḷhaṃ rogātaṅkaṃ phusati, na ca mittavā hoti."
msgstr ""

msgctxt "an5.222:1.4"
msgid ""
"Ime kho, bhikkhave, pañca ādīnavā dīghacārikaṃ anavatthacārikaṃ anuyuttassa "
"viharato."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.222:2.1"
msgid "Pañcime, bhikkhave, ānisaṃsā samavatthacāre."
msgstr ""

msgctxt "an5.222:2.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.222:2.3"
msgid ""
"Anadhigataṃ adhigacchati, adhigatā na parihāyati, adhigatenekaccena visārado "
"hoti, na gāḷhaṃ rogātaṅkaṃ phusati, mittavā ca hoti."
msgstr ""

msgctxt "an5.222:2.4"
msgid "Ime kho, bhikkhave, pañca ānisaṃsā samavatthacāre”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.222:2.5"
msgid "Dutiyaṃ."
msgstr ""
