#. <section class="sutta" id="an5.17"><article><div class="hgroup"><p class="division">
msgctxt "an5.17:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.17:0.2"
msgid "2. Balavagga"
msgstr ""

#. </p><h1>
#. VAR: attahitasuttaṃ (bj)
msgctxt "an5.17:0.3"
msgid "17. Paṭhamahitasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.13"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.17:1.1"
msgid ""
"“Pañcahi, bhikkhave, dhammehi samannāgato bhikkhu attahitāya paṭipanno hoti, "
"no parahitāya."
msgstr ""

msgctxt "an5.17:1.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.17:1.3"
msgid ""
"Idha, bhikkhave, bhikkhu attanā sīlasampanno hoti, no paraṃ sīlasampadāya "
"samādapeti;"
msgstr ""

msgctxt "an5.17:1.4"
msgid "attanā samādhisampanno hoti, no paraṃ samādhisampadāya samādapeti;"
msgstr ""

msgctxt "an5.17:1.5"
msgid "attanā paññāsampanno hoti, no paraṃ paññāsampadāya samādapeti;"
msgstr ""

msgctxt "an5.17:1.6"
msgid "attanā vimuttisampanno hoti, no paraṃ vimuttisampadāya samādapeti;"
msgstr ""

msgctxt "an5.17:1.7"
msgid ""
"attanā vimuttiñāṇadassanasampanno hoti, no paraṃ vimuttiñāṇadassanasampadāya "
"samādapeti."
msgstr ""

msgctxt "an5.17:1.8"
msgid ""
"Imehi kho, bhikkhave, pañcahi aṅgehi samannāgato bhikkhu attahitāya "
"paṭipanno hoti, no parahitāyā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.17:1.9"
msgid "Sattamaṃ."
msgstr ""
