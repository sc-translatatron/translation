#. <section class="sutta" id="an4.88"><article><div class="hgroup"><p class="division">
msgctxt "an4.88:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.88:0.2"
msgid "9. Macalavagga"
msgstr ""

#. </p><h1>
#. VAR: samaṇamacala-saññojanasuttaṃ (bj)
msgctxt "an4.88:0.3"
msgid "88. Saṃyojanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.88:1.1"
msgid "“Cattārome, bhikkhave, puggalā santo saṃvijjamānā lokasmiṃ."
msgstr ""

msgctxt "an4.88:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.88:1.3"
msgid "Samaṇamacalo, samaṇapuṇḍarīko, samaṇapadumo, samaṇesu samaṇasukhumālo."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.89"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.88:2.1"
msgid "Kathañca, bhikkhave, puggalo samaṇamacalo hoti?"
msgstr ""

msgctxt "an4.88:2.2"
msgid ""
"Idha, bhikkhave, bhikkhu tiṇṇaṃ saṃyojanānaṃ parikkhayā sotāpanno hoti "
"avinipātadhammo niyato sambodhiparāyaṇo."
msgstr ""

msgctxt "an4.88:2.3"
msgid "Evaṃ kho, bhikkhave, puggalo samaṇamacalo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.88:3.1"
msgid "Kathañca, bhikkhave, puggalo samaṇapuṇḍarīko hoti?"
msgstr ""

msgctxt "an4.88:3.2"
msgid ""
"Idha, bhikkhave, bhikkhu tiṇṇaṃ saṃyojanānaṃ parikkhayā, rāgadosamohānaṃ "
"tanuttā sakadāgāmī hoti, sakideva imaṃ lokaṃ āgantvā dukkhassantaṃ karoti."
msgstr ""

msgctxt "an4.88:3.3"
msgid "Evaṃ kho, bhikkhave, puggalo samaṇapuṇḍarīko hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.88:4.1"
msgid "Kathañca, bhikkhave, puggalo samaṇapadumo hoti?"
msgstr ""

msgctxt "an4.88:4.2"
msgid ""
"Idha, bhikkhave, bhikkhu pañcannaṃ orambhāgiyānaṃ saṃyojanānaṃ parikkhayā "
"opapātiko hoti tattha parinibbāyī anāvattidhammo tasmā lokā."
msgstr ""

msgctxt "an4.88:4.3"
msgid "Evaṃ kho, bhikkhave, puggalo samaṇapadumo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.88:5.1"
msgid "Kathañca, bhikkhave, puggalo samaṇesu samaṇasukhumālo hoti?"
msgstr ""

msgctxt "an4.88:5.2"
msgid ""
"Idha, bhikkhave, bhikkhu āsavānaṃ khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ "
"diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja viharati."
msgstr ""

msgctxt "an4.88:5.3"
msgid "Evaṃ kho, bhikkhave, puggalo samaṇesu samaṇasukhumālo hoti."
msgstr ""

msgctxt "an4.88:5.4"
msgid "Ime kho, bhikkhave, cattāro puggalā santo saṃvijjamānā lokasmin”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.88:5.5"
msgid "Aṭṭhamaṃ."
msgstr ""
