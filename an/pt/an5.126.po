#. <section class="sutta" id="an5.126"><article><div class="hgroup"><p class="division">
msgctxt "an5.126:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.126:0.2"
msgid "13. Gilānavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.126:0.3"
msgid "126. Dutiyaanāyussāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.126:1.1"
msgid "“Pañcime, bhikkhave, dhammā anāyussā."
msgstr ""

msgctxt "an5.126:1.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.126:1.3"
msgid ""
"Asappāyakārī hoti, sappāye mattaṃ na jānāti, apariṇatabhojī ca hoti, dussīlo "
"ca, pāpamitto ca."
msgstr ""

msgctxt "an5.126:1.4"
msgid "Ime kho, bhikkhave, pañca dhammā anāyussā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.126:2.1"
msgid "Pañcime, bhikkhave, dhammā āyussā."
msgstr ""

msgctxt "an5.126:2.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.126:2.3"
msgid ""
"Sappāyakārī hoti, sappāye mattaṃ jānāti, pariṇatabhojī ca hoti, sīlavā ca, "
"kalyāṇamitto ca."
msgstr ""

msgctxt "an5.126:2.4"
msgid "Ime kho, bhikkhave, pañca dhammā āyussā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.126:2.5"
msgid "Chaṭṭhaṃ."
msgstr ""
