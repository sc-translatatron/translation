#. <section class="sutta" id="an6.108"><article><div class="hgroup"><p class="division">
msgctxt "an6.108:0.1"
msgid "Aṅguttara Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "an6.108:0.2"
msgid "11. Tikavagga"
msgstr ""

#. </p><h1>
msgctxt "an6.108:0.3"
msgid "108. Duccaritasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an6.108:1.1"
msgid "“Tayome, bhikkhave, dhammā."
msgstr ""

msgctxt "an6.108:1.2"
msgid "Katame tayo?"
msgstr ""

msgctxt "an6.108:1.3"
msgid "Kāyaduccaritaṃ, vacīduccaritaṃ, manoduccaritaṃ."
msgstr ""

msgctxt "an6.108:1.4"
msgid "Ime kho, bhikkhave, tayo dhammā."
msgstr ""

msgctxt "an6.108:1.5"
msgid ""
"Imesaṃ kho, bhikkhave, tiṇṇaṃ dhammānaṃ pahānāya tayo dhammā bhāvetabbā."
msgstr ""

msgctxt "an6.108:1.6"
msgid "Katame tayo?"
msgstr ""

msgctxt "an6.108:1.7"
msgid ""
"Kāyaduccaritassa pahānāya kāyasucaritaṃ bhāvetabbaṃ, vacīduccaritassa "
"pahānāya vacīsucaritaṃ bhāvetabbaṃ, manoduccaritassa pahānāya manosucaritaṃ "
"bhāvetabbaṃ."
msgstr ""

msgctxt "an6.108:1.8"
msgid ""
"Imesaṃ kho, bhikkhave, tiṇṇaṃ dhammānaṃ pahānāya ime tayo dhammā "
"bhāvetabbā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.108:1.9"
msgid "Dutiyaṃ."
msgstr ""
