#. <section class="sutta" id="an4.140"><article><div class="hgroup"><p class="division">
msgctxt "an4.140:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.140:0.2"
msgid "14. Puggalavagga"
msgstr ""

#. </p><h1>
msgctxt "an4.140:0.3"
msgid "140. Vādīsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.139"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.140:1.1"
msgid "“Cattārome, bhikkhave, vādī."
msgstr ""

msgctxt "an4.140:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.140:1.3"
msgid "Atthi, bhikkhave, vādī atthato pariyādānaṃ gacchati, no byañjanato;"
msgstr ""

msgctxt "an4.140:1.4"
msgid "atthi, bhikkhave, vādī byañjanato pariyādānaṃ gacchati, no atthato;"
msgstr ""

msgctxt "an4.140:1.5"
msgid "atthi, bhikkhave, vādī atthato ca byañjanato ca pariyādānaṃ gacchati;"
msgstr ""

msgctxt "an4.140:1.6"
msgid "atthi, bhikkhave, vādī nevatthato no byañjanato pariyādānaṃ gacchati."
msgstr ""

msgctxt "an4.140:1.7"
msgid "Ime kho, bhikkhave, cattāro vādī."
msgstr ""

#. VAR: samannāgato → samannāgato bhikkhu (bj, s1-3, km)
msgctxt "an4.140:1.8"
msgid ""
"Aṭṭhānametaṃ, bhikkhave, anavakāso yaṃ catūhi paṭisambhidāhi samannāgato "
"atthato vā byañjanato vā pariyādānaṃ gaccheyyā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.140:1.9"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.140:1.10"
msgid "Puggalavaggo catuttho."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.140:2.1"
msgid "Saṃyojanaṃ paṭibhāno,"
msgstr ""

#. <br>
msgctxt "an4.140:2.2"
msgid "ugghaṭitaññu uṭṭhānaṃ;"
msgstr ""

#. <br>
msgctxt "an4.140:2.3"
msgid "Sāvajjo dve ca sīlāni,"
msgstr ""

#. <br>
msgctxt "an4.140:2.4"
msgid "nikaṭṭha dhamma vādī cāti."
msgstr ""
