#. <section class="sutta" id="an6.66"><article><div class="hgroup"><p class="division">
msgctxt "an6.66:0.1"
msgid "Aṅguttara Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "an6.66:0.2"
msgid "7. Devatāvagga"
msgstr ""

#. </p><h1>
msgctxt "an6.66:0.3"
msgid "66. Arahattasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an6.66:1.1"
msgid "“Cha, bhikkhave, dhamme appahāya abhabbo arahattaṃ sacchikātuṃ."
msgstr ""

msgctxt "an6.66:1.2"
msgid "Katame cha?"
msgstr ""

#. VAR: Thinaṃ → thīnaṃ (bj, s1-3, km, pts1)
msgctxt "an6.66:1.3"
msgid "Thinaṃ, middhaṃ, uddhaccaṃ, kukkuccaṃ, assaddhiyaṃ, pamādaṃ—"
msgstr ""

msgctxt "an6.66:1.4"
msgid "ime kho, bhikkhave, cha dhamme appahāya abhabbo arahattaṃ sacchikātuṃ."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.422"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an6.66:2.1"
msgid "Cha, bhikkhave, dhamme pahāya bhabbo arahattaṃ sacchikātuṃ."
msgstr ""

msgctxt "an6.66:2.2"
msgid "Katame cha?"
msgstr ""

msgctxt "an6.66:2.3"
msgid "Thinaṃ, middhaṃ, uddhaccaṃ, kukkuccaṃ, assaddhiyaṃ, pamādaṃ—"
msgstr ""

msgctxt "an6.66:2.4"
msgid "ime kho, bhikkhave, cha dhamme pahāya bhabbo arahattaṃ sacchikātun”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.66:2.5"
msgid "Dutiyaṃ."
msgstr ""
