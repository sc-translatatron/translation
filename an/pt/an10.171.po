#. <section class="sutta" id="an10.171"><article><div class="hgroup"><p class="division">
msgctxt "an10.171:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.171:0.2"
msgid "17. Jāṇussoṇivagga"
msgstr ""

#. </p><h1>
msgctxt "an10.171:0.3"
msgid "171. Paṭhamaadhammasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.171:1.1"
msgid "“Adhammo ca, bhikkhave, veditabbo anattho ca;"
msgstr ""

msgctxt "an10.171:1.2"
msgid "dhammo ca veditabbo attho ca."
msgstr ""

msgctxt "an10.171:1.3"
msgid ""
"Adhammañca viditvā anatthañca, dhammañca viditvā atthañca yathā dhammo yathā "
"attho tathā paṭipajjitabbaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.171:2.1"
msgid "Katamo ca, bhikkhave, adhammo ca anattho ca?"
msgstr ""

msgctxt "an10.171:2.2"
msgid ""
"Pāṇātipāto, adinnādānaṃ, kāmesumicchācāro, musāvādo, pisuṇā vācā, pharusā "
"vācā, samphappalāpo, abhijjhā, byāpādo, micchādiṭṭhi—"
msgstr ""

msgctxt "an10.171:2.3"
msgid "ayaṃ vuccati, bhikkhave, adhammo ca anattho ca."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an10.171:3.1"
msgid "Katamo ca, bhikkhave, dhammo ca attho ca?"
msgstr ""

msgctxt "an10.171:3.2"
msgid ""
"Pāṇātipātā veramaṇī, adinnādānā veramaṇī, kāmesumicchācārā veramaṇī, "
"musāvādā veramaṇī, pisuṇāya vācāya veramaṇī, pharusāya vācāya veramaṇī, "
"samphappalāpā veramaṇī, anabhijjhā, abyāpādo, sammādiṭṭhi—"
msgstr ""

msgctxt "an10.171:3.3"
msgid "ayaṃ vuccati, bhikkhave, dhammo ca attho ca."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an10.171:4.1"
msgid "‘Adhammo ca, bhikkhave, veditabbo anattho ca;"
msgstr ""

msgctxt "an10.171:4.2"
msgid "dhammo ca veditabbo attho ca."
msgstr ""

msgctxt "an10.171:4.3"
msgid ""
"Adhammañca viditvā anatthañca, dhammañca viditvā atthañca yathā dhammo yathā "
"attho tathā paṭipajjitabban’ti, "
msgstr ""

msgctxt "an10.171:4.4"
msgid "iti yaṃ taṃ vuttaṃ, idametaṃ paṭicca vuttan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.171:4.5"
msgid "Pañcamaṃ."
msgstr ""
