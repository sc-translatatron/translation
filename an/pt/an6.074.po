#. <section class="sutta" id="an6.74"><article><div class="hgroup"><p class="division">
msgctxt "an6.74:0.1"
msgid "Aṅguttara Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "an6.74:0.2"
msgid "7. Devatāvagga"
msgstr ""

#. </p><h1>
msgctxt "an6.74:0.3"
msgid "74. Dutiyatajjhānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an6.74:1.1"
msgid ""
"“Cha, bhikkhave, dhamme appahāya abhabbo paṭhamaṃ jhānaṃ upasampajja "
"viharituṃ."
msgstr ""

msgctxt "an6.74:1.2"
msgid "Katame cha?"
msgstr ""

msgctxt "an6.74:1.3"
msgid ""
"Kāmavitakkaṃ, byāpādavitakkaṃ, vihiṃsāvitakkaṃ, kāmasaññaṃ, byāpādasaññaṃ, "
"vihiṃsāsaññaṃ—"
msgstr ""

msgctxt "an6.74:1.4"
msgid ""
"ime kho, bhikkhave, cha dhamme appahāya abhabbo paṭhamaṃ jhānaṃ upasampajja "
"viharituṃ."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.429"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an6.74:2.1"
msgid ""
"Cha, bhikkhave, dhamme pahāya bhabbo paṭhamaṃ jhānaṃ upasampajja viharituṃ."
msgstr ""

msgctxt "an6.74:2.2"
msgid "Katame cha?"
msgstr ""

msgctxt "an6.74:2.3"
msgid ""
"Kāmavitakkaṃ, byāpādavitakkaṃ, vihiṃsāvitakkaṃ, kāmasaññaṃ, byāpādasaññaṃ, "
"vihiṃsāsaññaṃ—"
msgstr ""

msgctxt "an6.74:2.4"
msgid ""
"ime kho, bhikkhave, cha dhamme pahāya bhabbo paṭhamaṃ jhānaṃ upasampajja "
"viharitun”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.74:2.5"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.74:2.6"
msgid "Devatāvaggo sattamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an6.74:3.1"
msgid "Anāgāmi arahaṃ mittā,"
msgstr ""

#. <br>
msgctxt "an6.74:3.2"
msgid "saṅgaṇikārāmadevatā;"
msgstr ""

#. <br>
msgctxt "an6.74:3.3"
msgid "Samādhisakkhibhabbaṃ balaṃ,"
msgstr ""

#. <br>
msgctxt "an6.74:3.4"
msgid "tajjhānā apare duveti."
msgstr ""
