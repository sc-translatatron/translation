#. <section class="sutta" id="an4.9"><article><div class="hgroup"><p class="division">
msgctxt "an4.9:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.9:0.2"
msgid "1. Bhaṇḍagāmavagga"
msgstr ""

#. </p><h1>
#. VAR: taṇhāsuttaṃ (bj)
msgctxt "an4.9:0.3"
msgid "9. Taṇhuppādasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.10"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.9:1.1"
msgid ""
"“Cattārome, bhikkhave, taṇhuppādā yattha bhikkhuno taṇhā uppajjamānā "
"uppajjati."
msgstr ""

msgctxt "an4.9:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.9:1.3"
msgid "Cīvarahetu vā, bhikkhave, bhikkhuno taṇhā uppajjamānā uppajjati;"
msgstr ""

msgctxt "an4.9:1.4"
msgid "piṇḍapātahetu vā, bhikkhave, bhikkhuno taṇhā uppajjamānā uppajjati;"
msgstr ""

msgctxt "an4.9:1.5"
msgid "senāsanahetu vā, bhikkhave, bhikkhuno taṇhā uppajjamānā uppajjati;"
msgstr ""

msgctxt "an4.9:1.6"
msgid "itibhavābhavahetu vā, bhikkhave, bhikkhuno taṇhā uppajjamānā uppajjati."
msgstr ""

msgctxt "an4.9:1.7"
msgid ""
"Ime kho, bhikkhave, cattāro taṇhuppādā yattha bhikkhuno taṇhā uppajjamānā "
"uppajjatīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.9:2.1"
msgid "Taṇhā dutiyo puriso,"
msgstr ""

#. <br>
msgctxt "an4.9:2.2"
msgid "dīghamaddhāna saṃsaraṃ;"
msgstr ""

#. <br>
msgctxt "an4.9:2.3"
msgid "Itthabhāvaññathābhāvaṃ,"
msgstr ""

#. <br>
msgctxt "an4.9:2.4"
msgid "saṃsāraṃ nātivattati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.9:3.1"
msgid "Evamādīnavaṃ ñatvā,"
msgstr ""

#. <br>
msgctxt "an4.9:3.2"
msgid "Taṇhaṃ dukkhassa sambhavaṃ;"
msgstr ""

#. <br>
msgctxt "an4.9:3.3"
msgid "Vītataṇho anādāno,"
msgstr ""

#. <br>
msgctxt "an4.9:3.4"
msgid "Sato bhikkhu paribbaje”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an4.9:3.5"
msgid "Navamaṃ."
msgstr ""
