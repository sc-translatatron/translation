#. <section class="sutta" id="an3.143"><article><div class="hgroup"><p class="division">
msgctxt "an3.143:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.143:0.2"
msgid "14. Yodhājīvavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.143:0.3"
msgid "143. Paṭhamamoranivāpasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.143:1.1"
msgid "Ekaṃ samayaṃ bhagavā rājagahe viharati moranivāpe paribbājakārāme."
msgstr ""

msgctxt "an3.143:1.2"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "an3.143:1.3"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "an3.143:1.4"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "an3.143:1.5"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.143:2.1"
msgid ""
"“Tīhi, bhikkhave, dhammehi samannāgato bhikkhu accantaniṭṭho hoti "
"accantayogakkhemī accantabrahmacārī accantapariyosāno seṭṭho devamanussānaṃ."
msgstr ""

msgctxt "an3.143:2.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an3.143:2.3"
msgid ""
"Asekkhena sīlakkhandhena, asekkhena samādhikkhandhena, asekkhena "
"paññākkhandhena."
msgstr ""

msgctxt "an3.143:2.4"
msgid ""
"Imehi kho, bhikkhave, tīhi dhammehi samannāgato bhikkhu accantaniṭṭho hoti "
"accantayogakkhemī accantabrahmacārī accantapariyosāno seṭṭho "
"devamanussānan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.143:2.5"
msgid "Ekādasamaṃ."
msgstr ""
