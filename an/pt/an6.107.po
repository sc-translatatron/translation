#. <section class="sutta" id="an6.107"><article><div class="hgroup"><p class="division">
msgctxt "an6.107:0.1"
msgid "Aṅguttara Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "an6.107:0.2"
msgid "11. Tikavagga"
msgstr ""

#. </p><h1>
msgctxt "an6.107:0.3"
msgid "107. Rāgasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.446"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an6.107:1.1"
msgid "“Tayome, bhikkhave, dhammā."
msgstr ""

msgctxt "an6.107:1.2"
msgid "Katame tayo?"
msgstr ""

msgctxt "an6.107:1.3"
msgid "Rāgo, doso, moho."
msgstr ""

msgctxt "an6.107:1.4"
msgid "Ime kho, bhikkhave, tayo dhammā."
msgstr ""

msgctxt "an6.107:1.5"
msgid ""
"Imesaṃ kho, bhikkhave, tiṇṇaṃ dhammānaṃ pahānāya tayo dhammā bhāvetabbā."
msgstr ""

msgctxt "an6.107:1.6"
msgid "Katame tayo?"
msgstr ""

msgctxt "an6.107:1.7"
msgid ""
"Rāgassa pahānāya asubhā bhāvetabbā, dosassa pahānāya mettā bhāvetabbā, "
"mohassa pahānāya paññā bhāvetabbā."
msgstr ""

msgctxt "an6.107:1.8"
msgid ""
"Imesaṃ kho, bhikkhave, tiṇṇaṃ dhammānaṃ pahānāya ime tayo dhammā "
"bhāvetabbā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.107:1.9"
msgid "Paṭhamaṃ."
msgstr ""
