#. <section class="sutta" id="an5.251"><article><div class="hgroup"><p class="division">
msgctxt "an5.251:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
#. VAR: vaggāsaṅgahitā suttantā (s1-3)
msgctxt "an5.251:0.2"
msgid "26. Upasampadāvagga"
msgstr ""

#. </p><h1>
msgctxt "an5.251:0.3"
msgid "251. Upasampādetabbasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.251:1.1"
msgid "“Pañcahi, bhikkhave, dhammehi samannāgatena bhikkhunā upasampādetabbaṃ."
msgstr ""

msgctxt "an5.251:1.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.251:1.3"
msgid "Idha, bhikkhave, bhikkhu asekhena sīlakkhandhena samannāgato hoti;"
msgstr ""

msgctxt "an5.251:1.4"
msgid "asekhena samādhikkhandhena samannāgato hoti;"
msgstr ""

msgctxt "an5.251:1.5"
msgid "asekhena paññākkhandhena samannāgato hoti;"
msgstr ""

msgctxt "an5.251:1.6"
msgid "asekhena vimuttikkhandhena samannāgato hoti;"
msgstr ""

msgctxt "an5.251:1.7"
msgid "asekhena vimuttiñāṇadassanakkhandhena samannāgato hoti."
msgstr ""

msgctxt "an5.251:1.8"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgatena bhikkhunā "
"upasampādetabban”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.251:1.9"
msgid "Paṭhamaṃ."
msgstr ""
