#. <section class="sutta" id="an4.208"><article><div class="hgroup"><p class="division">
msgctxt "an4.208:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.208:0.2"
msgid "21. Sappurisavagga"
msgstr ""

#. </p><h1>
#. VAR: dutiyapāpa-kalyāṇasuttaṃ (bj)
msgctxt "an4.208:0.3"
msgid "208. Dutiyapāpadhammasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.208:1.1"
msgid "“Pāpañca vo, bhikkhave, desessāmi, pāpena pāpatarañca;"
msgstr ""

msgctxt "an4.208:1.2"
msgid "kalyāṇañca, kalyāṇena kalyāṇatarañca."
msgstr ""

msgctxt "an4.208:1.3"
msgid "Taṃ suṇātha … pe …"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.208:2.1"
msgid "“Katamo ca, bhikkhave, pāpo?"
msgstr ""

msgctxt "an4.208:2.2"
msgid ""
"Idha, bhikkhave, ekacco micchādiṭṭhiko hoti … pe … micchāñāṇī hoti, "
"micchāvimutti hoti."
msgstr ""

msgctxt "an4.208:2.3"
msgid "Ayaṃ vuccati, bhikkhave, pāpo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.208:3.1"
msgid "Katamo ca, bhikkhave, pāpena pāpataro?"
msgstr ""

msgctxt "an4.208:3.2"
msgid ""
"Idha, bhikkhave, ekacco attanā ca micchādiṭṭhiko hoti, parañca "
"micchādiṭṭhiyā samādapeti … pe … attanā ca micchāñāṇī hoti, parañca "
"micchāñāṇe samādapeti;"
msgstr ""

msgctxt "an4.208:3.3"
msgid "attanā ca micchāvimutti hoti, parañca micchāvimuttiyā samādapeti."
msgstr ""

msgctxt "an4.208:3.4"
msgid "Ayaṃ vuccati, bhikkhave, pāpena pāpataro."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.208:4.1"
msgid "Katamo ca, bhikkhave, kalyāṇo?"
msgstr ""

msgctxt "an4.208:4.2"
msgid ""
"Idha, bhikkhave, ekacco sammādiṭṭhiko hoti … pe … sammāñāṇī hoti, "
"sammāvimutti hoti."
msgstr ""

msgctxt "an4.208:4.3"
msgid "Ayaṃ vuccati, bhikkhave, kalyāṇo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.208:5.1"
msgid "Katamo ca, bhikkhave, kalyāṇena kalyāṇataro?"
msgstr ""

msgctxt "an4.208:5.2"
msgid ""
"Idha, bhikkhave, ekacco attanā ca sammādiṭṭhiko hoti, parañca sammādiṭṭhiyā "
"samādapeti … pe … attanā ca sammāñāṇī hoti, parañca sammāñāṇe samādapeti;"
msgstr ""

msgctxt "an4.208:5.3"
msgid "attanā ca sammāvimutti hoti, parañca sammāvimuttiyā samādapeti."
msgstr ""

msgctxt "an4.208:5.4"
msgid "Ayaṃ vuccati, bhikkhave, kalyāṇena kalyāṇataro”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.208:5.5"
msgid "Aṭṭhamaṃ."
msgstr ""
