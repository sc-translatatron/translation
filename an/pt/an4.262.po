#. <section class="sutta" id="an4.262"><article><div class="hgroup"><p class="division">
msgctxt "an4.262:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.262:0.2"
msgid "26. Abhiññāvagga"
msgstr ""

#. </p><h1>
#. VAR: araññasenāsanasuttaṃ (bj)
msgctxt "an4.262:0.3"
msgid "262. Araññasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.262:1.1"
msgid ""
"“Catūhi, bhikkhave, dhammehi samannāgato bhikkhu nālaṃ araññavanapatthāni "
"pantāni senāsanāni paṭisevituṃ."
msgstr ""

msgctxt "an4.262:1.2"
msgid "Katamehi catūhi?"
msgstr ""

msgctxt "an4.262:1.3"
msgid ""
"Kāmavitakkena, byāpādavitakkena, vihiṃsāvitakkena, duppañño hoti jaḷo "
"elamūgo—"
msgstr ""

msgctxt "an4.262:1.4"
msgid ""
"imehi kho, bhikkhave, catūhi dhammehi samannāgato bhikkhu nālaṃ "
"araññavanapatthāni pantāni senāsanāni paṭisevituṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.262:2.1"
msgid ""
"Catūhi, bhikkhave, dhammehi samannāgato bhikkhu alaṃ araññavanapatthāni "
"pantāni senāsanāni paṭisevituṃ."
msgstr ""

msgctxt "an4.262:2.2"
msgid "Katamehi catūhi?"
msgstr ""

msgctxt "an4.262:2.3"
msgid ""
"Nekkhammavitakkena, abyāpādavitakkena, avihiṃsāvitakkena, paññavā hoti ajaḷo "
"anelamūgo—"
msgstr ""

msgctxt "an4.262:2.4"
msgid ""
"imehi kho, bhikkhave, catūhi dhammehi samannāgato bhikkhu alaṃ "
"araññavanapatthāni pantāni senāsanāni paṭisevitun”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.262:2.5"
msgid "Navamaṃ."
msgstr ""
