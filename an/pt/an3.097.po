#. <section class="sutta" id="an3.97"><article><div class="hgroup"><p class="division">
msgctxt "an3.97:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.97:0.2"
msgid "10. Loṇakapallavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.97:0.3"
msgid "97. Dutiyaājānīyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.97:1.1"
msgid ""
"“Tīhi, bhikkhave, aṅgehi samannāgato rañño bhadro assājānīyo rājāraho hoti "
"rājabhoggo, rañño aṅganteva saṅkhayaṃ gacchati."
msgstr ""

msgctxt "an3.97:1.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an3.97:1.3"
msgid ""
"Idha, bhikkhave, rañño bhadro assājānīyo vaṇṇasampanno ca hoti balasampanno "
"ca javasampanno ca."
msgstr ""

msgctxt "an3.97:1.4"
msgid ""
"Imehi kho, bhikkhave, tīhi aṅgehi samannāgato rañño bhadro assājānīyo "
"rājāraho hoti rājabhoggo, rañño aṅganteva saṅkhyaṃ gacchati."
msgstr ""

msgctxt "an3.97:1.5"
msgid ""
"Evamevaṃ kho, bhikkhave, tīhi dhammehi samannāgato bhikkhu āhuneyyo hoti … "
"pe … anuttaraṃ puññakkhettaṃ lokassa."
msgstr ""

msgctxt "an3.97:1.6"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an3.97:1.7"
msgid ""
"Idha, bhikkhave, bhikkhu vaṇṇasampanno ca hoti balasampanno ca javasampanno "
"ca."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.97:2.1"
msgid "Kathañca, bhikkhave, bhikkhu vaṇṇasampanno hoti?"
msgstr ""

msgctxt "an3.97:2.2"
msgid ""
"Idha, bhikkhave, bhikkhu sīlavā hoti … pe … samādāya sikkhati sikkhāpadesu."
msgstr ""

msgctxt "an3.97:2.3"
msgid "Evaṃ kho, bhikkhave, bhikkhu vaṇṇasampanno hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an3.97:3.1"
msgid "Kathañca, bhikkhave, bhikkhu balasampanno hoti?"
msgstr ""

msgctxt "an3.97:3.2"
msgid ""
"Idha, bhikkhave, bhikkhu āraddhavīriyo viharati akusalānaṃ dhammānaṃ "
"pahānāya kusalānaṃ dhammānaṃ upasampadāya thāmavā daḷhaparakkamo "
"anikkhittadhuro kusalesu dhammesu."
msgstr ""

msgctxt "an3.97:3.3"
msgid "Evaṃ kho, bhikkhave, bhikkhu balasampanno hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an3.97:4.1"
msgid "Kathañca, bhikkhave, bhikkhu javasampanno hoti?"
msgstr ""

msgctxt "an3.97:4.2"
msgid ""
"Idha, bhikkhave, bhikkhu pañcannaṃ orambhāgiyānaṃ saṃyojanānaṃ parikkhayā "
"opapātiko hoti tattha parinibbāyī anāvattidhammo tasmā lokā."
msgstr ""

msgctxt "an3.97:4.3"
msgid "Evaṃ kho, bhikkhave, bhikkhu javasampanno hoti."
msgstr ""

msgctxt "an3.97:4.4"
msgid ""
"Imehi kho, bhikkhave, tīhi dhammehi samannāgato bhikkhu āhuneyyo hoti … pe … "
"anuttaraṃ puññakkhettaṃ lokassā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.97:4.5"
msgid "Chaṭṭhaṃ."
msgstr ""
