#. <section class="sutta" id="an4.205"><article><div class="hgroup"><p class="division">
msgctxt "an4.205:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.205:0.2"
msgid "21. Sappurisavagga"
msgstr ""

#. </p><h1>
#. VAR: pañcamasappurisasuttaṃ (bj)
msgctxt "an4.205:0.3"
msgid "205. Aṭṭhaṅgikasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.205:1.1"
msgid ""
"“Asappurisañca vo, bhikkhave, desessāmi, asappurisena asappurisatarañca;"
msgstr ""

msgctxt "an4.205:1.2"
msgid "sappurisañca, sappurisena sappurisatarañca. Taṃ suṇātha … pe …."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.221"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.205:2.1"
msgid "“Katamo ca, bhikkhave, asappuriso?"
msgstr ""

msgctxt "an4.205:2.2"
msgid ""
"Idha, bhikkhave, ekacco micchādiṭṭhiko hoti, micchāsaṅkappo hoti, micchāvāco "
"hoti, micchākammanto hoti, micchāājīvo hoti, micchāvāyāmo hoti, micchāsati "
"hoti, micchāsamādhi hoti."
msgstr ""

msgctxt "an4.205:2.3"
msgid "Ayaṃ vuccati, bhikkhave, asappuriso."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.205:3.1"
msgid "Katamo ca, bhikkhave, asappurisena asappurisataro?"
msgstr ""

msgctxt "an4.205:3.2"
msgid ""
"Idha bhikkhave, ekacco attanā ca micchādiṭṭhiko hoti, parañca micchādiṭṭhiyā "
"samādapeti;"
msgstr ""

msgctxt "an4.205:3.3"
msgid "attanā ca micchāsaṅkappo hoti, parañca micchāsaṅkappe samādapeti;"
msgstr ""

msgctxt "an4.205:3.4"
msgid "attanā ca micchāvāco hoti, parañca micchāvācāya samādapeti;"
msgstr ""

msgctxt "an4.205:3.5"
msgid "attanā ca micchākammanto hoti, parañca micchākammante samādapeti;"
msgstr ""

msgctxt "an4.205:3.6"
msgid "attanā ca micchāājīvo hoti, parañca micchāājīve samādapeti;"
msgstr ""

msgctxt "an4.205:3.7"
msgid "attanā ca micchāvāyāmo hoti, parañca micchāvāyāme samādapeti;"
msgstr ""

msgctxt "an4.205:3.8"
msgid "attanā ca micchāsati hoti, parañca micchāsatiyā samādapeti;"
msgstr ""

msgctxt "an4.205:3.9"
msgid "attanā ca micchāsamādhi hoti, parañca micchāsamādhimhi samādapeti."
msgstr ""

msgctxt "an4.205:3.10"
msgid "Ayaṃ vuccati, bhikkhave, asappurisena asappurisataro."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.205:4.1"
msgid "Katamo ca, bhikkhave, sappuriso?"
msgstr ""

msgctxt "an4.205:4.2"
msgid ""
"Idha, bhikkhave, ekacco sammādiṭṭhiko hoti, sammāsaṅkappo hoti, sammāvāco "
"hoti, sammākammanto hoti, sammāājīvo hoti, sammāvāyāmo hoti, sammāsati hoti, "
"sammāsamādhi hoti."
msgstr ""

msgctxt "an4.205:4.3"
msgid "Ayaṃ vuccati, bhikkhave, sappuriso."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.205:5.1"
msgid "Katamo ca, bhikkhave, sappurisena sappurisataro?"
msgstr ""

msgctxt "an4.205:5.2"
msgid ""
"Idha, bhikkhave, ekacco attanā ca sammādiṭṭhiko hoti, parañca sammādiṭṭhiyā "
"samādapeti;"
msgstr ""

msgctxt "an4.205:5.3"
msgid "attanā ca sammāsaṅkappo hoti, parañca sammāsaṅkappe samādapeti;"
msgstr ""

msgctxt "an4.205:5.4"
msgid "attanā ca sammāvāco hoti, parañca sammāvācāya samādapeti;"
msgstr ""

msgctxt "an4.205:5.5"
msgid "attanā ca sammākammanto hoti, parañca sammākammante samādapeti;"
msgstr ""

msgctxt "an4.205:5.6"
msgid "attanā ca sammāājīvo hoti, parañca sammāājīve samādapeti;"
msgstr ""

msgctxt "an4.205:5.7"
msgid "attanā ca sammāvāyāmo hoti, parañca sammāvāyāme samādapeti;"
msgstr ""

msgctxt "an4.205:5.8"
msgid "attanā ca sammāsati hoti, parañca sammāsatiyā samādapeti;"
msgstr ""

msgctxt "an4.205:5.9"
msgid "attanā ca sammāsamādhi hoti, parañca sammāsamādhimhi samādapeti."
msgstr ""

msgctxt "an4.205:5.10"
msgid "Ayaṃ vuccati, bhikkhave, sappurisena sappurisataro”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.205:5.11"
msgid "Pañcamaṃ."
msgstr ""
