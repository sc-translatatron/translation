#. <section class="sutta" id="an4.149"><article><div class="hgroup"><p class="division">
msgctxt "an4.149:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.149:0.2"
msgid "15. Ābhāvagga"
msgstr ""

#. </p><h1>
#. VAR: vacīsucaritasuttaṃ (bj)
msgctxt "an4.149:0.3"
msgid "149. Sucaritasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.149:1.1"
msgid "“Cattārimāni, bhikkhave, vacīsucaritāni."
msgstr ""

msgctxt "an4.149:1.2"
msgid "Katamāni cattāri?"
msgstr ""

msgctxt "an4.149:1.3"
msgid "Saccavācā, apisuṇā vācā, saṇhā vācā, mantabhāsā—"
msgstr ""

msgctxt "an4.149:1.4"
msgid "imāni kho, bhikkhave, cattāri vacīsucaritānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.149:1.5"
msgid "Navamaṃ."
msgstr ""
