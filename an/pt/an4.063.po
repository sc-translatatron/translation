#. <section class="sutta" id="an4.63"><article><div class="hgroup"><p class="division">
msgctxt "an4.63:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.63:0.2"
msgid "7. Pattakammavagga"
msgstr ""

#. </p><h1>
msgctxt "an4.63:0.3"
msgid "63. Brahmasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.63:1.1"
msgid ""
"“Sabrahmakāni, bhikkhave, tāni kulāni yesaṃ puttānaṃ mātāpitaro ajjhāgāre "
"pūjitā honti."
msgstr ""

msgctxt "an4.63:1.2"
msgid ""
"Sapubbācariyakāni, bhikkhave, tāni kulāni, yesaṃ puttānaṃ mātāpitaro "
"ajjhāgāre pūjitā honti."
msgstr ""

#. VAR: Sapubbadevatāni → sapubbadevāni (s1-3, km)
msgctxt "an4.63:1.3"
msgid ""
"Sapubbadevatāni, bhikkhave, tāni kulāni yesaṃ puttānaṃ mātāpitaro ajjhāgāre "
"pūjitā honti."
msgstr ""

msgctxt "an4.63:1.4"
msgid ""
"Sāhuneyyakāni, bhikkhave, tāni kulāni yesaṃ puttānaṃ mātāpitaro ajjhāgāre "
"pūjitā honti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
#. VAR: mātāpitūnaṃ → mātāpitunnaṃ (bj, pts1)
msgctxt "an4.63:2.1"
msgid "Brahmāti, bhikkhave, mātāpitūnaṃ etaṃ adhivacanaṃ."
msgstr ""

msgctxt "an4.63:2.2"
msgid "Pubbācariyāti, bhikkhave, mātāpitūnaṃ etaṃ adhivacanaṃ."
msgstr ""

#. VAR: Pubbadevatāti → pubbadevāti (bj, s1-3, km)
msgctxt "an4.63:2.3"
msgid "Pubbadevatāti, bhikkhave, mātāpitūnaṃ etaṃ adhivacanaṃ."
msgstr ""

msgctxt "an4.63:2.4"
msgid "Āhuneyyāti, bhikkhave, mātāpitūnaṃ etaṃ adhivacanaṃ."
msgstr ""

msgctxt "an4.63:2.5"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "an4.63:2.6"
msgid ""
"Bahukārā, bhikkhave, mātāpitaro, puttānaṃ āpādakā posakā imassa lokassa "
"dassetāroti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.63:3.1"
msgid "Brahmāti mātāpitaro,"
msgstr ""

#. <br>
msgctxt "an4.63:3.2"
msgid "pubbācariyāti vuccare;"
msgstr ""

#. <br>
msgctxt "an4.63:3.3"
msgid "Āhuneyyā ca puttānaṃ,"
msgstr ""

#. <br>
msgctxt "an4.63:3.4"
msgid "pajāya anukampakā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.63:4.1"
msgid "Tasmā hi ne namasseyya,"
msgstr ""

#. <br>
msgctxt "an4.63:4.2"
msgid "sakkareyya ca paṇḍito;"
msgstr ""

#. <br>
msgctxt "an4.63:4.3"
msgid "Annena atha pānena,"
msgstr ""

#. <br>
msgctxt "an4.63:4.4"
msgid "vatthena sayanena ca;"
msgstr ""

#. <br>
msgctxt "an4.63:4.5"
msgid "Ucchādanena nhāpanena,"
msgstr ""

#. <br>
msgctxt "an4.63:4.6"
msgid "pādānaṃ dhovanena ca."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.63:5.1"
msgid "Tāya naṃ pāricariyāya,"
msgstr ""

#. <br>
msgctxt "an4.63:5.2"
msgid "mātāpitūsu paṇḍitā;"
msgstr ""

#. <br>
msgctxt "an4.63:5.3"
msgid "Idheva naṃ pasaṃsanti,"
msgstr ""

#. <br>
msgctxt "an4.63:5.4"
msgid "pecca sagge pamodatī”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an4.63:5.5"
msgid "Tatiyaṃ."
msgstr ""
