#. <section class="sutta" id="an3.45"><article><div class="hgroup"><p class="division">
msgctxt "an3.45:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.45:0.2"
msgid "5. Cūḷavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.45:0.3"
msgid "45. Paṇḍitasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.45:1.1"
msgid "“Tīṇimāni, bhikkhave, paṇḍitapaññattāni sappurisapaññattāni."
msgstr ""

msgctxt "an3.45:1.2"
msgid "Katamāni tīṇi?"
msgstr ""

msgctxt "an3.45:1.3"
msgid "Dānaṃ, bhikkhave, paṇḍitapaññattaṃ sappurisapaññattaṃ."
msgstr ""

msgctxt "an3.45:1.4"
msgid "Pabbajjā, bhikkhave, paṇḍitapaññattā sappurisapaññattā."
msgstr ""

msgctxt "an3.45:1.5"
msgid "Mātāpitūnaṃ, bhikkhave, upaṭṭhānaṃ paṇḍitapaññattaṃ sappurisapaññattaṃ."
msgstr ""

msgctxt "an3.45:1.6"
msgid "Imāni kho, bhikkhave, tīṇi paṇḍitapaññattāni sappurisapaññattānīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.45:2.1"
msgid "Sabbhi dānaṃ upaññattaṃ,"
msgstr ""

#. <br>
msgctxt "an3.45:2.2"
msgid "ahiṃsā saṃyamo damo;"
msgstr ""

#. <br>
msgctxt "an3.45:2.3"
msgid "Mātāpitu upaṭṭhānaṃ,"
msgstr ""

#. <br>
msgctxt "an3.45:2.4"
msgid "santānaṃ brahmacārinaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an3.45:3.1"
msgid "Sataṃ etāni ṭhānāni,"
msgstr ""

#. <br>
msgctxt "an3.45:3.2"
msgid "yāni sevetha paṇḍito;"
msgstr ""

#. <br>
msgctxt "an3.45:3.3"
msgid "Ariyo dassanasampanno,"
msgstr ""

#. <br>
msgctxt "an3.45:3.4"
msgid "sa lokaṃ bhajate sivan”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an3.45:3.5"
msgid "Pañcamaṃ."
msgstr ""
