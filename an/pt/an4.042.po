#. <section class="sutta" id="an4.42"><article><div class="hgroup"><p class="division">
msgctxt "an4.42:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.42:0.2"
msgid "5. Rohitassavagga"
msgstr ""

#. </p><h1>
msgctxt "an4.42:0.3"
msgid "42. Pañhabyākaraṇasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. VAR: pañhabyākaraṇāni → pañhābyākaraṇāni (mr)
msgctxt "an4.42:1.1"
msgid "“Cattārimāni, bhikkhave, pañhabyākaraṇāni."
msgstr ""

msgctxt "an4.42:1.2"
msgid "Katamāni cattāri?"
msgstr ""

msgctxt "an4.42:1.3"
msgid "Atthi, bhikkhave, pañho ekaṃsabyākaraṇīyo;"
msgstr ""

msgctxt "an4.42:1.4"
msgid "atthi, bhikkhave, pañho vibhajjabyākaraṇīyo;"
msgstr ""

msgctxt "an4.42:1.5"
msgid "atthi, bhikkhave, pañho paṭipucchābyākaraṇīyo;"
msgstr ""

msgctxt "an4.42:1.6"
msgid "atthi, bhikkhave, pañho ṭhapanīyo."
msgstr ""

msgctxt "an4.42:1.7"
msgid "Imāni kho, bhikkhave, cattāri pañhabyākaraṇānīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.42:2.1"
msgid "Ekaṃsavacanaṃ ekaṃ,"
msgstr ""

#. <br>
msgctxt "an4.42:2.2"
msgid "vibhajjavacanāparaṃ;"
msgstr ""

#. <br>
msgctxt "an4.42:2.3"
msgid "Tatiyaṃ paṭipuccheyya,"
msgstr ""

#. <br>
msgctxt "an4.42:2.4"
msgid "catutthaṃ pana ṭhāpaye."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
#. VAR: tesaṃ → nesaṃ (bj, s1-3, km)
msgctxt "an4.42:3.1"
msgid "Yo ca tesaṃ tattha tattha,"
msgstr ""

#. <br>
msgctxt "an4.42:3.2"
msgid "jānāti anudhammataṃ;"
msgstr ""

#. <br>
msgctxt "an4.42:3.3"
msgid "Catupañhassa kusalo,"
msgstr ""

#. <br>
msgctxt "an4.42:3.4"
msgid "āhu bhikkhuṃ tathāvidhaṃ."
msgstr ""

# Comm: Durāsado duppasahoti parehi ghaṭṭetuṃ vā abhibhavituṃ vā na sakkā
#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.42:4.1"
msgid "Durāsado duppasaho,"
msgstr ""

#. <br>
msgctxt "an4.42:4.2"
msgid "gambhīro duppadhaṃsiyo;"
msgstr ""

#. <br>
msgctxt "an4.42:4.3"
msgid "Atho atthe anatthe ca,"
msgstr ""

#. <br>
#. VAR: ubhayassa hoti kovido → ubhayatthassa kovido (s1-3, km)
msgctxt "an4.42:4.4"
msgid "ubhayassa hoti kovido."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.42:5.1"
msgid "Anatthaṃ parivajjeti,"
msgstr ""

#. <br>
msgctxt "an4.42:5.2"
msgid "atthaṃ gaṇhāti paṇḍito;"
msgstr ""

#. <br>
msgctxt "an4.42:5.3"
msgid "Atthābhisamayā dhīro,"
msgstr ""

#. <br>
msgctxt "an4.42:5.4"
msgid "paṇḍitoti pavuccatī”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an4.42:5.5"
msgid "Dutiyaṃ."
msgstr ""
