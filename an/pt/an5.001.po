#. <section class="sutta" id="an5.1"><article><div class="hgroup"><p class="division">
msgctxt "an5.1:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.1:0.2"
msgid "1. Sekhabalavagga"
msgstr ""

#. </p><h1>
#. VAR: sekhabalasaṅkhittasuttaṃ (bj)
msgctxt "an5.1:0.3"
msgid "1. Saṅkhittasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.1"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "an5.1:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "an5.1:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "an5.1:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "an5.1:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "an5.1:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "an5.1:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
#. VAR: sekhabalāni → sekkhabalāni (mr)
msgctxt "an5.1:2.1"
msgid "“Pañcimāni, bhikkhave, sekhabalāni."
msgstr ""

msgctxt "an5.1:2.2"
msgid "Katamāni pañca?"
msgstr ""

#. VAR: hirībalaṃ → hiribalaṃ (bj, s3, pts1)
#. VAR: vīriyabalaṃ → viriyabalaṃ (bj, s1-3, km, pts1)
msgctxt "an5.1:2.3"
msgid "Saddhābalaṃ, hirībalaṃ, ottappabalaṃ, vīriyabalaṃ, paññābalaṃ—"
msgstr ""

msgctxt "an5.1:2.4"
msgid "imāni kho, bhikkhave, pañca sekhabalāni."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.1:3.1"
msgid "Tasmātiha, bhikkhave, evaṃ sikkhitabbaṃ:"
msgstr ""

msgctxt "an5.1:3.2"
msgid ""
"‘saddhābalena samannāgatā bhavissāma sekhabalena, hirībalena samannāgatā "
"bhavissāma sekhabalena, ottappabalena samannāgatā bhavissāma sekhabalena, "
"vīriyabalena samannāgatā bhavissāma sekhabalena, paññābalena samannāgatā "
"bhavissāma sekhabalenā’ti."
msgstr ""

msgctxt "an5.1:3.3"
msgid "Evañhi vo, bhikkhave, sikkhitabban”ti."
msgstr ""

msgctxt "an5.1:3.4"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "an5.1:3.5"
msgid "Attamanā te bhikkhū bhagavato bhāsitaṃ abhinandunti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.1:3.6"
msgid "Paṭhamaṃ."
msgstr ""
