#. <section class="sutta" id="an4.169"><article><div class="hgroup"><p class="division">
msgctxt "an4.169:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.169:0.2"
msgid "17. Paṭipadāvagga"
msgstr ""

#. </p><h1>
#. VAR: kilesaparinibbānasuttaṃ (bj)
msgctxt "an4.169:0.3"
msgid "169. Sasaṅkhārasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.169:1.1"
msgid "“Cattārome, bhikkhave, puggalā santo saṃvijjamānā lokasmiṃ."
msgstr ""

msgctxt "an4.169:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.169:1.3"
msgid ""
"Idha, bhikkhave, ekacco puggalo diṭṭheva dhamme sasaṅkhāraparinibbāyī hoti."
msgstr ""

msgctxt "an4.169:1.4"
msgid ""
"Idha pana, bhikkhave, ekacco puggalo kāyassa bhedā sasaṅkhāraparinibbāyī "
"hoti."
msgstr ""

msgctxt "an4.169:1.5"
msgid ""
"Idha pana, bhikkhave, ekacco puggalo diṭṭheva dhamme asaṅkhāraparinibbāyī "
"hoti."
msgstr ""

msgctxt "an4.169:1.6"
msgid ""
"Idha pana, bhikkhave, ekacco puggalo kāyassa bhedā asaṅkhāraparinibbāyī hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.156"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.169:2.1"
msgid ""
"Kathañca, bhikkhave, puggalo diṭṭheva dhamme sasaṅkhāraparinibbāyī hoti?"
msgstr ""

msgctxt "an4.169:2.2"
msgid ""
"Idha, bhikkhave, bhikkhu asubhānupassī kāye viharati, āhāre paṭikūlasaññī, "
"sabbaloke anabhiratisaññī, sabbasaṅkhāresu aniccānupassī."
msgstr ""

msgctxt "an4.169:2.3"
msgid "Maraṇasaññā kho panassa ajjhattaṃ sūpaṭṭhitā hoti."
msgstr ""

msgctxt "an4.169:2.4"
msgid "So imāni pañca sekhabalāni upanissāya viharati—"
msgstr ""

msgctxt "an4.169:2.5"
msgid "saddhābalaṃ, hiribalaṃ, ottappabalaṃ, vīriyabalaṃ, paññābalaṃ."
msgstr ""

msgctxt "an4.169:2.6"
msgid "Tassimāni pañcindriyāni adhimattāni pātubhavanti—"
msgstr ""

msgctxt "an4.169:2.7"
msgid ""
"saddhindriyaṃ, vīriyindriyaṃ, satindriyaṃ, samādhindriyaṃ, paññindriyaṃ."
msgstr ""

msgctxt "an4.169:2.8"
msgid ""
"So imesaṃ pañcannaṃ indriyānaṃ adhimattattā diṭṭheva dhamme "
"sasaṅkhāraparinibbāyī hoti."
msgstr ""

msgctxt "an4.169:2.9"
msgid ""
"Evaṃ kho, bhikkhave, puggalo diṭṭheva dhamme sasaṅkhāraparinibbāyī hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.169:3.1"
msgid "Kathañca, bhikkhave, puggalo kāyassa bhedā sasaṅkhāraparinibbāyī hoti?"
msgstr ""

msgctxt "an4.169:3.2"
msgid ""
"Idha, bhikkhave, bhikkhu asubhānupassī kāye viharati, āhāre paṭikūlasaññī, "
"sabbaloke anabhiratisaññī, sabbasaṅkhāresu aniccānupassī."
msgstr ""

msgctxt "an4.169:3.3"
msgid "Maraṇasaññā kho panassa ajjhattaṃ sūpaṭṭhitā hoti."
msgstr ""

msgctxt "an4.169:3.4"
msgid "So imāni pañca sekhabalāni upanissāya viharati—"
msgstr ""

msgctxt "an4.169:3.5"
msgid "saddhābalaṃ, hiribalaṃ, ottappabalaṃ, vīriyabalaṃ, paññābalaṃ."
msgstr ""

msgctxt "an4.169:3.6"
msgid "Tassimāni pañcindriyāni mudūni pātubhavanti—"
msgstr ""

msgctxt "an4.169:3.7"
msgid ""
"saddhindriyaṃ, vīriyindriyaṃ, satindriyaṃ, samādhindriyaṃ, paññindriyaṃ."
msgstr ""

msgctxt "an4.169:3.8"
msgid ""
"So imesaṃ pañcannaṃ indriyānaṃ muduttā kāyassa bhedā sasaṅkhāraparinibbāyī "
"hoti."
msgstr ""

msgctxt "an4.169:3.9"
msgid "Evaṃ kho, bhikkhave, puggalo kāyassa bhedā sasaṅkhāraparinibbāyī hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.169:4.1"
msgid "Kathañca, bhikkhave, puggalo diṭṭheva dhamme asaṅkhāraparinibbāyī hoti?"
msgstr ""

msgctxt "an4.169:4.2"
msgid ""
"Idha, bhikkhave, bhikkhu vivicceva kāmehi … pe … paṭhamaṃ jhānaṃ … pe … "
"dutiyaṃ jhānaṃ … pe … tatiyaṃ jhānaṃ … pe … catutthaṃ jhānaṃ upasampajja "
"viharati."
msgstr ""

msgctxt "an4.169:4.3"
msgid "So imāni pañca sekhabalāni upanissāya viharati—"
msgstr ""

msgctxt "an4.169:4.4"
msgid "saddhābalaṃ … pe … paññābalaṃ."
msgstr ""

msgctxt "an4.169:4.5"
msgid "Tassimāni pañcindriyāni adhimattāni pātubhavanti—"
msgstr ""

msgctxt "an4.169:4.6"
msgid "saddhindriyaṃ … pe … paññindriyaṃ."
msgstr ""

msgctxt "an4.169:4.7"
msgid ""
"So imesaṃ pañcannaṃ indriyānaṃ adhimattattā diṭṭheva dhamme "
"asaṅkhāraparinibbāyī hoti."
msgstr ""

msgctxt "an4.169:4.8"
msgid "Evaṃ kho, bhikkhave, puggalo diṭṭheva dhamme asaṅkhāraparinibbāyī hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.169:5.1"
msgid "Kathañca, bhikkhave, puggalo kāyassa bhedā asaṅkhāraparinibbāyī hoti?"
msgstr ""

msgctxt "an4.169:5.2"
msgid ""
"Idha, bhikkhave, bhikkhu vivicceva kāmehi … pe … paṭhamaṃ jhānaṃ … pe … "
"dutiyaṃ jhānaṃ … pe … tatiyaṃ jhānaṃ … pe … catutthaṃ jhānaṃ upasampajja "
"viharati."
msgstr ""

msgctxt "an4.169:5.3"
msgid "So imāni pañca sekhabalāni upanissāya viharati—"
msgstr ""

msgctxt "an4.169:5.4"
msgid "saddhābalaṃ, hiribalaṃ, ottappabalaṃ, vīriyabalaṃ, paññābalaṃ."
msgstr ""

msgctxt "an4.169:5.5"
msgid "Tassimāni pañcindriyāni … pe … paññindriyaṃ."
msgstr ""

msgctxt "an4.169:5.6"
msgid ""
"So imesaṃ pañcannaṃ indriyānaṃ muduttā kāyassa bhedā asaṅkhāraparinibbāyī "
"hoti."
msgstr ""

msgctxt "an4.169:5.7"
msgid "Evaṃ kho, bhikkhave, puggalo kāyassa bhedā asaṅkhāraparinibbāyī hoti."
msgstr ""

msgctxt "an4.169:5.8"
msgid "Ime kho, bhikkhave, cattāro puggalā santo saṃvijjamānā lokasmin”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.169:5.9"
msgid "Navamaṃ."
msgstr ""
