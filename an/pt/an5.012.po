#. <section class="sutta" id="an5.12"><article><div class="hgroup"><p class="division">
msgctxt "an5.12:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.12:0.2"
msgid "2. Balavagga"
msgstr ""

#. </p><h1>
#. VAR: sekhabalaaggasuttaṃ (bj)
msgctxt "an5.12:0.3"
msgid "12. Kūṭasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.10"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.12:1.1"
msgid "“Pañcimāni, bhikkhave, sekhabalāni."
msgstr ""

msgctxt "an5.12:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "an5.12:1.3"
msgid "Saddhābalaṃ, hirībalaṃ, ottappabalaṃ, vīriyabalaṃ, paññābalaṃ—"
msgstr ""

msgctxt "an5.12:1.4"
msgid "imāni kho, bhikkhave, pañca sekhabalāni."
msgstr ""

msgctxt "an5.12:1.5"
msgid ""
"Imesaṃ kho, bhikkhave, pañcannaṃ sekhabalānaṃ etaṃ aggaṃ etaṃ saṅgāhikaṃ "
"etaṃ saṅghātaniyaṃ, yadidaṃ paññābalaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.12:2.1"
msgid ""
"Seyyathāpi, bhikkhave, kūṭāgārassa etaṃ aggaṃ etaṃ saṅgāhikaṃ etaṃ "
"saṅghātaniyaṃ, yadidaṃ kūṭaṃ."
msgstr ""

msgctxt "an5.12:2.2"
msgid ""
"Evamevaṃ kho, bhikkhave, imesaṃ pañcannaṃ sekhabalānaṃ etaṃ aggaṃ etaṃ "
"saṅgāhikaṃ etaṃ saṅghātaniyaṃ, yadidaṃ paññābalaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.12:3.1"
msgid "Tasmātiha, bhikkhave, evaṃ sikkhitabbaṃ:"
msgstr ""

msgctxt "an5.12:3.2"
msgid ""
"‘saddhābalena samannāgatā bhavissāma sekhabalena, hirībalena … ottappabalena "
"… vīriyabalena … paññābalena samannāgatā bhavissāma sekhabalenā’ti."
msgstr ""

msgctxt "an5.12:3.3"
msgid "Evañhi vo, bhikkhave, sikkhitabban”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.12:3.4"
msgid "Dutiyaṃ."
msgstr ""
