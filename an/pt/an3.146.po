#. <section class="sutta" id="an3.146"><article><div class="hgroup"><p class="division">
msgctxt "an3.146:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.146:0.2"
msgid "15. Maṅgalavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.146:0.3"
msgid "146. Akusalasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.146:1.1"
msgid ""
"“Tīhi, bhikkhave, dhammehi samannāgato yathābhataṃ nikkhitto evaṃ niraye."
msgstr ""

msgctxt "an3.146:1.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an3.146:1.3"
msgid "Akusalena kāyakammena, akusalena vacīkammena, akusalena manokammena—"
msgstr ""

msgctxt "an3.146:1.4"
msgid ""
"imehi kho, bhikkhave, tīhi dhammehi samannāgato yathābhataṃ nikkhitto evaṃ "
"niraye."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.146:2.1"
msgid "Tīhi, bhikkhave, dhammehi samannāgato yathābhataṃ nikkhitto evaṃ sagge."
msgstr ""

msgctxt "an3.146:2.2"
msgid "Katamehi tīhi?"
msgstr ""

msgctxt "an3.146:2.3"
msgid "Kusalena kāyakammena, kusalena vacīkammena, kusalena manokammena—"
msgstr ""

msgctxt "an3.146:2.4"
msgid ""
"imehi kho, bhikkhave, tīhi dhammehi samannāgato yathābhataṃ nikkhitto evaṃ "
"sagge”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.146:2.5"
msgid "Paṭhamaṃ."
msgstr ""
