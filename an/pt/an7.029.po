#. <section class="sutta" id="an7.29"><article><div class="hgroup"><p class="division">
msgctxt "an7.29:0.1"
msgid "Aṅguttara Nikāya 7"
msgstr ""

#. </p><p>
msgctxt "an7.29:0.2"
msgid "3. Vajjisattakavagga"
msgstr ""

#. </p><h1>
#. VAR: 29. Dutiyaparihānisutta → upāsakaaparibhāgiyasuttaṃ (bj)
msgctxt "an7.29:0.3"
msgid "29. Dutiyaparihānisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an7.29:1.1"
msgid "“Sattime, bhikkhave, dhammā upāsakassa parihānāya saṃvattanti."
msgstr ""

msgctxt "an7.29:1.2"
msgid "Katame satta?"
msgstr ""

msgctxt "an7.29:1.3"
msgid "Bhikkhudassanaṃ hāpeti, "
msgstr ""

msgctxt "an7.29:1.4"
msgid "saddhammassavanaṃ pamajjati, "
msgstr ""

msgctxt "an7.29:1.5"
msgid "adhisīle na sikkhati, "
msgstr ""

msgctxt "an7.29:1.6"
msgid "appasādabahulo hoti, "
msgstr ""

msgctxt "an7.29:1.7"
msgid ""
"bhikkhūsu theresu ceva navesu ca majjhimesu ca upārambhacitto dhammaṃ suṇāti "
"randhagavesī, "
msgstr ""

msgctxt "an7.29:1.8"
msgid "ito bahiddhā dakkhiṇeyyaṃ gavesati, "
msgstr ""

msgctxt "an7.29:1.9"
msgid "tattha ca pubbakāraṃ karoti."
msgstr ""

msgctxt "an7.29:1.10"
msgid "Ime kho, bhikkhave, satta dhammā upāsakassa parihānāya saṃvattanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an7.29:2.1"
msgid "Sattime, bhikkhave, dhammā upāsakassa aparihānāya saṃvattanti."
msgstr ""

msgctxt "an7.29:2.2"
msgid "Katame satta?"
msgstr ""

msgctxt "an7.29:2.3"
msgid "Bhikkhudassanaṃ na hāpeti, "
msgstr ""

msgctxt "an7.29:2.4"
msgid "saddhammassavanaṃ nappamajjati, "
msgstr ""

msgctxt "an7.29:2.5"
msgid "adhisīle sikkhati, "
msgstr ""

msgctxt "an7.29:2.6"
msgid "pasādabahulo hoti, "
msgstr ""

msgctxt "an7.29:2.7"
msgid ""
"bhikkhūsu theresu ceva navesu ca majjhimesu ca anupārambhacitto dhammaṃ "
"suṇāti na randhagavesī, "
msgstr ""

msgctxt "an7.29:2.8"
msgid "na ito bahiddhā dakkhiṇeyyaṃ gavesati, "
msgstr ""

msgctxt "an7.29:2.9"
msgid "idha ca pubbakāraṃ karoti."
msgstr ""

msgctxt "an7.29:2.10"
msgid "Ime kho, bhikkhave, satta dhammā upāsakassa aparihānāya saṃvattantī”ti."
msgstr ""

msgctxt "an7.29:2.11"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "an7.29:2.12"
msgid "Idaṃ vatvāna sugato athāparaṃ etadavoca satthā:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.26"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "an7.29:3.1"
msgid "“Dassanaṃ bhāvitattānaṃ,"
msgstr ""

#. <br>
msgctxt "an7.29:3.2"
msgid "Yo hāpeti upāsako;"
msgstr ""

#. <br>
msgctxt "an7.29:3.3"
msgid "Savanañca ariyadhammānaṃ,"
msgstr ""

#. <br>
msgctxt "an7.29:3.4"
msgid "Adhisīle na sikkhati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an7.29:4.1"
msgid "Appasādo ca bhikkhūsu,"
msgstr ""

#. <br>
msgctxt "an7.29:4.2"
msgid "bhiyyo bhiyyo pavaḍḍhati;"
msgstr ""

#. <br>
msgctxt "an7.29:4.3"
msgid "Upārambhakacitto ca,"
msgstr ""

#. <br>
msgctxt "an7.29:4.4"
msgid "saddhammaṃ sotumicchati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an7.29:5.1"
msgid "Ito ca bahiddhā aññaṃ,"
msgstr ""

#. <br>
msgctxt "an7.29:5.2"
msgid "dakkhiṇeyyaṃ gavesati;"
msgstr ""

#. <br>
msgctxt "an7.29:5.3"
msgid "Tattheva ca pubbakāraṃ,"
msgstr ""

#. <br>
msgctxt "an7.29:5.4"
msgid "yo karoti upāsako."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an7.29:6.1"
msgid "Ete kho parihāniye,"
msgstr ""

#. <br>
msgctxt "an7.29:6.2"
msgid "satta dhamme sudesite;"
msgstr ""

#. <br>
msgctxt "an7.29:6.3"
msgid "Upāsako sevamāno,"
msgstr ""

#. <br>
msgctxt "an7.29:6.4"
msgid "saddhammā parihāyati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an7.29:7.1"
msgid "Dassanaṃ bhāvitattānaṃ,"
msgstr ""

#. <br>
msgctxt "an7.29:7.2"
msgid "Yo na hāpeti upāsako;"
msgstr ""

#. <br>
msgctxt "an7.29:7.3"
msgid "Savanañca ariyadhammānaṃ,"
msgstr ""

#. <br>
msgctxt "an7.29:7.4"
msgid "Adhisīle ca sikkhati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an7.29:8.1"
msgid "Pasādo cassa bhikkhūsu,"
msgstr ""

#. <br>
msgctxt "an7.29:8.2"
msgid "bhiyyo bhiyyo pavaḍḍhati;"
msgstr ""

#. <br>
msgctxt "an7.29:8.3"
msgid "Anupārambhacitto ca,"
msgstr ""

#. <br>
msgctxt "an7.29:8.4"
msgid "saddhammaṃ sotumicchati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an7.29:9.1"
msgid "Na ito bahiddhā aññaṃ,"
msgstr ""

#. <br>
msgctxt "an7.29:9.2"
msgid "dakkhiṇeyyaṃ gavesati;"
msgstr ""

#. <br>
msgctxt "an7.29:9.3"
msgid "Idheva ca pubbakāraṃ,"
msgstr ""

#. <br>
msgctxt "an7.29:9.4"
msgid "yo karoti upāsako."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "an7.29:10.1"
msgid "Ete kho aparihāniye,"
msgstr ""

#. <br>
msgctxt "an7.29:10.2"
msgid "Satta dhamme sudesite;"
msgstr ""

#. <br>
msgctxt "an7.29:10.3"
msgid "Upāsako sevamāno,"
msgstr ""

#. <br>
msgctxt "an7.29:10.4"
msgid "Saddhammā na parihāyatī”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an7.29:10.5"
msgid "Navamaṃ."
msgstr ""
