#. <section class="sutta" id="an5.130"><article><div class="hgroup"><p class="division">
msgctxt "an5.130:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.130:0.2"
msgid "13. Gilānavagga"
msgstr ""

#. </p><h1>
#. VAR: sampadāsuttaṃ (bj)
msgctxt "an5.130:0.3"
msgid "130. Byasanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.147"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.130:1.1"
msgid "“Pañcimāni, bhikkhave, byasanāni."
msgstr ""

msgctxt "an5.130:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "an5.130:1.3"
msgid ""
"Ñātibyasanaṃ, bhogabyasanaṃ, rogabyasanaṃ, sīlabyasanaṃ, diṭṭhibyasanaṃ."
msgstr ""

msgctxt "an5.130:1.4"
msgid ""
"Na, bhikkhave, sattā ñātibyasanahetu vā bhogabyasanahetu vā rogabyasanahetu "
"vā kāyassa bhedā paraṃ maraṇā apāyaṃ duggatiṃ vinipātaṃ nirayaṃ upapajjanti."
msgstr ""

msgctxt "an5.130:1.5"
msgid ""
"Sīlabyasanahetu vā, bhikkhave, sattā diṭṭhibyasanahetu vā kāyassa bhedā "
"paraṃ maraṇā apāyaṃ duggatiṃ vinipātaṃ nirayaṃ upapajjanti."
msgstr ""

msgctxt "an5.130:1.6"
msgid "Imāni kho, bhikkhave, pañca byasanāni."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.130:2.1"
msgid "Pañcimā, bhikkhave, sampadā."
msgstr ""

msgctxt "an5.130:2.2"
msgid "Katamā pañca?"
msgstr ""

msgctxt "an5.130:2.3"
msgid "Ñātisampadā, bhogasampadā, ārogyasampadā, sīlasampadā, diṭṭhisampadā."
msgstr ""

msgctxt "an5.130:2.4"
msgid ""
"Na, bhikkhave, sattā ñātisampadāhetu vā bhogasampadāhetu vā "
"ārogyasampadāhetu vā kāyassa bhedā paraṃ maraṇā sugatiṃ saggaṃ lokaṃ "
"upapajjanti."
msgstr ""

msgctxt "an5.130:2.5"
msgid ""
"Sīlasampadāhetu vā, bhikkhave, sattā diṭṭhisampadāhetu vā kāyassa bhedā "
"paraṃ maraṇā sugatiṃ saggaṃ lokaṃ upapajjanti."
msgstr ""

msgctxt "an5.130:2.6"
msgid "Imā kho, bhikkhave, pañca sampadā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.130:2.7"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.130:2.8"
msgid "Gilānavaggo tatiyo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.130:3.1"
msgid "Gilāno satisūpaṭṭhi,"
msgstr ""

#. <br>
msgctxt "an5.130:3.2"
msgid "Dve upaṭṭhākā duvāyusā;"
msgstr ""

#. <br>
msgctxt "an5.130:3.3"
msgid "Vapakāsasamaṇasukhā,"
msgstr ""

#. <br>
msgctxt "an5.130:3.4"
msgid "Parikuppaṃ byasanena cāti."
msgstr ""
