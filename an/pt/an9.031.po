#. <section class="sutta" id="an9.31"><article><div class="hgroup"><p class="division">
msgctxt "an9.31:0.1"
msgid "Aṅguttara Nikāya 9"
msgstr ""

#. </p><p>
msgctxt "an9.31:0.2"
msgid "3. Sattāvāsavagga"
msgstr ""

#. </p><h1>
msgctxt "an9.31:0.3"
msgid "31. Anupubbanirodhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an9.31:1.1"
msgid "“Navayime, bhikkhave, anupubbanirodhā."
msgstr ""

msgctxt "an9.31:1.2"
msgid "Katame nava?"
msgstr ""

#. VAR: kāmasaññā → āmisasaññā (s1, s3) | āmissasaññā (s2)
msgctxt "an9.31:1.3"
msgid "Paṭhamaṃ jhānaṃ samāpannassa kāmasaññā niruddhā hoti;"
msgstr ""

msgctxt "an9.31:1.4"
msgid "dutiyaṃ jhānaṃ samāpannassa vitakkavicārā niruddhā honti;"
msgstr ""

msgctxt "an9.31:1.5"
msgid "tatiyaṃ jhānaṃ samāpannassa pīti niruddhā hoti;"
msgstr ""

msgctxt "an9.31:1.6"
msgid "catutthaṃ jhānaṃ samāpannassa assāsapassāsā niruddhā honti;"
msgstr ""

msgctxt "an9.31:1.7"
msgid "ākāsānañcāyatanaṃ samāpannassa rūpasaññā niruddhā hoti;"
msgstr ""

msgctxt "an9.31:1.8"
msgid "viññāṇañcāyatanaṃ samāpannassa ākāsānañcāyatanasaññā niruddhā hoti;"
msgstr ""

msgctxt "an9.31:1.9"
msgid "ākiñcaññāyatanaṃ samāpannassa viññāṇañcāyatanasaññā niruddhā hoti;"
msgstr ""

msgctxt "an9.31:1.10"
msgid ""
"nevasaññānāsaññāyatanaṃ samāpannassa ākiñcaññāyatanasaññā niruddhā hoti;"
msgstr ""

msgctxt "an9.31:1.11"
msgid "saññāvedayitanirodhaṃ samāpannassa saññā ca vedanā ca niruddhā honti."
msgstr ""

msgctxt "an9.31:1.12"
msgid "Ime kho, bhikkhave, nava anupubbanirodhā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an9.31:1.13"
msgid "Ekādasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an9.31:1.14"
msgid "Sattāvāsavaggo tatiyo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an9.31:2.1"
msgid "Tiṭhānaṃ khaḷuṅko taṇhā,"
msgstr ""

#. <br>
msgctxt "an9.31:2.2"
msgid "Sattapaññā silāyupo;"
msgstr ""

#. <br>
msgctxt "an9.31:2.3"
msgid "Dve verā dve āghātāni,"
msgstr ""

#. <br>
msgctxt "an9.31:2.4"
msgid "Anupubbanirodhena cāti."
msgstr ""
