#. <section class="sutta" id="an5.245"><article><div class="hgroup"><p class="division">
msgctxt "an5.245:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.245:0.2"
msgid "25. Duccaritavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.245:0.3"
msgid "245. Dutiyaduccaritasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.245:1.1"
msgid "“Pañcime, bhikkhave, ādīnavā duccarite."
msgstr ""

msgctxt "an5.245:1.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.245:1.3"
msgid "Attāpi attānaṃ upavadati;"
msgstr ""

msgctxt "an5.245:1.4"
msgid "anuvicca viññū garahanti;"
msgstr ""

msgctxt "an5.245:1.5"
msgid "pāpako kittisaddo abbhuggacchati;"
msgstr ""

msgctxt "an5.245:1.6"
msgid "saddhammā vuṭṭhāti;"
msgstr ""

msgctxt "an5.245:1.7"
msgid "asaddhamme patiṭṭhāti."
msgstr ""

msgctxt "an5.245:1.8"
msgid "Ime kho, bhikkhave, pañca ādīnavā duccarite."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.245:2.1"
msgid "Pañcime, bhikkhave, ānisaṃsā sucarite."
msgstr ""

msgctxt "an5.245:2.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.245:2.3"
msgid "Attāpi attānaṃ na upavadati;"
msgstr ""

msgctxt "an5.245:2.4"
msgid "anuvicca viññū pasaṃsanti;"
msgstr ""

msgctxt "an5.245:2.5"
msgid "kalyāṇo kittisaddo abbhuggacchati;"
msgstr ""

msgctxt "an5.245:2.6"
msgid "asaddhammā vuṭṭhāti;"
msgstr ""

msgctxt "an5.245:2.7"
msgid "saddhamme patiṭṭhāti."
msgstr ""

msgctxt "an5.245:2.8"
msgid "Ime kho, bhikkhave, pañca ānisaṃsā sucarite”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.245:2.9"
msgid "Pañcamaṃ."
msgstr ""
