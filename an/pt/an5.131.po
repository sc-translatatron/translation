#. <section class="sutta" id="an5.131"><article><div class="hgroup"><p class="division">
msgctxt "an5.131:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.131:0.2"
msgid "14. Rājavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.131:0.3"
msgid "131. Paṭhamacakkānuvattanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.148"></a>
#. <a class="sc" id="sc1"></a>
#. VAR: vatteti → pavatteti (bj, s1-3, pts1, mr)
msgctxt "an5.131:1.1"
msgid ""
"“Pañcahi, bhikkhave, aṅgehi samannāgato rājā cakkavattī dhammeneva cakkaṃ "
"vatteti;"
msgstr ""

#. VAR: appaṭivattiyaṃ → appativattiyaṃ (bj)
msgctxt "an5.131:1.2"
msgid ""
"taṃ hoti cakkaṃ appaṭivattiyaṃ kenaci manussabhūtena paccatthikena pāṇinā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.131:2.1"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.131:2.2"
msgid ""
"Idha, bhikkhave, rājā cakkavattī atthaññū ca hoti, dhammaññū ca, mattaññū "
"ca, kālaññū ca, parisaññū ca."
msgstr ""

msgctxt "an5.131:2.3"
msgid ""
"Imehi kho, bhikkhave, pañcahi aṅgehi samannāgato rājā cakkavattī dhammeneva "
"cakkaṃ pavatteti;"
msgstr ""

msgctxt "an5.131:2.4"
msgid ""
"taṃ hoti cakkaṃ appaṭivattiyaṃ kenaci manussabhūtena paccatthikena pāṇinā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.131:3.1"
msgid ""
"Evamevaṃ kho, bhikkhave, pañcahi dhammehi samannāgato tathāgato arahaṃ "
"sammāsambuddho dhammeneva anuttaraṃ dhammacakkaṃ pavatteti;"
msgstr ""

msgctxt "an5.131:3.2"
msgid ""
"taṃ hoti cakkaṃ appaṭivattiyaṃ samaṇena vā brāhmaṇena vā devena vā mārena vā "
"brahmunā vā kenaci vā lokasmiṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an5.131:4.1"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "an5.131:4.2"
msgid ""
"Idha, bhikkhave, tathāgato arahaṃ sammāsambuddho atthaññū, dhammaññū, "
"mattaññū, kālaññū, parisaññū."
msgstr ""

msgctxt "an5.131:4.3"
msgid ""
"Imehi kho, bhikkhave, pañcahi dhammehi samannāgato tathāgato arahaṃ "
"sammāsambuddho dhammeneva anuttaraṃ dhammacakkaṃ pavatteti;"
msgstr ""

msgctxt "an5.131:4.4"
msgid ""
"taṃ hoti dhammacakkaṃ appaṭivattiyaṃ samaṇena vā brāhmaṇena vā devena vā "
"mārena vā brahmunā vā kenaci vā lokasmin”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.131:4.5"
msgid "Paṭhamaṃ."
msgstr ""
