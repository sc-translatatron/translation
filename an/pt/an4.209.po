#. <section class="sutta" id="an4.209"><article><div class="hgroup"><p class="division">
msgctxt "an4.209:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.209:0.2"
msgid "21. Sappurisavagga"
msgstr ""

#. </p><h1>
#. VAR: paṭhamapāpadhamma-kalyāṇadhammasuttaṃ (bj)
msgctxt "an4.209:0.3"
msgid "209. Tatiyapāpadhammasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.224"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.209:1.1"
msgid ""
"“Pāpadhammañca vo, bhikkhave, desessāmi, pāpadhammena pāpadhammatarañca;"
msgstr ""

msgctxt "an4.209:1.2"
msgid ""
"kalyāṇadhammañca, kalyāṇadhammena kalyāṇadhammatarañca. Taṃ suṇātha … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.209:2.1"
msgid "“Katamo ca, bhikkhave, pāpadhammo?"
msgstr ""

msgctxt "an4.209:2.2"
msgid "Idha, bhikkhave, ekacco pāṇātipātī hoti … pe … micchādiṭṭhiko hoti."
msgstr ""

msgctxt "an4.209:2.3"
msgid "Ayaṃ vuccati, bhikkhave, pāpadhammo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.209:3.1"
msgid "Katamo ca, bhikkhave, pāpadhammena pāpadhammataro?"
msgstr ""

msgctxt "an4.209:3.2"
msgid ""
"Idha bhikkhave, ekacco attanā ca pāṇātipātī hoti, parañca pāṇātipāte "
"samādapeti … pe … attanā ca micchādiṭṭhiko hoti, parañca micchādiṭṭhiyā "
"samādapeti."
msgstr ""

msgctxt "an4.209:3.3"
msgid "Ayaṃ vuccati, bhikkhave, pāpadhammena pāpadhammataro."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.209:4.1"
msgid "Katamo ca, bhikkhave, kalyāṇadhammo?"
msgstr ""

msgctxt "an4.209:4.2"
msgid ""
"Idha, bhikkhave, ekacco pāṇātipātā paṭivirato hoti … pe … sammādiṭṭhiko hoti."
msgstr ""

msgctxt "an4.209:4.3"
msgid "Ayaṃ vuccati, bhikkhave, kalyāṇadhammo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.209:5.1"
msgid "Katamo ca, bhikkhave, kalyāṇadhammena kalyāṇadhammataro?"
msgstr ""

msgctxt "an4.209:5.2"
msgid ""
"Idha, bhikkhave, ekacco attanā ca pāṇātipātā paṭivirato hoti, parañca "
"pāṇātipātā veramaṇiyā samādapeti … pe … attanā ca sammādiṭṭhiko hoti, "
"parañca sammādiṭṭhiyā samādapeti."
msgstr ""

msgctxt "an4.209:5.3"
msgid "Ayaṃ vuccati, bhikkhave, kalyāṇadhammena kalyāṇadhammataro”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.209:5.4"
msgid "Navamaṃ."
msgstr ""
