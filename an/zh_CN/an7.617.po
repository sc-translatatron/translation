#. <section class="sutta" id="an7.617"><article><div class="hgroup"><p class="division">
msgctxt "an7.617:0.1"
msgid "Aṅguttara Nikāya 7"
msgstr ""

#. </p><p>
msgctxt "an7.617:0.2"
msgid "11. Rāgapeyyāla"
msgstr ""

#. </p><h1>
msgctxt "an7.617:0.3"
msgid "617"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an7.617:1.1"
msgid "“Rāgassa, bhikkhave, abhiññāya satta dhammā bhāvetabbā."
msgstr ""

msgctxt "an7.617:1.2"
msgid "Katame satta?"
msgstr ""

msgctxt "an7.617:1.3"
msgid ""
"Asubhasaññā, maraṇasaññā, āhāre paṭikūlasaññā, sabbaloke anabhiratasaññā, "
"aniccasaññā, anicce dukkhasaññā, dukkhe anattasaññā—"
msgstr ""

msgctxt "an7.617:1.4"
msgid "rāgassa, bhikkhave, abhiññāya ime satta dhammā bhāvetabbā”ti."
msgstr ""
