#. <section class="sutta" id="an7.15"><article><div class="hgroup"><p class="division">
msgctxt "an7.15:0.1"
msgid "Aṅguttara Nikāya 7"
msgstr ""

#. </p><p>
msgctxt "an7.15:0.2"
msgid "2. Anusayavagga"
msgstr ""

#. </p><h1>
#. VAR: 15. Udakūpamāsutta → udakūpamapuggalasuttaṃ (bj)
msgctxt "an7.15:0.3"
msgid "15. Udakūpamāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an7.15:1.1"
msgid "“Sattime, bhikkhave, udakūpamā puggalā santo saṃvijjamānā lokasmiṃ."
msgstr ""

msgctxt "an7.15:1.2"
msgid "Katame satta?"
msgstr ""

msgctxt "an7.15:1.3"
msgid "Idha, bhikkhave, ekacco puggalo sakiṃ nimuggo nimuggova hoti;"
msgstr ""

msgctxt "an7.15:1.4"
msgid "idha pana, bhikkhave, ekacco puggalo ummujjitvā nimujjati;"
msgstr ""

msgctxt "an7.15:1.5"
msgid "idha pana, bhikkhave, ekacco puggalo ummujjitvā ṭhito hoti;"
msgstr ""

msgctxt "an7.15:1.6"
msgid "idha pana, bhikkhave, ekacco puggalo ummujjitvā vipassati viloketi;"
msgstr ""

msgctxt "an7.15:1.7"
msgid "idha pana, bhikkhave, ekacco puggalo ummujjitvā patarati;"
msgstr ""

msgctxt "an7.15:1.8"
msgid "idha pana, bhikkhave, ekacco puggalo ummujjitvā patigādhappatto hoti;"
msgstr ""

#. VAR: pāraṅgato → pāragato (bj, s1-3, km, pts1)
msgctxt "an7.15:1.9"
msgid ""
"idha pana, bhikkhave, ekacco puggalo ummujjitvā tiṇṇo hoti pāraṅgato thale "
"tiṭṭhati brāhmaṇo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an7.15:2.1"
msgid "Kathañca, bhikkhave, puggalo sakiṃ nimuggo nimuggova hoti?"
msgstr ""

msgctxt "an7.15:2.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo samannāgato hoti ekantakāḷakehi akusalehi "
"dhammehi."
msgstr ""

msgctxt "an7.15:2.3"
msgid "Evaṃ kho, bhikkhave, puggalo sakiṃ nimuggo nimuggova hoti. (1)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an7.15:3.1"
msgid "Kathañca, bhikkhave, puggalo ummujjitvā nimujjati?"
msgstr ""

msgctxt "an7.15:3.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo ummujjati sādhu saddhā kusalesu dhammesu, "
"sādhu hirī … pe …"
msgstr ""

msgctxt "an7.15:3.3"
msgid "sādhu ottappaṃ …"
msgstr ""

#. VAR: vīriyaṃ → viriyaṃ (bj, s1-3, km, pts1)
msgctxt "an7.15:3.4"
msgid "sādhu vīriyaṃ …"
msgstr ""

msgctxt "an7.15:3.5"
msgid "sādhu paññā kusalesu dhammesūti."
msgstr ""

msgctxt "an7.15:3.6"
msgid ""
"Tassa sā saddhā neva tiṭṭhati no vaḍḍhati hāyatiyeva, tassa sā hirī … pe …"
msgstr ""

msgctxt "an7.15:3.7"
msgid "tassa taṃ ottappaṃ …"
msgstr ""

msgctxt "an7.15:3.8"
msgid "tassa taṃ vīriyaṃ …"
msgstr ""

msgctxt "an7.15:3.9"
msgid "tassa sā paññā neva tiṭṭhati no vaḍḍhati hāyatiyeva."
msgstr ""

msgctxt "an7.15:3.10"
msgid "Evaṃ kho, bhikkhave, puggalo ummujjitvā nimujjati. (2)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.12"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "an7.15:4.1"
msgid "Kathañca, bhikkhave, puggalo ummujjitvā ṭhito hoti?"
msgstr ""

msgctxt "an7.15:4.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo ummujjati sādhu saddhā kusalesu dhammesu, "
"sādhu hirī … pe …"
msgstr ""

msgctxt "an7.15:4.3"
msgid "sādhu ottappaṃ …"
msgstr ""

msgctxt "an7.15:4.4"
msgid "sādhu vīriyaṃ …"
msgstr ""

msgctxt "an7.15:4.5"
msgid "sādhu paññā kusalesu dhammesūti."
msgstr ""

msgctxt "an7.15:4.6"
msgid "Tassa sā saddhā neva hāyati no vaḍḍhati ṭhitā hoti."
msgstr ""

msgctxt "an7.15:4.7"
msgid "Tassa sā hirī … pe …"
msgstr ""

msgctxt "an7.15:4.8"
msgid "tassa taṃ ottappaṃ …"
msgstr ""

msgctxt "an7.15:4.9"
msgid "tassa taṃ vīriyaṃ …"
msgstr ""

msgctxt "an7.15:4.10"
msgid "tassa sā paññā neva hāyati no vaḍḍhati ṭhitā hoti."
msgstr ""

msgctxt "an7.15:4.11"
msgid "Evaṃ kho, bhikkhave, puggalo ummujjitvā ṭhito hoti. (3)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an7.15:5.1"
msgid "Kathañca, bhikkhave, puggalo ummujjitvā vipassati viloketi?"
msgstr ""

msgctxt "an7.15:5.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo ummujjati sādhu saddhā kusalesu dhammesu, "
"sādhu hirī … pe …"
msgstr ""

msgctxt "an7.15:5.3"
msgid "sādhu ottappaṃ …"
msgstr ""

msgctxt "an7.15:5.4"
msgid "sādhu vīriyaṃ …"
msgstr ""

msgctxt "an7.15:5.5"
msgid "sādhu paññā kusalesu dhammesūti."
msgstr ""

msgctxt "an7.15:5.6"
msgid ""
"So tiṇṇaṃ saṃyojanānaṃ parikkhayā sotāpanno hoti avinipātadhammo niyato "
"sambodhiparāyaṇo."
msgstr ""

msgctxt "an7.15:5.7"
msgid "Evaṃ kho, bhikkhave, puggalo ummujjitvā vipassati viloketi. (4)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an7.15:6.1"
msgid "Kathañca, bhikkhave, puggalo ummujjitvā patarati?"
msgstr ""

msgctxt "an7.15:6.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo ummujjati sādhu saddhā kusalesu dhammesu, "
"sādhu hirī … pe …"
msgstr ""

msgctxt "an7.15:6.3"
msgid "sādhu ottappaṃ …"
msgstr ""

msgctxt "an7.15:6.4"
msgid "sādhu vīriyaṃ …"
msgstr ""

msgctxt "an7.15:6.5"
msgid "sādhu paññā kusalesu dhammesūti."
msgstr ""

#. VAR: sakideva → sakiṃdeva (mr)
#. VAR: dukkhassantaṃ karoti → dukkhassantakaro hoti (mr)
msgctxt "an7.15:6.6"
msgid ""
"So tiṇṇaṃ saṃyojanānaṃ parikkhayā rāgadosamohānaṃ tanuttā sakadāgāmī hoti, "
"sakideva imaṃ lokaṃ āgantvā dukkhassantaṃ karoti."
msgstr ""

msgctxt "an7.15:6.7"
msgid "Evaṃ kho, bhikkhave, puggalo ummujjitvā patarati. (5)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an7.15:7.1"
msgid "Kathañca, bhikkhave, puggalo ummujjitvā patigādhappatto hoti?"
msgstr ""

msgctxt "an7.15:7.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo ummujjati sādhu saddhā kusalesu dhammesu, "
"sādhu hirī … pe …"
msgstr ""

msgctxt "an7.15:7.3"
msgid "sādhu ottappaṃ …"
msgstr ""

msgctxt "an7.15:7.4"
msgid "sādhu vīriyaṃ …"
msgstr ""

msgctxt "an7.15:7.5"
msgid "sādhu paññā kusalesu dhammesūti."
msgstr ""

msgctxt "an7.15:7.6"
msgid ""
"So pañcannaṃ orambhāgiyānaṃ saṃyojanānaṃ parikkhayā opapātiko hoti tattha "
"parinibbāyī anāvattidhammo tasmā lokā."
msgstr ""

msgctxt "an7.15:7.7"
msgid "Evaṃ kho, bhikkhave, puggalo ummujjitvā patigādhappatto hoti. (6)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.13"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "an7.15:8.1"
msgid ""
"Kathañca, bhikkhave, puggalo ummujjitvā tiṇṇo hoti pāraṅgato thale tiṭṭhati "
"brāhmaṇo."
msgstr ""

msgctxt "an7.15:8.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo ummujjati sādhu saddhā kusalesu dhammesu, "
"sādhu hirī … pe …"
msgstr ""

msgctxt "an7.15:8.3"
msgid "sādhu ottappaṃ …"
msgstr ""

msgctxt "an7.15:8.4"
msgid "sādhu vīriyaṃ …"
msgstr ""

msgctxt "an7.15:8.5"
msgid "sādhu paññā kusalesu dhammesūti."
msgstr ""

msgctxt "an7.15:8.6"
msgid ""
"So āsavānaṃ khayā anāsavaṃ cetovimuttiṃ paññāvimuttiṃ diṭṭheva dhamme sayaṃ "
"abhiññā sacchikatvā upasampajja viharati."
msgstr ""

msgctxt "an7.15:8.7"
msgid ""
"Evaṃ kho, bhikkhave, puggalo ummujjitvā tiṇṇo hoti pāraṅgato thale tiṭṭhati "
"brāhmaṇo. (7)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an7.15:9.1"
msgid ""
"Ime kho, bhikkhave, satta udakūpamā puggalā santo saṃvijjamānā lokasmin”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an7.15:9.2"
msgid "Pañcamaṃ."
msgstr ""
