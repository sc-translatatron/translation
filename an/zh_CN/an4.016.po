#. <section class="sutta" id="an4.16"><article><div class="hgroup"><p class="division">
msgctxt "an4.16:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.16:0.2"
msgid "2. Caravagga"
msgstr ""

#. </p><h1>
msgctxt "an4.16:0.3"
msgid "16. Sokhummasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.18"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.16:1.1"
msgid "“Cattārimāni, bhikkhave, sokhummāni."
msgstr ""

msgctxt "an4.16:1.2"
msgid "Katamāni cattāri?"
msgstr ""

msgctxt "an4.16:1.3"
msgid "Idha, bhikkhave, bhikkhu rūpasokhummena samannāgato hoti paramena;"
msgstr ""

msgctxt "an4.16:1.4"
msgid ""
"tena ca rūpasokhummena aññaṃ rūpasokhummaṃ uttaritaraṃ vā paṇītataraṃ vā na "
"samanupassati;"
msgstr ""

msgctxt "an4.16:1.5"
msgid ""
"tena ca rūpasokhummena aññaṃ rūpasokhummaṃ uttaritaraṃ vā paṇītataraṃ vā na "
"pattheti."
msgstr ""

msgctxt "an4.16:1.6"
msgid "Vedanāsokhummena samannāgato hoti paramena;"
msgstr ""

msgctxt "an4.16:1.7"
msgid ""
"tena ca vedanāsokhummena aññaṃ vedanāsokhummaṃ uttaritaraṃ vā paṇītataraṃ vā "
"na samanupassati;"
msgstr ""

msgctxt "an4.16:1.8"
msgid ""
"tena ca vedanāsokhummena aññaṃ vedanāsokhummaṃ uttaritaraṃ vā paṇītataraṃ vā "
"na pattheti."
msgstr ""

msgctxt "an4.16:1.9"
msgid "Saññāsokhummena samannāgato hoti paramena;"
msgstr ""

msgctxt "an4.16:1.10"
msgid ""
"tena ca saññāsokhummena aññaṃ saññāsokhummaṃ uttaritaraṃ vā paṇītataraṃ vā "
"na samanupassati;"
msgstr ""

msgctxt "an4.16:1.11"
msgid ""
"tena ca saññāsokhummena aññaṃ saññāsokhummaṃ uttaritaraṃ vā paṇītataraṃ vā "
"na pattheti."
msgstr ""

msgctxt "an4.16:1.12"
msgid "Saṅkhārasokhummena samannāgato hoti paramena;"
msgstr ""

msgctxt "an4.16:1.13"
msgid ""
"tena ca saṅkhārasokhummena aññaṃ saṅkhārasokhummaṃ uttaritaraṃ vā "
"paṇītataraṃ vā na samanupassati;"
msgstr ""

msgctxt "an4.16:1.14"
msgid ""
"tena ca saṅkhārasokhummena aññaṃ saṅkhārasokhummaṃ uttaritaraṃ vā "
"paṇītataraṃ vā na pattheti."
msgstr ""

msgctxt "an4.16:1.15"
msgid "Imāni kho, bhikkhave, cattāri sokhummānīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.16:2.1"
msgid "Rūpasokhummataṃ ñatvā,"
msgstr ""

#. <br>
msgctxt "an4.16:2.2"
msgid "vedanānañca sambhavaṃ;"
msgstr ""

#. <br>
msgctxt "an4.16:2.3"
msgid "Saññā yato samudeti,"
msgstr ""

#. <br>
msgctxt "an4.16:2.4"
msgid "atthaṃ gacchati yattha ca;"
msgstr ""

#. <br>
msgctxt "an4.16:2.5"
msgid "Saṅkhāre parato ñatvā,"
msgstr ""

#. <br>
msgctxt "an4.16:2.6"
msgid "dukkhato no ca attato."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.16:3.1"
msgid "Sa ve sammaddaso bhikkhu,"
msgstr ""

#. <br>
msgctxt "an4.16:3.2"
msgid "santo santipade rato;"
msgstr ""

#. <br>
msgctxt "an4.16:3.3"
msgid "Dhāreti antimaṃ dehaṃ,"
msgstr ""

#. <br>
msgctxt "an4.16:3.4"
msgid "jetvā māraṃ savāhinin”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an4.16:3.5"
msgid "Chaṭṭhaṃ."
msgstr ""
