msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.91:0.2"
msgid "10. Kakudhavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.91:0.3"
msgid "91. Paṭhamasampadāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.91:1.1"
msgid "“Pañcimā, bhikkhave, sampadā."
msgstr ""

msgctxt "an5.91:1.2"
msgid "Katamā pañca?"
msgstr ""

msgctxt "an5.91:1.3"
msgid "Saddhāsampadā, sīlasampadā, sutasampadā, cāgasampadā, paññāsampadā—"
msgstr ""

msgctxt "an5.91:1.4"
msgid "imā kho, bhikkhave, pañca sampadā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.91:1.5"
msgid "Paṭhamaṃ."
msgstr ""
