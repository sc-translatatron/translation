#. <section id="vagga"><section class="sutta" data-uid="an2.141" id="141"><article><div class="hgroup"><p class="division">
msgctxt "an2.141-150:0.1"
msgid "Aṅguttara Nikāya 2"
msgstr ""

#. </p><p>
msgctxt "an2.141-150:0.2"
msgid "13. Dānavagga"
msgstr ""

#. </p><h1>
msgctxt "an2.141-150:0.3"
msgid "141"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc141.1"></a>
msgctxt "an2.141-150:141.1.1"
msgid "“Dvemāni, bhikkhave, dānāni."
msgstr ""

msgctxt "an2.141-150:141.1.2"
msgid "Katamāni dve?"
msgstr ""

msgctxt "an2.141-150:141.1.3"
msgid "Āmisadānañca dhammadānañca."
msgstr ""

msgctxt "an2.141-150:141.1.4"
msgid "Imāni kho, bhikkhave, dve dānāni."
msgstr ""

msgctxt "an2.141-150:141.1.5"
msgid "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ dānānaṃ yadidaṃ dhammadānan”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.142" id="142"><article><div class="hgroup"><h1>
msgctxt "an2.141-150:141.1.6"
msgid "142"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc142.1"></a>
msgctxt "an2.141-150:142.1.1"
msgid "“Dveme, bhikkhave, yāgā."
msgstr ""

msgctxt "an2.141-150:142.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.141-150:142.1.3"
msgid "Āmisayāgo ca dhammayāgo ca."
msgstr ""

msgctxt "an2.141-150:142.1.4"
msgid "Ime kho, bhikkhave, dve yāgā."
msgstr ""

msgctxt "an2.141-150:142.1.5"
msgid "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ yāgānaṃ yadidaṃ dhammayāgo”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.143" id="143"><article><div class="hgroup"><h1>
msgctxt "an2.141-150:142.1.6"
msgid "143"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.92"></a>
#. <a class="sc" id="sc143.1"></a>
msgctxt "an2.141-150:143.1.1"
msgid "“Dveme, bhikkhave, cāgā."
msgstr ""

msgctxt "an2.141-150:143.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.141-150:143.1.3"
msgid "Āmisacāgo ca dhammacāgo ca."
msgstr ""

msgctxt "an2.141-150:143.1.4"
msgid "Ime kho, bhikkhave, dve cāgā."
msgstr ""

msgctxt "an2.141-150:143.1.5"
msgid "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ cāgānaṃ yadidaṃ dhammacāgo”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.144" id="144"><article><div class="hgroup"><h1>
msgctxt "an2.141-150:143.1.6"
msgid "144"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc144.1"></a>
msgctxt "an2.141-150:144.1.1"
msgid "“Dveme, bhikkhave, pariccāgā."
msgstr ""

msgctxt "an2.141-150:144.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.141-150:144.1.3"
msgid "Āmisapariccāgo ca dhammapariccāgo ca."
msgstr ""

msgctxt "an2.141-150:144.1.4"
msgid "Ime kho, bhikkhave, dve pariccāgā."
msgstr ""

msgctxt "an2.141-150:144.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ pariccāgānaṃ yadidaṃ dhammapariccāgo”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.145" id="145"><article><div class="hgroup"><h1>
msgctxt "an2.141-150:144.1.6"
msgid "145"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc145.1"></a>
msgctxt "an2.141-150:145.1.1"
msgid "“Dveme, bhikkhave, bhogā."
msgstr ""

msgctxt "an2.141-150:145.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.141-150:145.1.3"
msgid "Āmisabhogo ca dhammabhogo ca."
msgstr ""

msgctxt "an2.141-150:145.1.4"
msgid "Ime kho, bhikkhave, dve bhogā."
msgstr ""

msgctxt "an2.141-150:145.1.5"
msgid "Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ bhogānaṃ yadidaṃ dhammabhogo”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.146" id="146"><article><div class="hgroup"><h1>
msgctxt "an2.141-150:145.1.6"
msgid "146"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc146.1"></a>
msgctxt "an2.141-150:146.1.1"
msgid "“Dveme, bhikkhave, sambhogā."
msgstr ""

msgctxt "an2.141-150:146.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.141-150:146.1.3"
msgid "Āmisasambhogo ca dhammasambhogo ca."
msgstr ""

msgctxt "an2.141-150:146.1.4"
msgid "Ime kho, bhikkhave, dve sambhogā."
msgstr ""

msgctxt "an2.141-150:146.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ sambhogānaṃ yadidaṃ dhammasambhogo”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.147" id="147"><article><div class="hgroup"><h1>
msgctxt "an2.141-150:146.1.6"
msgid "147"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc147.1"></a>
msgctxt "an2.141-150:147.1.1"
msgid "“Dveme, bhikkhave, saṃvibhāgā."
msgstr ""

msgctxt "an2.141-150:147.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.141-150:147.1.3"
msgid "Āmisasaṃvibhāgo ca dhammasaṃvibhāgo ca."
msgstr ""

msgctxt "an2.141-150:147.1.4"
msgid "Ime kho, bhikkhave, dve saṃvibhāgā."
msgstr ""

msgctxt "an2.141-150:147.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ saṃvibhāgānaṃ yadidaṃ "
"dhammasaṃvibhāgo”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.148" id="148"><article><div class="hgroup"><h1>
msgctxt "an2.141-150:147.1.6"
msgid "148"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc148.1"></a>
msgctxt "an2.141-150:148.1.1"
msgid "“Dveme, bhikkhave, saṅgahā."
msgstr ""

msgctxt "an2.141-150:148.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.141-150:148.1.3"
msgid "Āmisasaṅgaho ca dhammasaṅgaho ca."
msgstr ""

msgctxt "an2.141-150:148.1.4"
msgid "Ime kho, bhikkhave, dve saṅgahā."
msgstr ""

msgctxt "an2.141-150:148.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ saṅgahānaṃ yadidaṃ dhammasaṅgaho”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.149" id="149"><article><div class="hgroup"><h1>
msgctxt "an2.141-150:148.1.6"
msgid "149"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc149.1"></a>
msgctxt "an2.141-150:149.1.1"
msgid "“Dveme, bhikkhave, anuggahā."
msgstr ""

msgctxt "an2.141-150:149.1.2"
msgid "Katame dve?"
msgstr ""

msgctxt "an2.141-150:149.1.3"
msgid "Āmisānuggaho ca dhammānuggaho ca."
msgstr ""

msgctxt "an2.141-150:149.1.4"
msgid "Ime kho, bhikkhave, dve anuggahā."
msgstr ""

msgctxt "an2.141-150:149.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imesaṃ dvinnaṃ anuggahānaṃ yadidaṃ dhammānuggaho”ti."
msgstr ""

#. </p></article></section><section class="sutta" data-uid="an2.150" id="150"><article><div class="hgroup"><h1>
msgctxt "an2.141-150:149.1.6"
msgid "150"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc150.1"></a>
msgctxt "an2.141-150:150.1.1"
msgid "“Dvemā, bhikkhave, anukampā."
msgstr ""

msgctxt "an2.141-150:150.1.2"
msgid "Katamā dve?"
msgstr ""

msgctxt "an2.141-150:150.1.3"
msgid "Āmisānukampā ca dhammānukampā ca."
msgstr ""

msgctxt "an2.141-150:150.1.4"
msgid "Imā kho, bhikkhave, dve anukampā."
msgstr ""

msgctxt "an2.141-150:150.1.5"
msgid ""
"Etadaggaṃ, bhikkhave, imāsaṃ dvinnaṃ anukampānaṃ yadidaṃ dhammānukampā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an2.141-150:150.1.6"
msgid "Dānavaggo tatiyo."
msgstr ""
