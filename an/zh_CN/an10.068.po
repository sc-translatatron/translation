#. <section class="sutta" id="an10.68"><article><div class="hgroup"><p class="division">
msgctxt "an10.68:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.68:0.2"
msgid "7. Yamakavagga"
msgstr ""

#. </p><h1>
msgctxt "an10.68:0.3"
msgid "68. Dutiyanaḷakapānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an10.68:1.1"
msgid "Ekaṃ samayaṃ bhagavā naḷakapāne viharati palāsavane."
msgstr ""

msgctxt "an10.68:1.2"
msgid ""
"Tena kho pana samayena bhagavā tadahuposathe bhikkhusaṅghaparivuto nisinno "
"hoti."
msgstr ""

msgctxt "an10.68:1.3"
msgid ""
"Atha kho bhagavā bahudeva rattiṃ bhikkhūnaṃ dhammiyā kathāya sandassetvā "
"samādapetvā samuttejetvā sampahaṃsetvā tuṇhībhūtaṃ tuṇhībhūtaṃ "
"bhikkhusaṅghaṃ anuviloketvā āyasmantaṃ sāriputtaṃ āmantesi:"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.126"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.68:2.1"
msgid "“Vigatathinamiddho kho, sāriputta, bhikkhusaṅgho."
msgstr ""

msgctxt "an10.68:2.2"
msgid "Paṭibhātu taṃ, sāriputta, bhikkhūnaṃ dhammī kathā."
msgstr ""

msgctxt "an10.68:2.3"
msgid "Piṭṭhi me āgilāyati;"
msgstr ""

msgctxt "an10.68:2.4"
msgid "tamahaṃ āyamissāmī”ti."
msgstr ""

msgctxt "an10.68:2.5"
msgid "“Evaṃ, bhante”ti kho āyasmā sāriputto bhagavato paccassosi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an10.68:3.1"
msgid ""
"Atha kho bhagavā catugguṇaṃ saṅghāṭiṃ paññāpetvā dakkhiṇena passena "
"sīhaseyyaṃ kappesi pāde pādaṃ accādhāya sato sampajāno uṭṭhānasaññaṃ manasi "
"karitvā."
msgstr ""

msgctxt "an10.68:3.2"
msgid "Tatra kho āyasmā sāriputto bhikkhū āmantesi:"
msgstr ""

msgctxt "an10.68:3.3"
msgid "“āvuso bhikkhave”ti."
msgstr ""

msgctxt "an10.68:3.4"
msgid "“Āvuso”ti kho te bhikkhū āyasmato sāriputtassa paccassosuṃ."
msgstr ""

msgctxt "an10.68:3.5"
msgid "Āyasmā sāriputto etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an10.68:4.1"
msgid "“Yassa kassaci, āvuso, saddhā natthi kusalesu dhammesu, hirī natthi …"
msgstr ""

msgctxt "an10.68:4.2"
msgid "ottappaṃ natthi …"
msgstr ""

msgctxt "an10.68:4.3"
msgid "vīriyaṃ natthi …"
msgstr ""

msgctxt "an10.68:4.4"
msgid "paññā natthi …"
msgstr ""

msgctxt "an10.68:4.5"
msgid "sotāvadhānaṃ natthi …"
msgstr ""

msgctxt "an10.68:4.6"
msgid "dhammadhāraṇā natthi …"
msgstr ""

msgctxt "an10.68:4.7"
msgid "atthūpaparikkhā natthi …"
msgstr ""

msgctxt "an10.68:4.8"
msgid "dhammānudhammappaṭipatti natthi …"
msgstr ""

msgctxt "an10.68:4.9"
msgid ""
"appamādo natthi kusalesu dhammesu, tassa yā ratti vā divaso vā āgacchati, "
"hāniyeva pāṭikaṅkhā kusalesu dhammesu no vuddhi."
msgstr ""

msgctxt "an10.68:4.10"
msgid ""
"Seyyathāpi, āvuso, kāḷapakkhe candassa yā ratti vā divaso vā āgacchati, "
"hāyateva vaṇṇena hāyati maṇḍalena hāyati ābhāya hāyati ārohapariṇāhena;"
msgstr ""

msgctxt "an10.68:4.11"
msgid ""
"evamevaṃ kho, āvuso, yassa kassaci saddhā natthi kusalesu dhammesu, hirī "
"natthi …"
msgstr ""

msgctxt "an10.68:4.12"
msgid "ottappaṃ natthi …"
msgstr ""

msgctxt "an10.68:4.13"
msgid "vīriyaṃ natthi …"
msgstr ""

msgctxt "an10.68:4.14"
msgid "paññā natthi …"
msgstr ""

msgctxt "an10.68:4.15"
msgid "sotāvadhānaṃ natthi …"
msgstr ""

msgctxt "an10.68:4.16"
msgid "dhammadhāraṇā natthi …"
msgstr ""

msgctxt "an10.68:4.17"
msgid "atthūpaparikkhā natthi …"
msgstr ""

msgctxt "an10.68:4.18"
msgid "dhammānudhammappaṭipatti natthi …"
msgstr ""

msgctxt "an10.68:4.19"
msgid ""
"appamādo natthi kusalesu dhammesu, tassa yā ratti vā divaso vā āgacchati, "
"hāniyeva pāṭikaṅkhā kusalesu dhammesu no vuddhi."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.127"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "an10.68:5.1"
msgid "Yassa kassaci, āvuso, saddhā atthi kusalesu dhammesu, hirī atthi …"
msgstr ""

msgctxt "an10.68:5.2"
msgid "ottappaṃ atthi …"
msgstr ""

msgctxt "an10.68:5.3"
msgid "vīriyaṃ atthi …"
msgstr ""

msgctxt "an10.68:5.4"
msgid "paññā atthi …"
msgstr ""

msgctxt "an10.68:5.5"
msgid "sotāvadhānaṃ atthi …"
msgstr ""

msgctxt "an10.68:5.6"
msgid "dhammadhāraṇā atthi …"
msgstr ""

msgctxt "an10.68:5.7"
msgid "atthūpaparikkhā atthi …"
msgstr ""

msgctxt "an10.68:5.8"
msgid "dhammānudhammappaṭipatti atthi …"
msgstr ""

msgctxt "an10.68:5.9"
msgid ""
"appamādo atthi kusalesu dhammesu, tassa yā ratti vā divaso vā āgacchati, "
"vuddhiyeva pāṭikaṅkhā kusalesu dhammesu no parihāni."
msgstr ""

msgctxt "an10.68:5.10"
msgid ""
"Seyyathāpi, āvuso, juṇhapakkhe candassa yā ratti vā divaso vā āgacchati, "
"vaḍḍhateva vaṇṇena vaḍḍhati maṇḍalena vaḍḍhati ābhāya vaḍḍhati "
"ārohapariṇāhena;"
msgstr ""

msgctxt "an10.68:5.11"
msgid ""
"evamevaṃ kho, āvuso, yassa kassaci saddhā atthi kusalesu dhammesu … pe …"
msgstr ""

msgctxt "an10.68:5.12"
msgid ""
"appamādo atthi kusalesu dhammesu, tassa yā ratti vā divaso vā āgacchati, "
"vuddhiyeva pāṭikaṅkhā kusalesu dhammesu no parihānī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an10.68:6.1"
msgid "Atha kho bhagavā paccuṭṭhāya āyasmantaṃ sāriputtaṃ āmantesi:"
msgstr ""

msgctxt "an10.68:6.2"
msgid "“sādhu sādhu, sāriputta."
msgstr ""

msgctxt "an10.68:6.3"
msgid "Yassa kassaci, sāriputta, saddhā natthi kusalesu dhammesu hirī natthi …"
msgstr ""

msgctxt "an10.68:6.4"
msgid "ottappaṃ natthi …"
msgstr ""

msgctxt "an10.68:6.5"
msgid "paññā natthi …"
msgstr ""

msgctxt "an10.68:6.6"
msgid "vīriyaṃ natthi …"
msgstr ""

msgctxt "an10.68:6.7"
msgid "sotāvadhānaṃ natthi …"
msgstr ""

msgctxt "an10.68:6.8"
msgid "dhammadhāraṇā natthi …"
msgstr ""

msgctxt "an10.68:6.9"
msgid "atthūpaparikkhā natthi …"
msgstr ""

msgctxt "an10.68:6.10"
msgid "dhammānudhammappaṭipatti natthi …"
msgstr ""

msgctxt "an10.68:6.11"
msgid ""
"appamādo natthi kusalesu dhammesu tassa yā ratti vā divaso vā āgacchati, "
"hāniyeva pāṭikaṅkhā kusalesu dhammesu no vuddhi."
msgstr ""

msgctxt "an10.68:6.12"
msgid ""
"Seyyathāpi, sāriputta, kāḷapakkhe candassa yā ratti vā divaso vā āgacchati, "
"hāyateva vaṇṇena hāyati maṇḍalena hāyati ābhāya hāyati ārohapariṇāhena;"
msgstr ""

msgctxt "an10.68:6.13"
msgid ""
"evamevaṃ kho, sāriputta, yassa kassaci saddhā natthi kusalesu dhammesu … pe …"
msgstr ""

msgctxt "an10.68:6.14"
msgid ""
"appamādo natthi kusalesu dhammesu, tassa yā ratti vā divaso vā āgacchati, "
"hāniyeva pāṭikaṅkhā kusalesu dhammesu no vuddhi."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.128"></a>
#. <a class="sc" id="sc7"></a>
msgctxt "an10.68:7.1"
msgid "Yassa kassaci, sāriputta, saddhā atthi kusalesu dhammesu hirī atthi …"
msgstr ""

msgctxt "an10.68:7.2"
msgid "ottappaṃ atthi …"
msgstr ""

msgctxt "an10.68:7.3"
msgid "vīriyaṃ atthi …"
msgstr ""

msgctxt "an10.68:7.4"
msgid "paññā atthi …"
msgstr ""

msgctxt "an10.68:7.5"
msgid "sotāvadhānaṃ atthi …"
msgstr ""

msgctxt "an10.68:7.6"
msgid "dhammadhāraṇā atthi …"
msgstr ""

msgctxt "an10.68:7.7"
msgid "atthūpaparikkhā atthi …"
msgstr ""

msgctxt "an10.68:7.8"
msgid "dhammānudhammappaṭipatti atthi …"
msgstr ""

msgctxt "an10.68:7.9"
msgid ""
"appamādo atthi kusalesu dhammesu, tassa yā ratti vā divaso vā āgacchati, "
"vuddhiyeva pāṭikaṅkhā kusalesu dhammesu no parihāni."
msgstr ""

msgctxt "an10.68:7.10"
msgid ""
"Seyyathāpi, sāriputta, juṇhapakkhe candassa yā ratti vā divaso vā āgacchati, "
"vaḍḍhateva vaṇṇena vaḍḍhati maṇḍalena vaḍḍhati ābhāya vaḍḍhati "
"ārohapariṇāhena;"
msgstr ""

msgctxt "an10.68:7.11"
msgid ""
"evamevaṃ kho, sāriputta, yassa kassaci saddhā atthi kusalesu dhammesu … pe …"
msgstr ""

msgctxt "an10.68:7.12"
msgid ""
"appamādo atthi kusalesu dhammesu, tassa yā ratti vā divaso vā āgacchati, "
"vuddhiyeva pāṭikaṅkhā kusalesu dhammesu no parihānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.68:7.13"
msgid "Aṭṭhamaṃ."
msgstr ""
