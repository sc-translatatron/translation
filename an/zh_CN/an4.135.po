#. <section class="sutta" id="an4.135"><article><div class="hgroup"><p class="division">
msgctxt "an4.135:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.135:0.2"
msgid "14. Puggalavagga"
msgstr ""

#. </p><h1>
#. VAR: vajjapuggalasuttaṃ (bj)
msgctxt "an4.135:0.3"
msgid "135. Sāvajjasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.135:1.1"
msgid "“Cattārome, bhikkhave, puggalā santo saṃvijjamānā lokasmiṃ."
msgstr ""

msgctxt "an4.135:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.135:1.3"
msgid "Sāvajjo, vajjabahulo, appavajjo, anavajjo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.135:2.1"
msgid "Kathañca, bhikkhave, puggalo sāvajjo hoti?"
msgstr ""

msgctxt "an4.135:2.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo sāvajjena kāyakammena samannāgato hoti, "
"sāvajjena vacīkammena samannāgato hoti, sāvajjena manokammena samannāgato "
"hoti."
msgstr ""

msgctxt "an4.135:2.3"
msgid "Evaṃ kho, bhikkhave, puggalo sāvajjo hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.136"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.135:3.1"
msgid "Kathañca, bhikkhave, puggalo vajjabahulo hoti?"
msgstr ""

msgctxt "an4.135:3.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo sāvajjena bahulaṃ kāyakammena samannāgato "
"hoti, appaṃ anavajjena;"
msgstr ""

msgctxt "an4.135:3.3"
msgid "sāvajjena bahulaṃ vacīkammena samannāgato hoti, appaṃ anavajjena;"
msgstr ""

msgctxt "an4.135:3.4"
msgid "sāvajjena bahulaṃ manokammena samannāgato hoti, appaṃ anavajjena."
msgstr ""

msgctxt "an4.135:3.5"
msgid "Evaṃ kho, bhikkhave, puggalo vajjabahulo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.135:4.1"
msgid "Kathañca, bhikkhave, puggalo appavajjo hoti?"
msgstr ""

msgctxt "an4.135:4.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo anavajjena bahulaṃ kāyakammena samannāgato "
"hoti, appaṃ sāvajjena;"
msgstr ""

msgctxt "an4.135:4.3"
msgid "anavajjena bahulaṃ vacīkammena samannāgato hoti, appaṃ sāvajjena;"
msgstr ""

msgctxt "an4.135:4.4"
msgid "anavajjena bahulaṃ manokammena samannāgato hoti, appaṃ sāvajjena."
msgstr ""

msgctxt "an4.135:4.5"
msgid "Evaṃ kho, bhikkhave, puggalo appavajjo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.135:5.1"
msgid "Kathañca, bhikkhave, puggalo anavajjo hoti?"
msgstr ""

msgctxt "an4.135:5.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo anavajjena kāyakammena samannāgato hoti, "
"anavajjena vacīkammena samannāgato hoti, anavajjena manokammena samannāgato "
"hoti."
msgstr ""

msgctxt "an4.135:5.3"
msgid "Evaṃ kho, bhikkhave, puggalo anavajjo hoti."
msgstr ""

msgctxt "an4.135:5.4"
msgid "Ime kho, bhikkhave, cattāro puggalā santo saṃvijjamānā lokasmin”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.135:5.5"
msgid "Pañcamaṃ."
msgstr ""
