#. <section class="sutta" id="an7.5"><article><div class="hgroup"><p class="division">
msgctxt "an7.5:0.1"
msgid "Aṅguttara Nikāya 7"
msgstr ""

#. </p><p>
msgctxt "an7.5:0.2"
msgid "1. Dhanavagga"
msgstr ""

#. </p><h1>
msgctxt "an7.5:0.3"
msgid "5. Saṅkhittadhanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.5"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an7.5:1.1"
msgid "“Sattimāni, bhikkhave, dhanāni."
msgstr ""

msgctxt "an7.5:1.2"
msgid "Katamāni satta?"
msgstr ""

msgctxt "an7.5:1.3"
msgid ""
"Saddhādhanaṃ, sīladhanaṃ, hirīdhanaṃ, ottappadhanaṃ, sutadhanaṃ, cāgadhanaṃ, "
"paññādhanaṃ."
msgstr ""

msgctxt "an7.5:1.4"
msgid "Imāni kho, bhikkhave, satta dhanānīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an7.5:2.1"
msgid "Saddhādhanaṃ sīladhanaṃ,"
msgstr ""

#. <br>
msgctxt "an7.5:2.2"
msgid "hirī ottappiyaṃ dhanaṃ;"
msgstr ""

#. <br>
msgctxt "an7.5:2.3"
msgid "Sutadhanañca cāgo ca,"
msgstr ""

#. <br>
msgctxt "an7.5:2.4"
msgid "paññā ve sattamaṃ dhanaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an7.5:3.1"
msgid "Yassa ete dhanā atthi,"
msgstr ""

#. <br>
msgctxt "an7.5:3.2"
msgid "itthiyā purisassa vā;"
msgstr ""

#. <br>
msgctxt "an7.5:3.3"
msgid "Adaliddoti taṃ āhu,"
msgstr ""

#. <br>
msgctxt "an7.5:3.4"
msgid "amoghaṃ tassa jīvitaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an7.5:4.1"
msgid "Tasmā saddhañca sīlañca,"
msgstr ""

#. <br>
msgctxt "an7.5:4.2"
msgid "Pasādaṃ dhammadassanaṃ;"
msgstr ""

#. <br>
msgctxt "an7.5:4.3"
msgid "Anuyuñjetha medhāvī,"
msgstr ""

#. <br>
#. VAR: Saraṃ → varaṃ (mr)
msgctxt "an7.5:4.4"
msgid "Saraṃ buddhāna sāsanan”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an7.5:4.5"
msgid "Pañcamaṃ."
msgstr ""
