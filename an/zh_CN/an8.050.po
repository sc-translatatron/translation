#. <section class="sutta" id="an8.50"><article><div class="hgroup"><p class="division">
msgctxt "an8.50:0.1"
msgid "Aṅguttara Nikāya 8"
msgstr ""

#. </p><p>
msgctxt "an8.50:0.2"
msgid "5. Uposathavagga"
msgstr ""

#. </p><h1>
msgctxt "an8.50:0.3"
msgid "50. Dutiyaidhalokikasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.272"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an8.50:1.1"
msgid ""
"“Catūhi, bhikkhave, dhammehi samannāgato mātugāmo idhalokavijayāya paṭipanno "
"hoti, ayaṃsa loko āraddho hoti."
msgstr ""

msgctxt "an8.50:1.2"
msgid "Katamehi catūhi?"
msgstr ""

msgctxt "an8.50:1.3"
msgid ""
"Idha, bhikkhave, mātugāmo susaṃvihitakammanto hoti, saṅgahitaparijano, "
"bhattu manāpaṃ carati, sambhataṃ anurakkhati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an8.50:2.1"
msgid "Kathañca, bhikkhave, mātugāmo susaṃvihitakammanto hoti?"
msgstr ""

msgctxt "an8.50:2.2"
msgid "Idha, bhikkhave, mātugāmo ye te bhattu abbhantarā kammantā … pe …"
msgstr ""

msgctxt "an8.50:2.3"
msgid "evaṃ kho, bhikkhave, mātugāmo susaṃvihitakammanto hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an8.50:3.1"
msgid "Kathañca, bhikkhave, mātugāmo saṅgahitaparijano hoti?"
msgstr ""

msgctxt "an8.50:3.2"
msgid "Idha, bhikkhave, mātugāmo yo so bhattu abbhantaro antojano … pe …"
msgstr ""

msgctxt "an8.50:3.3"
msgid "evaṃ kho, bhikkhave, mātugāmo saṅgahitaparijano hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an8.50:4.1"
msgid "Kathañca, bhikkhave, mātugāmo bhattu manāpaṃ carati?"
msgstr ""

msgctxt "an8.50:4.2"
msgid ""
"Idha, bhikkhave, mātugāmo yaṃ bhattu amanāpasaṅkhātaṃ taṃ jīvitahetupi na "
"ajjhācarati."
msgstr ""

msgctxt "an8.50:4.3"
msgid "Evaṃ kho, bhikkhave, mātugāmo bhattu manāpaṃ carati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an8.50:5.1"
msgid "Kathañca, bhikkhave, mātugāmo sambhataṃ anurakkhati?"
msgstr ""

msgctxt "an8.50:5.2"
msgid "Idha, bhikkhave, mātugāmo yaṃ bhattā āharati … pe …"
msgstr ""

msgctxt "an8.50:5.3"
msgid "evaṃ kho, bhikkhave, mātugāmo sambhataṃ anurakkhati."
msgstr ""

msgctxt "an8.50:5.4"
msgid ""
"Imehi kho, bhikkhave, catūhi dhammehi samannāgato mātugāmo idhalokavijayāya "
"paṭipanno hoti, ayaṃsa loko āraddho hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an8.50:6.1"
msgid ""
"Catūhi, bhikkhave, dhammehi samannāgato mātugāmo paralokavijayāya paṭipanno "
"hoti, paraloko āraddho hoti."
msgstr ""

msgctxt "an8.50:6.2"
msgid "Katamehi catūhi?"
msgstr ""

msgctxt "an8.50:6.3"
msgid ""
"Idha, bhikkhave, mātugāmo saddhāsampanno hoti, sīlasampanno hoti, "
"cāgasampanno hoti, paññāsampanno hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.273"></a>
#. <a class="sc" id="sc7"></a>
msgctxt "an8.50:7.1"
msgid "Kathañca, bhikkhave, mātugāmo saddhāsampanno hoti?"
msgstr ""

msgctxt "an8.50:7.2"
msgid "Idha, bhikkhave, mātugāmo saddho hoti … pe …"
msgstr ""

msgctxt "an8.50:7.3"
msgid "evaṃ kho, bhikkhave, mātugāmo saddhāsampanno hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an8.50:8.1"
msgid "Kathañca, bhikkhave, mātugāmo sīlasampanno hoti?"
msgstr ""

msgctxt "an8.50:8.2"
msgid "Idha, bhikkhave, mātugāmo pāṇātipātā paṭivirato hoti … pe …"
msgstr ""

msgctxt "an8.50:8.3"
msgid "surāmerayamajjapamādaṭṭhānā paṭivirato hoti."
msgstr ""

msgctxt "an8.50:8.4"
msgid "Evaṃ kho, bhikkhave, mātugāmo sīlasampanno hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an8.50:9.1"
msgid "Kathañca, bhikkhave, mātugāmo cāgasampanno hoti?"
msgstr ""

msgctxt "an8.50:9.2"
msgid ""
"Idha, bhikkhave, mātugāmo vigatamalamaccherena cetasā agāraṃ ajjhāvasati … "
"pe …"
msgstr ""

msgctxt "an8.50:9.3"
msgid "evaṃ kho, bhikkhave, mātugāmo cāgasampanno hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "an8.50:10.1"
msgid "Kathañca, bhikkhave, mātugāmo paññāsampanno hoti?"
msgstr ""

msgctxt "an8.50:10.2"
msgid "Idha, bhikkhave, mātugāmo paññavā hoti … pe …"
msgstr ""

msgctxt "an8.50:10.3"
msgid "evaṃ kho, bhikkhave, mātugāmo paññāsampanno hoti."
msgstr ""

msgctxt "an8.50:10.4"
msgid ""
"Imehi kho, bhikkhave, catūhi dhammehi samannāgato mātugāmo paralokavijayāya "
"paṭipanno hoti, paraloko āraddho hotīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc11"></a>
msgctxt "an8.50:11.1"
msgid "Susaṃvihitakammantā,"
msgstr ""

#. <br>
msgctxt "an8.50:11.2"
msgid "saṅgahitaparijjanā;"
msgstr ""

#. <br>
msgctxt "an8.50:11.3"
msgid "Bhattu manāpaṃ carati,"
msgstr ""

#. <br>
msgctxt "an8.50:11.4"
msgid "sambhataṃ anurakkhati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "an8.50:12.1"
msgid "Saddhā sīlena sampannā,"
msgstr ""

#. <br>
msgctxt "an8.50:12.2"
msgid "vadaññū vītamaccharā;"
msgstr ""

#. <br>
msgctxt "an8.50:12.3"
msgid "Niccaṃ maggaṃ visodheti,"
msgstr ""

#. <br>
msgctxt "an8.50:12.4"
msgid "sotthānaṃ samparāyikaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc13"></a>
msgctxt "an8.50:13.1"
msgid "Iccete aṭṭha dhammā ca,"
msgstr ""

#. <br>
msgctxt "an8.50:13.2"
msgid "yassā vijjanti nāriyā;"
msgstr ""

#. <br>
msgctxt "an8.50:13.3"
msgid "Tampi sīlavatiṃ āhu,"
msgstr ""

#. <br>
msgctxt "an8.50:13.4"
msgid "dhammaṭṭhaṃ saccavādiniṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc14"></a>
msgctxt "an8.50:14.1"
msgid "Soḷasākārasampannā,"
msgstr ""

#. <br>
msgctxt "an8.50:14.2"
msgid "Aṭṭhaṅgasusamāgatā;"
msgstr ""

#. <br>
msgctxt "an8.50:14.3"
msgid "Tādisī sīlavatī upāsikā,"
msgstr ""

#. <br>
msgctxt "an8.50:14.4"
msgid "Upapajjati devalokaṃ manāpan”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an8.50:14.5"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an8.50:14.6"
msgid "Uposathavaggo pañcamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc15"></a>
msgctxt "an8.50:15.1"
msgid "Saṅkhitte vitthate visākhe,"
msgstr ""

#. <br>
msgctxt "an8.50:15.2"
msgid "Vāseṭṭho bojjhāya pañcamaṃ;"
msgstr ""

#. <br>
msgctxt "an8.50:15.3"
msgid "Anuruddhaṃ puna visākhe,"
msgstr ""

#. <br>
msgctxt "an8.50:15.4"
msgid "Nakulā idhalokikā dveti."
msgstr ""

#. </p></blockquote><p class="endsutta">
msgctxt "an8.50:15.5"
msgid "Paṭhamo paṇṇāsako samatto."
msgstr ""
