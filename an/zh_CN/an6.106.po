#. <section class="sutta" id="an6.106"><article><div class="hgroup"><p class="division">
msgctxt "an6.106:0.1"
msgid "Aṅguttara Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "an6.106:0.2"
msgid "10. Ānisaṃsavagga"
msgstr ""

#. </p><h1>
msgctxt "an6.106:0.3"
msgid "106. Taṇhāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.445"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an6.106:1.1"
msgid "“Tisso imā, bhikkhave, taṇhā pahātabbā, tayo ca mānā."
msgstr ""

msgctxt "an6.106:1.2"
msgid "Katamā tisso taṇhā pahātabbā?"
msgstr ""

msgctxt "an6.106:1.3"
msgid "Kāmataṇhā, bhavataṇhā, vibhavataṇhā—"
msgstr ""

msgctxt "an6.106:1.4"
msgid "imā tisso taṇhā pahātabbā."
msgstr ""

msgctxt "an6.106:1.5"
msgid "Katame tayo mānā pahātabbā?"
msgstr ""

msgctxt "an6.106:1.6"
msgid "Māno, omāno, atimāno—"
msgstr ""

msgctxt "an6.106:1.7"
msgid "ime tayo mānā pahātabbā."
msgstr ""

msgctxt "an6.106:1.8"
msgid ""
"Yato kho, bhikkhave, bhikkhuno imā tisso taṇhā pahīnā honti, ime ca tayo "
"mānā;"
msgstr ""

msgctxt "an6.106:1.9"
msgid ""
"ayaṃ vuccati, bhikkhave, bhikkhu acchecchi taṇhaṃ, vivattayi saṃyojanaṃ, "
"sammā mānābhisamayā antamakāsi dukkhassā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.106:1.10"
msgid "Ekādasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an6.106:1.11"
msgid "Ānisaṃsavaggo dasamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an6.106:2.1"
msgid "Pātubhāvo ānisaṃso,"
msgstr ""

#. <br>
msgctxt "an6.106:2.2"
msgid "aniccadukkhaanattato;"
msgstr ""

#. <br>
msgctxt "an6.106:2.3"
msgid "Nibbānaṃ anavatthi,"
msgstr ""

#. <br>
msgctxt "an6.106:2.4"
msgid "ukkhittāsi atammayo;"
msgstr ""

#. <br>
msgctxt "an6.106:2.5"
msgid "Bhavā taṇhāyekā dasāti."
msgstr ""

#. </p></blockquote><p class="endsutta">
msgctxt "an6.106:2.6"
msgid "Dutiyo paṇṇāsako samatto."
msgstr ""
