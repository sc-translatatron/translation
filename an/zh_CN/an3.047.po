#. <section class="sutta" id="an3.47"><article><div class="hgroup"><p class="division">
msgctxt "an3.47:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.47:0.2"
msgid "5. Cūḷavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.47:0.3"
msgid "47. Saṅkhatalakkhaṇasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.47:1.1"
msgid "“Tīṇimāni, bhikkhave, saṅkhatassa saṅkhatalakkhaṇāni."
msgstr ""

msgctxt "an3.47:1.2"
msgid "Katamāni tīṇi?"
msgstr ""

msgctxt "an3.47:1.3"
msgid "Uppādo paññāyati, vayo paññāyati, ṭhitassa aññathattaṃ paññāyati."
msgstr ""

msgctxt "an3.47:1.4"
msgid "Imāni kho, bhikkhave, tīṇi saṅkhatassa saṅkhatalakkhaṇānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.47:1.5"
msgid "Sattamaṃ."
msgstr ""

#. </p><h2>
msgctxt "an3.47:1.6"
msgid "Asaṅkhatalakkhaṇasutta"
msgstr ""

#. </h2><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.47:2.1"
msgid "“Tīṇimāni, bhikkhave, asaṅkhatassa asaṅkhatalakkhaṇāni."
msgstr ""

msgctxt "an3.47:2.2"
msgid "Katamāni tīṇi?"
msgstr ""

msgctxt "an3.47:2.3"
msgid ""
"Na uppādo paññāyati, na vayo paññāyati, na ṭhitassa aññathattaṃ paññāyati."
msgstr ""

msgctxt "an3.47:2.4"
msgid "Imāni kho, bhikkhave, tīṇi asaṅkhatassa asaṅkhatalakkhaṇānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.47:2.5"
msgid "Aṭṭhamaṃ."
msgstr ""
