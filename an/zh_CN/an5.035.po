#. <section class="sutta" id="an5.35"><article><div class="hgroup"><p class="division">
msgctxt "an5.35:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.35:0.2"
msgid "4. Sumanavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.35:0.3"
msgid "35. Dānānisaṃsasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.41"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.35:1.1"
msgid "“Pañcime, bhikkhave, dāne ānisaṃsā."
msgstr ""

msgctxt "an5.35:1.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.35:1.3"
msgid "Bahuno janassa piyo hoti manāpo;"
msgstr ""

msgctxt "an5.35:1.4"
msgid "santo sappurisā bhajanti;"
msgstr ""

msgctxt "an5.35:1.5"
msgid "kalyāṇo kittisaddo abbhuggacchati;"
msgstr ""

#. VAR: gihidhammā anapagato → gihidhammā anapeto (bj, pts1) | gihidhammamanupagato (mr)
msgctxt "an5.35:1.6"
msgid "gihidhammā anapagato hoti;"
msgstr ""

msgctxt "an5.35:1.7"
msgid "kāyassa bhedā paraṃ maraṇā sugatiṃ saggaṃ lokaṃ upapajjati."
msgstr ""

msgctxt "an5.35:1.8"
msgid "Ime kho, bhikkhave, pañca dāne ānisaṃsāti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.35:2.1"
msgid "Dadamāno piyo hoti,"
msgstr ""

#. <br>
msgctxt "an5.35:2.2"
msgid "sataṃ dhammaṃ anukkamaṃ;"
msgstr ""

#. <br>
#. VAR: sadā bhajanti → bhajanti sappurisā (sī )
msgctxt "an5.35:2.3"
msgid "Santo naṃ sadā bhajanti,"
msgstr ""

#. <br>
msgctxt "an5.35:2.4"
msgid "saññatā brahmacārayo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.35:3.1"
msgid "Te tassa dhammaṃ desenti,"
msgstr ""

#. <br>
msgctxt "an5.35:3.2"
msgid "Sabbadukkhāpanūdanaṃ;"
msgstr ""

#. <br>
msgctxt "an5.35:3.3"
msgid "Yaṃ so dhammaṃ idhaññāya,"
msgstr ""

#. <br>
msgctxt "an5.35:3.4"
msgid "Parinibbāti anāsavo”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an5.35:3.5"
msgid "Pañcamaṃ."
msgstr ""
