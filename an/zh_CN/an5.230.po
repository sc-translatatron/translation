#. <section class="sutta" id="an5.230"><article><div class="hgroup"><p class="division">
msgctxt "an5.230:0.1"
msgid "Aṅguttara Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "an5.230:0.2"
msgid "23. Dīghacārikavagga"
msgstr ""

#. </p><h1>
msgctxt "an5.230:0.3"
msgid "230. Dutiyakaṇhasappasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.261"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.230:1.1"
msgid "“Pañcime, bhikkhave, ādīnavā kaṇhasappe."
msgstr ""

msgctxt "an5.230:1.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.230:1.3"
msgid "Kodhano, upanāhī, ghoraviso, dujjivho, mittadubbhī."
msgstr ""

msgctxt "an5.230:1.4"
msgid "Ime kho, bhikkhave, pañca ādīnavā kaṇhasappe."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an5.230:2.1"
msgid "Evamevaṃ kho, bhikkhave, pañcime ādīnavā mātugāme."
msgstr ""

msgctxt "an5.230:2.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.230:2.3"
msgid "Kodhano, upanāhī, ghoraviso, dujjivho, mittadubbhī."
msgstr ""

msgctxt "an5.230:2.4"
msgid "Tatridaṃ, bhikkhave, mātugāmassa ghoravisatā—"
msgstr ""

msgctxt "an5.230:2.5"
msgid "yebhuyyena, bhikkhave, mātugāmo tibbarāgo."
msgstr ""

msgctxt "an5.230:2.6"
msgid "Tatridaṃ, bhikkhave, mātugāmassa dujjivhatā—"
msgstr ""

msgctxt "an5.230:2.7"
msgid "yebhuyyena, bhikkhave, mātugāmo pisuṇavāco."
msgstr ""

msgctxt "an5.230:2.8"
msgid "Tatridaṃ, bhikkhave, mātugāmassa mittadubbhitā—"
msgstr ""

msgctxt "an5.230:2.9"
msgid "yebhuyyena, bhikkhave, mātugāmo aticārinī."
msgstr ""

msgctxt "an5.230:2.10"
msgid "Ime kho, bhikkhave, pañca ādīnavā mātugāme”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.230:2.11"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "an5.230:2.12"
msgid "Dīghacārikavaggo tatiyo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an5.230:3.1"
msgid "Dve dīghacārikā vuttā,"
msgstr ""

#. <br>
msgctxt "an5.230:3.2"
msgid "atinivāsamaccharī;"
msgstr ""

#. <br>
msgctxt "an5.230:3.3"
msgid "Dve ca kulūpakā bhogā,"
msgstr ""

#. <br>
msgctxt "an5.230:3.4"
msgid "bhattaṃ sappāpare duveti."
msgstr ""
