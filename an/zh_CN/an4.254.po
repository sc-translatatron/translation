#. <section class="sutta" id="an4.254"><article><div class="hgroup"><p class="division">
msgctxt "an4.254:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.254:0.2"
msgid "26. Abhiññāvagga"
msgstr ""

#. </p><h1>
msgctxt "an4.254:0.3"
msgid "254. Abhiññāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.247"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.254:1.1"
msgid "“Cattārome, bhikkhave, dhammā."
msgstr ""

msgctxt "an4.254:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.254:1.3"
msgid "Atthi, bhikkhave, dhammā abhiññā pariññeyyā;"
msgstr ""

msgctxt "an4.254:1.4"
msgid "atthi, bhikkhave, dhammā abhiññā pahātabbā;"
msgstr ""

msgctxt "an4.254:1.5"
msgid "atthi, bhikkhave, dhammā abhiññā bhāvetabbā;"
msgstr ""

msgctxt "an4.254:1.6"
msgid "atthi, bhikkhave, dhammā abhiññā sacchikātabbā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.254:2.1"
msgid "Katame ca, bhikkhave, dhammā abhiññā pariññeyyā?"
msgstr ""

#. VAR: Pañcupādānakkhandhā → pañcupādānakkhandhātissa vacanīyaṃ (mr)
msgctxt "an4.254:2.2"
msgid "Pañcupādānakkhandhā—"
msgstr ""

msgctxt "an4.254:2.3"
msgid "ime vuccanti, bhikkhave, dhammā abhiññā pariññeyyā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.254:3.1"
msgid "Katame ca, bhikkhave, dhammā abhiññā pahātabbā?"
msgstr ""

msgctxt "an4.254:3.2"
msgid "Avijjā ca bhavataṇhā ca—"
msgstr ""

msgctxt "an4.254:3.3"
msgid "ime vuccanti, bhikkhave, dhammā abhiññā pahātabbā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.254:4.1"
msgid "Katame ca, bhikkhave, dhammā abhiññā bhāvetabbā?"
msgstr ""

msgctxt "an4.254:4.2"
msgid "Samatho ca vipassanā ca—"
msgstr ""

msgctxt "an4.254:4.3"
msgid "ime vuccanti, bhikkhave, dhammā abhiññā bhāvetabbā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.254:5.1"
msgid "Katame ca, bhikkhave, dhammā abhiññā sacchikātabbā?"
msgstr ""

msgctxt "an4.254:5.2"
msgid "Vijjā ca vimutti ca—"
msgstr ""

msgctxt "an4.254:5.3"
msgid "ime vuccanti, bhikkhave, dhammā abhiññā sacchikātabbā."
msgstr ""

msgctxt "an4.254:5.4"
msgid "Ime kho, bhikkhave, cattāro dhammā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.254:5.5"
msgid "Paṭhamaṃ."
msgstr ""
