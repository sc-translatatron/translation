#. <section class="sutta" id="an7.80"><article><div class="hgroup"><p class="division">
msgctxt "an7.80:0.1"
msgid "Aṅguttara Nikāya 7"
msgstr ""

#. </p><p>
msgctxt "an7.80:0.2"
msgid "8. Vinayavagga"
msgstr ""

#. </p><h1>
msgctxt "an7.80:0.3"
msgid "80. Dutiyavinayadharasobhanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an7.80:1.1"
msgid "“Sattahi, bhikkhave, dhammehi samannāgato vinayadharo sobhati."
msgstr ""

msgctxt "an7.80:1.2"
msgid "Katamehi sattahi?"
msgstr ""

msgctxt "an7.80:1.3"
msgid "Āpattiṃ jānāti, "
msgstr ""

msgctxt "an7.80:1.4"
msgid "anāpattiṃ jānāti, "
msgstr ""

msgctxt "an7.80:1.5"
msgid "lahukaṃ āpattiṃ jānāti, "
msgstr ""

msgctxt "an7.80:1.6"
msgid "garukaṃ āpattiṃ jānāti, "
msgstr ""

msgctxt "an7.80:1.7"
msgid ""
"ubhayāni kho panassa pātimokkhāni vitthārena svāgatāni honti suvibhattāni "
"suppavattīni suvinicchitāni suttaso anubyañjanaso, "
msgstr ""

msgctxt "an7.80:1.8"
msgid "catunnaṃ jhānānaṃ … pe … akasiralābhī, "
msgstr ""

msgctxt "an7.80:1.9"
msgid "āsavānaṃ khayā … pe … sacchikatvā upasampajja viharati."
msgstr ""

msgctxt "an7.80:1.10"
msgid ""
"Imehi kho, bhikkhave, sattahi dhammehi samannāgato vinayadharo sobhatī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an7.80:1.11"
msgid "Chaṭṭhaṃ."
msgstr ""
