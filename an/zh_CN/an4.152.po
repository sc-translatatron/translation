msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.152:0.2"
msgid "16. Indriyavagga"
msgstr ""

#. </p><h1>
#. VAR: balasuttaṃ (bj)
msgctxt "an4.152:0.3"
msgid "152. Saddhābalasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.152:1.1"
msgid "“Cattārimāni, bhikkhave, balāni."
msgstr ""

msgctxt "an4.152:1.2"
msgid "Katamāni cattāri?"
msgstr ""

msgctxt "an4.152:1.3"
msgid "Saddhābalaṃ, vīriyabalaṃ, satibalaṃ, samādhibalaṃ—"
msgstr ""

msgctxt "an4.152:1.4"
msgid "imāni kho, bhikkhave, cattāri balānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.152:1.5"
msgid "Dutiyaṃ."
msgstr ""
