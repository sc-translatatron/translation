#. <section class="sutta" id="an7.58"><article><div class="hgroup"><p class="division">
msgctxt "an7.58:0.1"
msgid "Aṅguttara Nikāya 7"
msgstr ""

#. </p><p>
msgctxt "an7.58:0.2"
msgid "6. Abyākatavagga"
msgstr ""

#. </p><h1>
msgctxt "an7.58:0.3"
msgid "58. Arakkheyyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an7.58:1.1"
msgid "“Cattārimāni, bhikkhave, tathāgatassa arakkheyyāni, tīhi ca anupavajjo."
msgstr ""

msgctxt "an7.58:1.2"
msgid "Katamāni cattāri tathāgatassa arakkheyyāni?"
msgstr ""

msgctxt "an7.58:1.3"
msgid "Parisuddhakāyasamācāro, bhikkhave, tathāgato;"
msgstr ""

msgctxt "an7.58:1.4"
msgid "natthi tathāgatassa kāyaduccaritaṃ yaṃ tathāgato rakkheyya:"
msgstr ""

msgctxt "an7.58:1.5"
msgid "‘mā me idaṃ paro aññāsī’ti. (1)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an7.58:2.1"
msgid "Parisuddhavacīsamācāro, bhikkhave, tathāgato;"
msgstr ""

msgctxt "an7.58:2.2"
msgid "natthi tathāgatassa vacīduccaritaṃ yaṃ tathāgato rakkheyya:"
msgstr ""

msgctxt "an7.58:2.3"
msgid "‘mā me idaṃ paro aññāsī’ti. (2)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an7.58:3.1"
msgid "Parisuddhamanosamācāro, bhikkhave, tathāgato;"
msgstr ""

msgctxt "an7.58:3.2"
msgid "natthi tathāgatassa manoduccaritaṃ yaṃ tathāgato rakkheyya:"
msgstr ""

msgctxt "an7.58:3.3"
msgid "‘mā me idaṃ paro aññāsī’ti. (3)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an7.58:4.1"
msgid "Parisuddhājīvo, bhikkhave, tathāgato;"
msgstr ""

msgctxt "an7.58:4.2"
msgid "natthi tathāgatassa micchāājīvo yaṃ tathāgato rakkheyya:"
msgstr ""

msgctxt "an7.58:4.3"
msgid "‘mā me idaṃ paro aññāsī’ti. (4)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an7.58:5.1"
msgid "Imāni cattāri tathāgatassa arakkheyyāni."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.83"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "an7.58:6.1"
msgid "Katamehi tīhi anupavajjo?"
msgstr ""

msgctxt "an7.58:6.2"
msgid "Svākkhātadhammo, bhikkhave, tathāgato."
msgstr ""

msgctxt "an7.58:6.3"
msgid ""
"Tatra vata maṃ samaṇo vā brāhmaṇo vā devo vā māro vā brahmā vā koci vā "
"lokasmiṃ sahadhammena paṭicodessati:"
msgstr ""

msgctxt "an7.58:6.4"
msgid "‘itipi tvaṃ na svākkhātadhammo’ti."
msgstr ""

msgctxt "an7.58:6.5"
msgid "Nimittametaṃ, bhikkhave, na samanupassāmi."
msgstr ""

#. VAR: Etamahaṃ → etampahaṃ (bj, s1-3, pts1)
msgctxt "an7.58:6.6"
msgid ""
"Etamahaṃ, bhikkhave, nimittaṃ asamanupassanto khemappatto abhayappatto "
"vesārajjappatto viharāmi. (1)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an7.58:7.1"
msgid "Supaññattā kho pana me, bhikkhave, sāvakānaṃ nibbānagāminī paṭipadā."
msgstr ""

msgctxt "an7.58:7.2"
msgid ""
"Yathāpaṭipannā mama sāvakā āsavānaṃ khayā anāsavaṃ cetovimuttiṃ "
"paññāvimuttiṃ diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja "
"viharanti."
msgstr ""

msgctxt "an7.58:7.3"
msgid ""
"Tatra vata maṃ samaṇo vā brāhmaṇo vā devo vā māro vā brahmā vā koci vā "
"lokasmiṃ sahadhammena paṭicodessati:"
msgstr ""

msgctxt "an7.58:7.4"
msgid ""
"‘itipi te na supaññattā sāvakānaṃ nibbānagāminī paṭipadā. Yathāpaṭipannā "
"tava sāvakā āsavānaṃ khayā … pe … sacchikatvā upasampajja viharantī’ti."
msgstr ""

msgctxt "an7.58:7.5"
msgid "Nimittametaṃ, bhikkhave, na samanupassāmi."
msgstr ""

msgctxt "an7.58:7.6"
msgid ""
"Etamahaṃ, bhikkhave, nimittaṃ asamanupassanto khemappatto abhayappatto "
"vesārajjappatto viharāmi. (2)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.84"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "an7.58:8.1"
msgid ""
"Anekasatā kho pana me, bhikkhave, sāvakaparisā āsavānaṃ khayā … pe … "
"sacchikatvā upasampajja viharanti."
msgstr ""

msgctxt "an7.58:8.2"
msgid ""
"Tatra vata maṃ samaṇo vā brāhmaṇo vā devo vā māro vā brahmā vā koci vā "
"lokasmiṃ sahadhammena paṭicodessati:"
msgstr ""

msgctxt "an7.58:8.3"
msgid ""
"‘itipi te na anekasatā sāvakaparisā āsavānaṃ khayā anāsavaṃ cetovimuttiṃ "
"paññāvimuttiṃ diṭṭheva dhamme sayaṃ abhiññā sacchikatvā upasampajja "
"viharantī’ti."
msgstr ""

msgctxt "an7.58:8.4"
msgid "Nimittametaṃ, bhikkhave, na samanupassāmi."
msgstr ""

msgctxt "an7.58:8.5"
msgid ""
"Etamahaṃ, bhikkhave, nimittaṃ asamanupassanto khemappatto abhayappatto "
"vesārajjappatto viharāmi. (3)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an7.58:9.1"
msgid "Imehi tīhi anupavajjo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "an7.58:10.1"
msgid ""
"Imāni kho, bhikkhave, cattāri tathāgatassa arakkheyyāni, imehi ca tīhi "
"anupavajjo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an7.58:10.2"
msgid "Pañcamaṃ."
msgstr ""
