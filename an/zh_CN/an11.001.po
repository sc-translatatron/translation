#. <section class="sutta" id="an11.1"><article><div class="hgroup"><p class="division">
msgctxt "an11.1:0.1"
msgid "Aṅguttara Nikāya 11"
msgstr ""

#. </p><p>
#. VAR: 1. Nissayavagga → paṭhamavaggo (s1) | nissāyavaggo (s2, s3)
msgctxt "an11.1:0.2"
msgid "1. Nissayavagga"
msgstr ""

#. </p><h1>
msgctxt "an11.1:0.3"
msgid "1. Kimatthiyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.311"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "an11.1:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "an11.1:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "an11.1:1.3"
msgid ""
"Atha kho āyasmā ānando yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavantaṃ "
"abhivādetvā ekamantaṃ nisīdi. Ekamantaṃ nisinno kho āyasmā ānando "
"bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "an11.1:1.4"
msgid "“kimatthiyāni, bhante, kusalāni sīlāni kimānisaṃsānī”ti?"
msgstr ""

msgctxt "an11.1:1.5"
msgid ""
"“Avippaṭisāratthāni kho, ānanda, kusalāni sīlāni avippaṭisārānisaṃsānī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an11.1:2.1"
msgid "“Avippaṭisāro pana, bhante, kimatthiyo kimānisaṃso”?"
msgstr ""

msgctxt "an11.1:2.2"
msgid "“Avippaṭisāro kho, ānanda, pāmojjattho pāmojjānisaṃso”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an11.1:3.1"
msgid "“Pāmojjaṃ pana, bhante, kimatthiyaṃ kimānisaṃsaṃ”?"
msgstr ""

msgctxt "an11.1:3.2"
msgid "“Pāmojjaṃ kho, ānanda, pītatthaṃ pītānisaṃsaṃ”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an11.1:4.1"
msgid "“Pīti pana, bhante, kimatthiyā kimānisaṃsā”?"
msgstr ""

msgctxt "an11.1:4.2"
msgid "“Pīti kho, ānanda, passaddhatthā passaddhānisaṃsā”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an11.1:5.1"
msgid "“Passaddhi pana, bhante, kimatthiyā kimānisaṃsā”?"
msgstr ""

msgctxt "an11.1:5.2"
msgid "“Passaddhi kho, ānanda, sukhatthā sukhānisaṃsā”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an11.1:6.1"
msgid "“Sukhaṃ pana, bhante, kimatthiyaṃ kimānisaṃsaṃ”?"
msgstr ""

msgctxt "an11.1:6.2"
msgid "“Sukhaṃ kho, ānanda, samādhatthaṃ samādhānisaṃsaṃ”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "an11.1:7.1"
msgid "“Samādhi pana, bhante, kimatthiyo kimānisaṃso”?"
msgstr ""

msgctxt "an11.1:7.2"
msgid ""
"“Samādhi kho, ānanda, yathābhūtañāṇadassanattho "
"yathābhūtañāṇadassanānisaṃso”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an11.1:8.1"
msgid "“Yathābhūtañāṇadassanaṃ pana, bhante, kimatthiyaṃ kimānisaṃsaṃ”?"
msgstr ""

msgctxt "an11.1:8.2"
msgid "“Yathābhūtañāṇadassanaṃ kho, ānanda, nibbidatthaṃ nibbidānisaṃsaṃ”."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.312"></a>
#. <a class="sc" id="sc9"></a>
msgctxt "an11.1:9.1"
msgid "“Nibbidā pana, bhante, kimatthiyā kimānisaṃsā”?"
msgstr ""

msgctxt "an11.1:9.2"
msgid "“Nibbidā kho, ānanda, virāgatthā virāgānisaṃsā”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "an11.1:10.1"
msgid "“Virāgo pana, bhante, kimatthiyo kimānisaṃso”?"
msgstr ""

msgctxt "an11.1:10.2"
msgid "“Virāgo kho, ānanda, vimuttiñāṇadassanattho vimuttiñāṇadassanānisaṃso."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "an11.1:11.1"
msgid ""
"Iti kho, ānanda, kusalāni sīlāni avippaṭisāratthāni avippaṭisārānisaṃsāni, "
msgstr ""

msgctxt "an11.1:11.2"
msgid "avippaṭisāro pāmojjattho pāmojjānisaṃso, "
msgstr ""

msgctxt "an11.1:11.3"
msgid "pāmojjaṃ pītatthaṃ pītānisaṃsaṃ, "
msgstr ""

msgctxt "an11.1:11.4"
msgid "pīti passaddhatthā passaddhānisaṃsā, "
msgstr ""

msgctxt "an11.1:11.5"
msgid "passaddhi sukhatthā sukhānisaṃsā, "
msgstr ""

msgctxt "an11.1:11.6"
msgid "sukhaṃ samādhatthaṃ samādhānisaṃsaṃ, "
msgstr ""

msgctxt "an11.1:11.7"
msgid "samādhi yathābhūtañāṇadassanattho yathābhūtañāṇadassanānisaṃso, "
msgstr ""

msgctxt "an11.1:11.8"
msgid "yathābhūtañāṇadassanaṃ nibbidatthaṃ nibbidānisaṃsaṃ, "
msgstr ""

msgctxt "an11.1:11.9"
msgid "nibbidā virāgatthā virāgānisaṃsā, "
msgstr ""

msgctxt "an11.1:11.10"
msgid "virāgo vimuttiñāṇadassanattho vimuttiñāṇadassanānisaṃso."
msgstr ""

msgctxt "an11.1:11.11"
msgid "Iti kho, ānanda, kusalāni sīlāni anupubbena aggāya parentī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an11.1:11.12"
msgid "Paṭhamaṃ."
msgstr ""
