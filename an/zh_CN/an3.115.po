#. <section class="sutta" id="an3.115"><article><div class="hgroup"><p class="division">
msgctxt "an3.115:0.1"
msgid "Aṅguttara Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "an3.115:0.2"
msgid "12. Āpāyikavagga"
msgstr ""

#. </p><h1>
msgctxt "an3.115:0.3"
msgid "115. Appameyyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an3.115:1.1"
msgid "“Tayome, bhikkhave, puggalā santo saṃvijjamānā lokasmiṃ."
msgstr ""

msgctxt "an3.115:1.2"
msgid "Katame tayo?"
msgstr ""

msgctxt "an3.115:1.3"
msgid "Suppameyyo, duppameyyo, appameyyo."
msgstr ""

msgctxt "an3.115:1.4"
msgid "Katamo ca, bhikkhave, puggalo suppameyyo?"
msgstr ""

msgctxt "an3.115:1.5"
msgid ""
"Idha, bhikkhave, ekacco puggalo uddhato hoti unnaḷo capalo mukharo "
"vikiṇṇavāco muṭṭhassati asampajāno asamāhito vibbhantacitto pākatindriyo."
msgstr ""

msgctxt "an3.115:1.6"
msgid "Ayaṃ vuccati, bhikkhave, puggalo suppameyyo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an3.115:2.1"
msgid "Katamo ca, bhikkhave, puggalo duppameyyo?"
msgstr ""

msgctxt "an3.115:2.2"
msgid ""
"Idha, bhikkhave, ekacco puggalo anuddhato hoti anunnaḷo acapalo amukharo "
"avikiṇṇavāco upaṭṭhitassati sampajāno samāhito ekaggacitto saṃvutindriyo."
msgstr ""

msgctxt "an3.115:2.3"
msgid "Ayaṃ vuccati, bhikkhave, puggalo duppameyyo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an3.115:3.1"
msgid "Katamo ca, bhikkhave, puggalo appameyyo?"
msgstr ""

msgctxt "an3.115:3.2"
msgid "Idha, bhikkhave, bhikkhu arahaṃ hoti khīṇāsavo."
msgstr ""

msgctxt "an3.115:3.3"
msgid "Ayaṃ vuccati, bhikkhave, puggalo appameyyo."
msgstr ""

msgctxt "an3.115:3.4"
msgid "Ime kho, bhikkhave, tayo puggalā santo saṃvijjamānā lokasmin”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an3.115:3.5"
msgid "Tatiyaṃ."
msgstr ""
