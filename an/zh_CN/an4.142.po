#. <section class="sutta" id="an4.142"><article><div class="hgroup"><p class="division">
msgctxt "an4.142:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.142:0.2"
msgid "15. Ābhāvagga"
msgstr ""

#. </p><h1>
msgctxt "an4.142:0.3"
msgid "142. Pabhāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.142:1.1"
msgid "“Catasso imā, bhikkhave, pabhā."
msgstr ""

msgctxt "an4.142:1.2"
msgid "Katamā catasso?"
msgstr ""

msgctxt "an4.142:1.3"
msgid "Candappabhā, sūriyappabhā, aggippabhā, paññāpabhā—"
msgstr ""

msgctxt "an4.142:1.4"
msgid "imā kho, bhikkhave, catasso pabhā."
msgstr ""

msgctxt "an4.142:1.5"
msgid "Etadaggaṃ, bhikkhave, imāsaṃ catunnaṃ pabhānaṃ yadidaṃ paññāpabhā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.142:1.6"
msgid "Dutiyaṃ."
msgstr ""
