#. <section class="sutta" id="an4.101"><article><div class="hgroup"><p class="division">
msgctxt "an4.101:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.101:0.2"
msgid "11. Valāhakavagga"
msgstr ""

#. </p><h1>
msgctxt "an4.101:0.3"
msgid "101. Paṭhamavalāhakasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.102"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "an4.101:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "an4.101:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "an4.101:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "an4.101:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "an4.101:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "an4.101:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.101:2.1"
msgid "“Cattārome, bhikkhave, valāhakā."
msgstr ""

msgctxt "an4.101:2.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.101:2.3"
msgid ""
"Gajjitā no vassitā, vassitā no gajjitā, neva gajjitā no vassitā, gajjitā ca "
"vassitā ca."
msgstr ""

msgctxt "an4.101:2.4"
msgid "Ime kho, bhikkhave, cattāro valāhakā."
msgstr ""

msgctxt "an4.101:2.5"
msgid ""
"Evamevaṃ kho, bhikkhave, cattāro valāhakūpamā puggalā santo saṃvijjamānā "
"lokasmiṃ."
msgstr ""

msgctxt "an4.101:2.6"
msgid "Katame cattāro?"
msgstr ""

msgctxt "an4.101:2.7"
msgid ""
"Gajjitā no vassitā, vassitā no gajjitā, neva gajjitā no vassitā, gajjitā ca "
"vassitā ca."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.101:3.1"
msgid "Kathañca, bhikkhave, puggalo gajjitā hoti no vassitā?"
msgstr ""

msgctxt "an4.101:3.2"
msgid "Idha, bhikkhave, ekacco puggalo bhāsitā hoti, no kattā."
msgstr ""

msgctxt "an4.101:3.3"
msgid "Evaṃ kho, bhikkhave, puggalo gajjitā hoti, no vassitā."
msgstr ""

msgctxt "an4.101:3.4"
msgid "Seyyathāpi so, bhikkhave, valāhako gajjitā, no vassitā;"
msgstr ""

msgctxt "an4.101:3.5"
msgid "tathūpamāhaṃ, bhikkhave, imaṃ puggalaṃ vadāmi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.101:4.1"
msgid "Kathañca, bhikkhave, puggalo vassitā hoti, no gajjitā?"
msgstr ""

msgctxt "an4.101:4.2"
msgid "Idha, bhikkhave, ekacco puggalo kattā hoti, no bhāsitā."
msgstr ""

msgctxt "an4.101:4.3"
msgid "Evaṃ kho, bhikkhave, puggalo vassitā hoti, no gajjitā."
msgstr ""

msgctxt "an4.101:4.4"
msgid "Seyyathāpi so, bhikkhave, valāhako vassitā, no gajjitā;"
msgstr ""

msgctxt "an4.101:4.5"
msgid "tathūpamāhaṃ, bhikkhave, imaṃ puggalaṃ vadāmi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an4.101:5.1"
msgid "Kathañca, bhikkhave, puggalo neva gajjitā hoti, no vassitā?"
msgstr ""

msgctxt "an4.101:5.2"
msgid "Idha, bhikkhave, ekacco puggalo neva bhāsitā hoti, no kattā."
msgstr ""

msgctxt "an4.101:5.3"
msgid "Evaṃ kho, bhikkhave, puggalo neva gajjitā hoti, no vassitā."
msgstr ""

#. VAR: neva gajjitā → neva gajjitā hoti (mr)
msgctxt "an4.101:5.4"
msgid "Seyyathāpi so, bhikkhave, valāhako neva gajjitā, no vassitā;"
msgstr ""

msgctxt "an4.101:5.5"
msgid "tathūpamāhaṃ, bhikkhave, imaṃ puggalaṃ vadāmi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an4.101:6.1"
msgid "Kathañca, bhikkhave, puggalo gajjitā ca hoti vassitā ca?"
msgstr ""

msgctxt "an4.101:6.2"
msgid "Idha, bhikkhave, ekacco puggalo bhāsitā ca hoti kattā ca."
msgstr ""

msgctxt "an4.101:6.3"
msgid "Evaṃ kho, bhikkhave, puggalo gajjitā ca hoti vassitā ca."
msgstr ""

#. VAR: valāhako gajjitā ca → gajjitā ca hoti (pts1, mr)
msgctxt "an4.101:6.4"
msgid "Seyyathāpi so, bhikkhave, valāhako gajjitā ca vassitā ca;"
msgstr ""

msgctxt "an4.101:6.5"
msgid "tathūpamāhaṃ, bhikkhave, imaṃ puggalaṃ vadāmi."
msgstr ""

msgctxt "an4.101:6.6"
msgid ""
"Ime kho, bhikkhave, cattāro valāhakūpamā puggalā santo saṃvijjamānā "
"lokasmin”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an4.101:6.7"
msgid "Paṭhamaṃ."
msgstr ""
