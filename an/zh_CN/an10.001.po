#. <section class="sutta" id="an10.1"><article><div class="hgroup"><p class="division">
msgctxt "an10.1:0.1"
msgid "Aṅguttara Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "an10.1:0.2"
msgid "1. Ānisaṃsavagga"
msgstr ""

#. </p><h1>
msgctxt "an10.1:0.3"
msgid "1. Kimatthiyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.1"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "an10.1:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "an10.1:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "an10.1:1.3"
msgid ""
"Atha kho āyasmā ānando yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavantaṃ "
"abhivādetvā ekamantaṃ nisīdi. Ekamantaṃ nisinno kho āyasmā ānando "
"bhagavantaṃ etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an10.1:2.1"
msgid "“Kimatthiyāni, bhante, kusalāni sīlāni kimānisaṃsānī”ti?"
msgstr ""

msgctxt "an10.1:2.2"
msgid ""
"“Avippaṭisāratthāni kho, ānanda, kusalāni sīlāni avippaṭisārānisaṃsānī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an10.1:3.1"
msgid "“Avippaṭisāro pana, bhante, kimatthiyo kimānisaṃso”ti?"
msgstr ""

#. VAR: pāmojjattho pāmojjānisaṃso”ti → pāmujjattho pāmujjānisaṃsoti (si, pts1) | pāmujjattho pāmujjānisaṃso (s1-3)
msgctxt "an10.1:3.2"
msgid "“Avippaṭisāro kho, ānanda, pāmojjattho pāmojjānisaṃso”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an10.1:4.1"
msgid "“Pāmojjaṃ pana, bhante, kimatthiyaṃ kimānisaṃsan”ti?"
msgstr ""

msgctxt "an10.1:4.2"
msgid "“Pāmojjaṃ kho, ānanda, pītatthaṃ pītānisaṃsan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "an10.1:5.1"
msgid "“Pīti pana, bhante, kimatthiyā kimānisaṃsā”ti?"
msgstr ""

msgctxt "an10.1:5.2"
msgid "“Pīti kho, ānanda, passaddhatthā passaddhānisaṃsā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "an10.1:6.1"
msgid "“Passaddhi pana, bhante, kimatthiyā kimānisaṃsā”ti?"
msgstr ""

msgctxt "an10.1:6.2"
msgid "“Passaddhi kho, ānanda, sukhatthā sukhānisaṃsā”ti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.2"></a>
#. <a class="sc" id="sc7"></a>
msgctxt "an10.1:7.1"
msgid "“Sukhaṃ pana, bhante, kimatthiyaṃ kimānisaṃsan”ti?"
msgstr ""

msgctxt "an10.1:7.2"
msgid "“Sukhaṃ kho, ānanda, samādhatthaṃ samādhānisaṃsan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "an10.1:8.1"
msgid "“Samādhi pana, bhante, kimatthiyo kimānisaṃso”ti?"
msgstr ""

msgctxt "an10.1:8.2"
msgid ""
"“Samādhi kho, ānanda, yathābhūtañāṇadassanattho "
"yathābhūtañāṇadassanānisaṃso”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an10.1:9.1"
msgid "“Yathābhūtañāṇadassanaṃ pana, bhante, kimatthiyaṃ kimānisaṃsan”ti?"
msgstr ""

msgctxt "an10.1:9.2"
msgid ""
"“Yathābhūtañāṇadassanaṃ kho, ānanda, nibbidāvirāgatthaṃ "
"nibbidāvirāgānisaṃsan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "an10.1:10.1"
msgid "“Nibbidāvirāgo pana, bhante, kimatthiyo kimānisaṃso”ti?"
msgstr ""

#. VAR: vimuttiñāṇadassanānisaṃso → … nisaṃsoti (bj, pts1, mr)
msgctxt "an10.1:10.2"
msgid ""
"“Nibbidāvirāgo kho, ānanda, vimuttiñāṇadassanattho vimuttiñāṇadassanānisaṃso."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "an10.1:11.1"
msgid ""
"Iti kho, ānanda, kusalāni sīlāni avippaṭisāratthāni avippaṭisārānisaṃsāni;"
msgstr ""

msgctxt "an10.1:11.2"
msgid "avippaṭisāro pāmojjattho pāmojjānisaṃso;"
msgstr ""

msgctxt "an10.1:11.3"
msgid "pāmojjaṃ pītatthaṃ pītānisaṃsaṃ;"
msgstr ""

msgctxt "an10.1:11.4"
msgid "pīti passaddhatthā passaddhānisaṃsā;"
msgstr ""

msgctxt "an10.1:11.5"
msgid "passaddhi sukhatthā sukhānisaṃsā;"
msgstr ""

msgctxt "an10.1:11.6"
msgid "sukhaṃ samādhatthaṃ samādhānisaṃsaṃ;"
msgstr ""

msgctxt "an10.1:11.7"
msgid "samādhi yathābhūtañāṇadassanattho yathābhūtañāṇadassanānisaṃso;"
msgstr ""

msgctxt "an10.1:11.8"
msgid "yathābhūtañāṇadassanaṃ nibbidāvirāgatthaṃ nibbidāvirāgānisaṃsaṃ;"
msgstr ""

msgctxt "an10.1:11.9"
msgid "nibbidāvirāgo vimuttiñāṇadassanattho vimuttiñāṇadassanānisaṃso."
msgstr ""

#. VAR: aggāya parentī”ti → arahattāya pūrentīti (s1-3)
msgctxt "an10.1:11.10"
msgid "Iti kho, ānanda, kusalāni sīlāni anupubbena aggāya parentī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.1:11.11"
msgid "Paṭhamaṃ."
msgstr ""
