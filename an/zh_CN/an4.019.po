#. <section class="sutta" id="an4.19"><article><div class="hgroup"><p class="division">
msgctxt "an4.19:0.1"
msgid "Aṅguttara Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "an4.19:0.2"
msgid "2. Caravagga"
msgstr ""

#. </p><h1>
#. VAR: agatināgatisuttaṃ (bj)
msgctxt "an4.19:0.3"
msgid "19. Tatiyaagatisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.19"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "an4.19:1.1"
msgid "“Cattārimāni, bhikkhave, agatigamanāni."
msgstr ""

msgctxt "an4.19:1.2"
msgid "Katamāni cattāri?"
msgstr ""

msgctxt "an4.19:1.3"
msgid ""
"Chandāgatiṃ gacchati, dosāgatiṃ gacchati, mohāgatiṃ gacchati, bhayāgatiṃ "
"gacchati—"
msgstr ""

msgctxt "an4.19:1.4"
msgid "imāni kho, bhikkhave, cattāri agatigamanāni."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "an4.19:2.1"
msgid "Cattārimāni, bhikkhave, nāgatigamanāni."
msgstr ""

msgctxt "an4.19:2.2"
msgid "Katamāni cattāri?"
msgstr ""

msgctxt "an4.19:2.3"
msgid ""
"Na chandāgatiṃ gacchati, na dosāgatiṃ gacchati, na mohāgatiṃ gacchati, na "
"bhayāgatiṃ gacchati—"
msgstr ""

msgctxt "an4.19:2.4"
msgid "imāni kho, bhikkhave, cattāri nāgatigamanānīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "an4.19:3.1"
msgid "Chandā dosā bhayā mohā,"
msgstr ""

#. <br>
msgctxt "an4.19:3.2"
msgid "yo dhammaṃ ativattati;"
msgstr ""

#. <br>
msgctxt "an4.19:3.3"
msgid "Nihīyati tassa yaso,"
msgstr ""

#. <br>
msgctxt "an4.19:3.4"
msgid "kāḷapakkheva candimā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "an4.19:4.1"
msgid "Chandā dosā bhayā mohā,"
msgstr ""

#. <br>
msgctxt "an4.19:4.2"
msgid "yo dhammaṃ nātivattati;"
msgstr ""

#. <br>
msgctxt "an4.19:4.3"
msgid "Āpūrati tassa yaso,"
msgstr ""

#. <br>
msgctxt "an4.19:4.4"
msgid "sukkapakkheva candimā”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "an4.19:4.5"
msgid "Navamaṃ."
msgstr ""
