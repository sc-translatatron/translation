msgctxt "an5.306:0.2"
msgid "29. Rāgapeyyāla"
msgstr ""

#. </p><h1>
msgctxt "an5.306:0.3"
msgid "306"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "an5.306:1.1"
msgid "“Rāgassa, bhikkhave, abhiññāya pañca dhammā bhāvetabbā."
msgstr ""

msgctxt "an5.306:1.2"
msgid "Katame pañca?"
msgstr ""

msgctxt "an5.306:1.3"
msgid ""
"Saddhindriyaṃ, vīriyindriyaṃ, satindriyaṃ, samādhindriyaṃ, paññindriyaṃ—"
msgstr ""

msgctxt "an5.306:1.4"
msgid "rāgassa, bhikkhave, abhiññāya ime pañca dhammā bhāvetabbā”ti. (4)"
msgstr ""
