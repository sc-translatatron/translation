msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "an10.106:9.1"
msgid "Sammāñāṇissa, bhikkhave, micchāñāṇaṃ nijjiṇṇaṃ hoti;"
msgstr ""

msgctxt "an10.106:9.2"
msgid ""
"ye ca micchāñāṇapaccayā aneke pāpakā akusalā dhammā sambhavanti te cassa "
"nijjiṇṇā honti;"
msgstr ""

msgctxt "an10.106:9.3"
msgid "sammāñāṇapaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti. (9)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "an10.106:10.1"
msgid "Sammāvimuttissa, bhikkhave, micchāvimutti nijjiṇṇā hoti;"
msgstr ""

msgctxt "an10.106:10.2"
msgid ""
"ye ca micchāvimuttipaccayā aneke pāpakā akusalā dhammā sambhavanti te cassa "
"nijjiṇṇā honti;"
msgstr ""

msgctxt "an10.106:10.3"
msgid ""
"sammāvimuttipaccayā ca aneke kusalā dhammā bhāvanāpāripūriṃ gacchanti. (10)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "an10.106:11.1"
msgid "Imāni kho, bhikkhave, dasa nijjaravatthūnī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "an10.106:11.2"
msgid "Chaṭṭhaṃ."
msgstr ""
