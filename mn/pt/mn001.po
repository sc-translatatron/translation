#. <section class="sutta" id="mn1"><article><div class="hgroup"><p class="division">
msgctxt "mn1:0.1"
msgid "Majjhima Nikāya 1"
msgstr ""

#. </p><h1>
msgctxt "mn1:0.2"
msgid "Mūlapariyāyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="nya" id="nya1"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli1.1"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "mn1:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "mn1:1.2"
msgid "ekaṃ samayaṃ bhagavā ukkaṭṭhāyaṃ viharati subhagavane sālarājamūle."
msgstr ""

msgctxt "mn1:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "mn1:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "mn1:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "mn1:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. <a class="nya" id="nya2"></a>
msgctxt "mn1:2.1"
msgid "“sabbadhammamūlapariyāyaṃ vo, bhikkhave, desessāmi."
msgstr ""

msgctxt "mn1:2.2"
msgid "Taṃ suṇātha, sādhukaṃ manasi karotha, bhāsissāmī”ti."
msgstr ""

msgctxt "mn1:2.3"
msgid "“Evaṃ, bhante”ti kho te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "mn1:2.4"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya3"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "mn1:3.1"
msgid ""
"“Idha, bhikkhave, assutavā puthujjano ariyānaṃ adassāvī ariyadhammassa "
"akovido ariyadhamme avinīto, sappurisānaṃ adassāvī sappurisadhammassa "
"akovido sappurisadhamme avinīto—"
msgstr ""

#. VAR: pathaviṃ → paṭhaviṃ (bj, s1-3, km, pts1)
msgctxt "mn1:3.2"
msgid "pathaviṃ pathavito sañjānāti;"
msgstr ""

msgctxt "mn1:3.3"
msgid ""
"pathaviṃ pathavito saññatvā pathaviṃ maññati, pathaviyā maññati, pathavito "
"maññati, pathaviṃ meti maññati, pathaviṃ abhinandati."
msgstr ""

msgctxt "mn1:3.4"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "mn1:3.5"
msgid "‘Apariññātaṃ tassā’ti vadāmi. (1)"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya4"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "mn1:4.1"
msgid "Āpaṃ āpato sañjānāti;"
msgstr ""

msgctxt "mn1:4.2"
msgid ""
