#. <section class="sutta" id="mn3"><article><div class="hgroup"><p class="division">
msgctxt "mn3:0.1"
msgid "Majjhima Nikāya 3"
msgstr ""

#. </p><h1>
msgctxt "mn3:0.2"
msgid "Dhammadāyādasutta"
msgstr ""

#. </h1></div><p>
#. <a class="nya" id="nya1"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "mn3:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "mn3:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "mn3:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "mn3:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "mn3:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "mn3:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya2"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "mn3:2.1"
msgid "“Dhammadāyādā me, bhikkhave, bhavatha, mā āmisadāyādā."
msgstr ""

msgctxt "mn3:2.2"
msgid "Atthi me tumhesu anukampā:"
msgstr ""

msgctxt "mn3:2.3"
msgid "‘kinti me sāvakā dhammadāyādā bhaveyyuṃ, no āmisadāyādā’ti."
msgstr ""

# reading ādissa for ādiya
#. VAR: ādiyā → ādissā (bj, s1-3, pts1)
msgctxt "mn3:2.4"
msgid ""
"Tumhe ca me, bhikkhave, āmisadāyādā bhaveyyātha no dhammadāyādā, tumhepi "
"tena ādiyā bhaveyyātha:"
msgstr ""

msgctxt "mn3:2.5"
msgid "‘āmisadāyādā satthusāvakā viharanti, no dhammadāyādā’ti;"
msgstr ""

msgctxt "mn3:2.6"
msgid "ahampi tena ādiyo bhaveyyaṃ:"
msgstr ""

msgctxt "mn3:2.7"
msgid "‘āmisadāyādā satthusāvakā viharanti, no dhammadāyādā’ti."
msgstr ""

msgctxt "mn3:2.8"
msgid ""
"Tumhe ca me, bhikkhave, dhammadāyādā bhaveyyātha, no āmisadāyādā, tumhepi "
"tena na ādiyā bhaveyyātha:"
msgstr ""

msgctxt "mn3:2.9"
msgid "‘dhammadāyādā satthusāvakā viharanti, no āmisadāyādā’ti;"
msgstr ""

msgctxt "mn3:2.10"
msgid "ahampi tena na ādiyo bhaveyyaṃ:"
msgstr ""

msgctxt "mn3:2.11"
msgid "‘dhammadāyādā satthusāvakā viharanti, no āmisadāyādā’ti."
msgstr ""

msgctxt "mn3:2.12"
msgid "Tasmātiha me, bhikkhave, dhammadāyādā bhavatha, mā āmisadāyādā."
msgstr ""

msgctxt "mn3:2.13"
msgid "Atthi me tumhesu anukampā:"
msgstr ""

msgctxt "mn3:2.14"
msgid "‘kinti me sāvakā dhammadāyādā bhaveyyuṃ, no āmisadāyādā’ti."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya3"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli1.13"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "mn3:3.1"
msgid ""
"Idhāhaṃ, bhikkhave, bhuttāvī assaṃ pavārito paripuṇṇo pariyosito suhito "
"yāvadattho;"
msgstr ""

# BB has left-over, which is a little misleading. Normally I’d use that for sesa. But here it is not necessarily the leftovers from the bowl. More likely, since, additional food has been refused, it is extra food in the offering dishes.
msgctxt "mn3:3.2"
msgid "siyā ca me piṇḍapāto atirekadhammo chaḍḍanīyadhammo."
msgstr ""

msgctxt "mn3:3.3"
msgid "Atha dve bhikkhū āgaccheyyuṃ jighacchādubbalyaparetā."
msgstr ""

msgctxt "mn3:3.4"
msgid "Tyāhaṃ evaṃ vadeyyaṃ:"
msgstr ""

msgctxt "mn3:3.5"
msgid ""
"‘ahaṃ khomhi, bhikkhave, bhuttāvī pavārito paripuṇṇo pariyosito suhito "
"yāvadattho;"
msgstr ""

msgctxt "mn3:3.6"
msgid "atthi ca me ayaṃ piṇḍapāto atirekadhammo chaḍḍanīyadhammo."
msgstr ""

#. VAR: tumhe bhuñjissatha → sace tumhe na bhuñjissatha (bj, s1-3, pts1)
msgctxt "mn3:3.7"
msgid ""
"Sace ākaṅkhatha, bhuñjatha, no ce tumhe bhuñjissatha, idānāhaṃ appaharite vā "
"chaḍḍessāmi, appāṇake vā udake opilāpessāmī’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "mn3:3.8"
msgid "Tatrekassa bhikkhuno evamassa:"
msgstr ""

msgctxt "mn3:3.9"
msgid "‘bhagavā kho bhuttāvī pavārito paripuṇṇo pariyosito suhito yāvadattho;"
msgstr ""

msgctxt "mn3:3.10"
msgid "atthi cāyaṃ bhagavato piṇḍapāto atirekadhammo chaḍḍanīyadhammo."
msgstr ""

msgctxt "mn3:3.11"
msgid ""
"Sace mayaṃ na bhuñjissāma, idāni bhagavā appaharite vā chaḍḍessati, appāṇake "
"vā udake opilāpessati."
msgstr ""

msgctxt "mn3:3.12"
msgid "Vuttaṃ kho panetaṃ bhagavatā:"
msgstr ""

msgctxt "mn3:3.13"
msgid "“dhammadāyādā me, bhikkhave, bhavatha, mā āmisadāyādā”ti."
msgstr ""

msgctxt "mn3:3.14"
msgid "Āmisaññataraṃ kho panetaṃ, yadidaṃ piṇḍapāto."
msgstr ""

#. VAR: rattindivaṃ → rattidivaṃ (mr)
msgctxt "mn3:3.15"
msgid ""
"Yannūnāhaṃ imaṃ piṇḍapātaṃ abhuñjitvā imināva jighacchādubbalyena evaṃ imaṃ "
"rattindivaṃ vītināmeyyan’ti."
msgstr ""

msgctxt "mn3:3.16"
msgid ""
"So taṃ piṇḍapātaṃ abhuñjitvā teneva jighacchādubbalyena evaṃ taṃ rattindivaṃ "
"vītināmeyya."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "mn3:3.17"
msgid "Atha dutiyassa bhikkhuno evamassa:"
msgstr ""

msgctxt "mn3:3.18"
msgid "‘bhagavā kho bhuttāvī pavārito paripuṇṇo pariyosito suhito yāvadattho;"
msgstr ""

msgctxt "mn3:3.19"
msgid "atthi cāyaṃ bhagavato piṇḍapāto atirekadhammo chaḍḍanīyadhammo."
msgstr ""

msgctxt "mn3:3.20"
msgid ""
"Sace mayaṃ na bhuñjissāma, idāni bhagavā appaharite vā chaḍḍessati, appāṇake "
"vā udake opilāpessati."
msgstr ""

#. VAR: paṭivinodetvā → paṭivinetvā (bj, pts1)
msgctxt "mn3:3.21"
msgid ""
"Yannūnāhaṃ imaṃ piṇḍapātaṃ bhuñjitvā jighacchādubbalyaṃ paṭivinodetvā evaṃ "
"imaṃ rattindivaṃ vītināmeyyan’ti."
msgstr ""

msgctxt "mn3:3.22"
msgid ""
"So taṃ piṇḍapātaṃ bhuñjitvā jighacchādubbalyaṃ paṭivinodetvā evaṃ taṃ "
"rattindivaṃ vītināmeyya."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "mn3:3.23"
msgid ""
"Kiñcāpi so, bhikkhave, bhikkhu taṃ piṇḍapātaṃ bhuñjitvā jighacchādubbalyaṃ "
"paṭivinodetvā evaṃ taṃ rattindivaṃ vītināmeyya, atha kho asuyeva me purimo "
"bhikkhu pujjataro ca pāsaṃsataro ca."
msgstr ""

msgctxt "mn3:3.24"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "mn3:3.25"
msgid ""
"Tañhi tassa, bhikkhave, bhikkhuno dīgharattaṃ appicchatāya santuṭṭhiyā "
"sallekhāya subharatāya vīriyārambhāya saṃvattissati."
msgstr ""

msgctxt "mn3:3.26"
msgid "Tasmātiha me, bhikkhave, dhammadāyādā bhavatha, mā āmisadāyādā."
msgstr ""

msgctxt "mn3:3.27"
msgid "Atthi me tumhesu anukampā:"
msgstr ""

msgctxt "mn3:3.28"
msgid "‘kinti me sāvakā dhammadāyādā bhaveyyuṃ, no āmisadāyādā’”ti."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya4"></a>
#. <a class="sc" id="sc7"></a>
msgctxt "mn3:4.1"
msgid "Idamavoca bhagavā."
msgstr ""

#. VAR: vatvāna → vatvā (bj, pts1)
msgctxt "mn3:4.2"
msgid "Idaṃ vatvāna sugato uṭṭhāyāsanā vihāraṃ pāvisi."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.14"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "mn3:4.3"
msgid "Tatra kho āyasmā sāriputto acirapakkantassa bhagavato bhikkhū āmantesi:"
msgstr ""

msgctxt "mn3:4.4"
msgid "“āvuso bhikkhave”ti."
msgstr ""

msgctxt "mn3:4.5"
msgid "“Āvuso”ti kho te bhikkhū āyasmato sāriputtassa paccassosuṃ."
msgstr ""

msgctxt "mn3:4.6"
msgid "Āyasmā sāriputto etadavoca:"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya5"></a>
#. <a class="sc" id="sc9"></a>
msgctxt "mn3:5.1"
msgid ""
"“Kittāvatā nu kho, āvuso, satthu pavivittassa viharato sāvakā vivekaṃ "
"nānusikkhanti, kittāvatā ca pana satthu pavivittassa viharato sāvakā "
"vivekamanusikkhantī”ti?"
msgstr ""

msgctxt "mn3:5.2"
msgid ""
"“Dūratopi kho mayaṃ, āvuso, āgacchāma āyasmato sāriputtassa santike etassa "
"bhāsitassa atthamaññātuṃ."
msgstr ""

msgctxt "mn3:5.3"
msgid "Sādhu vatāyasmantaṃyeva sāriputtaṃ paṭibhātu etassa bhāsitassa attho;"
msgstr ""

msgctxt "mn3:5.4"
msgid "āyasmato sāriputtassa sutvā bhikkhū dhāressantī”ti."
msgstr ""

msgctxt "mn3:5.5"
msgid "“Tena hāvuso, suṇātha, sādhukaṃ manasi karotha, bhāsissāmī”ti."
msgstr ""

msgctxt "mn3:5.6"
msgid "“Evamāvuso”ti kho te bhikkhū āyasmato sāriputtassa paccassosuṃ."
msgstr ""

msgctxt "mn3:5.7"
msgid "Āyasmā sāriputto etadavoca:"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya6"></a>
#. <a class="sc" id="sc10"></a>
msgctxt "mn3:6.1"
msgid ""
"“Kittāvatā nu kho, āvuso, satthu pavivittassa viharato sāvakā vivekaṃ "
"nānusikkhanti?"
msgstr ""

msgctxt "mn3:6.2"
msgid "Idhāvuso, satthu pavivittassa viharato sāvakā vivekaṃ nānusikkhanti, "
msgstr ""

msgctxt "mn3:6.3"
msgid "yesañca dhammānaṃ satthā pahānamāha, te ca dhamme nappajahanti, "
msgstr ""

#. VAR: bāhulikā → bāhullikā (s1-3)
msgctxt "mn3:6.4"
msgid ""
"bāhulikā ca honti, sāthalikā, okkamane pubbaṅgamā, paviveke nikkhittadhurā."
msgstr ""

msgctxt "mn3:6.5"
msgid "Tatrāvuso, therā bhikkhū tīhi ṭhānehi gārayhā bhavanti."
msgstr ""

msgctxt "mn3:6.6"
msgid "‘Satthu pavivittassa viharato sāvakā vivekaṃ nānusikkhantī’ti—"
msgstr ""

msgctxt "mn3:6.7"
msgid "iminā paṭhamena ṭhānena therā bhikkhū gārayhā bhavanti."
msgstr ""

msgctxt "mn3:6.8"
msgid "‘Yesañca dhammānaṃ satthā pahānamāha te ca dhamme nappajahantī’ti—"
msgstr ""

msgctxt "mn3:6.9"
msgid "iminā dutiyena ṭhānena therā bhikkhū gārayhā bhavanti."
msgstr ""

msgctxt "mn3:6.10"
msgid ""
"‘Bāhulikā ca, sāthalikā, okkamane pubbaṅgamā, paviveke nikkhittadhurā’ti—"
msgstr ""

msgctxt "mn3:6.11"
msgid "iminā tatiyena ṭhānena therā bhikkhū gārayhā bhavanti."
msgstr ""

msgctxt "mn3:6.12"
msgid "Therā, āvuso, bhikkhū imehi tīhi ṭhānehi gārayhā bhavanti."
msgstr ""

msgctxt "mn3:6.13"
msgid "Tatrāvuso, majjhimā bhikkhū … pe …"
msgstr ""

msgctxt "mn3:6.14"
msgid "navā bhikkhū tīhi ṭhānehi gārayhā bhavanti."
msgstr ""

msgctxt "mn3:6.15"
msgid "‘Satthu pavivittassa viharato sāvakā vivekaṃ nānusikkhantī’ti—"
msgstr ""

msgctxt "mn3:6.16"
msgid "iminā paṭhamena ṭhānena navā bhikkhū gārayhā bhavanti."
msgstr ""

msgctxt "mn3:6.17"
msgid "‘Yesañca dhammānaṃ satthā pahānamāha te ca dhamme nappajahantī’ti—"
msgstr ""

msgctxt "mn3:6.18"
msgid "iminā dutiyena ṭhānena navā bhikkhū gārayhā bhavanti."
msgstr ""

msgctxt "mn3:6.19"
msgid ""
"‘Bāhulikā ca honti, sāthalikā, okkamane pubbaṅgamā, paviveke "
"nikkhittadhurā’ti—"
msgstr ""

msgctxt "mn3:6.20"
msgid "iminā tatiyena ṭhānena navā bhikkhū gārayhā bhavanti."
msgstr ""

msgctxt "mn3:6.21"
msgid "Navā, āvuso, bhikkhū imehi tīhi ṭhānehi gārayhā bhavanti."
msgstr ""

msgctxt "mn3:6.22"
msgid ""
"Ettāvatā kho, āvuso, satthu pavivittassa viharato sāvakā vivekaṃ "
"nānusikkhanti."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya7"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli1.15"></a>
#. <a class="sc" id="sc11"></a>
msgctxt "mn3:7.1"
msgid ""
"Kittāvatā ca panāvuso, satthu pavivittassa viharato sāvakā "
"vivekamanusikkhanti?"
msgstr ""

msgctxt "mn3:7.2"
msgid "Idhāvuso, satthu pavivittassa viharato sāvakā vivekamanusikkhanti—"
msgstr ""

msgctxt "mn3:7.3"
msgid "yesañca dhammānaṃ satthā pahānamāha te ca dhamme pajahanti;"
msgstr ""

msgctxt "mn3:7.4"
msgid ""
"na ca bāhulikā honti, na sāthalikā okkamane nikkhittadhurā paviveke "
"pubbaṅgamā."
msgstr ""

msgctxt "mn3:7.5"
msgid "Tatrāvuso, therā bhikkhū tīhi ṭhānehi pāsaṃsā bhavanti."
msgstr ""

msgctxt "mn3:7.6"
msgid "‘Satthu pavivittassa viharato sāvakā vivekamanusikkhantī’ti—"
msgstr ""

msgctxt "mn3:7.7"
msgid "iminā paṭhamena ṭhānena therā bhikkhū pāsaṃsā bhavanti."
msgstr ""

msgctxt "mn3:7.8"
msgid "‘Yesañca dhammānaṃ satthā pahānamāha te ca dhamme pajahantī’ti—"
msgstr ""

msgctxt "mn3:7.9"
msgid "iminā dutiyena ṭhānena therā bhikkhū pāsaṃsā bhavanti."
msgstr ""

msgctxt "mn3:7.10"
msgid ""
"‘Na ca bāhulikā, na sāthalikā okkamane nikkhittadhurā paviveke pubbaṅgamā’ti—"
msgstr ""

msgctxt "mn3:7.11"
msgid "iminā tatiyena ṭhānena therā bhikkhū pāsaṃsā bhavanti."
msgstr ""

msgctxt "mn3:7.12"
msgid "Therā, āvuso, bhikkhū imehi tīhi ṭhānehi pāsaṃsā bhavanti."
msgstr ""

msgctxt "mn3:7.13"
msgid "Tatrāvuso, majjhimā bhikkhū … pe …"
msgstr ""

msgctxt "mn3:7.14"
msgid "navā bhikkhū tīhi ṭhānehi pāsaṃsā bhavanti."
msgstr ""

msgctxt "mn3:7.15"
msgid "‘Satthu pavivittassa viharato sāvakā vivekamanusikkhantī’ti—"
msgstr ""

msgctxt "mn3:7.16"
msgid "iminā paṭhamena ṭhānena navā bhikkhū pāsaṃsā bhavanti."
msgstr ""

msgctxt "mn3:7.17"
msgid "‘Yesañca dhammānaṃ satthā pahānamāha te ca dhamme pajahantī’ti—"
msgstr ""

msgctxt "mn3:7.18"
msgid "iminā dutiyena ṭhānena navā bhikkhū pāsaṃsā bhavanti."
msgstr ""

msgctxt "mn3:7.19"
msgid ""
"‘Na ca bāhulikā, na sāthalikā okkamane nikkhittadhurā paviveke pubbaṅgamā’ti—"
msgstr ""

msgctxt "mn3:7.20"
msgid "iminā tatiyena ṭhānena navā bhikkhū pāsaṃsā bhavanti."
msgstr ""

msgctxt "mn3:7.21"
msgid "Navā, āvuso, bhikkhū imehi tīhi ṭhānehi pāsaṃsā bhavanti."
msgstr ""

msgctxt "mn3:7.22"
msgid ""
"Ettāvatā kho, āvuso, satthu pavivittassa viharato sāvakā vivekamanusikkhanti."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya8"></a>
#. <a class="sc" id="sc12"></a>
msgctxt "mn3:8.1"
msgid "Tatrāvuso, lobho ca pāpako doso ca pāpako."
msgstr ""

msgctxt "mn3:8.2"
msgid ""
"Lobhassa ca pahānāya dosassa ca pahānāya atthi majjhimā paṭipadā "
"cakkhukaraṇī ñāṇakaraṇī upasamāya abhiññāya sambodhāya nibbānāya saṃvattati."
msgstr ""

msgctxt "mn3:8.3"
msgid ""
"Katamā ca sā, āvuso, majjhimā paṭipadā cakkhukaraṇī ñāṇakaraṇī upasamāya "
"abhiññāya sambodhāya nibbānāya saṃvattati?"
msgstr ""

#. VAR: seyyathidaṃ → seyyathīdaṃ (bj, s1-3, pts1)
msgctxt "mn3:8.4"
msgid "Ayameva ariyo aṭṭhaṅgiko maggo, seyyathidaṃ—"
msgstr ""

msgctxt "mn3:8.5"
msgid ""
"sammādiṭṭhi sammāsaṅkappo sammāvācā sammākammanto sammāājīvo sammāvāyāmo "
"sammāsati sammāsamādhi."
msgstr ""

msgctxt "mn3:8.6"
msgid ""
"Ayaṃ kho sā, āvuso, majjhimā paṭipadā cakkhukaraṇī ñāṇakaraṇī upasamāya "
"abhiññāya sambodhāya nibbānāya saṃvattati."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya9-15"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli1.16"></a>
#. <a class="sc" id="sc13"></a>
msgctxt "mn3:9-15.1"
msgid "Tatrāvuso, kodho ca pāpako upanāho ca pāpako … pe …"
msgstr ""

msgctxt "mn3:9-15.2"
msgid "makkho ca pāpako paḷāso ca pāpako, "
msgstr ""

msgctxt "mn3:9-15.3"
msgid "issā ca pāpikā maccherañca pāpakaṃ, "
msgstr ""

msgctxt "mn3:9-15.4"
msgid "māyā ca pāpikā sāṭheyyañca pāpakaṃ, "
msgstr ""

msgctxt "mn3:9-15.5"
msgid "thambho ca pāpako sārambho ca pāpako, "
msgstr ""

msgctxt "mn3:9-15.6"
msgid "māno ca pāpako atimāno ca pāpako, "
msgstr ""

msgctxt "mn3:9-15.7"
msgid "mado ca pāpako pamādo ca pāpako."
msgstr ""

msgctxt "mn3:9-15.8"
msgid ""
"Madassa ca pahānāya pamādassa ca pahānāya atthi majjhimā paṭipadā "
"cakkhukaraṇī ñāṇakaraṇī upasamāya abhiññāya sambodhāya nibbānāya saṃvattati."
msgstr ""

msgctxt "mn3:9-15.9"
msgid ""
"Katamā ca sā, āvuso, majjhimā paṭipadā cakkhukaraṇī ñāṇakaraṇī upasamāya "
"abhiññāya sambodhāya nibbānāya saṃvattati?"
msgstr ""

msgctxt "mn3:9-15.10"
msgid "Ayameva ariyo aṭṭhaṅgiko maggo, seyyathidaṃ—"
msgstr ""

msgctxt "mn3:9-15.11"
msgid ""
"sammādiṭṭhi sammāsaṅkappo sammāvācā sammākammanto sammāājīvo sammāvāyāmo "
"sammāsati sammāsamādhi."
msgstr ""

msgctxt "mn3:9-15.12"
msgid ""
"Ayaṃ kho sā, āvuso, majjhimā paṭipadā cakkhukaraṇī ñāṇakaraṇī upasamāya "
"abhiññāya sambodhāya nibbānāya saṃvattatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc14"></a>
msgctxt "mn3:9-15.13"
msgid "Idamavocāyasmā sāriputto."
msgstr ""

msgctxt "mn3:9-15.14"
msgid "Attamanā te bhikkhū āyasmato sāriputtassa bhāsitaṃ abhinandunti."
msgstr ""

#. </p><p class="endsection">
msgctxt "mn3:9-15.15"
msgid "Dhammadāyādasuttaṃ niṭṭhitaṃ tatiyaṃ."
msgstr ""
