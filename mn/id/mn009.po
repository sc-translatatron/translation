msgid ""
"ettāvatāpi kho, āvuso, ariyasāvako sammādiṭṭhi hoti, ujugatāssa diṭṭhi, "
"dhamme aveccappasādena samannāgato, āgato imaṃ saddhamman”ti."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya56-58"></a>
#. <a class="sc" id="sc32"></a>
msgctxt "mn9:56-58.1"
msgid "“Sādhāvuso”ti kho … pe … apucchuṃ—"
msgstr ""

msgctxt "mn9:56-58.2"
msgid "siyā panāvuso … pe …"
msgstr ""

msgctxt "mn9:56-58.3"
msgid "“siyā, āvuso."
msgstr ""

msgctxt "mn9:56-58.4"
msgid ""
"Yato kho, āvuso, ariyasāvako viññāṇañca pajānāti, viññāṇasamudayañca "
"pajānāti, viññāṇanirodhañca pajānāti, viññāṇanirodhagāminiṃ paṭipadañca "
"pajānāti—"
msgstr ""

msgctxt "mn9:56-58.5"
msgid ""
"ettāvatāpi kho, āvuso, ariyasāvako sammādiṭṭhi hoti, ujugatāssa diṭṭhi, "
"dhamme aveccappasādena samannāgato, āgato imaṃ saddhammaṃ."
msgstr ""

msgctxt "mn9:56-58.6"
msgid ""
"Katamaṃ panāvuso, viññāṇaṃ, katamo viññāṇasamudayo, katamo viññāṇanirodho, "
"katamā viññāṇanirodhagāminī paṭipadā?"
msgstr ""

msgctxt "mn9:56-58.7"
msgid "Chayime, āvuso, viññāṇakāyā—"
msgstr ""

msgctxt "mn9:56-58.8"
msgid ""
"cakkhuviññāṇaṃ, sotaviññāṇaṃ, ghānaviññāṇaṃ, jivhāviññāṇaṃ, kāyaviññāṇaṃ, "
"manoviññāṇaṃ."
msgstr ""

msgctxt "mn9:56-58.9"
msgid ""
"Saṅkhārasamudayā viññāṇasamudayo, saṅkhāranirodhā viññāṇanirodho, ayameva "
"ariyo aṭṭhaṅgiko maggo viññāṇanirodhagāminī paṭipadā, seyyathidaṃ—"
msgstr ""

msgctxt "mn9:56-58.10"
msgid "sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya59"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli1.54"></a>
#. <a class="sc" id="sc33"></a>
msgctxt "mn9:59.1"
msgid ""
"Yato kho, āvuso, ariyasāvako evaṃ viññāṇaṃ pajānāti, evaṃ viññāṇasamudayaṃ "
"pajānāti, evaṃ viññāṇanirodhaṃ pajānāti, evaṃ viññāṇanirodhagāminiṃ "
"paṭipadaṃ pajānāti, so sabbaso rāgānusayaṃ pahāya … pe … dukkhassantakaro "
"hoti—"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya60-62"></a>
#. <a class="sc" id="sc34"></a>
msgctxt "mn9:60-62.1"
msgid ""
"ettāvatāpi kho, āvuso, ariyasāvako sammādiṭṭhi hoti, ujugatāssa diṭṭhi, "
"dhamme aveccappasādena samannāgato, āgato imaṃ saddhamman”ti. "
msgstr ""

msgctxt "mn9:60-62.2"
msgid "“Sādhāvuso”ti kho … pe … apucchuṃ—"
msgstr ""

msgctxt "mn9:60-62.3"
msgid "siyā panāvuso … pe …"
msgstr ""

msgctxt "mn9:60-62.4"
msgid "“siyā, āvuso."
msgstr ""

msgctxt "mn9:60-62.5"
msgid ""
"Yato kho, āvuso, ariyasāvako saṅkhāre ca pajānāti, saṅkhārasamudayañca "
"pajānāti, saṅkhāranirodhañca pajānāti, saṅkhāranirodhagāminiṃ paṭipadañca "
"pajānāti—"
msgstr ""

msgctxt "mn9:60-62.6"
msgid ""
"ettāvatāpi kho, āvuso, ariyasāvako sammādiṭṭhi hoti, ujugatāssa diṭṭhi, "
"dhamme aveccappasādena samannāgato, āgato imaṃ saddhammaṃ."
msgstr ""

msgctxt "mn9:60-62.7"
msgid ""
"Katame panāvuso, saṅkhārā, katamo saṅkhārasamudayo, katamo saṅkhāranirodho, "
"katamā saṅkhāranirodhagāminī paṭipadā?"
msgstr ""

msgctxt "mn9:60-62.8"
msgid "Tayome, āvuso, saṅkhārā—"
msgstr ""

msgctxt "mn9:60-62.9"
msgid "kāyasaṅkhāro, vacīsaṅkhāro, cittasaṅkhāro."
msgstr ""

msgctxt "mn9:60-62.10"
msgid ""
"Avijjāsamudayā saṅkhārasamudayo, avijjānirodhā saṅkhāranirodho, ayameva "
"ariyo aṭṭhaṅgiko maggo saṅkhāranirodhagāminī paṭipadā, seyyathidaṃ—"
msgstr ""

msgctxt "mn9:60-62.11"
msgid "sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya63"></a>
#. <a class="sc" id="sc35"></a>
msgctxt "mn9:63.1"
msgid ""
"Yato kho, āvuso, ariyasāvako evaṃ saṅkhāre pajānāti, evaṃ saṅkhārasamudayaṃ "
"pajānāti, evaṃ saṅkhāranirodhaṃ pajānāti, evaṃ saṅkhāranirodhagāminiṃ "
"paṭipadaṃ pajānāti, so sabbaso rāgānusayaṃ pahāya, paṭighānusayaṃ "
"paṭivinodetvā, ‘asmī’ti diṭṭhimānānusayaṃ samūhanitvā, avijjaṃ pahāya vijjaṃ "
"uppādetvā, diṭṭheva dhamme dukkhassantakaro hoti—"
msgstr ""

msgctxt "mn9:63.2"
msgid ""
"ettāvatāpi kho, āvuso, ariyasāvako sammādiṭṭhi hoti, ujugatāssa diṭṭhi, "
"dhamme aveccappasādena samannāgato, āgato imaṃ saddhamman”ti."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya64-66"></a>
#. <a class="sc" id="sc36"></a>
msgctxt "mn9:64-66.1"
msgid "“Sādhāvuso”ti kho … pe … apucchuṃ—"
msgstr ""

msgctxt "mn9:64-66.2"
msgid "siyā panāvuso … pe …"
msgstr ""

msgctxt "mn9:64-66.3"
msgid "“siyā, āvuso."
msgstr ""

msgctxt "mn9:64-66.4"
msgid ""
"Yato kho, āvuso, ariyasāvako avijjañca pajānāti, avijjāsamudayañca pajānāti, "
"avijjānirodhañca pajānāti, avijjānirodhagāminiṃ paṭipadañca pajānāti—"
msgstr ""

msgctxt "mn9:64-66.5"
msgid ""
"ettāvatāpi kho, āvuso, ariyasāvako sammādiṭṭhi hoti, ujugatāssa diṭṭhi, "
"dhamme aveccappasādena samannāgato, āgato imaṃ saddhammaṃ."
msgstr ""

msgctxt "mn9:64-66.6"
msgid ""
"Katamā panāvuso, avijjā, katamo avijjāsamudayo, katamo avijjānirodho, katamā "
"avijjānirodhagāminī paṭipadā?"
msgstr ""

msgctxt "mn9:64-66.7"
msgid ""
"Yaṃ kho, āvuso, dukkhe aññāṇaṃ, dukkhasamudaye aññāṇaṃ, dukkhanirodhe "
"aññāṇaṃ, dukkhanirodhagāminiyā paṭipadāya aññāṇaṃ—"
msgstr ""

msgctxt "mn9:64-66.8"
msgid "ayaṃ vuccatāvuso, avijjā."
msgstr ""

msgctxt "mn9:64-66.9"
msgid ""
"Āsavasamudayā avijjāsamudayo, āsavanirodhā avijjānirodho, ayameva ariyo "
"aṭṭhaṅgiko maggo avijjānirodhagāminī paṭipadā, seyyathidaṃ—"
msgstr ""

msgctxt "mn9:64-66.10"
msgid "sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya67"></a>
#. <a class="sc" id="sc37"></a>
msgctxt "mn9:67.1"
msgid ""
"Yato kho, āvuso, ariyasāvako evaṃ avijjaṃ pajānāti, evaṃ avijjāsamudayaṃ "
"pajānāti, evaṃ avijjānirodhaṃ pajānāti, evaṃ avijjānirodhagāminiṃ paṭipadaṃ "
"pajānāti, so sabbaso rāgānusayaṃ pahāya, paṭighānusayaṃ paṭivinodetvā, ‘asmī’"
"ti diṭṭhimānānusayaṃ samūhanitvā, avijjaṃ pahāya vijjaṃ uppādetvā, diṭṭheva "
"dhamme dukkhassantakaro hoti—"
msgstr ""

msgctxt "mn9:67.2"
msgid ""
"ettāvatāpi kho, āvuso, ariyasāvako sammādiṭṭhi hoti, ujugatāssa diṭṭhi, "
"dhamme aveccappasādena samannāgato, āgato imaṃ saddhamman”ti."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya68"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli1.55"></a>
#. <a class="sc" id="sc38"></a>
msgctxt "mn9:68.1"
msgid ""
"“Sādhāvuso”ti kho te bhikkhū āyasmato sāriputtassa bhāsitaṃ abhinanditvā "
"anumoditvā āyasmantaṃ sāriputtaṃ uttari pañhaṃ apucchuṃ:"
msgstr ""

msgctxt "mn9:68.2"
msgid ""
"“siyā panāvuso, aññopi pariyāyo yathā ariyasāvako sammādiṭṭhi hoti, "
"ujugatāssa diṭṭhi, dhamme aveccappasādena samannāgato, āgato imaṃ "
"saddhamman”ti?"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya69"></a>
#. <a class="sc" id="sc39"></a>
msgctxt "mn9:69.1"
msgid "“Siyā, āvuso."
msgstr ""

msgctxt "mn9:69.2"
msgid ""
"Yato kho, āvuso, ariyasāvako āsavañca pajānāti, āsavasamudayañca pajānāti, "
"āsavanirodhañca pajānāti, āsavanirodhagāminiṃ paṭipadañca pajānāti—"
msgstr ""

msgctxt "mn9:69.3"
msgid ""
"ettāvatāpi kho, āvuso, ariyasāvako sammādiṭṭhi hoti, ujugatāssa diṭṭhi, "
"dhamme aveccappasādena samannāgato, āgato imaṃ saddhammaṃ."
msgstr ""

#. <a class="nya" id="nya70"></a>
msgctxt "mn9:70.1"
msgid ""
"Katamo panāvuso, āsavo, katamo āsavasamudayo, katamo āsavanirodho, katamā "
"āsavanirodhagāminī paṭipadāti?"
msgstr ""

msgctxt "mn9:70.2"
msgid "Tayome, āvuso, āsavā—"
msgstr ""

msgctxt "mn9:70.3"
msgid "kāmāsavo, bhavāsavo, avijjāsavo."
msgstr ""

msgctxt "mn9:70.4"
msgid ""
"Avijjāsamudayā āsavasamudayo, avijjānirodhā āsavanirodho, ayameva ariyo "
"aṭṭhaṅgiko maggo āsavanirodhagāminī paṭipadā, seyyathidaṃ—"
msgstr ""

msgctxt "mn9:70.5"
msgid "sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya71"></a>
#. <a class="sc" id="sc40"></a>
msgctxt "mn9:71.1"
msgid ""
"Yato kho, āvuso, ariyasāvako evaṃ āsavaṃ pajānāti, evaṃ āsavasamudayaṃ "
"pajānāti, evaṃ āsavanirodhaṃ pajānāti, evaṃ āsavanirodhagāminiṃ paṭipadaṃ "
"pajānāti, so sabbaso rāgānusayaṃ pahāya, paṭighānusayaṃ paṭivinodetvā, ‘asmī’"
"ti diṭṭhimānānusayaṃ samūhanitvā, avijjaṃ pahāya vijjaṃ uppādetvā, diṭṭheva "
"dhamme dukkhassantakaro hoti—"
msgstr ""

msgctxt "mn9:71.2"
msgid ""
"ettāvatāpi kho, āvuso, ariyasāvako sammādiṭṭhi hoti, ujugatāssa diṭṭhi, "
"dhamme aveccappasādena samannāgato, āgato imaṃ saddhamman”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc41"></a>
msgctxt "mn9:71.3"
msgid "Idamavocāyasmā sāriputto."
msgstr ""

msgctxt "mn9:71.4"
msgid "Attamanā te bhikkhū āyasmato sāriputtassa bhāsitaṃ abhinandunti."
msgstr ""

#. </p><p class="endsection">
msgctxt "mn9:71.5"
msgid "Sammādiṭṭhisuttaṃ niṭṭhitaṃ navamaṃ."
msgstr ""

