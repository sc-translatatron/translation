#. <section class="sutta" id="mn148"><article><div class="hgroup"><p class="division">
msgctxt "mn148:0.1"
msgid "Majjhima Nikāya 148"
msgstr ""

#. </p><h1>
msgctxt "mn148:0.2"
msgid "Chachakkasutta"
msgstr ""

#. </h1></div><p>
#. <a class="nya" id="nya1"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "mn148:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "mn148:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "mn148:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "mn148:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "mn148:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "mn148:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. <a class="nya" id="nya2"></a>
msgctxt "mn148:2.1"
msgid ""
"“dhammaṃ vo, bhikkhave, desessāmi ādikalyāṇaṃ majjhekalyāṇaṃ "
"pariyosānakalyāṇaṃ sātthaṃ sabyañjanaṃ, kevalaparipuṇṇaṃ parisuddhaṃ "
"brahmacariyaṃ pakāsessāmi, yadidaṃ—"
msgstr ""

msgctxt "mn148:2.2"
msgid "cha chakkāni."
msgstr ""

msgctxt "mn148:2.3"
msgid "Taṃ suṇātha, sādhukaṃ manasi karotha, bhāsissāmī”ti."
msgstr ""

msgctxt "mn148:2.4"
msgid "“Evaṃ, bhante”ti kho te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "mn148:2.5"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya3"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "mn148:3.1"
msgid ""
"“Cha ajjhattikāni āyatanāni veditabbāni, cha bāhirāni āyatanāni veditabbāni, "
"cha viññāṇakāyā veditabbā, cha phassakāyā veditabbā, cha vedanākāyā "
"veditabbā, cha taṇhākāyā veditabbā."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya4"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "mn148:4.1"
msgid "‘Cha ajjhattikāni āyatanāni veditabbānī’ti—"
msgstr ""

msgctxt "mn148:4.2"
msgid "iti kho panetaṃ vuttaṃ. Kiñcetaṃ paṭicca vuttaṃ?"
msgstr ""

msgctxt "mn148:4.3"
msgid ""
"Cakkhāyatanaṃ, sotāyatanaṃ, ghānāyatanaṃ, jivhāyatanaṃ, kāyāyatanaṃ, "
"manāyatanaṃ."
msgstr ""

msgctxt "mn148:4.4"
msgid "‘Cha ajjhattikāni āyatanāni veditabbānī’ti—"
msgstr ""

msgctxt "mn148:4.5"
msgid "iti yaṃ taṃ vuttaṃ, idametaṃ paṭicca vuttaṃ."
msgstr ""

msgctxt "mn148:4.6"
msgid "Idaṃ paṭhamaṃ chakkaṃ. (1)"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya5"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli3.281"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "mn148:5.1"
msgid "‘Cha bāhirāni āyatanāni veditabbānī’ti—"
msgstr ""

msgctxt "mn148:5.2"
msgid "iti kho panetaṃ vuttaṃ. Kiñcetaṃ paṭicca vuttaṃ?"
msgstr ""

msgctxt "mn148:5.3"
msgid ""
"Rūpāyatanaṃ, saddāyatanaṃ, gandhāyatanaṃ, rasāyatanaṃ, phoṭṭhabbāyatanaṃ, "
"dhammāyatanaṃ."
msgstr ""

msgctxt "mn148:5.4"
msgid "‘Cha bāhirāni āyatanāni veditabbānī’ti—"
msgstr ""

msgctxt "mn148:5.5"
msgid "iti yaṃ taṃ vuttaṃ, idametaṃ paṭicca vuttaṃ."
msgstr ""

msgctxt "mn148:5.6"
msgid "Idaṃ dutiyaṃ chakkaṃ. (2)"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya6"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "mn148:6.1"
msgid "‘Cha viññāṇakāyā veditabbā’ti—"
msgstr ""

msgctxt "mn148:6.2"
msgid "iti kho panetaṃ vuttaṃ. Kiñcetaṃ paṭicca vuttaṃ?"
msgstr ""

msgctxt "mn148:6.3"
msgid "Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇaṃ, "
msgstr ""

msgctxt "mn148:6.4"
msgid "sotañca paṭicca sadde ca uppajjati sotaviññāṇaṃ, "
msgstr ""

msgctxt "mn148:6.5"
msgid "ghānañca paṭicca gandhe ca uppajjati ghānaviññāṇaṃ, "
msgstr ""

msgctxt "mn148:6.6"
msgid "jivhañca paṭicca rase ca uppajjati jivhāviññāṇaṃ, "
msgstr ""

msgctxt "mn148:6.7"
msgid "kāyañca paṭicca phoṭṭhabbe ca uppajjati kāyaviññāṇaṃ, "
msgstr ""

msgctxt "mn148:6.8"
msgid "manañca paṭicca dhamme ca uppajjati manoviññāṇaṃ."
msgstr ""

msgctxt "mn148:6.9"
msgid "‘Cha viññāṇakāyā veditabbā’ti—"
msgstr ""

msgctxt "mn148:6.10"
msgid "iti yaṃ taṃ vuttaṃ, idametaṃ paṭicca vuttaṃ."
msgstr ""

msgctxt "mn148:6.11"
msgid "Idaṃ tatiyaṃ chakkaṃ. (3)"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya7"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "mn148:7.1"
msgid "‘Cha phassakāyā veditabbā’ti—"
msgstr ""

msgctxt "mn148:7.2"
msgid "iti kho panetaṃ vuttaṃ. Kiñcetaṃ paṭicca vuttaṃ?"
msgstr ""

msgctxt "mn148:7.3"
msgid ""
"Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇaṃ, tiṇṇaṃ saṅgati phasso;"
msgstr ""

msgctxt "mn148:7.4"
msgid "sotañca paṭicca sadde ca uppajjati sotaviññāṇaṃ, tiṇṇaṃ saṅgati phasso;"
msgstr ""

msgctxt "mn148:7.5"
msgid ""
"ghānañca paṭicca gandhe ca uppajjati ghānaviññāṇaṃ, tiṇṇaṃ saṅgati phasso;"
msgstr ""

msgctxt "mn148:7.6"
msgid ""
"jivhañca paṭicca rase ca uppajjati jivhāviññāṇaṃ, tiṇṇaṃ saṅgati phasso;"
msgstr ""

msgctxt "mn148:7.7"
msgid ""
"kāyañca paṭicca phoṭṭhabbe ca uppajjati kāyaviññāṇaṃ, tiṇṇaṃ saṅgati phasso;"
msgstr ""

msgctxt "mn148:7.8"
msgid ""
"manañca paṭicca dhamme ca uppajjati manoviññāṇaṃ, tiṇṇaṃ saṅgati phasso."
msgstr ""

msgctxt "mn148:7.9"
msgid "‘Cha phassakāyā veditabbā’ti—"
msgstr ""

msgctxt "mn148:7.10"
msgid "iti yaṃ taṃ vuttaṃ, idametaṃ paṭicca vuttaṃ."
msgstr ""

msgctxt "mn148:7.11"
msgid "Idaṃ catutthaṃ chakkaṃ. (4)"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya8"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli3.282"></a>
#. <a class="sc" id="sc7"></a>
msgctxt "mn148:8.1"
msgid "‘Cha vedanākāyā veditabbā’ti—"
msgstr ""

msgctxt "mn148:8.2"
msgid "iti kho panetaṃ vuttaṃ. Kiñcetaṃ paṭicca vuttaṃ?"
msgstr ""

msgctxt "mn148:8.3"
msgid ""
"Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇaṃ, tiṇṇaṃ saṅgati phasso, "
"phassapaccayā vedanā;"
msgstr ""

msgctxt "mn148:8.4"
msgid ""
"sotañca paṭicca sadde ca uppajjati sotaviññāṇaṃ, tiṇṇaṃ saṅgati phasso, "
"phassapaccayā vedanā;"
msgstr ""

msgctxt "mn148:8.5"
msgid ""
"ghānañca paṭicca gandhe ca uppajjati ghānaviññāṇaṃ, tiṇṇaṃ saṅgati phasso, "
"phassapaccayā vedanā;"
msgstr ""

msgctxt "mn148:8.6"
msgid ""
"jivhañca paṭicca rase ca uppajjati jivhāviññāṇaṃ, tiṇṇaṃ saṅgati phasso, "
"phassapaccayā vedanā;"
msgstr ""

msgctxt "mn148:8.7"
msgid ""
"kāyañca paṭicca phoṭṭhabbe ca uppajjati kāyaviññāṇaṃ, tiṇṇaṃ saṅgati phasso, "
"phassapaccayā vedanā;"
msgstr ""

msgctxt "mn148:8.8"
msgid ""
"manañca paṭicca dhamme ca uppajjati manoviññāṇaṃ, tiṇṇaṃ saṅgati phasso, "
"phassapaccayā vedanā."
msgstr ""

msgctxt "mn148:8.9"
msgid "‘Cha vedanākāyā veditabbā’ti—"
msgstr ""

msgctxt "mn148:8.10"
msgid "iti yaṃ taṃ vuttaṃ, idametaṃ paṭicca vuttaṃ."
msgstr ""

msgctxt "mn148:8.11"
msgid "Idaṃ pañcamaṃ chakkaṃ. (5)"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya9"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "mn148:9.1"
msgid "‘Cha taṇhākāyā veditabbā’ti—"
msgstr ""

msgctxt "mn148:9.2"
msgid "iti kho panetaṃ vuttaṃ. Kiñcetaṃ paṭicca vuttaṃ?"
msgstr ""

msgctxt "mn148:9.3"
msgid ""
"Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇaṃ, tiṇṇaṃ saṅgati phasso, "
"phassapaccayā vedanā, vedanāpaccayā taṇhā;"
msgstr ""

msgctxt "mn148:9.4"
msgid "sotañca paṭicca sadde ca uppajjati sotaviññāṇaṃ … pe …"
msgstr ""

msgctxt "mn148:9.5"
msgid "ghānañca paṭicca gandhe ca uppajjati ghānaviññāṇaṃ …"
msgstr ""

msgctxt "mn148:9.6"
msgid "jivhañca paṭicca rase ca uppajjati jivhāviññāṇaṃ …"
msgstr ""

msgctxt "mn148:9.7"
msgid "kāyañca paṭicca phoṭṭhabbe ca uppajjati kāyaviññāṇaṃ …"
msgstr ""

msgctxt "mn148:9.8"
msgid ""
"manañca paṭicca dhamme ca uppajjati manoviññāṇaṃ, tiṇṇaṃ saṅgati phasso, "
"phassapaccayā vedanā, vedanāpaccayā taṇhā."
msgstr ""

msgctxt "mn148:9.9"
msgid "‘Cha taṇhākāyā veditabbā’ti—"
msgstr ""

msgctxt "mn148:9.10"
msgid "iti yaṃ taṃ vuttaṃ, idametaṃ paṭicca vuttaṃ."
msgstr ""

msgctxt "mn148:9.11"
msgid "Idaṃ chaṭṭhaṃ chakkaṃ. (6)"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya10"></a>
#. <a class="sc" id="sc9"></a>
msgctxt "mn148:10.1"
msgid "‘Cakkhu attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:10.2"
msgid "Cakkhussa uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:10.3"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:10.4"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:10.5"
msgid "‘cakkhu attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:10.6"
msgid "Iti cakkhu anattā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "mn148:10.7"
msgid "‘Rūpā attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:10.8"
msgid "Rūpānaṃ uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:10.9"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:10.10"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:10.11"
msgid "‘rūpā attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:10.12"
msgid "Iti cakkhu anattā, rūpā anattā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "mn148:10.13"
msgid "‘Cakkhuviññāṇaṃ attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:10.14"
msgid "Cakkhuviññāṇassa uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:10.15"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:10.16"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:10.17"
msgid "‘cakkhuviññāṇaṃ attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:10.18"
msgid "Iti cakkhu anattā, rūpā anattā, cakkhuviññāṇaṃ anattā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "mn148:10.19"
msgid "‘Cakkhusamphasso attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:10.20"
msgid "Cakkhusamphassassa uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:10.21"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:10.22"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:10.23"
msgid "‘cakkhusamphasso attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:10.24"
msgid ""
"Iti cakkhu anattā, rūpā anattā, cakkhuviññāṇaṃ anattā, cakkhusamphasso "
"anattā."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.283"></a>
#. <a class="sc" id="sc13"></a>
msgctxt "mn148:10.25"
msgid "‘Vedanā attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:10.26"
msgid "Vedanāya uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:10.27"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:10.28"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:10.29"
msgid "‘vedanā attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:10.30"
msgid ""
"Iti cakkhu anattā, rūpā anattā, cakkhuviññāṇaṃ anattā, cakkhusamphasso "
"anattā, vedanā anattā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc14"></a>
msgctxt "mn148:10.31"
msgid "‘Taṇhā attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:10.32"
msgid "Taṇhāya uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:10.33"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:10.34"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:10.35"
msgid "‘taṇhā attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:10.36"
msgid ""
"Iti cakkhu anattā, rūpā anattā, cakkhuviññāṇaṃ anattā, cakkhusamphasso "
"anattā, vedanā anattā, taṇhā anattā."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya11"></a>
#. <a class="sc" id="sc15"></a>
msgctxt "mn148:11.1"
msgid "‘Sotaṃ attā’ti yo vadeyya … pe …"
msgstr ""

msgctxt "mn148:11.2"
msgid "‘ghānaṃ attā’ti yo vadeyya … pe …"
msgstr ""

msgctxt "mn148:11.3"
msgid "‘jivhā attā’ti yo vadeyya … pe …"
msgstr ""

msgctxt "mn148:11.4"
msgid "‘kāyo attā’ti yo vadeyya … pe …"
msgstr ""

msgctxt "mn148:11.5"
msgid "‘mano attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:11.6"
msgid "Manassa uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:11.7"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:11.8"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:11.9"
msgid "‘mano attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:11.10"
msgid "Iti mano anattā."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya12"></a>
#. <a class="sc" id="sc16"></a>
msgctxt "mn148:12.1"
msgid "‘Dhammā attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:12.2"
msgid "Dhammānaṃ uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:12.3"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:12.4"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:12.5"
msgid "‘dhammā attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:12.6"
msgid "Iti mano anattā, dhammā anattā."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya13"></a>
#. <a class="sc" id="sc17"></a>
msgctxt "mn148:13.1"
msgid "‘Manoviññāṇaṃ attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:13.2"
msgid "Manoviññāṇassa uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:13.3"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:13.4"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:13.5"
msgid "‘manoviññāṇaṃ attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:13.6"
msgid "Iti mano anattā, dhammā anattā, manoviññāṇaṃ anattā."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya14"></a>
#. <a class="sc" id="sc18"></a>
msgctxt "mn148:14.1"
msgid "‘Manosamphasso attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:14.2"
msgid "Manosamphassassa uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:14.3"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:14.4"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:14.5"
msgid "‘manosamphasso attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:14.6"
msgid ""
"Iti mano anattā, dhammā anattā, manoviññāṇaṃ anattā, manosamphasso anattā."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya15"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli3.284"></a>
#. <a class="sc" id="sc19"></a>
msgctxt "mn148:15.1"
msgid "‘Vedanā attā’ti yo vadeyya taṃ na upapajjati."
msgstr ""

msgctxt "mn148:15.2"
msgid "Vedanāya uppādopi vayopi paññāyati."
msgstr ""

msgctxt "mn148:15.3"
msgid ""
"Yassa kho pana uppādopi vayopi paññāyati, ‘attā me uppajjati ca veti cā’ti "
"iccassa evamāgataṃ hoti."
msgstr ""

msgctxt "mn148:15.4"
msgid "Tasmā taṃ na upapajjati:"
msgstr ""

msgctxt "mn148:15.5"
msgid "‘vedanā attā’ti yo vadeyya."
msgstr ""

msgctxt "mn148:15.6"
msgid ""
"Iti mano anattā, dhammā anattā, manoviññāṇaṃ anattā, manosamphasso anattā, "
"vedanā anattā."
msgstr ""

#. </p><p>
