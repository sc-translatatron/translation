#. <section class="sutta" id="mn64"><article><div class="hgroup"><p class="division">
msgctxt "mn64:0.1"
msgid "Majjhima Nikāya 64"
msgstr ""

#. </p><h1>
msgctxt "mn64:0.2"
msgid "Mahāmālukyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="nya" id="nya1"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "mn64:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "mn64:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "mn64:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "mn64:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "mn64:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "mn64:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. <a class="nya" id="nya2"></a>
msgctxt "mn64:2.1"
msgid ""
"“dhāretha no tumhe, bhikkhave, mayā desitāni pañcorambhāgiyāni "
"saṃyojanānī”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "mn64:2.2"
msgid "Evaṃ vutte, āyasmā mālukyaputto bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "mn64:2.3"
msgid ""
"“ahaṃ kho, bhante, dhāremi bhagavatā desitāni pañcorambhāgiyāni "
"saṃyojanānī”ti."
msgstr ""

msgctxt "mn64:2.4"
msgid ""
"“Yathā kathaṃ pana tvaṃ, mālukyaputta, dhāresi mayā desitāni "
"pañcorambhāgiyāni saṃyojanānī”ti?"
msgstr ""

msgctxt "mn64:2.5"
msgid ""
"“Sakkāyadiṭṭhiṃ kho ahaṃ, bhante, bhagavatā orambhāgiyaṃ saṃyojanaṃ desitaṃ "
"dhāremi;"
msgstr ""

msgctxt "mn64:2.6"
msgid ""
"vicikicchaṃ kho ahaṃ, bhante, bhagavatā orambhāgiyaṃ saṃyojanaṃ desitaṃ "
"dhāremi;"
msgstr ""

msgctxt "mn64:2.7"
msgid ""
"sīlabbataparāmāsaṃ kho ahaṃ, bhante, bhagavatā orambhāgiyaṃ saṃyojanaṃ "
"desitaṃ dhāremi;"
msgstr ""

msgctxt "mn64:2.8"
msgid ""
"kāmacchandaṃ kho ahaṃ, bhante, bhagavatā orambhāgiyaṃ saṃyojanaṃ desitaṃ "
"dhāremi;"
msgstr ""

msgctxt "mn64:2.9"
msgid ""
"byāpādaṃ kho ahaṃ, bhante, bhagavatā orambhāgiyaṃ saṃyojanaṃ desitaṃ dhāremi."
msgstr ""

msgctxt "mn64:2.10"
msgid ""
"Evaṃ kho ahaṃ, bhante, dhāremi bhagavatā desitāni pañcorambhāgiyāni "
"saṃyojanānī”ti."
msgstr ""

# Normally such constructions with nāma indicate reproval as at MN 22, etc. It is unclear why that tone is adopted here.
#. </p><p>
#. <a class="nya" id="nya3"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli1.433"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "mn64:3.1"
msgid ""
"“Kassa kho nāma tvaṃ, mālukyaputta, imāni evaṃ pañcorambhāgiyāni saṃyojanāni "
"desitāni dhāresi?"
msgstr ""

msgctxt "mn64:3.2"
msgid ""
"Nanu, mālukyaputta, aññatitthiyā paribbājakā iminā taruṇūpamena upārambhena "
"upārambhissanti?"
msgstr ""

msgctxt "mn64:3.3"
msgid ""
"Daharassa hi, mālukyaputta, kumārassa mandassa uttānaseyyakassa sakkāyotipi "
"na hoti, kuto panassa uppajjissati sakkāyadiṭṭhi?"
msgstr ""

#. VAR: Anusetvevassa → anuseti tvevassa (bj, s1-3, pts1)
msgctxt "mn64:3.4"
msgid "Anusetvevassa sakkāyadiṭṭhānusayo."
msgstr ""

msgctxt "mn64:3.5"
msgid ""
"Daharassa hi, mālukyaputta, kumārassa mandassa uttānaseyyakassa dhammātipi "
"na hoti, kuto panassa uppajjissati dhammesu vicikicchā?"
msgstr ""

msgctxt "mn64:3.6"
msgid "Anusetvevassa vicikicchānusayo."
msgstr ""

msgctxt "mn64:3.7"
msgid ""
"Daharassa hi, mālukyaputta, kumārassa mandassa uttānaseyyakassa sīlātipi na "
"hoti, kuto panassa uppajjissati sīlesu sīlabbataparāmāso?"
msgstr ""

msgctxt "mn64:3.8"
msgid "Anusetvevassa sīlabbataparāmāsānusayo."
msgstr ""

msgctxt "mn64:3.9"
msgid ""
"Daharassa hi, mālukyaputta, kumārassa mandassa uttānaseyyakassa kāmātipi na "
"hoti, kuto panassa uppajjissati kāmesu kāmacchando?"
msgstr ""

msgctxt "mn64:3.10"
msgid "Anusetvevassa kāmarāgānusayo."
msgstr ""

msgctxt "mn64:3.11"
msgid ""
"Daharassa hi, mālukyaputta, kumārassa mandassa uttānaseyyakassa sattātipi na "
"hoti, kuto panassa uppajjissati sattesu byāpādo?"
msgstr ""

msgctxt "mn64:3.12"
msgid "Anusetvevassa byāpādānusayo."
msgstr ""

msgctxt "mn64:3.13"
msgid ""
"Nanu, mālukyaputta, aññatitthiyā paribbājakā iminā taruṇūpamena upārambhena "
"upārambhissantī”ti?"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya4"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "mn64:4.1"
msgid "Evaṃ vutte, āyasmā ānando bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "mn64:4.2"
msgid "“etassa, bhagavā, kālo, etassa, sugata, kālo "
msgstr ""

msgctxt "mn64:4.3"
msgid ""
"yaṃ bhagavā pañcorambhāgiyāni saṃyojanāni deseyya. Bhagavato sutvā bhikkhū "
"dhāressantī”ti."
msgstr ""

msgctxt "mn64:4.4"
msgid "“Tena hānanda, suṇāhi, sādhukaṃ manasi karohi; bhāsissāmī”ti."
msgstr ""

msgctxt "mn64:4.5"
msgid "“Evaṃ, bhante”ti kho āyasmā ānando bhagavato paccassosi."
msgstr ""

msgctxt "mn64:4.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="nya" id="nya5"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "mn64:5.1"
msgid ""
"“Idhānanda, assutavā puthujjano ariyānaṃ adassāvī ariyadhammassa akovido "
"ariyadhamme avinīto, sappurisānaṃ adassāvī sappurisadhammassa akovido "
"sappurisadhamme avinīto "
msgstr ""

msgctxt "mn64:5.2"
msgid "sakkāyadiṭṭhipariyuṭṭhitena cetasā viharati sakkāyadiṭṭhiparetena;"
msgstr ""

msgctxt "mn64:5.3"
msgid "uppannāya ca sakkāyadiṭṭhiyā nissaraṇaṃ yathābhūtaṃ nappajānāti."
msgstr ""

msgctxt "mn64:5.4"
msgid "Tassa sā sakkāyadiṭṭhi thāmagatā appaṭivinītā orambhāgiyaṃ saṃyojanaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "mn64:5.5"
msgid "Vicikicchāpariyuṭṭhitena cetasā viharati vicikicchāparetena;"
msgstr ""

msgctxt "mn64:5.6"
msgid "uppannāya ca vicikicchāya nissaraṇaṃ yathābhūtaṃ nappajānāti."
msgstr ""

msgctxt "mn64:5.7"
msgid "Tassa sā vicikicchā thāmagatā appaṭivinītā orambhāgiyaṃ saṃyojanaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "mn64:5.8"
msgid ""
"Sīlabbataparāmāsapariyuṭṭhitena cetasā viharati sīlabbataparāmāsaparetena;"
msgstr ""

msgctxt "mn64:5.9"
msgid "uppannassa ca sīlabbataparāmāsassa nissaraṇaṃ yathābhūtaṃ nappajānāti."
msgstr ""

msgctxt "mn64:5.10"
msgid ""
"Tassa so sīlabbataparāmāso thāmagato appaṭivinīto orambhāgiyaṃ saṃyojanaṃ."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli1.434"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "mn64:5.11"
msgid "Kāmarāgapariyuṭṭhitena cetasā viharati kāmarāgaparetena;"
msgstr ""

msgctxt "mn64:5.12"
msgid "uppannassa ca kāmarāgassa nissaraṇaṃ yathābhūtaṃ nappajānāti."
msgstr ""

msgctxt "mn64:5.13"
msgid "Tassa so kāmarāgo thāmagato appaṭivinīto orambhāgiyaṃ saṃyojanaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "mn64:5.14"
msgid "Byāpādapariyuṭṭhitena cetasā viharati byāpādaparetena;"
msgstr ""

msgctxt "mn64:5.15"
msgid "uppannassa ca byāpādassa nissaraṇaṃ yathābhūtaṃ nappajānāti."
msgstr ""

msgctxt "mn64:5.16"
msgid "Tassa so byāpādo thāmagato appaṭivinīto orambhāgiyaṃ saṃyojanaṃ."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya6"></a>
#. <a class="sc" id="sc10"></a>
msgctxt "mn64:6.1"
msgid ""
"Sutavā ca kho, ānanda, ariyasāvako ariyānaṃ dassāvī ariyadhammassa kovido "
"ariyadhamme suvinīto, sappurisānaṃ dassāvī sappurisadhammassa kovido "
"sappurisadhamme suvinīto na sakkāyadiṭṭhipariyuṭṭhitena cetasā viharati na "
"sakkāyadiṭṭhiparetena;"
msgstr ""

msgctxt "mn64:6.2"
msgid "uppannāya ca sakkāyadiṭṭhiyā nissaraṇaṃ yathābhūtaṃ pajānāti."
msgstr ""

msgctxt "mn64:6.3"
msgid "Tassa sā sakkāyadiṭṭhi sānusayā pahīyati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "mn64:6.4"
msgid "Na vicikicchāpariyuṭṭhitena cetasā viharati na vicikicchāparetena;"
msgstr ""

msgctxt "mn64:6.5"
msgid "uppannāya ca vicikicchāya nissaraṇaṃ yathābhūtaṃ pajānāti."
msgstr ""

msgctxt "mn64:6.6"
msgid "Tassa sā vicikicchā sānusayā pahīyati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "mn64:6.7"
msgid ""
"Na sīlabbataparāmāsapariyuṭṭhitena cetasā viharati na "
"sīlabbataparāmāsaparetena;"
msgstr ""

msgctxt "mn64:6.8"
msgid "uppannassa ca sīlabbataparāmāsassa nissaraṇaṃ yathābhūtaṃ pajānāti."
msgstr ""

msgctxt "mn64:6.9"
msgid "Tassa so sīlabbataparāmāso sānusayo pahīyati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc13"></a>
msgctxt "mn64:6.10"
msgid "Na kāmarāgapariyuṭṭhitena cetasā viharati na kāmarāgaparetena;"
msgstr ""

msgctxt "mn64:6.11"
msgid "uppannassa ca kāmarāgassa nissaraṇaṃ yathābhūtaṃ pajānāti."
msgstr ""

msgctxt "mn64:6.12"
msgid "Tassa so kāmarāgo sānusayo pahīyati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc14"></a>
msgctxt "mn64:6.13"
msgid "Na byāpādapariyuṭṭhitena cetasā viharati na byāpādaparetena;"
msgstr ""

msgctxt "mn64:6.14"
msgid "uppannassa ca byāpādassa nissaraṇaṃ yathābhūtaṃ pajānāti."
msgstr ""

msgctxt "mn64:6.15"
msgid "Tassa so byāpādo sānusayo pahīyati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc15"></a>
msgctxt "mn64:6.16"
msgid ""
"Yo, ānanda, maggo yā paṭipadā pañcannaṃ orambhāgiyānaṃ saṃyojanānaṃ pahānāya "
"taṃ maggaṃ taṃ paṭipadaṃ anāgamma pañcorambhāgiyāni saṃyojanāni ñassati vā "
"dakkhati vā pajahissati vāti—netaṃ ṭhānaṃ vijjati."
msgstr ""

msgctxt "mn64:6.17"
msgid ""
"Seyyathāpi, ānanda, mahato rukkhassa tiṭṭhato sāravato tacaṃ acchetvā "
"phegguṃ acchetvā sāracchedo bhavissatīti—"
msgstr ""

msgctxt "mn64:6.18"
msgid "netaṃ ṭhānaṃ vijjati;"
msgstr ""

msgctxt "mn64:6.19"
msgid ""
"evameva kho, ānanda, yo maggo yā paṭipadā pañcannaṃ orambhāgiyānaṃ "
"saṃyojanānaṃ pahānāya taṃ maggaṃ taṃ paṭipadaṃ anāgamma pañcorambhāgiyāni "
"saṃyojanāni ñassati vā dakkhati vā pajahissati vāti—netaṃ ṭhānaṃ vijjati."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya7"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli1.435"></a>
#. <a class="sc" id="sc16"></a>
msgctxt "mn64:7.1"
msgid ""
"Yo ca kho, ānanda, maggo yā paṭipadā pañcannaṃ orambhāgiyānaṃ saṃyojanānaṃ "
"pahānāya taṃ maggaṃ taṃ paṭipadaṃ āgamma pañcorambhāgiyāni saṃyojanāni "
"ñassati vā dakkhati vā pajahissati vāti—ṭhānametaṃ vijjati."
msgstr ""

#. <a class="nya" id="nya8"></a>
msgctxt "mn64:8.1"
msgid ""
"Seyyathāpi, ānanda, mahato rukkhassa tiṭṭhato sāravato tacaṃ chetvā phegguṃ "
"chetvā sāracchedo bhavissatīti—ṭhānametaṃ vijjati."
msgstr ""

msgctxt "mn64:8.2"
msgid ""
"evameva kho, ānanda, yo maggo yā paṭipadā pañcannaṃ orambhāgiyānaṃ "
"saṃyojanānaṃ pahānāya taṃ maggaṃ taṃ paṭipadaṃ āgamma pañcorambhāgiyāni "
"saṃyojanāni ñassati vā dakkhati vā pajahissati vāti—ṭhānametaṃ vijjati."
msgstr ""

msgctxt "mn64:8.3"
msgid "Seyyathāpi, ānanda, gaṅgā nadī pūrā udakassa samatittikā kākapeyyā."
msgstr ""

msgctxt "mn64:8.4"
msgid "Atha dubbalako puriso āgaccheyya:"
msgstr ""

#. VAR: gacchissāmī’ti → gacchāmīti (bj, pts1)
msgctxt "mn64:8.5"
msgid ""
"‘ahaṃ imissā gaṅgāya nadiyā tiriyaṃ bāhāya sotaṃ chetvā sotthinā pāraṃ "
"gacchissāmī’ti;"
msgstr ""

msgctxt "mn64:8.6"
msgid ""
"so na sakkuṇeyya gaṅgāya nadiyā tiriyaṃ bāhāya sotaṃ chetvā sotthinā pāraṃ "
"gantuṃ."
msgstr ""

#. VAR: yesaṃ kesañci → yassa kassaci (sabbattha)
msgctxt "mn64:8.7"
msgid ""
"Evameva kho, ānanda, yesaṃ kesañci sakkāyanirodhāya dhamme desiyamāne cittaṃ "
"na pakkhandati nappasīdati na santiṭṭhati na vimuccati;"
msgstr ""

msgctxt "mn64:8.8"
msgid "seyyathāpi so dubbalako puriso evamete daṭṭhabbā."
msgstr ""

msgctxt "mn64:8.9"
msgid "Seyyathāpi, ānanda, gaṅgā nadī pūrā udakassa samatittikā kākapeyyā."
msgstr ""

msgctxt "mn64:8.10"
msgid "Atha balavā puriso āgaccheyya:"
msgstr ""

msgctxt "mn64:8.11"
msgid ""
"‘ahaṃ imissā gaṅgāya nadiyā tiriyaṃ bāhāya sotaṃ chetvā sotthinā pāraṃ "
"gacchissāmī’ti;"
msgstr ""

msgctxt "mn64:8.12"
msgid ""
"so sakkuṇeyya gaṅgāya nadiyā tiriyaṃ bāhāya sotaṃ chetvā sotthinā pāraṃ "
"gantuṃ."
msgstr ""

msgctxt "mn64:8.13"
msgid ""
"Evameva kho, ānanda, yesaṃ kesañci sakkāyanirodhāya dhamme desiyamāne cittaṃ "
"pakkhandati pasīdati santiṭṭhati vimuccati;"
msgstr ""

msgctxt "mn64:8.14"
msgid "seyyathāpi so balavā puriso evamete daṭṭhabbā."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya9"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli1.436"></a>
#. <a class="sc" id="sc17"></a>
msgctxt "mn64:9.1"
msgid ""
"Katamo cānanda, maggo, katamā paṭipadā pañcannaṃ orambhāgiyānaṃ saṃyojanānaṃ "
"pahānāya?"
msgstr ""

msgctxt "mn64:9.2"
msgid ""
"Idhānanda, bhikkhu upadhivivekā akusalānaṃ dhammānaṃ pahānā sabbaso "
"kāyaduṭṭhullānaṃ paṭippassaddhiyā vivicceva kāmehi vivicca akusalehi "
"dhammehi savitakkaṃ savicāraṃ vivekajaṃ pītisukhaṃ paṭhamaṃ jhānaṃ "
"upasampajja viharati."
msgstr ""

msgctxt "mn64:9.3"
msgid ""
"So yadeva tattha hoti rūpagataṃ vedanāgataṃ saññāgataṃ saṅkhāragataṃ "
"viññāṇagataṃ te dhamme aniccato dukkhato rogato gaṇḍato sallato aghato "
"ābādhato parato palokato suññato anattato samanupassati."
msgstr ""

#. VAR: paṭivāpeti → paṭipāpeti (s1-3) | patiṭṭhāpeti (mr)
msgctxt "mn64:9.4"
msgid "So tehi dhammehi cittaṃ paṭivāpeti."
msgstr ""

msgctxt "mn64:9.5"
msgid ""
"So tehi dhammehi cittaṃ paṭivāpetvā amatāya dhātuyā cittaṃ upasaṃharati:"
msgstr ""

msgctxt "mn64:9.6"
msgid ""
"‘etaṃ santaṃ etaṃ paṇītaṃ yadidaṃ sabbasaṅkhārasamatho "
"sabbūpadhipaṭinissaggo taṇhākkhayo virāgo nirodho nibbānan’ti."
msgstr ""

msgctxt "mn64:9.7"
msgid "So tattha ṭhito āsavānaṃ khayaṃ pāpuṇāti;"
msgstr ""

msgctxt "mn64:9.8"
msgid ""
"no ce āsavānaṃ khayaṃ pāpuṇāti teneva dhammarāgena tāya dhammanandiyā "
"pañcannaṃ orambhāgiyānaṃ saṃyojanānaṃ parikkhayā opapātiko hoti, tattha "
"parinibbāyī, anāvattidhammo tasmā lokā."
msgstr ""

msgctxt "mn64:9.9"
msgid ""
"Ayampi kho, ānanda, maggo ayaṃ paṭipadā pañcannaṃ orambhāgiyānaṃ "
"saṃyojanānaṃ pahānāya."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya10-12"></a>
#. <a class="sc" id="sc18"></a>
msgctxt "mn64:10-12.1"
msgid ""
"Puna caparaṃ, ānanda, bhikkhu vitakkavicārānaṃ vūpasamā … pe … dutiyaṃ "
"jhānaṃ upasampajja viharati … pe …"
msgstr ""

msgctxt "mn64:10-12.2"
msgid "tatiyaṃ jhānaṃ … pe …"
msgstr ""

msgctxt "mn64:10-12.3"
msgid "catutthaṃ jhānaṃ upasampajja viharati."
msgstr ""

msgctxt "mn64:10-12.4"
msgid ""
"So yadeva tattha hoti rūpagataṃ vedanāgataṃ saññāgataṃ saṅkhāragataṃ "
"viññāṇagataṃ …"
msgstr ""

msgctxt "mn64:10-12.5"
msgid "pe …"
msgstr ""

msgctxt "mn64:10-12.6"
msgid "anāvattidhammo tasmā lokā."
msgstr ""

msgctxt "mn64:10-12.7"
msgid ""
"Ayampi kho, ānanda, maggo ayaṃ paṭipadā pañcannaṃ orambhāgiyānaṃ "
"saṃyojanānaṃ pahānāya."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya13"></a>
#. <a class="sc" id="sc19"></a>
msgctxt "mn64:13.1"
msgid ""
"Puna caparaṃ, ānanda, bhikkhu sabbaso rūpasaññānaṃ samatikkamā "
"paṭighasaññānaṃ atthaṅgamā nānattasaññānaṃ amanasikārā ‘ananto ākāso’ti "
"ākāsānañcāyatanaṃ upasampajja viharati."
msgstr ""

msgctxt "mn64:13.2"
msgid ""
"So yadeva tattha hoti vedanāgataṃ saññāgataṃ saṅkhāragataṃ viññāṇagataṃ …"
msgstr ""

msgctxt "mn64:13.3"
msgid "pe …"
msgstr ""

msgctxt "mn64:13.4"
msgid "anāvattidhammo tasmā lokā."
msgstr ""

msgctxt "mn64:13.5"
msgid ""
"Ayampi kho, ānanda, maggo ayaṃ paṭipadā pañcannaṃ orambhāgiyānaṃ "
"saṃyojanānaṃ pahānāya."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya14"></a>
#. <a class="sc" id="sc20"></a>
msgctxt "mn64:14.1"
msgid ""
"Puna caparaṃ, ānanda, bhikkhu sabbaso ākāsānañcāyatanaṃ samatikkamma ‘"
"anantaṃ viññāṇan’ti viññāṇañcāyatanaṃ upasampajja viharati."
msgstr ""

msgctxt "mn64:14.2"
msgid ""
"So yadeva tattha hoti vedanāgataṃ saññāgataṃ saṅkhāragataṃ viññāṇagataṃ …"
msgstr ""

msgctxt "mn64:14.3"
msgid "pe …"
msgstr ""

msgctxt "mn64:14.4"
msgid "anāvattidhammo tasmā lokā."
msgstr ""

msgctxt "mn64:14.5"
msgid ""
"Ayampi kho, ānanda, maggo ayaṃ paṭipadā pañcannaṃ orambhāgiyānaṃ "
"saṃyojanānaṃ pahānāya."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya15"></a>
#. <a class="pts-vp-pli" id="pts-vp-pli1.437"></a>
#. <a class="sc" id="sc21"></a>
msgctxt "mn64:15.1"
msgid ""
"Puna caparaṃ, ānanda, bhikkhu sabbaso viññāṇañcāyatanaṃ samatikkamma ‘natthi "
"kiñcī’ti ākiñcaññāyatanaṃ upasampajja viharati."
msgstr ""

msgctxt "mn64:15.2"
msgid ""
"So yadeva tattha hoti vedanāgataṃ saññāgataṃ saṅkhāragataṃ viññāṇagataṃ …"
msgstr ""

msgctxt "mn64:15.3"
msgid "pe …"
msgstr ""

msgctxt "mn64:15.4"
msgid "anāvattidhammo tasmā lokā."
msgstr ""

msgctxt "mn64:15.5"
msgid ""
"Ayampi kho, ānanda, maggo ayaṃ paṭipadā pañcannaṃ orambhāgiyānaṃ "
"saṃyojanānaṃ pahānāyā”ti."
msgstr ""

#. </p><p>
#. <a class="nya" id="nya16"></a>
#. <a class="sc" id="sc22"></a>
msgctxt "mn64:16.1"
msgid ""
"“Eso ce, bhante, maggo esā paṭipadā pañcannaṃ orambhāgiyānaṃ saṃyojanānaṃ "
"pahānāya, atha kiñcarahi idhekacce bhikkhū cetovimuttino ekacce bhikkhū "
"paññāvimuttino”ti?"
msgstr ""

# Reading tesaham
#. VAR: Ettha kho panesāhaṃ → ettha kho tesāhaṃ (bj, s1-3, km, pts1)
msgctxt "mn64:16.2"
msgid "“Ettha kho panesāhaṃ, ānanda, indriyavemattataṃ vadāmī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc23"></a>
msgctxt "mn64:16.3"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "mn64:16.4"
msgid "Attamano āyasmā ānando bhagavato bhāsitaṃ abhinandīti."
msgstr ""

#. </p><p class="endsection">
msgctxt "mn64:16.5"
msgid "Mahāmālukyasuttaṃ niṭṭhitaṃ catutthaṃ."
msgstr ""
