msgctxt "thig1.6:4.1"
msgid ""
"“Dhīre nirodhaṃ <span class=\"var\" title=\"phussehi (bj)\" id=\"note2\""
">phusehi</span>,"
msgstr "Dhīrā, may you realise cessation,"

#. <br>
msgctxt "thig1.6:4.2"
msgid "Saññāvūpasamaṃ sukhaṃ;"
msgstr "Subsiding of perception, happiness"

#. <br>
msgctxt "thig1.6:4.3"
msgid "Ārādhayāhi nibbānaṃ,"
msgstr "Realise Nibbāna:"

#. <br>
msgctxt "thig1.6:4.4"
msgid ""
"<span class=\"var\" title=\"yogakkhemaṃ anuttaranti (bj, s2, s3) | "
"yogakkhemaṃ anuttaraṃ (pts1-2)\" id=\"note3\">Yogakkhemamanuttaran”ti</span>."
msgstr "Unsurpassed security from bondage"

