#. </p><h1>
msgctxt "sn1.62:0.3"
msgid "62. Cittasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.62:1.1"
msgid "“Kenassu nīyati loko,"
msgstr ""

#. <br>
msgctxt "sn1.62:1.2"
msgid "kenassu parikassati;"
msgstr ""

#. <br>
msgctxt "sn1.62:1.3"
msgid "Kissassu ekadhammassa,"
msgstr ""

#. <br>
msgctxt "sn1.62:1.4"
msgid "sabbeva vasamanvagū”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.62:2.1"
msgid "“Cittena nīyati loko,"
msgstr ""

#. <br>
msgctxt "sn1.62:2.2"
msgid "cittena parikassati;"
msgstr ""

#. <br>
msgctxt "sn1.62:2.3"
msgid "Cittassa ekadhammassa,"
msgstr ""

#. <br>
msgctxt "sn1.62:2.4"
msgid "sabbeva vasamanvagū”ti."
msgstr ""
