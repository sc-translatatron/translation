#. <section class="sutta" id="sn20.2"><article><div class="hgroup"><p class="division">
msgctxt "sn20.2:0.1"
msgid "Saṃyutta Nikāya 20"
msgstr ""

#. </p><p>
msgctxt "sn20.2:0.2"
msgid "1. Opammavagga"
msgstr ""

#. </p><h1>
#. VAR: 2. Nakhasikhasutta → nakhasikkhāsuttaṃ (bj)
msgctxt "sn20.2:0.3"
msgid "2. Nakhasikhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn20.2:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn20.2:1.2"
msgid ""
"Atha kho bhagavā parittaṃ nakhasikhāyaṃ paṃsuṃ āropetvā bhikkhū āmantesi:"
msgstr ""

msgctxt "sn20.2:1.3"
msgid "“Taṃ kiṃ maññatha, bhikkhave, "
msgstr ""

#. VAR: cāyaṃ → ayaṃ vā (bj) | yā cāyam (pts1-2)
msgctxt "sn20.2:1.4"
msgid ""
"katamaṃ nu kho bahutaraṃ, yo cāyaṃ mayā paritto nakhasikhāyaṃ paṃsu āropito "
"yā cāyaṃ mahāpathavī”ti?"
msgstr ""

msgctxt "sn20.2:1.5"
msgid "“Etadeva, bhante, bahutaraṃ yadidaṃ mahāpathavī."
msgstr ""

msgctxt "sn20.2:1.6"
msgid "Appamattakoyaṃ bhagavatā paritto nakhasikhāyaṃ paṃsu āropito."
msgstr ""

msgctxt "sn20.2:1.7"
msgid ""
"Saṅkhampi na upeti upanidhimpi na upeti kalabhāgampi na upeti mahāpathaviṃ "
"upanidhāya bhagavatā paritto nakhasikhāyaṃ paṃsu āropito”ti."
msgstr ""

msgctxt "sn20.2:1.8"
msgid "“Evameva kho, bhikkhave, appakā te sattā ye manussesu paccājāyanti;"
msgstr ""

msgctxt "sn20.2:1.9"
msgid "atha kho eteyeva bahutarā sattā ye aññatra manussehi paccājāyanti."
msgstr ""

msgctxt "sn20.2:1.10"
msgid "Tasmātiha, bhikkhave, evaṃ sikkhitabbaṃ:"
msgstr ""

msgctxt "sn20.2:1.11"
msgid "‘appamattā viharissāmā’ti."
msgstr ""

msgctxt "sn20.2:1.12"
msgid "Evañhi vo, bhikkhave, sikkhitabban”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn20.2:1.13"
msgid "Dutiyaṃ."
msgstr ""
