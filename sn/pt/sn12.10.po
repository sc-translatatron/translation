#. <section class="sutta" id="sn12.10"><article><div class="hgroup"><p class="division">
msgctxt "sn12.10:0.1"
msgid "Saṃyutta Nikāya 12"
msgstr ""

#. </p><p>
msgctxt "sn12.10:0.2"
msgid "1. Buddhavagga"
msgstr ""

#. </p><h1>
#. VAR: 10. Gotamasutta → mahā sakyamuni gotamo (pts1-2)
msgctxt "sn12.10:0.3"
msgid "10. Gotamasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.10"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn12.10:1.1"
msgid ""
"“Pubbeva me, bhikkhave, sambodhā anabhisambuddhassa bodhisattasseva sato "
"etadahosi:"
msgstr ""

msgctxt "sn12.10:1.2"
msgid ""
"‘kicchaṃ vatāyaṃ loko āpanno jāyati ca jīyati ca mīyati ca cavati ca "
"upapajjati ca."
msgstr ""

msgctxt "sn12.10:1.3"
msgid "Atha ca panimassa dukkhassa nissaraṇaṃ nappajānāti jarāmaraṇassa."
msgstr ""

msgctxt "sn12.10:1.4"
msgid "Kudāssu nāma imassa dukkhassa nissaraṇaṃ paññāyissati jarāmaraṇassā’ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn12.10:2.1"
msgid "Tassa mayhaṃ, bhikkhave, etadahosi:"
msgstr ""

msgctxt "sn12.10:2.2"
msgid "‘kimhi nu kho sati jarāmaraṇaṃ hoti, kiṃpaccayā jarāmaraṇan’ti?"
msgstr ""

msgctxt "sn12.10:2.3"
msgid "Tassa mayhaṃ, bhikkhave, yoniso manasikārā ahu paññāya abhisamayo:"
msgstr ""

msgctxt "sn12.10:2.4"
msgid "‘jātiyā kho sati jarāmaraṇaṃ hoti, jātipaccayā jarāmaraṇan’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn12.10:3.1"
msgid "Tassa mayhaṃ, bhikkhave, etadahosi:"
msgstr ""

msgctxt "sn12.10:3.2"
msgid "‘kimhi nu kho sati jāti hoti … pe …"
msgstr ""

msgctxt "sn12.10:3.3"
msgid "bhavo …"
msgstr ""

msgctxt "sn12.10:3.4"
msgid "upādānaṃ …"
msgstr ""

msgctxt "sn12.10:3.5"
msgid "taṇhā …"
msgstr ""

msgctxt "sn12.10:3.6"
msgid "vedanā …"
msgstr ""

msgctxt "sn12.10:3.7"
msgid "phasso …"
msgstr ""

msgctxt "sn12.10:3.8"
msgid "saḷāyatanaṃ …"
msgstr ""

msgctxt "sn12.10:3.9"
msgid "nāmarūpaṃ …"
msgstr ""

msgctxt "sn12.10:3.10"
msgid "viññāṇaṃ …"
msgstr ""

msgctxt "sn12.10:3.11"
msgid "saṅkhārā honti, kiṃpaccayā saṅkhārā’ti?"
msgstr ""

msgctxt "sn12.10:3.12"
msgid "Tassa mayhaṃ, bhikkhave, yoniso manasikārā ahu paññāya abhisamayo:"
msgstr ""

msgctxt "sn12.10:3.13"
msgid "‘avijjāya kho sati saṅkhārā honti, avijjāpaccayā saṅkhārā’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn12.10:4.1"
msgid "Iti hidaṃ avijjāpaccayā saṅkhārā;"
msgstr ""

msgctxt "sn12.10:4.2"
msgid "saṅkhārapaccayā viññāṇaṃ … pe …"
msgstr ""

msgctxt "sn12.10:4.3"
msgid "evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

msgctxt "sn12.10:4.4"
msgid ""
"‘Samudayo, samudayo’ti kho me, bhikkhave, pubbe ananussutesu dhammesu "
"cakkhuṃ udapādi, ñāṇaṃ udapādi, paññā udapādi, vijjā udapādi, āloko udapādi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn12.10:5.1"
msgid "Tassa mayhaṃ, bhikkhave, etadahosi:"
msgstr ""

msgctxt "sn12.10:5.2"
msgid ""
"‘kimhi nu kho asati jarāmaraṇaṃ na hoti, kissa nirodhā jarāmaraṇanirodho’ti?"
msgstr ""

msgctxt "sn12.10:5.3"
msgid "Tassa mayhaṃ, bhikkhave, yoniso manasikārā ahu paññāya abhisamayo:"
msgstr ""

msgctxt "sn12.10:5.4"
msgid ""
"‘jātiyā kho asati jarāmaraṇaṃ na hoti, jātinirodhā jarāmaraṇanirodho’ti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.11"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "sn12.10:6.1"
msgid "Tassa mayhaṃ, bhikkhave, etadahosi:"
msgstr ""

msgctxt "sn12.10:6.2"
msgid "‘kimhi nu kho asati jāti na hoti … pe …"
msgstr ""

msgctxt "sn12.10:6.3"
msgid "bhavo …"
msgstr ""

msgctxt "sn12.10:6.4"
msgid "upādānaṃ …"
msgstr ""

msgctxt "sn12.10:6.5"
msgid "taṇhā …"
msgstr ""

msgctxt "sn12.10:6.6"
msgid "vedanā …"
msgstr ""

msgctxt "sn12.10:6.7"
msgid "phasso …"
msgstr ""

msgctxt "sn12.10:6.8"
msgid "saḷāyatanaṃ …"
msgstr ""

msgctxt "sn12.10:6.9"
msgid "nāmarūpaṃ …"
msgstr ""

msgctxt "sn12.10:6.10"
msgid "viññāṇaṃ …"
msgstr ""

msgctxt "sn12.10:6.11"
msgid "saṅkhārā na honti, kissa nirodhā saṅkhāranirodho’ti?"
msgstr ""

msgctxt "sn12.10:6.12"
msgid "Tassa mayhaṃ, bhikkhave, yoniso manasikārā ahu paññāya abhisamayo:"
msgstr ""

msgctxt "sn12.10:6.13"
msgid ""
"‘avijjāya kho asati saṅkhārā na honti, avijjānirodhā saṅkhāranirodho’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn12.10:7.1"
msgid "Iti hidaṃ avijjānirodhā saṅkhāranirodho;"
msgstr ""

msgctxt "sn12.10:7.2"
msgid "saṅkhāranirodhā viññāṇanirodho … pe …"
msgstr ""

msgctxt "sn12.10:7.3"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hoti."
msgstr ""

msgctxt "sn12.10:7.4"
msgid ""
"‘Nirodho, nirodho’ti kho me, bhikkhave, pubbe ananussutesu dhammesu cakkhuṃ "
"udapādi, ñāṇaṃ udapādi, paññā udapādi, vijjā udapādi, āloko udapādī”ti."
msgstr ""

msgctxt "sn12.10:7.5"
msgid "Dasamo."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.10:7.6"
msgid "Buddhavaggo paṭhamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn12.10:8.1"
msgid "Desanā vibhaṅgapaṭipadā ca,"
msgstr ""

#. <br>
msgctxt "sn12.10:8.2"
msgid "Vipassī sikhī ca vessabhū;"
msgstr ""

#. <br>
msgctxt "sn12.10:8.3"
msgid "Kakusandho koṇāgamano kassapo,"
msgstr ""

#. <br>
msgctxt "sn12.10:8.4"
msgid "Mahāsakyamuni ca gotamoti."
msgstr ""
