#. <section class="sutta" id="sn35.115"><article><div class="hgroup"><p class="division">
msgctxt "sn35.115:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.115:0.2"
msgid "12. Lokakāmaguṇavagga"
msgstr ""

#. </p><h1>
msgctxt "sn35.115:0.3"
msgid "115. Dutiyamārapāsasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.115:1.1"
msgid ""
"“Santi, bhikkhave, cakkhuviññeyyā rūpā, iṭṭhā kantā manāpā piyarūpā "
"kāmūpasaṃhitā rajanīyā."
msgstr ""

msgctxt "sn35.115:1.2"
msgid "Tañce bhikkhu abhinandati abhivadati ajjhosāya tiṭṭhati—"
msgstr ""

msgctxt "sn35.115:1.3"
msgid ""
"ayaṃ vuccati, bhikkhave, bhikkhu baddho cakkhuviññeyyesu rūpesu, āvāsagato "
"mārassa, mārassa vasaṃ gato, paṭimukkassa mārapāso."
msgstr ""

msgctxt "sn35.115:1.4"
msgid "Baddho so mārabandhanena yathākāmakaraṇīyo pāpimato … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.115:2.1"
msgid "Santi, bhikkhave, jivhāviññeyyā rasā … pe …"
msgstr ""

msgctxt "sn35.115:2.2"
msgid ""
"santi, bhikkhave, manoviññeyyā dhammā, iṭṭhā kantā manāpā piyarūpā "
"kāmūpasaṃhitā rajanīyā."
msgstr ""

msgctxt "sn35.115:2.3"
msgid "Tañce bhikkhu abhinandati abhivadati ajjhosāya tiṭṭhati—"
msgstr ""

msgctxt "sn35.115:2.4"
msgid ""
"ayaṃ vuccati, bhikkhave, bhikkhu baddho manoviññeyyesu dhammesu, āvāsagato "
"mārassa, mārassa vasaṃ gato, paṭimukkassa mārapāso."
msgstr ""

msgctxt "sn35.115:2.5"
msgid "Baddho so mārabandhanena yathākāmakaraṇīyo pāpimato … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn35.115:3.1"
msgid ""
"Santi ca kho, bhikkhave, cakkhuviññeyyā rūpā, iṭṭhā kantā manāpā piyarūpā "
"kāmūpasaṃhitā rajanīyā."
msgstr ""

msgctxt "sn35.115:3.2"
msgid "Tañce bhikkhu nābhinandati nābhivadati nājjhosāya tiṭṭhati—"
msgstr ""

msgctxt "sn35.115:3.3"
msgid ""
"ayaṃ vuccati, bhikkhave, bhikkhu mutto cakkhuviññeyyehi rūpehi, nāvāsagato "
"mārassa, na mārassa vasaṃ gato, ummukkassa mārapāso."
msgstr ""

msgctxt "sn35.115:3.4"
msgid "Mutto so mārabandhanena na yathākāmakaraṇīyo pāpimato … pe …."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.93"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn35.115:4.1"
msgid "Santi, bhikkhave, jivhāviññeyyā rasā … pe …"
msgstr ""

msgctxt "sn35.115:4.2"
msgid ""
"santi, bhikkhave, manoviññeyyā dhammā, iṭṭhā kantā manāpā piyarūpā "
"kāmūpasaṃhitā rajanīyā."
msgstr ""

msgctxt "sn35.115:4.3"
msgid "Tañce bhikkhu nābhinandati nābhivadati nājjhosāya tiṭṭhati—"
msgstr ""

msgctxt "sn35.115:4.4"
msgid ""
"ayaṃ vuccati, bhikkhave, bhikkhu mutto manoviññeyyehi dhammehi, nāvāsagato "
"mārassa, na mārassa vasaṃ gato, ummukkassa mārapāso."
msgstr ""

msgctxt "sn35.115:4.5"
msgid "Mutto so mārabandhanena na yathākāmakaraṇīyo pāpimato”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.115:4.6"
msgid "Dutiyaṃ."
msgstr ""
