#. <section class="sutta" id="sn35.174"><article><div class="hgroup"><p class="division">
msgctxt "sn35.174-176:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.174-176:0.2"
msgid "17. Saṭṭhipeyyālavagga"
msgstr ""

#. </p><h1>
msgctxt "sn35.174-176:0.3"
msgid "174–​176. Anattachandādisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.174-176:1.1"
msgid ""
"“Yo, bhikkhave, anattā, tatra vo chando pahātabbo, rāgo pahātabbo, "
"chandarāgo pahātabbo."
msgstr ""

msgctxt "sn35.174-176:1.2"
msgid "Ko ca, bhikkhave, anattā?"
msgstr ""

msgctxt "sn35.174-176:1.3"
msgid ""
"Cakkhu, bhikkhave, anattā; tatra vo chando pahātabbo, rāgo pahātabbo, "
"chandarāgo pahātabbo … pe …"
msgstr ""

msgctxt "sn35.174-176:1.4"
msgid ""
"jivhā anattā; tatra vo chando pahātabbo, rāgo pahātabbo, chandarāgo "
"pahātabbo … pe …"
msgstr ""

msgctxt "sn35.174-176:1.5"
msgid ""
"mano anattā; tatra vo chando pahātabbo, rāgo pahātabbo, chandarāgo pahātabbo."
msgstr ""

msgctxt "sn35.174-176:1.6"
msgid ""
"Yo, bhikkhave, anattā tatra vo chando pahātabbo, rāgo pahātabbo, chandarāgo "
"pahātabbo”ti."
msgstr ""
