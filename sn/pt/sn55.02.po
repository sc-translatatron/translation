#. <section class="sutta" id="sn55.2"><article><div class="hgroup"><p class="division">
msgctxt "sn55.2:0.1"
msgid "Saṃyutta Nikāya 55"
msgstr ""

#. </p><p>
msgctxt "sn55.2:0.2"
msgid "1. Veḷudvāravagga"
msgstr ""

#. </p><h1>
#. VAR: 2. Brahmacariyogadhasutta → ogadha or saṭayhaṃ (pts1)
msgctxt "sn55.2:0.3"
msgid "2. Brahmacariyogadhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn55.2:1.1"
msgid ""
"“Catūhi, bhikkhave, dhammehi samannāgato ariyasāvako sotāpanno hoti "
"avinipātadhammo niyato sambodhiparāyaṇo."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.344"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn55.2:2.1"
msgid "Katamehi catūhi?"
msgstr ""

msgctxt "sn55.2:2.2"
msgid "Idha, bhikkhave, ariyasāvako buddhe aveccappasādena samannāgato hoti:"
msgstr ""

msgctxt "sn55.2:2.3"
msgid ""
"‘itipi so bhagavā arahaṃ sammāsambuddho vijjācaraṇasampanno sugato lokavidū "
"anuttaro purisadammasārathi satthā devamanussānaṃ buddho bhagavā’ti."
msgstr ""

msgctxt "sn55.2:2.4"
msgid "Dhamme … pe …"
msgstr ""

msgctxt "sn55.2:2.5"
msgid "saṅghe … pe …"
msgstr ""

msgctxt "sn55.2:2.6"
msgid ""
"ariyakantehi sīlehi samannāgato hoti akhaṇḍehi … pe … samādhisaṃvattanikehi."
msgstr ""

msgctxt "sn55.2:2.7"
msgid ""
"Imehi kho, bhikkhave, catūhi dhammehi samannāgato ariyasāvako sotāpanno hoti "
"avinipātadhammo niyato sambodhiparāyaṇo”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn55.2:3.1"
msgid "Idamavoca bhagavā."
msgstr ""

#. VAR: vatvāna → vatvā (bj, pts1)
msgctxt "sn55.2:3.2"
msgid "Idaṃ vatvāna sugato athāparaṃ etadavoca satthā:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn55.2:4.1"
msgid "“Yesaṃ saddhā ca sīlañca,"
msgstr ""

#. <br>
msgctxt "sn55.2:4.2"
msgid "Pasādo dhammadassanaṃ;"
msgstr ""

#. <br>
msgctxt "sn55.2:4.3"
msgid "Te ve kālena paccenti,"
msgstr ""

#. <br>
msgctxt "sn55.2:4.4"
msgid "Brahmacariyogadhaṃ sukhan”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn55.2:4.5"
msgid "Dutiyaṃ."
msgstr ""
