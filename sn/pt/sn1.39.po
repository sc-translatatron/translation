#. <section class="sutta" id="sn1.39"><article><div class="hgroup"><p class="division">
msgctxt "sn1.39:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.39:0.2"
msgid "4. Satullapakāyikavagga"
msgstr ""

#. </p><h1>
#. VAR: 39. Paṭhamapajjunnadhītusutta → pajjunnadhītusuttaṃ (bj) | pajjunnadhītusuttaṃ (1) (s1) | pajjunnadhītā (1) (pts1-2)
msgctxt "sn1.39:0.3"
msgid "39. Paṭhamapajjunnadhītusutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn1.39:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn1.39:1.2"
msgid "ekaṃ samayaṃ bhagavā vesāliyaṃ viharati mahāvane kūṭāgārasālāyaṃ."
msgstr ""

msgctxt "sn1.39:1.3"
msgid ""
"Atha kho kokanadā pajjunnassa dhītā abhikkantāya rattiyā abhikkantavaṇṇā "
"kevalakappaṃ mahāvanaṃ obhāsetvā yena bhagavā tenupasaṅkami; upasaṅkamitvā "
"bhagavantaṃ abhivādetvā ekamantaṃ aṭṭhāsi."
msgstr ""

msgctxt "sn1.39:1.4"
msgid ""
"Ekamantaṃ ṭhitā kho sā devatā kokanadā pajjunnassa dhītā bhagavato santike "
"imā gāthāyo abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts1ed" id="pts1ed1.30"></a>
#. <a class="pts2ed" id="pts2ed1.62"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.39:2.1"
msgid "“Vesāliyaṃ vane viharantaṃ,"
msgstr ""

# See BB on sattassa.
#. <br>
msgctxt "sn1.39:2.2"
msgid "Aggaṃ sattassa sambuddhaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.39:2.3"
msgid "Kokanadāhamasmi abhivande,"
msgstr ""

#. <br>
msgctxt "sn1.39:2.4"
msgid "Kokanadā pajjunnassa dhītā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.39:3.1"
msgid "Sutameva pure āsi,"
msgstr ""

#. <br>
msgctxt "sn1.39:3.2"
msgid "Dhammo cakkhumatānubuddho;"
msgstr ""

#. <br>
msgctxt "sn1.39:3.3"
msgid "Sāhaṃ dāni sakkhi jānāmi,"
msgstr ""

#. <br>
msgctxt "sn1.39:3.4"
msgid "Munino desayato sugatassa."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.39:4.1"
msgid "Ye keci ariyaṃ dhammaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.39:4.2"
msgid "Vigarahantā caranti dummedhā;"
msgstr ""

#. <br>
msgctxt "sn1.39:4.3"
msgid "Upenti roruvaṃ ghoraṃ,"
msgstr ""

#. <br>
msgctxt "sn1.39:4.4"
msgid "Cirarattaṃ dukkhaṃ anubhavanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn1.39:5.1"
msgid "Ye ca kho ariye dhamme,"
msgstr ""

#. <br>
msgctxt "sn1.39:5.2"
msgid "Khantiyā upasamena upetā;"
msgstr ""

#. <br>
msgctxt "sn1.39:5.3"
msgid "Pahāya mānusaṃ dehaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.39:5.4"
msgid "Devakāyaṃ paripūressantī”ti."
msgstr ""
