#. <section class="sutta" id="sn25.3"><article><div class="hgroup"><p class="division">
msgctxt "sn25.3:0.1"
msgid "Saṃyutta Nikāya 25"
msgstr ""

#. </p><p>
msgctxt "sn25.3:0.2"
msgid "1. Cakkhuvagga"
msgstr ""

#. </p><h1>
msgctxt "sn25.3:0.3"
msgid "3. Viññāṇasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn25.3:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn25.3:1.2"
msgid "“Cakkhuviññāṇaṃ, bhikkhave, aniccaṃ vipariṇāmi aññathābhāvi;"
msgstr ""

msgctxt "sn25.3:1.3"
msgid "sotaviññāṇaṃ …"
msgstr ""

msgctxt "sn25.3:1.4"
msgid "ghānaviññāṇaṃ …"
msgstr ""

msgctxt "sn25.3:1.5"
msgid "jivhāviññāṇaṃ …"
msgstr ""

msgctxt "sn25.3:1.6"
msgid "kāyaviññāṇaṃ …"
msgstr ""

msgctxt "sn25.3:1.7"
msgid "manoviññāṇaṃ aniccaṃ vipariṇāmi aññathābhāvi."
msgstr ""

msgctxt "sn25.3:1.8"
msgid "Yo bhikkhave … pe …"
msgstr ""

msgctxt "sn25.3:1.9"
msgid "sambodhiparāyano”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn25.3:1.10"
msgid "Tatiyaṃ."
msgstr ""
