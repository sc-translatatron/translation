#. <section class="sutta" id="sn35.142"><article><div class="hgroup"><p class="division">
msgctxt "sn35.142:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.142:0.2"
msgid "14. Devadahavagga"
msgstr ""

#. </p><h1>
#. VAR: 142. Ajjhattānattahetusutta → ajjhattaanattasuttaṃ (bj) | hetunā ajjhatta 3 (pts1)
msgctxt "sn35.142:0.3"
msgid "142. Ajjhattānattahetusutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.131"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.142:1.1"
msgid "“Cakkhuṃ, bhikkhave, anattā."
msgstr ""

msgctxt "sn35.142:1.2"
msgid "Yopi hetu, yopi paccayo cakkhussa uppādāya, sopi anattā."
msgstr ""

msgctxt "sn35.142:1.3"
msgid "Anattasambhūtaṃ, bhikkhave, cakkhu kuto attā bhavissati … pe …"
msgstr ""

msgctxt "sn35.142:1.4"
msgid "jivhā anattā."
msgstr ""

msgctxt "sn35.142:1.5"
msgid "Yopi hetu yopi paccayo jivhāya uppādāya, sopi anattā."
msgstr ""

msgctxt "sn35.142:1.6"
msgid "Anattasambhūtā, bhikkhave, jivhā kuto attā bhavissati … pe …"
msgstr ""

msgctxt "sn35.142:1.7"
msgid "mano anattā."
msgstr ""

msgctxt "sn35.142:1.8"
msgid "Yopi hetu yopi paccayo manassa uppādāya, sopi anattā."
msgstr ""

msgctxt "sn35.142:1.9"
msgid "Anattasambhūto, bhikkhave, mano kuto attā bhavissati."
msgstr ""

msgctxt "sn35.142:1.10"
msgid "Evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn35.142:1.11"
msgid "nāparaṃ itthattāyā’ti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.142:1.12"
msgid "Navamaṃ."
msgstr ""
