#. <section class="sutta" id="sn22.157"><article><div class="hgroup"><p class="division">
msgctxt "sn22.157:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.157:0.2"
msgid "15. Diṭṭhivagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.157:0.3"
msgid "157. Abhinivesasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.157:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.157:1.2"
msgid ""
"“Kismiṃ nu kho, bhikkhave, sati, kiṃ upādāya, kiṃ abhinivissa uppajjanti "
"saṃyojanābhinivesavinibandhā”ti?"
msgstr ""

msgctxt "sn22.157:1.3"
msgid "Bhagavaṃmūlakā no, bhante, dhammā … pe …"
msgstr ""

msgctxt "sn22.157:1.4"
msgid ""
"“rūpe kho, bhikkhave, sati, rūpaṃ upādāya, rūpaṃ abhinivissa uppajjanti "
"saṃyojanābhinivesavinibandhā."
msgstr ""

msgctxt "sn22.157:1.5"
msgid "Vedanāya sati …"
msgstr ""

msgctxt "sn22.157:1.6"
msgid "saññāya sati …"
msgstr ""

msgctxt "sn22.157:1.7"
msgid "saṅkhāresu sati …"
msgstr ""

msgctxt "sn22.157:1.8"
msgid ""
"viññāṇe sati, viññāṇaṃ upādāya, viññāṇaṃ abhinivissa uppajjanti "
"saṃyojanābhinivesavinibandhā."
msgstr ""

msgctxt "sn22.157:1.9"
msgid "Taṃ kiṃ maññatha, bhikkhave, "
msgstr ""

msgctxt "sn22.157:1.10"
msgid "rūpaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn22.157:1.11"
msgid "“Aniccaṃ, bhante”."
msgstr ""

msgctxt "sn22.157:1.12"
msgid "“Yaṃ panāniccaṃ … pe …"
msgstr ""

msgctxt "sn22.157:1.13"
msgid "api nu taṃ anupādāya uppajjeyyuṃ saṃyojanābhinivesavinibandhā”ti?"
msgstr ""

msgctxt "sn22.157:1.14"
msgid "“No hetaṃ, bhante” … pe …"
msgstr ""

msgctxt "sn22.157:1.15"
msgid "“evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn22.157:1.16"
msgid "nāparaṃ itthattāyāti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.157:1.17"
msgid "Aṭṭhamaṃ."
msgstr ""
