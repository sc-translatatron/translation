#. <section class="sutta" id="sn45.161"><article><div class="hgroup"><p class="division">
msgctxt "sn45.161:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.161:0.2"
msgid "13. Esanāvagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.161:0.3"
msgid "161. Esanāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.161:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.161:1.2"
msgid "“Tisso imā, bhikkhave, esanā."
msgstr ""

msgctxt "sn45.161:1.3"
msgid "Katamā tisso?"
msgstr ""

msgctxt "sn45.161:1.4"
msgid "Kāmesanā, bhavesanā, brahmacariyesanā—"
msgstr ""

msgctxt "sn45.161:1.5"
msgid "imā kho, bhikkhave, tisso esanā."
msgstr ""

msgctxt "sn45.161:1.6"
msgid ""
"Imāsaṃ kho, bhikkhave, tissannaṃ esanānaṃ abhiññāya ariyo aṭṭhaṅgiko maggo "
"bhāvetabbo."
msgstr ""

msgctxt "sn45.161:1.7"
msgid "Katamo ariyo aṭṭhaṅgiko maggo?"
msgstr ""

msgctxt "sn45.161:1.8"
msgid ""
"Idha, bhikkhave, bhikkhu sammādiṭṭhiṃ bhāveti vivekanissitaṃ … pe … "
"sammāsamādhiṃ bhāveti vivekanissitaṃ virāganissitaṃ nirodhanissitaṃ "
"vossaggapariṇāmiṃ."
msgstr ""

msgctxt "sn45.161:1.9"
msgid ""
"Imāsaṃ kho, bhikkhave, tissannaṃ esanānaṃ abhiññāya ayaṃ ariyo aṭṭhaṅgiko "
"maggo bhāvetabboti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn45.161:2.1"
msgid "Tisso imā kho, bhikkhave, esanā."
msgstr ""

msgctxt "sn45.161:2.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "sn45.161:2.3"
msgid "Kāmesanā, bhavesanā, brahmacariyesanā—"
msgstr ""

msgctxt "sn45.161:2.4"
msgid "imā kho, bhikkhave, tisso esanā."
msgstr ""

msgctxt "sn45.161:2.5"
msgid ""
"Imāsaṃ kho, bhikkhave, tissannaṃ esanānaṃ abhiññāya ariyo aṭṭhaṅgiko maggo "
"bhāvetabbo."
msgstr ""

msgctxt "sn45.161:2.6"
msgid "Katamo ariyo aṭṭhaṅgiko maggo?"
msgstr ""

msgctxt "sn45.161:2.7"
msgid ""
"Idha, bhikkhave, bhikkhu sammādiṭṭhiṃ bhāveti … pe … sammāsamādhiṃ bhāveti "
"rāgavinayapariyosānaṃ dosavinayapariyosānaṃ mohavinayapariyosānaṃ."
msgstr ""

msgctxt "sn45.161:2.8"
msgid ""
"Imāsaṃ kho, bhikkhave, tissannaṃ esanānaṃ abhiññāya ayaṃ ariyo aṭṭhaṅgiko "
"maggo bhāvetabboti."
msgstr ""

#. </p><p>
