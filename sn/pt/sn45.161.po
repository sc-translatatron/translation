msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn45.161:5.1"
msgid "Tisso imā, bhikkhave, esanā."
msgstr ""

msgctxt "sn45.161:5.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "sn45.161:5.3"
msgid "Kāmesanā, bhavesanā, brahmacariyesanā—"
msgstr ""

msgctxt "sn45.161:5.4"
msgid "imā kho, bhikkhave, tisso esanā."
msgstr ""

msgctxt "sn45.161:5.5"
msgid ""
"Imāsaṃ kho, bhikkhave, tissannaṃ esanānaṃ pariññāya … pe … ayaṃ ariyo "
"aṭṭhaṅgiko maggo bhāvetabboti."
msgstr ""

msgctxt "sn45.161:5.6"
msgid "(Yadapi abhiññā, tadapi pariññāya vitthāretabbaṃ.)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn45.161:6.1"
msgid "Tisso imā, bhikkhave, esanā."
msgstr ""

