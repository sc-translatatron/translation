#. <section class="sutta" id="sn36.2"><article><div class="hgroup"><p class="division">
msgctxt "sn36.2:0.1"
msgid "Saṃyutta Nikāya 36"
msgstr ""

#. </p><p>
msgctxt "sn36.2:0.2"
msgid "1. Sagāthāvagga"
msgstr ""

#. </p><h1>
msgctxt "sn36.2:0.3"
msgid "2. Sukhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn36.2:1.1"
msgid "“Tisso imā, bhikkhave, vedanā."
msgstr ""

msgctxt "sn36.2:1.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "sn36.2:1.3"
msgid "Sukhā vedanā, dukkhā vedanā, adukkhamasukhā vedanā—"
msgstr ""

msgctxt "sn36.2:1.4"
msgid "imā kho, bhikkhave, tisso vedanāti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.205"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn36.2:2.1"
msgid "Sukhaṃ vā yadi vā dukkhaṃ,"
msgstr ""

#. <br>
msgctxt "sn36.2:2.2"
msgid "adukkhamasukhaṃ saha;"
msgstr ""

#. <br>
msgctxt "sn36.2:2.3"
msgid "Ajjhattañca bahiddhā ca,"
msgstr ""

#. <br>
msgctxt "sn36.2:2.4"
msgid "yaṃ kiñci atthi veditaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn36.2:3.1"
msgid "Etaṃ dukkhanti ñatvāna,"
msgstr ""

#. <br>
msgctxt "sn36.2:3.2"
msgid "mosadhammaṃ palokinaṃ;"
msgstr ""

#. <br>
msgctxt "sn36.2:3.3"
msgid "Phussa phussa vayaṃ passaṃ,"
msgstr ""

#. <br>
msgctxt "sn36.2:3.4"
msgid "evaṃ tattha virajjatī”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn36.2:3.5"
msgid "Dutiyaṃ."
msgstr ""
