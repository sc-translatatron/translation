#. <section class="sutta" id="sn22.89"><article><div class="hgroup"><p class="division">
msgctxt "sn22.89:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.89:0.2"
msgid "9. Theravagga"
msgstr ""

#. </p><h1>
#. VAR: 89. Khemakasutta → khemo (pts1)
msgctxt "sn22.89:0.3"
msgid "89. Khemakasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.127"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.89:1.1"
msgid "Ekaṃ samayaṃ sambahulā therā bhikkhū kosambiyaṃ viharanti ghositārāme."
msgstr ""

msgctxt "sn22.89:1.2"
msgid ""
"Tena kho pana samayena āyasmā khemako badarikārāme viharati ābādhiko "
"dukkhito bāḷhagilāno."
msgstr ""

msgctxt "sn22.89:1.3"
msgid ""
"Atha kho therā bhikkhū sāyanhasamayaṃ paṭisallānā vuṭṭhitā āyasmantaṃ "
"dāsakaṃ āmantesuṃ:"
msgstr ""

msgctxt "sn22.89:1.4"
msgid ""
"“ehi tvaṃ, āvuso dāsaka, yena khemako bhikkhu tenupasaṅkama; upasaṅkamitvā "
"khemakaṃ bhikkhuṃ evaṃ vadehi:"
msgstr ""

msgctxt "sn22.89:1.5"
msgid "‘therā taṃ, āvuso khemaka, evamāhaṃsu—"
msgstr ""

msgctxt "sn22.89:1.6"
msgid ""
"kacci te, āvuso, khamanīyaṃ, kacci yāpanīyaṃ, kacci dukkhā vedanā "
"paṭikkamanti no abhikkamanti, paṭikkamosānaṃ paññāyati no abhikkamo’”ti?"
msgstr ""

msgctxt "sn22.89:1.7"
msgid ""
"“Evamāvuso”ti kho āyasmā dāsako therānaṃ bhikkhūnaṃ paṭissutvā yenāyasmā "
"khemako tenupasaṅkami; upasaṅkamitvā āyasmantaṃ khemakaṃ etadavoca:"
msgstr ""

msgctxt "sn22.89:1.8"
msgid "“therā taṃ, āvuso khemaka, evamāhaṃsu:"
msgstr ""

msgctxt "sn22.89:1.9"
msgid "‘kacci te, āvuso, khamanīyaṃ … pe … no abhikkamo’”ti?"
msgstr ""

msgctxt "sn22.89:1.10"
msgid ""
"“Na me, āvuso, khamanīyaṃ na yāpanīyaṃ … pe … abhikkamosānaṃ paññāyati no "
"paṭikkamo”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn22.89:2.1"
msgid ""
"Atha kho āyasmā dāsako yena therā bhikkhū tenupasaṅkami; upasaṅkamitvā there "
"bhikkhū etadavoca:"
msgstr ""

msgctxt "sn22.89:2.2"
msgid "“khemako, āvuso, bhikkhu evamāha:"
msgstr ""

msgctxt "sn22.89:2.3"
msgid ""
"‘na me, āvuso, khamanīyaṃ … pe … abhikkamosānaṃ paññāyati no paṭikkamo’”ti."
msgstr ""

msgctxt "sn22.89:2.4"
msgid ""
"“Ehi tvaṃ, āvuso dāsaka, yena khemako bhikkhu tenupasaṅkama; upasaṅkamitvā "
"khemakaṃ bhikkhuṃ evaṃ vadehi:"
msgstr ""

msgctxt "sn22.89:2.5"
msgid "‘therā taṃ, āvuso khemaka, evamāhaṃsu—"
msgstr ""

msgctxt "sn22.89:2.6"
msgid "pañcime, āvuso, upādānakkhandhā vuttā bhagavatā, seyyathidaṃ—"
msgstr ""

msgctxt "sn22.89:2.7"
msgid ""
"rūpupādānakkhandho, vedanupādānakkhandho, saññupādānakkhandho, "
"saṅkhārupādānakkhandho, viññāṇupādānakkhandho."
msgstr ""

msgctxt "sn22.89:2.8"
msgid ""
"Imesu āyasmā khemako pañcasu upādānakkhandhesu kiñci attaṃ vā attaniyaṃ vā "
"samanupassatī’”ti?"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.128"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn22.89:3.1"
msgid ""
"“Evamāvuso”ti kho āyasmā dāsako therānaṃ bhikkhūnaṃ paṭissutvā yenāyasmā "
"khemako tenupasaṅkami; upasaṅkamitvā … pe …"
msgstr ""

msgctxt "sn22.89:3.2"
msgid "therā taṃ, āvuso khemaka, evamāhaṃsu:"
msgstr ""

msgctxt "sn22.89:3.3"
msgid "“pañcime, āvuso, upādānakkhandhā vuttā bhagavatā, seyyathidaṃ—"
msgstr ""

msgctxt "sn22.89:3.4"
msgid "rūpupādānakkhandho … pe … viññāṇupādānakkhandho."
msgstr ""

msgctxt "sn22.89:3.5"
msgid ""
"Imesu āyasmā khemako pañcasu upādānakkhandhesu kiñci attaṃ vā attaniyaṃ vā "
"samanupassatī”ti?"
msgstr ""

msgctxt "sn22.89:3.6"
msgid "“Pañcime, āvuso, upādānakkhandhā vuttā bhagavatā, seyyathidaṃ—"
msgstr ""

msgctxt "sn22.89:3.7"
msgid "rūpupādānakkhandho … pe … viññāṇupādānakkhandho."
msgstr ""

msgctxt "sn22.89:3.8"
msgid ""
"Imesu khvāhaṃ, āvuso, pañcasu upādānakkhandhesu na kiñci attaṃ vā attaniyaṃ "
"vā samanupassāmī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn22.89:4.1"
msgid ""
"Atha kho āyasmā dāsako yena therā bhikkhū tenupasaṅkami; upasaṅkamitvā there "
"bhikkhū etadavoca:"
msgstr ""

msgctxt "sn22.89:4.2"
msgid "“khemako, āvuso, bhikkhu evamāha:"
msgstr ""

msgctxt "sn22.89:4.3"
msgid "‘pañcime, āvuso, upādānakkhandhā vuttā bhagavatā, seyyathidaṃ—"
msgstr ""

msgctxt "sn22.89:4.4"
msgid "rūpupādānakkhandho … pe … viññāṇupādānakkhandho."
msgstr ""

msgctxt "sn22.89:4.5"
msgid ""
"Imesu khvāhaṃ, āvuso, pañcasu upādānakkhandhesu na kiñci attaṃ vā attaniyaṃ "
"vā samanupassāmī’”ti."
msgstr ""

msgctxt "sn22.89:4.6"
msgid ""
"“Ehi tvaṃ, āvuso dāsaka, yena khemako bhikkhu tenupasaṅkama; upasaṅkamitvā "
"khemakaṃ bhikkhuṃ evaṃ vadehi:"
msgstr ""

msgctxt "sn22.89:4.7"
msgid "‘therā taṃ, āvuso khemaka, evamāhaṃsu—"
msgstr ""

msgctxt "sn22.89:4.8"
msgid "pañcime, āvuso, upādānakkhandhā vuttā bhagavatā, seyyathidaṃ—"
msgstr ""

msgctxt "sn22.89:4.9"
msgid "rūpupādānakkhandho … pe … viññāṇupādānakkhandho."
msgstr ""

msgctxt "sn22.89:4.10"
msgid ""
"No ce kirāyasmā khemako imesu pañcasu upādānakkhandhesu kiñci attaṃ vā "
"attaniyaṃ vā samanupassati."
msgstr ""

msgctxt "sn22.89:4.11"
msgid "Tenahāyasmā khemako arahaṃ khīṇāsavo’”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn22.89:5.1"
msgid ""
"“Evamāvuso”ti kho āyasmā dāsako therānaṃ bhikkhūnaṃ paṭissutvā yenāyasmā "
"khemako … pe …"
msgstr ""

msgctxt "sn22.89:5.2"
msgid "therā taṃ, āvuso khemaka, evamāhaṃsu:"
msgstr ""

msgctxt "sn22.89:5.3"
msgid "“pañcime, āvuso, upādānakkhandhā vuttā bhagavatā, seyyathidaṃ—"
msgstr ""

msgctxt "sn22.89:5.4"
msgid "rūpupādānakkhandho … pe … viññāṇupādānakkhandho;"
msgstr ""

msgctxt "sn22.89:5.5"
msgid ""
"no ce kirāyasmā khemako imesu pañcasu upādānakkhandhesu kiñci attaṃ vā "
"attaniyaṃ vā samanupassati, tenahāyasmā khemako arahaṃ khīṇāsavo”ti."
msgstr ""

msgctxt "sn22.89:5.6"
msgid "“Pañcime, āvuso, upādānakkhandhā vuttā bhagavatā, seyyathidaṃ—"
msgstr ""

msgctxt "sn22.89:5.7"
msgid "rūpupādānakkhandho … pe … viññāṇupādānakkhandho."
msgstr ""

msgctxt "sn22.89:5.8"
msgid ""
"Imesu khvāhaṃ, āvuso, pañcasu upādānakkhandhesu na kiñci attaṃ vā attaniyaṃ "
"vā samanupassāmi, na camhi arahaṃ khīṇāsavo;"
msgstr ""

msgctxt "sn22.89:5.9"
msgid ""
"api ca me, āvuso, pañcasu upādānakkhandhesu ‘asmī’ti adhigataṃ, "
"‘ayamahamasmī’ti na ca samanupassāmī”ti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.129"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "sn22.89:6.1"
msgid "Atha kho āyasmā dāsako yena therā bhikkhū … pe …"
msgstr ""

msgctxt "sn22.89:6.2"
msgid "there bhikkhū etadavoca:"
msgstr ""

msgctxt "sn22.89:6.3"
msgid "“khemako, āvuso, bhikkhu evamāha—"
msgstr ""

msgctxt "sn22.89:6.4"
msgid "pañcime, āvuso, upādānakkhandhā vuttā bhagavatā, seyyathidaṃ—"
msgstr ""

msgctxt "sn22.89:6.5"
msgid "rūpupādānakkhandho … pe … viññāṇupādānakkhandho."
msgstr ""

msgctxt "sn22.89:6.6"
msgid ""
"Imesu khvāhaṃ, āvuso, pañcasu upādānakkhandhesu na kiñci attaṃ vā attaniyaṃ "
"vā samanupassāmi, na camhi arahaṃ khīṇāsavo;"
msgstr ""

msgctxt "sn22.89:6.7"
msgid ""
"api ca me, āvuso, pañcasu upādānakkhandhesu ‘asmī’ti adhigataṃ, "
"‘ayamahamasmī’ti na ca samanupassāmī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn22.89:7.1"
msgid ""
"“Ehi tvaṃ, āvuso dāsaka, yena khemako bhikkhu tenupasaṅkama; upasaṅkamitvā "
"khemakaṃ bhikkhuṃ evaṃ vadehi:"
msgstr ""

msgctxt "sn22.89:7.2"
msgid "‘therā taṃ, āvuso khemaka, evamāhaṃsu—"
msgstr ""

msgctxt "sn22.89:7.3"
msgid "yametaṃ, āvuso khemaka, asmīti vadesi, kimetaṃ asmīti vadesi?"
msgstr ""

msgctxt "sn22.89:7.4"
msgid "Rūpaṃ asmīti vadesi, aññatra rūpā asmīti vadesi, "
msgstr ""

msgctxt "sn22.89:7.5"
msgid "vedanaṃ …"
msgstr ""

msgctxt "sn22.89:7.6"
msgid "saññaṃ …"
msgstr ""

msgctxt "sn22.89:7.7"
msgid "saṅkhāre …"
msgstr ""

msgctxt "sn22.89:7.8"
msgid "viññāṇaṃ asmīti vadesi, aññatra viññāṇā asmīti vadesi."
msgstr ""

msgctxt "sn22.89:7.9"
msgid "Yametaṃ, āvuso khemaka, asmīti vadesi. Kimetaṃ asmīti vadesī’”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn22.89:8.1"
msgid ""
"“Evamāvuso”ti kho āyasmā dāsako therānaṃ bhikkhūnaṃ paṭissutvā yenāyasmā "
"khemako tenupasaṅkami; upasaṅkamitvā āyasmantaṃ khemakaṃ etadavoca—"
msgstr ""

msgctxt "sn22.89:8.2"
msgid "therā taṃ, āvuso khemaka, evamāhaṃsu:"
msgstr ""

msgctxt "sn22.89:8.3"
msgid "“yametaṃ, āvuso khemaka, ‘asmī’ti vadesi, kimetaṃ ‘asmī’ti vadesi?"
msgstr ""

msgctxt "sn22.89:8.4"
msgid "Rūpaṃ ‘asmī’ti vadesi aññatra rūpā ‘asmī’ti vadesi?"
msgstr ""

msgctxt "sn22.89:8.5"
msgid "Vedanaṃ …"
msgstr ""

msgctxt "sn22.89:8.6"
msgid "saññaṃ …"
msgstr ""

msgctxt "sn22.89:8.7"
msgid "saṅkhāre …"
msgstr ""

msgctxt "sn22.89:8.8"
msgid "viññāṇaṃ ‘asmī’ti vadesi aññatra viññāṇā ‘asmī’ti vadesi?"
msgstr ""

msgctxt "sn22.89:8.9"
msgid "Yametaṃ, āvuso khemaka, ‘asmī’ti vadesi, kimetaṃ ‘asmī’ti vadesī”ti?"
msgstr ""

msgctxt "sn22.89:8.10"
msgid "“Alaṃ, āvuso dāsaka, kiṃ imāya sandhāvanikāya."
msgstr ""

msgctxt "sn22.89:8.11"
msgid "Āharāvuso, daṇḍaṃ;"
msgstr ""

msgctxt "sn22.89:8.12"
msgid "ahameva yena therā bhikkhū tenupasaṅkamissāmī”ti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.130"></a>
#. <a class="sc" id="sc9"></a>
msgctxt "sn22.89:9.1"
msgid ""
"Atha kho āyasmā khemako daṇḍamolubbha yena therā bhikkhū tenupasaṅkami; "
"upasaṅkamitvā therehi bhikkhūhi saddhiṃ sammodi."
msgstr ""

msgctxt "sn22.89:9.2"
msgid ""
"Sammodanīyaṃ kathaṃ sāraṇīyaṃ vītisāretvā ekamantaṃ nisīdi. Ekamantaṃ "
"nisinnaṃ kho āyasmantaṃ khemakaṃ therā bhikkhū etadavocuṃ:"
msgstr ""

msgctxt "sn22.89:9.3"
msgid "“yametaṃ, āvuso khemaka, ‘asmī’ti vadesi, kimetaṃ ‘asmī’ti vadesi?"
msgstr ""

msgctxt "sn22.89:9.4"
msgid "Rūpaṃ ‘asmī’ti vadesi, aññatra rūpā ‘asmī’ti vadesi?"
msgstr ""

msgctxt "sn22.89:9.5"
msgid "Vedanaṃ …"
msgstr ""

msgctxt "sn22.89:9.6"
msgid "saññaṃ …"
msgstr ""

msgctxt "sn22.89:9.7"
msgid "saṅkhāre …"
msgstr ""

msgctxt "sn22.89:9.8"
msgid "viññāṇaṃ ‘asmī’ti vadesi, aññatra viññāṇā ‘asmī’ti vadesi?"
msgstr ""

msgctxt "sn22.89:9.9"
msgid "Yametaṃ, āvuso khemaka, ‘asmī’ti vadesi, kimetaṃ ‘asmī’ti vadesī”ti?"
msgstr ""

msgctxt "sn22.89:9.10"
msgid ""
"“Na khvāhaṃ, āvuso, rūpaṃ ‘asmī’ti vadāmi; napi aññatra rūpā ‘asmī’ti vadāmi."
msgstr ""

msgctxt "sn22.89:9.11"
msgid "Na vedanaṃ …"
msgstr ""

msgctxt "sn22.89:9.12"
msgid "na saññaṃ …"
msgstr ""

msgctxt "sn22.89:9.13"
msgid "na saṅkhāre …"
msgstr ""

msgctxt "sn22.89:9.14"
msgid "na viññāṇaṃ ‘asmī’ti vadāmi; napi aññatra viññāṇā ‘asmī’ti vadāmi."
msgstr ""

msgctxt "sn22.89:9.15"
msgid ""
"Api ca me, āvuso, pañcasu upādānakkhandhesu ‘asmī’ti adhigataṃ ‘ayamahamasmī’"
"ti na ca samanupassāmi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn22.89:10.1"
msgid "Seyyathāpi, āvuso, uppalassa vā padumassa vā puṇḍarīkassa vā gandho."
msgstr ""

#. VAR: vaṇṇassa → vaṇṭassa (katthaci)
msgctxt "sn22.89:10.2"
msgid ""
"Yo nu kho evaṃ vadeyya: ‘pattassa gandho’ti vā ‘vaṇṇassa gandho’ti vā ‘"
"kiñjakkhassa gandho’ti vā sammā nu kho so vadamāno vadeyyā”ti?"
msgstr ""

msgctxt "sn22.89:10.3"
msgid "“No hetaṃ, āvuso”."
msgstr ""

msgctxt "sn22.89:10.4"
msgid "“Yathā kathaṃ, panāvuso, sammā byākaramāno byākareyyā”ti?"
msgstr ""

msgctxt "sn22.89:10.5"
msgid "“‘Pupphassa gandho’ti kho, āvuso, sammā byākaramāno byākareyyā”ti."
msgstr ""

msgctxt "sn22.89:10.6"
msgid ""
"“Evameva khvāhaṃ, āvuso, na rūpaṃ ‘asmī’ti vadāmi, napi aññatra rūpā ‘asmī’"
"ti vadāmi."
msgstr ""

msgctxt "sn22.89:10.7"
msgid "Na vedanaṃ …"
msgstr ""

msgctxt "sn22.89:10.8"
msgid "na saññaṃ …"
msgstr ""

msgctxt "sn22.89:10.9"
msgid "na saṅkhāre …"
msgstr ""

msgctxt "sn22.89:10.10"
msgid "na viññāṇaṃ ‘asmī’ti vadāmi, napi aññatra viññāṇā ‘asmī’ti vadāmi."
msgstr ""

msgctxt "sn22.89:10.11"
msgid ""
"Api ca me, āvuso, pañcasu upādānakkhandhesu ‘asmī’ti adhigataṃ ‘ayamahamasmī’"
"ti na ca samanupassāmi."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.131"></a>
#. <a class="sc" id="sc11"></a>
msgctxt "sn22.89:11.1"
msgid ""
"Kiñcāpi, āvuso, ariyasāvakassa pañcorambhāgiyāni saṃyojanāni pahīnāni "
"bhavanti, atha khvassa hoti: ‘yo ca pañcasu upādānakkhandhesu anusahagato "
"asmīti māno, asmīti chando, asmīti anusayo asamūhato."
msgstr ""

msgctxt "sn22.89:11.2"
msgid ""
"So aparena samayena pañcasu upādānakkhandhesu udayabbayānupassī viharati—"
msgstr ""

msgctxt "sn22.89:11.3"
msgid "iti rūpaṃ, iti rūpassa samudayo, iti rūpassa atthaṅgamo;"
msgstr ""

msgctxt "sn22.89:11.4"
msgid "iti vedanā …"
msgstr ""

msgctxt "sn22.89:11.5"
msgid "iti saññā …"
msgstr ""

msgctxt "sn22.89:11.6"
msgid "iti saṅkhārā …"
msgstr ""

msgctxt "sn22.89:11.7"
msgid "iti viññāṇaṃ, iti viññāṇassa samudayo, iti viññāṇassa atthaṅgamo’ti."
msgstr ""

msgctxt "sn22.89:11.8"
msgid ""
"Tassimesu pañcasu upādānakkhandhesu udayabbayānupassino viharato yopissa "
"hoti pañcasu upādānakkhandhesu anusahagato ‘asmī’ti, māno ‘asmī’ti, chando "
"‘asmī’ti anusayo asamūhato, sopi samugghātaṃ gacchati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn22.89:12.1"
msgid ""
"Seyyathāpi, āvuso, vatthaṃ saṃkiliṭṭhaṃ malaggahitaṃ. Tamenaṃ sāmikā "
"rajakassa anupadajjuṃ."
msgstr ""

msgctxt "sn22.89:12.2"
msgid ""
"Tamenaṃ rajako ūse vā khāre vā gomaye vā sammadditvā acche udake vikkhāleti."
msgstr ""

#. VAR: hoti yeva → hoteva (bj) | hoti yo (si) | hoti yo ca (s1-3, pts1)
msgctxt "sn22.89:12.3"
msgid ""
"Kiñcāpi taṃ hoti vatthaṃ parisuddhaṃ pariyodātaṃ, atha khvassa hoti yeva "
"anusahagato ūsagandho vā khāragandho vā gomayagandho vā asamūhato."
msgstr ""

msgctxt "sn22.89:12.4"
msgid ""
"Tamenaṃ rajako sāmikānaṃ deti. Tamenaṃ sāmikā gandhaparibhāvite karaṇḍake "
"nikkhipanti."
msgstr ""

msgctxt "sn22.89:12.5"
msgid ""
"Yopissa hoti anusahagato ūsagandho vā khāragandho vā gomayagandho vā "
"asamūhato, sopi samugghātaṃ gacchati."
msgstr ""

msgctxt "sn22.89:12.6"
msgid ""
"Evameva kho, āvuso, kiñcāpi ariyasāvakassa pañcorambhāgiyāni saṃyojanāni "
"pahīnāni bhavanti, atha khvassa hoti yeva pañcasu upādānakkhandhesu "
"anusahagato ‘asmī’ti, māno ‘asmī’ti, chando ‘asmī’ti anusayo asamūhato."
msgstr ""

msgctxt "sn22.89:12.7"
msgid ""
"So aparena samayena pañcasu upādānakkhandhesu udayabbayānupassī viharati."
msgstr ""

msgctxt "sn22.89:12.8"
msgid "‘Iti rūpaṃ, iti rūpassa samudayo, iti rūpassa atthaṅgamo;"
msgstr ""

msgctxt "sn22.89:12.9"
msgid "iti vedanā …"
msgstr ""

msgctxt "sn22.89:12.10"
msgid "iti saññā …"
msgstr ""

msgctxt "sn22.89:12.11"
msgid "iti saṅkhārā …"
msgstr ""

msgctxt "sn22.89:12.12"
msgid "iti viññāṇaṃ, iti viññāṇassa samudayo, iti viññāṇassa atthaṅgamo’ti."
msgstr ""

msgctxt "sn22.89:12.13"
msgid ""
"Tassa imesu pañcasu upādānakkhandhesu udayabbayānupassino viharato yopissa "
"hoti pañcasu upādānakkhandhesu anusahagato ‘asmī’ti, māno ‘asmī’ti, chando "
"‘asmī’ti anusayo asamūhato, sopi samugghātaṃ gacchatī”ti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.132"></a>
#. <a class="sc" id="sc13"></a>
msgctxt "sn22.89:13.1"
msgid "Evaṃ vutte, therā bhikkhū āyasmantaṃ khemakaṃ etadavocuṃ:"
msgstr ""

#. VAR: na kho → na kho pana (pts1, mr)
msgctxt "sn22.89:13.2"
msgid ""
"“na kho mayaṃ āyasmantaṃ khemakaṃ vihesāpekhā pucchimha, api cāyasmā khemako "
"pahosi tassa bhagavato sāsanaṃ vitthārena ācikkhituṃ desetuṃ paññāpetuṃ "
"paṭṭhapetuṃ vivarituṃ vibhajituṃ uttānīkātuṃ."
msgstr ""

msgctxt "sn22.89:13.3"
msgid ""
"Tayidaṃ āyasmatā khemakena tassa bhagavato sāsanaṃ vitthārena ācikkhitaṃ "
"desitaṃ paññāpitaṃ paṭṭhapitaṃ vivaritaṃ vibhajitaṃ uttānīkatan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc14"></a>
msgctxt "sn22.89:14.1"
msgid "Idamavoca āyasmā khemako."
msgstr ""

msgctxt "sn22.89:14.2"
msgid "Attamanā therā bhikkhū āyasmato khemakassa bhāsitaṃ abhinanduṃ."
msgstr ""

msgctxt "sn22.89:14.3"
msgid ""
"Imasmiñca pana veyyākaraṇasmiṃ bhaññamāne saṭṭhimattānaṃ therānaṃ bhikkhūnaṃ "
"anupādāya āsavehi cittāni vimucciṃsu, āyasmato khemakassa cāti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.89:14.4"
msgid "Sattamaṃ."
msgstr ""
