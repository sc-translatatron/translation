#. <section class="sutta" id="sn20.8"><article><div class="hgroup"><p class="division">
msgctxt "sn20.8:0.1"
msgid "Saṃyutta Nikāya 20"
msgstr ""

#. </p><p>
msgctxt "sn20.8:0.2"
msgid "1. Opammavagga"
msgstr ""

#. </p><h1>
#. VAR: 8. Kaliṅgarasutta → kaliṅgarūpadhānasuttaṃ (bj)
msgctxt "sn20.8:0.3"
msgid "8. Kaliṅgarasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn20.8:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn20.8:1.2"
msgid "ekaṃ samayaṃ bhagavā vesāliyaṃ viharati mahāvane kūṭāgārasālāyaṃ."
msgstr ""

msgctxt "sn20.8:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "sn20.8:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "sn20.8:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "sn20.8:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.268"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn20.8:2.1"
msgid ""
"“Kaliṅgarūpadhānā, bhikkhave, etarahi licchavī viharanti appamattā ātāpino "
"upāsanasmiṃ."
msgstr ""

msgctxt "sn20.8:2.2"
msgid ""
"Tesaṃ rājā māgadho ajātasattu vedehiputto na labhati otāraṃ na labhati "
"ārammaṇaṃ."
msgstr ""

#. VAR: sukhumālā → sukhumā (mr)
#. VAR: mudutalunahatthapādā → mudutalāhatthapādā (s1-3, km) | mudutaluṇahatthapādā (pts1-2)
msgctxt "sn20.8:2.3"
msgid ""
"Bhavissanti, bhikkhave, anāgatamaddhānaṃ licchavī sukhumālā "
"mudutalunahatthapādā"
msgstr ""

#. VAR: tūlabibbohanāsu → tūlabimbohanādīsu (bj) | thūlabimbohanāsu (s1-3, km) | tulabimbohanāsu (pts1-2) | tūlabibbohanādīsu (mr)
msgctxt "sn20.8:2.4"
msgid ""
"te mudukāsu seyyāsu tūlabibbohanāsu yāvasūriyuggamanā seyyaṃ kappissanti."
msgstr ""

msgctxt "sn20.8:2.5"
msgid ""
"Tesaṃ rājā māgadho ajātasattu vedehiputto lacchati otāraṃ lacchati ārammaṇaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn20.8:3.1"
msgid ""
"Kaliṅgarūpadhānā, bhikkhave, etarahi bhikkhū viharanti appamattā ātāpino "
"padhānasmiṃ."
msgstr ""

msgctxt "sn20.8:3.2"
msgid "Tesaṃ māro pāpimā na labhati otāraṃ na labhati ārammaṇaṃ."
msgstr ""

msgctxt "sn20.8:3.3"
msgid ""
"Bhavissanti, bhikkhave, anāgatamaddhānaṃ bhikkhū sukhumā "
"mudutalunahatthapādā."
msgstr ""

msgctxt "sn20.8:3.4"
msgid ""
"Te mudukāsu seyyāsu tūlabibbohanāsu yāvasūriyuggamanā seyyaṃ kappissanti."
msgstr ""

msgctxt "sn20.8:3.5"
msgid "Tesaṃ māro pāpimā lacchati otāraṃ lacchati ārammaṇaṃ."
msgstr ""

msgctxt "sn20.8:3.6"
msgid "Tasmātiha, bhikkhave, evaṃ sikkhitabbaṃ:"
msgstr ""

msgctxt "sn20.8:3.7"
msgid "‘kaliṅgarūpadhānā viharissāma appamattā ātāpino padhānasmin’ti."
msgstr ""

msgctxt "sn20.8:3.8"
msgid "Evañhi vo, bhikkhave, sikkhitabban”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn20.8:3.9"
msgid "Aṭṭhamaṃ."
msgstr ""
