#. <section class="sutta" id="sn11.20"><article><div class="hgroup"><p class="division">
msgctxt "sn11.20:0.1"
msgid "Saṃyutta Nikāya 11"
msgstr ""

#. </p><p>
msgctxt "sn11.20:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
#. VAR: 20. Saṅghavandanāsutta → tatiyasakkanamassanasuttaṃ (bj, s2, s3) | sakkanamassanasuttaṃ (3) (s1) | sakka-namassana (3) (pts1)
msgctxt "sn11.20:0.3"
msgid "20. Saṅghavandanāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.236"></a>
#. <a class="pts2ed" id="pts2ed1.506"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn11.20:1.1"
msgid "Sāvatthiyaṃ jetavane."
msgstr ""

msgctxt "sn11.20:1.2"
msgid "Tatra kho … pe … etadavoca:"
msgstr ""

msgctxt "sn11.20:1.3"
msgid "“bhūtapubbaṃ, bhikkhave, sakko devānamindo mātaliṃ saṅgāhakaṃ āmantesi:"
msgstr ""

msgctxt "sn11.20:1.4"
msgid ""
"‘yojehi, samma mātali, sahassayuttaṃ ājaññarathaṃ, uyyānabhūmiṃ gacchāma "
"subhūmiṃ dassanāyā’ti."
msgstr ""

msgctxt "sn11.20:1.5"
msgid ""
"‘Evaṃ, bhaddantavā’ti kho, bhikkhave, mātali saṅgāhako sakkassa "
"devānamindassa paṭissutvā, sahassayuttaṃ ājaññarathaṃ yojetvā sakkassa "
"devānamindassa paṭivedesi:"
msgstr ""

msgctxt "sn11.20:1.6"
msgid "‘yutto kho te, mārisa, sahassayutto ājaññaratho, "
msgstr ""

msgctxt "sn11.20:1.7"
msgid "yassadāni kālaṃ maññasī’ti."
msgstr ""

msgctxt "sn11.20:1.8"
msgid ""
"Atha kho, bhikkhave, sakko devānamindo vejayantapāsādā orohanto añjaliṃ "
"katvā sudaṃ bhikkhusaṃghaṃ namassati."
msgstr ""

msgctxt "sn11.20:1.9"
msgid ""
"Atha kho, bhikkhave, mātali saṅgāhako sakkaṃ devānamindaṃ gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn11.20:2.1"
msgid "‘Tañhi ete namasseyyuṃ,"
msgstr ""

#. <br>
msgctxt "sn11.20:2.2"
msgid "pūtidehasayā narā;"
msgstr ""

#. <br>
msgctxt "sn11.20:2.3"
msgid "Nimuggā kuṇapamhete,"
msgstr ""

#. <br>
msgctxt "sn11.20:2.4"
msgid "khuppipāsasamappitā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn11.20:3.1"
msgid "Kiṃ nu tesaṃ pihayasi,"
msgstr ""

#. <br>
msgctxt "sn11.20:3.2"
msgid "anāgārāna vāsava;"
msgstr ""

#. <br>
msgctxt "sn11.20:3.3"
msgid "Ācāraṃ isinaṃ brūhi,"
msgstr ""

#. <br>
msgctxt "sn11.20:3.4"
msgid "taṃ suṇoma vaco tavā’ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.507"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn11.20:4.1"
msgid "‘Etaṃ tesaṃ pihayāmi,"
msgstr ""

#. <br>
msgctxt "sn11.20:4.2"
msgid "anāgārāna mātali;"
msgstr ""

#. <br>
msgctxt "sn11.20:4.3"
msgid "Yamhā gāmā pakkamanti,"
msgstr ""

#. <br>
msgctxt "sn11.20:4.4"
msgid "anapekkhā vajanti te."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn11.20:5.1"
msgid "Na tesaṃ koṭṭhe openti,"
msgstr ""

#. <br>
#. VAR: kumbhi → kumbhe (bj) | kumbhā (s1-3, km, pts1, mr) | kumbhī (pts2)
#. VAR: kaḷopiyaṃ → khalopiyaṃ (bj)
msgctxt "sn11.20:5.2"
msgid "na kumbhi na kaḷopiyaṃ;"
msgstr ""

#. <br>
#. VAR: Paraniṭṭhitamesānā → paraniṭṭhitamesanā (s1-3, km, mr)
msgctxt "sn11.20:5.3"
msgid "Paraniṭṭhitamesānā,"
msgstr ""

#. <br>
msgctxt "sn11.20:5.4"
msgid "tena yāpenti subbatā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn11.20:6.1"
msgid "Sumantamantino dhīrā,"
msgstr ""

#. <br>
msgctxt "sn11.20:6.2"
msgid "tuṇhībhūtā samañcarā;"
msgstr ""

#. <br>
msgctxt "sn11.20:6.3"
msgid "Devā viruddhā asurehi,"
msgstr ""

#. <br>
msgctxt "sn11.20:6.4"
msgid "puthu maccā ca mātali."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn11.20:7.1"
msgid "Aviruddhā viruddhesu,"
msgstr ""

#. <br>
msgctxt "sn11.20:7.2"
msgid "attadaṇḍesu nibbutā;"
msgstr ""

#. <br>
msgctxt "sn11.20:7.3"
msgid "Sādānesu anādānā,"
msgstr ""

#. <br>
msgctxt "sn11.20:7.4"
msgid "te namassāmi mātalī’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn11.20:8.1"
msgid "‘Seṭṭhā hi kira lokasmiṃ,"
msgstr ""

#. <br>
msgctxt "sn11.20:8.2"
msgid "ye tvaṃ sakka namassasi;"
msgstr ""

#. <br>
msgctxt "sn11.20:8.3"
msgid "Ahampi te namassāmi,"
msgstr ""

#. <br>
msgctxt "sn11.20:8.4"
msgid "ye namassasi vāsavā’”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.508"></a>
#. <a class="sc" id="sc9"></a>
msgctxt "sn11.20:9.1"
msgid "Idaṃ vatvāna maghavā,"
msgstr ""

#. <br>
msgctxt "sn11.20:9.2"
msgid "devarājā sujampati;"
msgstr ""

#. <br>
msgctxt "sn11.20:9.3"
msgid "Bhikkhusaṅghaṃ namassitvā,"
msgstr ""

#. <br>
msgctxt "sn11.20:9.4"
msgid "pamukho rathamāruhīti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn11.20:9.5"
msgid "Dutiyo vaggo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc10"></a>
#. VAR: pana → vatapadena (bj, s1-3, km, pts2)
msgctxt "sn11.20:10.1"
msgid "Devā pana tayo vuttā,"
msgstr ""

#. <br>
msgctxt "sn11.20:10.2"
msgid "daliddañca rāmaṇeyyakaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.20:10.3"
msgid "Yajamānañca vandanā,"
msgstr ""

#. <br>
msgctxt "sn11.20:10.4"
msgid "tayo sakkanamassanāti."
msgstr ""
