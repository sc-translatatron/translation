#. <section class="sutta" id="sn10.7"><article><div class="hgroup"><p class="division">
msgctxt "sn10.7:0.1"
msgid "Saṃyutta Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "sn10.7:0.2"
msgid "1. Indakavagga"
msgstr ""

#. </p><h1>
msgctxt "sn10.7:0.3"
msgid "7. Punabbasusutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.210"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn10.7:1.1"
msgid ""
"Ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "sn10.7:1.2"
msgid ""
"Tena kho pana samayena bhagavā bhikkhū nibbānapaṭisaṃyuttāya dhammiyā "
"kathāya sandasseti samādapeti samuttejeti sampahaṃseti."
msgstr ""

msgctxt "sn10.7:1.3"
msgid ""
"Te ca bhikkhū aṭṭhiṃ katvā manasi katvā sabbacetasā samannāharitvā ohitasotā "
"dhammaṃ suṇanti."
msgstr ""

msgctxt "sn10.7:1.4"
msgid "Atha kho punabbasumātā yakkhinī puttake evaṃ tosesi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.453"></a>
#. <a class="sc" id="sc2"></a>
#. <a class="verse-num-sc" id="verse-num-sc828"></a>
msgctxt "sn10.7:2.1"
msgid "“Tuṇhī uttarike hohi,"
msgstr ""

#. <br>
msgctxt "sn10.7:2.2"
msgid "tuṇhī hohi punabbasu;"
msgstr ""

#. <br>
msgctxt "sn10.7:2.3"
msgid "Yāvāhaṃ buddhaseṭṭhassa,"
msgstr ""

#. <br>
msgctxt "sn10.7:2.4"
msgid "dhammaṃ sossāmi satthuno."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
#. <a class="verse-num-sc" id="verse-num-sc829"></a>
msgctxt "sn10.7:3.1"
msgid "Nibbānaṃ bhagavā āha,"
msgstr ""

#. <br>
msgctxt "sn10.7:3.2"
msgid "sabbaganthappamocanaṃ;"
msgstr ""

#. <br>
msgctxt "sn10.7:3.3"
msgid "Ativelā ca me hoti,"
msgstr ""

#. <br>
msgctxt "sn10.7:3.4"
msgid "asmiṃ dhamme piyāyanā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
#. <a class="verse-num-sc" id="verse-num-sc830"></a>
msgctxt "sn10.7:4.1"
msgid "Piyo loke sako putto,"
msgstr ""

#. <br>
msgctxt "sn10.7:4.2"
msgid "piyo loke sako pati;"
msgstr ""

#. <br>
msgctxt "sn10.7:4.3"
msgid "Tato piyatarā mayhaṃ,"
msgstr ""

#. <br>
msgctxt "sn10.7:4.4"
msgid "assa dhammassa magganā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
#. <a class="verse-num-sc" id="verse-num-sc831"></a>
msgctxt "sn10.7:5.1"
msgid "Na hi putto pati vāpi,"
msgstr ""

#. <br>
msgctxt "sn10.7:5.2"
msgid "piyo dukkhā pamocaye;"
msgstr ""

#. <br>
msgctxt "sn10.7:5.3"
msgid "Yathā saddhammassavanaṃ,"
msgstr ""

#. <br>
msgctxt "sn10.7:5.4"
msgid "dukkhā moceti pāṇinaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
#. <a class="verse-num-sc" id="verse-num-sc832"></a>
msgctxt "sn10.7:6.1"
msgid "Loke dukkhaparetasmiṃ,"
msgstr ""

#. <br>
msgctxt "sn10.7:6.2"
msgid "Jarāmaraṇasaṃyute;"
msgstr ""

#. <br>
msgctxt "sn10.7:6.3"
msgid "Jarāmaraṇamokkhāya,"
msgstr ""

#. <br>
msgctxt "sn10.7:6.4"
msgid "Yaṃ dhammaṃ abhisambudhaṃ;"
msgstr ""

#. <br>
msgctxt "sn10.7:6.5"
msgid "Taṃ dhammaṃ sotumicchāmi,"
msgstr ""

#. <br>
msgctxt "sn10.7:6.6"
msgid "Tuṇhī hohi punabbasū”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.454"></a>
#. <a class="sc" id="sc7"></a>
#. <a class="verse-num-sc" id="verse-num-sc833"></a>
msgctxt "sn10.7:7.1"
msgid "“Ammā na byāharissāmi,"
msgstr ""

#. <br>
msgctxt "sn10.7:7.2"
msgid "tuṇhībhūtāyamuttarā;"
msgstr ""

#. <br>
msgctxt "sn10.7:7.3"
msgid "Dhammameva nisāmehi,"
msgstr ""

#. <br>
msgctxt "sn10.7:7.4"
msgid "saddhammassavanaṃ sukhaṃ;"
msgstr ""

#. <br>
msgctxt "sn10.7:7.5"
msgid "Saddhammassa anaññāya,"
msgstr ""

#. <br>
msgctxt "sn10.7:7.6"
msgid "ammā dukkhaṃ carāmase."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
#. <a class="verse-num-sc" id="verse-num-sc834"></a>
msgctxt "sn10.7:8.1"
msgid "Esa devamanussānaṃ,"
msgstr ""

#. <br>
msgctxt "sn10.7:8.2"
msgid "sammūḷhānaṃ pabhaṅkaro;"
msgstr ""

#. <br>
msgctxt "sn10.7:8.3"
msgid "Buddho antimasārīro,"
msgstr ""

#. <br>
msgctxt "sn10.7:8.4"
msgid "dhammaṃ deseti cakkhumā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
#. <a class="verse-num-sc" id="verse-num-sc835"></a>
msgctxt "sn10.7:9.1"
msgid "“Sādhu kho paṇḍito nāma,"
msgstr ""

#. <br>
msgctxt "sn10.7:9.2"
msgid "putto jāto uresayo;"
msgstr ""

#. <br>
msgctxt "sn10.7:9.3"
msgid "Putto me buddhaseṭṭhassa,"
msgstr ""

#. <br>
msgctxt "sn10.7:9.4"
msgid "dhammaṃ suddhaṃ piyāyati."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.455"></a>
#. <a class="sc" id="sc10"></a>
#. <a class="verse-num-sc" id="verse-num-sc836"></a>
msgctxt "sn10.7:10.1"
msgid "Punabbasu sukhī hohi,"
msgstr ""

#. <br>
msgctxt "sn10.7:10.2"
msgid "ajjāhamhi samuggatā;"
msgstr ""

#. <br>
msgctxt "sn10.7:10.3"
msgid "Diṭṭhāni ariyasaccāni,"
msgstr ""

#. <br>
msgctxt "sn10.7:10.4"
msgid "uttarāpi suṇātu me”ti."
msgstr ""
