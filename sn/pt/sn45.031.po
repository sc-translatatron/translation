#. <section class="sutta" id="sn45.31"><article><div class="hgroup"><p class="division">
msgctxt "sn45.31:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.31:0.2"
msgid "4. Paṭipattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.31:0.3"
msgid "31. Paṭhamapaṭipattisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.23"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.31:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.31:1.2"
msgid "“Micchāpaṭipattiñca vo, bhikkhave, desessāmi, sammāpaṭipattiñca."
msgstr ""

msgctxt "sn45.31:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn45.31:1.4"
msgid "Katamā ca, bhikkhave, micchāpaṭipatti?"
msgstr ""

msgctxt "sn45.31:1.5"
msgid "Seyyathidaṃ—micchādiṭṭhi … pe … micchāsamādhi."
msgstr ""

msgctxt "sn45.31:1.6"
msgid "Ayaṃ vuccati, bhikkhave, micchāpaṭipatti."
msgstr ""

msgctxt "sn45.31:1.7"
msgid "Katamā ca, bhikkhave, sammāpaṭipatti?"
msgstr ""

msgctxt "sn45.31:1.8"
msgid "Seyyathidaṃ—sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

msgctxt "sn45.31:1.9"
msgid "Ayaṃ vuccati, bhikkhave, sammāpaṭipattī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.31:1.10"
msgid "Paṭhamaṃ."
msgstr ""
