#. <section class="sutta" id="sn43.2"><article><div class="hgroup"><p class="division">
msgctxt "sn43.2:0.1"
msgid "Saṃyutta Nikāya 43"
msgstr ""

#. </p><p>
msgctxt "sn43.2:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
#. VAR: 2. Samathavipassanāsutta → samatho (pts1)
msgctxt "sn43.2:0.3"
msgid "2. Samathavipassanāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.360"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn43.2:1.1"
msgid "“Asaṅkhatañca vo, bhikkhave, desessāmi asaṅkhatagāmiñca maggaṃ."
msgstr ""

msgctxt "sn43.2:1.2"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn43.2:1.3"
msgid "Katamañca, bhikkhave, asaṅkhataṃ?"
msgstr ""

msgctxt "sn43.2:1.4"
msgid "Yo, bhikkhave, rāgakkhayo dosakkhayo mohakkhayo—"
msgstr ""

msgctxt "sn43.2:1.5"
msgid "idaṃ vuccati, bhikkhave, asaṅkhataṃ."
msgstr ""

msgctxt "sn43.2:1.6"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.2:1.7"
msgid "Samatho ca vipassanā ca."
msgstr ""

msgctxt "sn43.2:1.8"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn43.2:1.9"
msgid "Dutiyaṃ."
msgstr ""
