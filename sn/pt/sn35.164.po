#. <section class="sutta" id="sn35.164"><article><div class="hgroup"><p class="division">
msgctxt "sn35.164:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.164:0.2"
msgid "16. Nandikkhayavagga"
msgstr ""

#. </p><h1>
#. VAR: 164. Koṭṭhikaanattasutta → koṭṭhitaanattasuttaṃ (bj) | koṭṭhiko 3 (pts1)
msgctxt "sn35.164:0.3"
msgid "164. Koṭṭhikaanattasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.147"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.164:1.1"
msgid "Ekamantaṃ … pe …"
msgstr ""

msgctxt "sn35.164:1.2"
msgid "vihareyyanti."
msgstr ""

msgctxt "sn35.164:1.3"
msgid "“Yo kho, koṭṭhika, anattā tatra te chando pahātabbo."
msgstr ""

msgctxt "sn35.164:1.4"
msgid "Ko ca, koṭṭhika, anattā?"
msgstr ""

msgctxt "sn35.164:1.5"
msgid "Cakkhu kho, koṭṭhika, anattā; tatra te chando pahātabbo."
msgstr ""

msgctxt "sn35.164:1.6"
msgid "Rūpā anattā; tatra te chando pahātabbo."
msgstr ""

msgctxt "sn35.164:1.7"
msgid "Cakkhuviññāṇaṃ anattā; tatra te chando pahātabbo."
msgstr ""

msgctxt "sn35.164:1.8"
msgid "Cakkhusamphasso anattā; tatra te chando pahātabbo."
msgstr ""

msgctxt "sn35.164:1.9"
msgid ""
"Yampidaṃ cakkhusamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi anattā; tatra te chando pahātabbo … pe …"
msgstr ""

msgctxt "sn35.164:1.10"
msgid "jivhā anattā; tatra te chando pahātabbo … pe …"
msgstr ""

msgctxt "sn35.164:1.11"
msgid "mano anattā; tatra te chando pahātabbo."
msgstr ""

msgctxt "sn35.164:1.12"
msgid "Dhammā anattā; tatra te chando pahātabbo."
msgstr ""

msgctxt "sn35.164:1.13"
msgid "Manoviññāṇaṃ …"
msgstr ""

msgctxt "sn35.164:1.14"
msgid "manosamphasso …"
msgstr ""

msgctxt "sn35.164:1.15"
msgid ""
"yampidaṃ manosamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi anattā; tatra te chando pahātabbo."
msgstr ""

msgctxt "sn35.164:1.16"
msgid "Yo kho, koṭṭhika, anattā, tatra te chando pahātabbo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.164:1.17"
msgid "Navamaṃ."
msgstr ""
