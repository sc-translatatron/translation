#. <section class="sutta" id="sn14.4"><article><div class="hgroup"><p class="division">
msgctxt "sn14.4:0.1"
msgid "Saṃyutta Nikāya 14"
msgstr ""

#. </p><p>
msgctxt "sn14.4:0.2"
msgid "1. Nānattavagga"
msgstr ""

#. </p><h1>
#. VAR: 4. Vedanānānattasutta → novedanānānattasuttaṃ (bj) | vedanā 1 (pts1-2)
msgctxt "sn14.4:0.3"
msgid "4. Vedanānānattasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.142"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn14.4:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn14.4:1.2"
msgid ""
"“Dhātunānattaṃ, bhikkhave, paṭicca uppajjati phassanānattaṃ, phassanānattaṃ "
"paṭicca uppajjati vedanānānattaṃ."
msgstr ""

msgctxt "sn14.4:1.3"
msgid "Katamañca, bhikkhave, dhātunānattaṃ?"
msgstr ""

msgctxt "sn14.4:1.4"
msgid "Cakkhudhātu … pe … manodhātu—"
msgstr ""

msgctxt "sn14.4:1.5"
msgid "idaṃ vuccati, bhikkhave, dhātunānattaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn14.4:2.1"
msgid ""
"Kathañca, bhikkhave, dhātunānattaṃ paṭicca uppajjati phassanānattaṃ, "
"phassanānattaṃ paṭicca uppajjati vedanānānattaṃ?"
msgstr ""

msgctxt "sn14.4:2.2"
msgid ""
"Cakkhudhātuṃ, bhikkhave, paṭicca uppajjati cakkhusamphasso, cakkhusamphassaṃ "
"paṭicca uppajjati cakkhusamphassajā vedanā … pe …"
msgstr ""

msgctxt "sn14.4:2.3"
msgid ""
"manodhātuṃ paṭicca uppajjati manosamphasso, manosamphassaṃ paṭicca uppajjati "
"manosamphassajā vedanā."
msgstr ""

msgctxt "sn14.4:2.4"
msgid ""
"Evaṃ kho, bhikkhave, dhātunānattaṃ paṭicca uppajjati phassanānattaṃ, "
"phassanānattaṃ paṭicca uppajjati vedanānānattan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn14.4:2.5"
msgid "Catutthaṃ."
msgstr ""
