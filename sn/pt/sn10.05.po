#. <section class="sutta" id="sn10.5"><article><div class="hgroup"><p class="division">
msgctxt "sn10.5:0.1"
msgid "Saṃyutta Nikāya 10"
msgstr ""

#. </p><p>
msgctxt "sn10.5:0.2"
msgid "1. Indakavagga"
msgstr ""

#. </p><h1>
msgctxt "sn10.5:0.3"
msgid "5. Sānusutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. <a class="verse-num-sc" id="verse-num-sc815"></a>
msgctxt "sn10.5:1.1"
msgid ""
"Ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "sn10.5:1.2"
msgid ""
"Tena kho pana samayena aññatarissā upāsikāya sānu nāma putto yakkhena gahito "
"hoti."
msgstr ""

msgctxt "sn10.5:1.3"
msgid "Atha kho sā upāsikā paridevamānā tāyaṃ velāyaṃ imā gāthāyo abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.449"></a>
#. <a class="sc" id="sc2"></a>
#. <a class="verse-num-sc" id="verse-num-sc816"></a>
msgctxt "sn10.5:2.1"
msgid "“Cātuddasiṃ pañcadasiṃ,"
msgstr ""

#. <br>
msgctxt "sn10.5:2.2"
msgid "yā ca pakkhassa aṭṭhamī;"
msgstr ""

#. <br>
msgctxt "sn10.5:2.3"
msgid "Pāṭihāriyapakkhañca,"
msgstr ""

#. <br>
msgctxt "sn10.5:2.4"
msgid "aṭṭhaṅgasusamāgataṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
#. <a class="verse-num-sc" id="verse-num-sc817"></a>
msgctxt "sn10.5:3.1"
msgid "Uposathaṃ upavasanti,"
msgstr ""

#. <br>
msgctxt "sn10.5:3.2"
msgid "brahmacariyaṃ caranti ye;"
msgstr ""

#. <br>
msgctxt "sn10.5:3.3"
msgid "Na tehi yakkhā kīḷanti,"
msgstr ""

#. <br>
msgctxt "sn10.5:3.4"
msgid "iti me arahataṃ sutaṃ;"
msgstr ""

#. <br>
msgctxt "sn10.5:3.5"
msgid "Sā dāni ajja passāmi,"
msgstr ""

#. <br>
msgctxt "sn10.5:3.6"
msgid "yakkhā kīḷanti sānunā”ti."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.209"></a>
#. <a class="sc" id="sc4"></a>
#. <a class="verse-num-sc" id="verse-num-sc818"></a>
msgctxt "sn10.5:4.1"
msgid "“Cātuddasiṃ pañcadasiṃ,"
msgstr ""

#. <br>
msgctxt "sn10.5:4.2"
msgid "yā ca pakkhassa aṭṭhamī;"
msgstr ""

#. <br>
msgctxt "sn10.5:4.3"
msgid "Pāṭihāriyapakkhañca,"
msgstr ""

#. <br>
msgctxt "sn10.5:4.4"
msgid "aṭṭhaṅgasusamāgataṃ;"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
#. <a class="verse-num-sc" id="verse-num-sc819"></a>
msgctxt "sn10.5:5.1"
msgid "Uposathaṃ upavasanti,"
msgstr ""

#. <br>
msgctxt "sn10.5:5.2"
msgid "brahmacariyaṃ caranti ye."
msgstr ""

#. <br>
msgctxt "sn10.5:5.3"
msgid "Na tehi yakkhā kīḷanti,"
msgstr ""

#. <br>
msgctxt "sn10.5:5.4"
msgid "sāhu te arahataṃ sutaṃ;"
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.450"></a>
#. <a class="sc" id="sc6"></a>
#. <a class="verse-num-sc" id="verse-num-sc820"></a>
msgctxt "sn10.5:6.1"
msgid "Sānuṃ pabuddhaṃ vajjāsi,"
msgstr ""

#. <br>
msgctxt "sn10.5:6.2"
msgid "yakkhānaṃ vacanaṃ idaṃ;"
msgstr ""

#. <br>
msgctxt "sn10.5:6.3"
msgid "Mākāsi pāpakaṃ kammaṃ,"
msgstr ""

#. <br>
msgctxt "sn10.5:6.4"
msgid "āvi vā yadi vā raho."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
#. <a class="verse-num-sc" id="verse-num-sc821"></a>
#. VAR: Sace ca → yañceva (bj) | saceva (s1-3, km, pts1, mr)
msgctxt "sn10.5:7.1"
msgid "Sace ca pāpakaṃ kammaṃ,"
msgstr ""

#. <br>
msgctxt "sn10.5:7.2"
msgid "karissasi karosi vā;"
msgstr ""

#. <br>
msgctxt "sn10.5:7.3"
msgid "Na te dukkhā pamutyatthi,"
msgstr ""

#. <br>
msgctxt "sn10.5:7.4"
msgid "uppaccāpi palāyato”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
#. <a class="verse-num-sc" id="verse-num-sc822"></a>
msgctxt "sn10.5:8.1"
msgid "“Mataṃ vā amma rodanti,"
msgstr ""

#. <br>
msgctxt "sn10.5:8.2"
msgid "yo vā jīvaṃ na dissati;"
msgstr ""

#. <br>
msgctxt "sn10.5:8.3"
msgid "Jīvantaṃ amma passantī,"
msgstr ""

#. <br>
msgctxt "sn10.5:8.4"
msgid "kasmā maṃ amma rodasī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
#. <a class="verse-num-sc" id="verse-num-sc823"></a>
msgctxt "sn10.5:9.1"
msgid "“Mataṃ vā putta rodanti,"
msgstr ""

#. <br>
msgctxt "sn10.5:9.2"
msgid "yo vā jīvaṃ na dissati;"
msgstr ""

#. <br>
msgctxt "sn10.5:9.3"
msgid "Yo ca kāme cajitvāna,"
msgstr ""

#. <br>
msgctxt "sn10.5:9.4"
msgid "punarāgacchate idha;"
msgstr ""

#. <br>
msgctxt "sn10.5:9.5"
msgid "Taṃ vāpi putta rodanti,"
msgstr ""

#. <br>
msgctxt "sn10.5:9.6"
msgid "puna jīvaṃ mato hi so."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.451"></a>
#. <a class="sc" id="sc10"></a>
#. <a class="verse-num-sc" id="verse-num-sc824"></a>
msgctxt "sn10.5:10.1"
msgid "Kukkuḷā ubbhato tāta,"
msgstr ""

#. <br>
#. VAR: kukkuḷaṃ → kukkulā (si) | kukkulaṃ (s3)
msgctxt "sn10.5:10.2"
msgid "kukkuḷaṃ patitumicchasi;"
msgstr ""

#. <br>
msgctxt "sn10.5:10.3"
msgid "Narakā ubbhato tāta,"
msgstr ""

#. <br>
msgctxt "sn10.5:10.4"
msgid "narakaṃ patitumicchasi."
msgstr ""

# It seems that here we see the origin of bhadanta = bhante, i.e. “good luck to thee”
#. </p><p>
#. <a class="sc" id="sc11"></a>
#. <a class="verse-num-sc" id="verse-num-sc825"></a>
msgctxt "sn10.5:11.1"
msgid "Abhidhāvatha bhaddante,"
msgstr ""

#. <br>
msgctxt "sn10.5:11.2"
msgid "kassa ujjhāpayāmase;"
msgstr ""

#. <br>
#. VAR: nīhataṃ → nīhaṭaṃ (bj) | nibbhataṃ (s1-3, km, mr) | nibhataṃ (pts1, mr)
msgctxt "sn10.5:11.3"
msgid "Ādittā nīhataṃ bhaṇḍaṃ,"
msgstr ""

# Phrasing as a question is suggested by CAF R-D tran. Otherwise I can’t make sense of “bhanda” in the previous line.
#. <br>
msgctxt "sn10.5:11.4"
msgid "puna ḍayhitumicchasī”ti."
msgstr ""
