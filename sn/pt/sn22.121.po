#. <section class="sutta" id="sn22.121"><article><div class="hgroup"><p class="division">
msgctxt "sn22.121:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.121:0.2"
msgid "12. Dhammakathikavagga"
msgstr ""

#. </p><h1>
#. VAR: 121. Upādāniyasutta → upādānam (pts1)
msgctxt "sn22.121:0.3"
msgid "121. Upādāniyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.121:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.121:1.2"
msgid "“Upādāniye ca, bhikkhave, dhamme desessāmi upādānañca."
msgstr ""

msgctxt "sn22.121:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn22.121:1.4"
msgid "Katame ca, bhikkhave, upādāniyā dhammā, katamaṃ upādānaṃ?"
msgstr ""

msgctxt "sn22.121:1.5"
msgid "Rūpaṃ, bhikkhave, upādāniyo dhammo, "
msgstr ""

msgctxt "sn22.121:1.6"
msgid "yo tattha chandarāgo, taṃ tattha upādānaṃ."
msgstr ""

msgctxt "sn22.121:1.7"
msgid "Vedanā … pe …"
msgstr ""

msgctxt "sn22.121:1.8"
msgid "saññā …"
msgstr ""

msgctxt "sn22.121:1.9"
msgid "saṅkhārā …"
msgstr ""

msgctxt "sn22.121:1.10"
msgid "viññāṇaṃ upādāniyo dhammo;"
msgstr ""

msgctxt "sn22.121:1.11"
msgid "yo tattha chandarāgo, taṃ tattha upādānaṃ."
msgstr ""

msgctxt "sn22.121:1.12"
msgid "Ime vuccanti, bhikkhave, upādāniyā dhammā, idaṃ upādānan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.121:1.13"
msgid "Navamaṃ."
msgstr ""
