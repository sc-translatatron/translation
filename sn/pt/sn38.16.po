#. <section class="sutta" id="sn38.16"><article><div class="hgroup"><p class="division">
msgctxt "sn38.16:0.1"
msgid "Saṃyutta Nikāya 38"
msgstr ""

#. </p><p>
msgctxt "sn38.16:0.2"
msgid "1. Jambukhādakavagga"
msgstr ""

#. </p><h1>
msgctxt "sn38.16:0.3"
msgid "16. Dukkarapañhāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn38.16:1.1"
msgid "“Kiṃ nu kho, āvuso sāriputta, imasmiṃ dhammavinaye dukkaran”ti?"
msgstr ""

msgctxt "sn38.16:1.2"
msgid "“Pabbajjā kho, āvuso, imasmiṃ dhammavinaye dukkarā”ti."
msgstr ""

msgctxt "sn38.16:1.3"
msgid "“Pabbajitena panāvuso, kiṃ dukkaran”ti?"
msgstr ""

msgctxt "sn38.16:1.4"
msgid "“Pabbajitena kho, āvuso, abhirati dukkarā”ti."
msgstr ""

msgctxt "sn38.16:1.5"
msgid "“Abhiratena panāvuso, kiṃ dukkaran”ti?"
msgstr ""

msgctxt "sn38.16:1.6"
msgid "“Abhiratena kho, āvuso, dhammānudhammappaṭipatti dukkarā”ti."
msgstr ""

msgctxt "sn38.16:1.7"
msgid "“Kīvaciraṃ panāvuso, dhammānudhammappaṭipanno bhikkhu arahaṃ assā”ti?"
msgstr ""

msgctxt "sn38.16:1.8"
msgid "“Naciraṃ, āvuso”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn38.16:1.9"
msgid "Soḷasamaṃ."
msgstr ""

#. </p><p class="endsutta">
msgctxt "sn38.16:1.10"
msgid "Jambukhādakasaṃyuttaṃ samattaṃ."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.261"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn38.16:2.1"
msgid "Nibbānaṃ arahattañca,"
msgstr ""

#. <br>
msgctxt "sn38.16:2.2"
msgid "dhammavādī kimatthiyaṃ;"
msgstr ""

#. <br>
msgctxt "sn38.16:2.3"
msgid "Assāso paramassāso,"
msgstr ""

#. <br>
msgctxt "sn38.16:2.4"
msgid "vedanā āsavāvijjā;"
msgstr ""

#. <br>
msgctxt "sn38.16:2.5"
msgid "Taṇhā oghā upādānaṃ,"
msgstr ""

#. <br>
msgctxt "sn38.16:2.6"
msgid "bhavo dukkhañca sakkāyo."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn38.16:2.7"
msgid "Imasmiṃ dhammavinaye dukkaranti."
msgstr ""
