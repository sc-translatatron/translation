#. <section class="sutta" id="sn22.97"><article><div class="hgroup"><p class="division">
msgctxt "sn22.97:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.97:0.2"
msgid "10. Pupphavagga"
msgstr ""

#. </p><h1>
#. VAR: 97. Nakhasikhāsutta → nakhasikhopamasuttaṃ (bj) | nakhāsikam (pts1)
msgctxt "sn22.97:0.3"
msgid "97. Nakhasikhāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.97:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.97:1.2"
msgid "Ekamantaṃ nisinno kho so bhikkhu bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn22.97:1.3"
msgid ""
"“atthi nu kho, bhante, kiñci rūpaṃ yaṃ rūpaṃ niccaṃ dhuvaṃ sassataṃ "
"avipariṇāmadhammaṃ sassatisamaṃ tatheva ṭhassati?"
msgstr ""

msgctxt "sn22.97:1.4"
msgid ""
"Atthi nu kho, bhante, kāci vedanā yā vedanā niccā dhuvā sassatā "
"avipariṇāmadhammā sassatisamaṃ tatheva ṭhassati?"
msgstr ""

msgctxt "sn22.97:1.5"
msgid "Atthi nu kho, bhante, kāci saññā … pe …"
msgstr ""

msgctxt "sn22.97:1.6"
msgid ""
"keci saṅkhārā, ye saṅkhārā niccā dhuvā sassatā avipariṇāmadhammā "
"sassatisamaṃ tatheva ṭhassanti?"
msgstr ""

msgctxt "sn22.97:1.7"
msgid ""
"Atthi nu kho, bhante, kiñci viññāṇaṃ, yaṃ viññāṇaṃ niccaṃ dhuvaṃ sassataṃ "
"avipariṇāmadhammaṃ sassatisamaṃ tatheva ṭhassatī”ti?"
msgstr ""

msgctxt "sn22.97:1.8"
msgid ""
"“Natthi kho, bhikkhu, kiñci rūpaṃ, yaṃ rūpaṃ niccaṃ dhuvaṃ sassataṃ "
"avipariṇāmadhammaṃ sassatisamaṃ tatheva ṭhassati."
msgstr ""

msgctxt "sn22.97:1.9"
msgid "Natthi kho, bhikkhu, kāci vedanā …"
msgstr ""

msgctxt "sn22.97:1.10"
msgid "kāci saññā …"
msgstr ""

msgctxt "sn22.97:1.11"
msgid "keci saṅkhārā … pe …"
msgstr ""

msgctxt "sn22.97:1.12"
msgid ""
"kiñci viññāṇaṃ, yaṃ viññāṇaṃ niccaṃ dhuvaṃ sassataṃ avipariṇāmadhammaṃ "
"sassatisamaṃ tatheva ṭhassatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn22.97:2.1"
msgid ""
"Atha kho bhagavā parittaṃ nakhasikhāyaṃ paṃsuṃ āropetvā taṃ bhikkhuṃ "
"etadavoca:"
msgstr ""

msgctxt "sn22.97:2.2"
msgid ""
"“ettakampi kho, bhikkhu, rūpaṃ natthi niccaṃ dhuvaṃ sassataṃ "
"avipariṇāmadhammaṃ sassatisamaṃ tatheva ṭhassati."
msgstr ""

msgctxt "sn22.97:2.3"
msgid ""
"Ettakañcepi, bhikkhu, rūpaṃ abhavissa niccaṃ dhuvaṃ sassataṃ "
"avipariṇāmadhammaṃ, nayidaṃ brahmacariyavāso paññāyetha sammā dukkhakkhayāya."
msgstr ""

msgctxt "sn22.97:2.4"
msgid ""
"Yasmā ca kho, bhikkhu, ettakampi rūpaṃ natthi niccaṃ dhuvaṃ sassataṃ "
"avipariṇāmadhammaṃ, tasmā brahmacariyavāso paññāyati sammā dukkhakkhayāya”."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.148"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn22.97:3.1"
msgid ""
"Ettakāpi kho, bhikkhu, vedanā natthi niccā dhuvā sassatā avipariṇāmadhammā "
"sassatisamaṃ tatheva ṭhassati."
msgstr ""

msgctxt "sn22.97:3.2"
msgid ""
"Ettakā cepi, bhikkhu, vedanā abhavissa niccā dhuvā sassatā "
"avipariṇāmadhammā, na yidaṃ brahmacariyavāso paññāyetha sammā dukkhakkhayāya."
msgstr ""

msgctxt "sn22.97:3.3"
msgid ""
"Yasmā ca kho, bhikkhu, ettakāpi vedanā natthi niccā dhuvā sassatā "
"avipariṇāmadhammā, tasmā brahmacariyavāso paññāyati sammā dukkhakkhayāya."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn22.97:4.1"
msgid "Ettakāpi kho, bhikkhu, saññā natthi … pe …"
msgstr ""

msgctxt "sn22.97:4.2"
msgid ""
"ettakāpi kho, bhikkhu, saṅkhārā natthi niccā dhuvā sassatā avipariṇāmadhammā "
"sassatisamaṃ tatheva ṭhassanti."
msgstr ""

msgctxt "sn22.97:4.3"
msgid ""
"Ettakā cepi, bhikkhu, saṅkhārā abhavissaṃsu niccā dhuvā sassatā "
"avipariṇāmadhammā, na yidaṃ brahmacariyavāso paññāyetha sammā dukkhakkhayāya."
msgstr ""

msgctxt "sn22.97:4.4"
msgid ""
"Yasmā ca kho, bhikkhu, ettakāpi saṅkhārā natthi niccā dhuvā sassatā "
"avipariṇāmadhammā, tasmā brahmacariyavāso paññāyati sammā dukkhakkhayāya."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn22.97:5.1"
msgid ""
"Ettakampi kho, bhikkhu, viññāṇaṃ natthi niccaṃ dhuvaṃ sassataṃ "
"avipariṇāmadhammaṃ sassatisamaṃ tatheva ṭhassati."
msgstr ""

msgctxt "sn22.97:5.2"
msgid ""
"Ettakampi kho, bhikkhu, viññāṇaṃ abhavissa niccaṃ dhuvaṃ sassataṃ "
"avipariṇāmadhammaṃ, na yidaṃ brahmacariyavāso paññāyetha sammā "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn22.97:5.3"
msgid ""
"Yasmā ca kho, bhikkhu, ettakampi viññāṇaṃ natthi niccaṃ dhuvaṃ sassataṃ "
"avipariṇāmadhammaṃ, tasmā brahmacariyavāso paññāyati sammā dukkhakkhayāya."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.149"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "sn22.97:6.1"
msgid "Taṃ kiṃ maññasi, bhikkhu, "
msgstr ""

msgctxt "sn22.97:6.2"
msgid "rūpaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn22.97:6.3"
msgid "“Aniccaṃ, bhante”."
msgstr ""

msgctxt "sn22.97:6.4"
msgid "“Vedanā …"
msgstr ""

msgctxt "sn22.97:6.5"
msgid "saññā …"
msgstr ""

msgctxt "sn22.97:6.6"
msgid "saṅkhārā …"
msgstr ""

msgctxt "sn22.97:6.7"
msgid "viññāṇaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn22.97:6.8"
msgid "“Aniccaṃ, bhante” … pe …"
msgstr ""

msgctxt "sn22.97:6.9"
msgid "“tasmātiha … pe …"
msgstr ""

msgctxt "sn22.97:6.10"
msgid "evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn22.97:6.11"
msgid "nāparaṃ itthattāyāti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.97:6.12"
msgid "Pañcamaṃ."
msgstr ""
