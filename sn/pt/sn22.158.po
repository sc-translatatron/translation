#. <section class="sutta" id="sn22.158"><article><div class="hgroup"><p class="division">
msgctxt "sn22.158:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.158:0.2"
msgid "15. Diṭṭhivagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.158:0.3"
msgid "158. Dutiyaabhinivesasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.187"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.158:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

#. VAR: saṃyojanābhinivesavinibandhājjhosānā”ti → vinibandhā ajjhosānāti (mr)
msgctxt "sn22.158:1.2"
msgid ""
"“Kismiṃ nu kho, bhikkhave, sati, kiṃ upādāya, kiṃ abhinivissa uppajjanti "
"saṃyojanābhinivesavinibandhājjhosānā”ti?"
msgstr ""

msgctxt "sn22.158:1.3"
msgid "Bhagavaṃmūlakā no, bhante, dhammā … pe …"
msgstr ""

msgctxt "sn22.158:1.4"
msgid ""
"“rūpe kho, bhikkhave, sati, rūpaṃ upādāya, rūpaṃ abhinivissa uppajjanti "
"saṃyojanābhinivesavinibandhājjhosānā."
msgstr ""

msgctxt "sn22.158:1.5"
msgid "Vedanāya sati …"
msgstr ""

msgctxt "sn22.158:1.6"
msgid "saññāya sati …"
msgstr ""

msgctxt "sn22.158:1.7"
msgid "saṅkhāresu sati …"
msgstr ""

msgctxt "sn22.158:1.8"
msgid ""
"viññāṇe sati, viññāṇaṃ upādāya, viññāṇaṃ abhinivissa uppajjanti "
"saṃyojanābhinivesavinibandhājjhosānā."
msgstr ""

msgctxt "sn22.158:1.9"
msgid "Taṃ kiṃ maññatha, bhikkhave, "
msgstr ""

msgctxt "sn22.158:1.10"
msgid "rūpaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn22.158:1.11"
msgid "“Aniccaṃ, bhante”."
msgstr ""

msgctxt "sn22.158:1.12"
msgid "“Yaṃ panāniccaṃ … pe …"
msgstr ""

msgctxt "sn22.158:1.13"
msgid ""
"api nu taṃ anupādāya uppajjeyyuṃ saṃyojanābhinivesavinibandhājjhosānā”ti?"
msgstr ""

msgctxt "sn22.158:1.14"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn22.158:1.15"
msgid "“Evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn22.158:1.16"
msgid "nāparaṃ itthattāyāti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.158:1.17"
msgid "Navamaṃ."
msgstr ""
