#. <section class="sutta" id="sn51.30"><article><div class="hgroup"><p class="division">
msgctxt "sn51.30:0.1"
msgid "Saṃyutta Nikāya 51"
msgstr ""

#. </p><p>
msgctxt "sn51.30:0.2"
msgid "3. Ayoguḷavagga"
msgstr ""

#. </p><h1>
msgctxt "sn51.30:0.3"
msgid "30. Dutiyabhikkhusutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn51.30:1.1"
msgid "Atha kho sambahulā bhikkhū yena bhagavā tenupasaṅkamiṃsu … pe …"
msgstr ""

msgctxt "sn51.30:1.2"
msgid "ekamantaṃ nisinne kho te bhikkhū bhagavā etadavoca:"
msgstr ""

msgctxt "sn51.30:1.3"
msgid ""
"“katamā nu kho, bhikkhave, iddhi, katamo iddhipādo, katamā iddhipādabhāvanā, "
"katamā iddhipādabhāvanāgāminī paṭipadā”ti?"
msgstr ""

msgctxt "sn51.30:1.4"
msgid "“Bhagavaṃmūlakā no, bhante, dhammā bhagavaṃnettikā … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn51.30:2.1"
msgid "“Katamā ca, bhikkhave, iddhi?"
msgstr ""

msgctxt "sn51.30:2.2"
msgid ""
"Idha, bhikkhave, bhikkhu anekavihitaṃ iddhividhaṃ paccanubhoti—ekopi hutvā "
"bahudhā hoti … pe … yāva brahmalokāpi kāyena vasaṃ vatteti—"
msgstr ""

msgctxt "sn51.30:2.3"
msgid "ayaṃ vuccati, bhikkhave, iddhi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn51.30:3.1"
msgid "Katamo ca, bhikkhave, iddhipādo?"
msgstr ""

msgctxt "sn51.30:3.2"
msgid ""
"Yo, bhikkhave, maggo, yā paṭipadā iddhilābhāya iddhipaṭilābhāya saṃvattati—"
msgstr ""

msgctxt "sn51.30:3.3"
msgid "ayaṃ vuccati, bhikkhave, iddhipādo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn51.30:4.1"
msgid "Katamā ca, bhikkhave, iddhipādabhāvanā?"
msgstr ""

msgctxt "sn51.30:4.2"
msgid ""
"Idha, bhikkhave, bhikkhu chandasamādhippadhānasaṅkhārasamannāgataṃ "
"iddhipādaṃ bhāveti, "
msgstr ""

msgctxt "sn51.30:4.3"
msgid "vīriyasamādhi … pe …"
msgstr ""

msgctxt "sn51.30:4.4"
msgid "cittasamādhi …"
msgstr ""

msgctxt "sn51.30:4.5"
msgid "vīmaṃsāsamādhippadhānasaṅkhārasamannāgataṃ iddhipādaṃ bhāveti—"
msgstr ""

msgctxt "sn51.30:4.6"
msgid "ayaṃ vuccati, bhikkhave, iddhipādabhāvanā."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.288"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn51.30:5.1"
msgid "Katamā ca, bhikkhave, iddhipādabhāvanāgāminī paṭipadā?"
msgstr ""

msgctxt "sn51.30:5.2"
msgid "Ayameva ariyo aṭṭhaṅgiko maggo, seyyathidaṃ—"
msgstr ""

msgctxt "sn51.30:5.3"
msgid "sammādiṭṭhi … pe … sammāsamādhi—"
msgstr ""

msgctxt "sn51.30:5.4"
msgid "ayaṃ vuccati, bhikkhave, iddhipādabhāvanāgāminī paṭipadā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn51.30:5.5"
msgid "Dasamaṃ."
msgstr ""
