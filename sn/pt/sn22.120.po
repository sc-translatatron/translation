#. <section class="sutta" id="sn22.120"><article><div class="hgroup"><p class="division">
msgctxt "sn22.120:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.120:0.2"
msgid "12. Dhammakathikavagga"
msgstr ""

#. </p><h1>
#. VAR: 120. Saṃyojaniyasutta → saññojanam (pts1)
msgctxt "sn22.120:0.3"
msgid "120. Saṃyojaniyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.167"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.120:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.120:1.2"
msgid "“Saṃyojaniye ca, bhikkhave, dhamme desessāmi saṃyojanañca."
msgstr ""

msgctxt "sn22.120:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn22.120:1.4"
msgid "Katame ca, bhikkhave, saṃyojaniyā dhammā, katamaṃ saṃyojanaṃ?"
msgstr ""

msgctxt "sn22.120:1.5"
msgid "Rūpaṃ, bhikkhave, saṃyojaniyo dhammo;"
msgstr ""

msgctxt "sn22.120:1.6"
msgid "yo tattha chandarāgo, taṃ tattha saṃyojanaṃ."
msgstr ""

msgctxt "sn22.120:1.7"
msgid "Vedanā … pe …"
msgstr ""

msgctxt "sn22.120:1.8"
msgid "saññā …"
msgstr ""

msgctxt "sn22.120:1.9"
msgid "saṅkhārā …"
msgstr ""

msgctxt "sn22.120:1.10"
msgid "viññāṇaṃ saṃyojaniyo dhammo;"
msgstr ""

msgctxt "sn22.120:1.11"
msgid "yo tattha chandarāgo, taṃ tattha saṃyojanaṃ."
msgstr ""

msgctxt "sn22.120:1.12"
msgid "Ime vuccanti, bhikkhave, saṃyojaniyā dhammā, idaṃ saṃyojanan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.120:1.13"
msgid "Aṭṭhamaṃ."
msgstr ""
