#. <section class="sutta" id="sn2.24"><article><div class="hgroup"><p class="division">
msgctxt "sn2.24:0.1"
msgid "Saṃyutta Nikāya 2"
msgstr ""

#. </p><p>
msgctxt "sn2.24:0.2"
msgid "3. Nānātitthiyavagga"
msgstr ""

#. </p><h1>
#. VAR: 24. Ghaṭīkārasutta → ghaṭikārasuttaṃ (s3)
msgctxt "sn2.24:0.3"
msgid "24. Ghaṭīkārasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.60"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn2.24:1.1"
msgid ""
"Ekamantaṃ ṭhito kho ghaṭīkāro devaputto bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn2.24:2.1"
msgid "“Avihaṃ upapannāse,"
msgstr ""

#. <br>
msgctxt "sn2.24:2.2"
msgid "vimuttā satta bhikkhavo;"
msgstr ""

#. <br>
msgctxt "sn2.24:2.3"
msgid "Rāgadosaparikkhīṇā,"
msgstr ""

#. <br>
msgctxt "sn2.24:2.4"
msgid "tiṇṇā loke visattikan”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.138"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn2.24:3.1"
msgid "“Ke ca te ataruṃ paṅkaṃ,"
msgstr ""

#. <br>
msgctxt "sn2.24:3.2"
msgid "maccudheyyaṃ suduttaraṃ;"
msgstr ""

#. <br>
msgctxt "sn2.24:3.3"
msgid "Ke hitvā mānusaṃ dehaṃ,"
msgstr ""

#. <br>
msgctxt "sn2.24:3.4"
msgid "dibbayogaṃ upaccagun”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
#. VAR: palagaṇḍo → phalagaṇḍo (si, s1-3, pts1-2, mr)
msgctxt "sn2.24:4.1"
msgid "“Upako palagaṇḍo ca,"
msgstr ""

#. <br>
msgctxt "sn2.24:4.2"
msgid "pukkusāti ca te tayo;"
msgstr ""

#. <br>
msgctxt "sn2.24:4.3"
msgid "Bhaddiyo khaṇḍadevo ca,"
msgstr ""

#. <br>
#. VAR: bāhuraggi ca saṅgiyo → bāhudantī ca piṅgiyo (bj) | bāhudakkhi ca siṅgiyo (s1) | bāhuraggi ca siṅgiyo (s2, s3) | bāhuraggi ca piṅgiyo (pts1-2)
msgctxt "sn2.24:4.4"
msgid "bāhuraggi ca saṅgiyo;"
msgstr ""

#. <br>
msgctxt "sn2.24:4.5"
msgid "Te hitvā mānusaṃ dehaṃ,"
msgstr ""

#. <br>
msgctxt "sn2.24:4.6"
msgid "dibbayogaṃ upaccagun”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn2.24:5.1"
msgid "“Kusalī bhāsasī tesaṃ,"
msgstr ""

#. <br>
msgctxt "sn2.24:5.2"
msgid "mārapāsappahāyinaṃ;"
msgstr ""

#. <br>
msgctxt "sn2.24:5.3"
msgid "Kassa te dhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.24:5.4"
msgid "acchiduṃ bhavabandhanan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn2.24:6.1"
msgid "“Na aññatra bhagavatā,"
msgstr ""

#. <br>
msgctxt "sn2.24:6.2"
msgid "nāññatra tava sāsanā;"
msgstr ""

#. <br>
msgctxt "sn2.24:6.3"
msgid "Yassa te dhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.24:6.4"
msgid "acchiduṃ bhavabandhanaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn2.24:7.1"
msgid "Yattha nāmañca rūpañca,"
msgstr ""

#. <br>
msgctxt "sn2.24:7.2"
msgid "asesaṃ uparujjhati;"
msgstr ""

#. <br>
msgctxt "sn2.24:7.3"
msgid "Taṃ te dhammaṃ idhaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.24:7.4"
msgid "acchiduṃ bhavabandhanan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn2.24:8.1"
msgid "“Gambhīraṃ bhāsasī vācaṃ,"
msgstr ""

#. <br>
msgctxt "sn2.24:8.2"
msgid "dubbijānaṃ sudubbudhaṃ;"
msgstr ""

#. <br>
msgctxt "sn2.24:8.3"
msgid "Kassa tvaṃ dhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.24:8.4"
msgid "vācaṃ bhāsasi īdisan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "sn2.24:9.1"
msgid "“Kumbhakāro pure āsiṃ,"
msgstr ""

#. <br>
msgctxt "sn2.24:9.2"
msgid "vekaḷiṅge ghaṭīkaro;"
msgstr ""

#. <br>
msgctxt "sn2.24:9.3"
msgid "Mātāpettibharo āsiṃ,"
msgstr ""

#. <br>
msgctxt "sn2.24:9.4"
msgid "kassapassa upāsako."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.140"></a>
#. <a class="sc" id="sc10"></a>
msgctxt "sn2.24:10.1"
msgid "Virato methunā dhammā,"
msgstr ""

#. <br>
msgctxt "sn2.24:10.2"
msgid "brahmacārī nirāmiso;"
msgstr ""

#. <br>
msgctxt "sn2.24:10.3"
msgid "Ahuvā te sagāmeyyo,"
msgstr ""

#. <br>
msgctxt "sn2.24:10.4"
msgid "ahuvā te pure sakhā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn2.24:11.1"
msgid "Sohamete pajānāmi,"
msgstr ""

#. <br>
msgctxt "sn2.24:11.2"
msgid "vimutte satta bhikkhavo;"
msgstr ""

#. <br>
msgctxt "sn2.24:11.3"
msgid "Rāgadosaparikkhīṇe,"
msgstr ""

#. <br>
msgctxt "sn2.24:11.4"
msgid "tiṇṇe loke visattikan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn2.24:12.1"
msgid "“Evametaṃ tadā āsi,"
msgstr ""

#. <br>
msgctxt "sn2.24:12.2"
msgid "yathā bhāsasi bhaggava;"
msgstr ""

#. <br>
msgctxt "sn2.24:12.3"
msgid "Kumbhakāro pure āsi,"
msgstr ""

#. <br>
msgctxt "sn2.24:12.4"
msgid "vekaḷiṅge ghaṭīkaro."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc13"></a>
msgctxt "sn2.24:13.1"
msgid "Mātāpettibharo āsi,"
msgstr ""

#. <br>
msgctxt "sn2.24:13.2"
msgid "kassapassa upāsako;"
msgstr ""

#. <br>
msgctxt "sn2.24:13.3"
msgid "Virato methunā dhammā,"
msgstr ""

#. <br>
msgctxt "sn2.24:13.4"
msgid "brahmacārī nirāmiso;"
msgstr ""

#. <br>
msgctxt "sn2.24:13.5"
msgid "Ahuvā me sagāmeyyo,"
msgstr ""

#. <br>
msgctxt "sn2.24:13.6"
msgid "ahuvā me pure sakhā”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.141"></a>
#. <a class="sc" id="sc14"></a>
msgctxt "sn2.24:14.1"
msgid "“Evametaṃ purāṇānaṃ,"
msgstr ""

#. <br>
msgctxt "sn2.24:14.2"
msgid "sahāyānaṃ ahu saṅgamo;"
msgstr ""

#. <br>
msgctxt "sn2.24:14.3"
msgid "Ubhinnaṃ bhāvitattānaṃ,"
msgstr ""

#. <br>
msgctxt "sn2.24:14.4"
msgid "sarīrantimadhārinan”ti."
msgstr ""
