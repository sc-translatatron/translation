#. <section class="sutta" id="sn1.13"><article><div class="hgroup"><p class="division">
msgctxt "sn1.13:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.13:0.2"
msgid "2. Nandanavagga"
msgstr ""

#. </p><h1>
#. VAR: 13. Natthiputtasamasutta → natthiputtasuttaṃ (bj) | natthiputtasamasuttaṃ (pts2)
msgctxt "sn1.13:0.3"
msgid "13. Natthiputtasamasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.13:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn1.13:1.2"
msgid "Ekamantaṃ ṭhitā kho sā devatā bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.13:2.1"
msgid "“Natthi puttasamaṃ pemaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.13:2.2"
msgid "natthi gosamitaṃ dhanaṃ;"
msgstr ""

#. <br>
#. VAR: sūriyasamā → suriyasamā (bj, s1-3, km, pts1-2)
msgctxt "sn1.13:2.3"
msgid "Natthi sūriyasamā ābhā,"
msgstr ""

#. <br>
msgctxt "sn1.13:2.4"
msgid "samuddaparamā sarā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.13:3.1"
msgid "“Natthi attasamaṃ pemaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.13:3.2"
msgid "natthi dhaññasamaṃ dhanaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.13:3.3"
msgid "Natthi paññāsamā ābhā,"
msgstr ""

#. <br>
msgctxt "sn1.13:3.4"
msgid "vuṭṭhi ve paramā sarā”ti."
msgstr ""
