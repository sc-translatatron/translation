#. <section class="sutta" id="sn26.9"><article><div class="hgroup"><p class="division">
msgctxt "sn26.9:0.1"
msgid "Saṃyutta Nikāya 26"
msgstr ""

#. </p><p>
msgctxt "sn26.9:0.2"
msgid "1. Uppādavagga"
msgstr ""

#. </p><h1>
msgctxt "sn26.9:0.3"
msgid "9. Dhātusutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn26.9:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn26.9:1.2"
msgid ""
"“Yo kho, bhikkhave, pathavīdhātuyā uppādo ṭhiti abhinibbatti pātubhāvo … pe …"
" jarāmaraṇassa pātubhāvo;"
msgstr ""

msgctxt "sn26.9:1.3"
msgid "yo āpodhātuyā …"
msgstr ""

msgctxt "sn26.9:1.4"
msgid "yo tejodhātuyā …"
msgstr ""

msgctxt "sn26.9:1.5"
msgid "yo vāyodhātuyā …"
msgstr ""

msgctxt "sn26.9:1.6"
msgid "yo ākāsadhātuyā …"
msgstr ""

msgctxt "sn26.9:1.7"
msgid ""
"yo viññāṇadhātuyā uppādo ṭhiti abhinibbatti pātubhāvo, dukkhasseso uppādo, "
"rogānaṃ ṭhiti, jarāmaraṇassa pātubhāvo."
msgstr ""

msgctxt "sn26.9:1.8"
msgid ""
"Yo ca kho, bhikkhave, pathavīdhātuyā nirodho … pe … jarāmaraṇassa atthaṅgamo;"
msgstr ""

msgctxt "sn26.9:1.9"
msgid "yo āpodhātuyā nirodho …"
msgstr ""

msgctxt "sn26.9:1.10"
msgid "yo tejodhātuyā nirodho …"
msgstr ""

msgctxt "sn26.9:1.11"
msgid "yo vāyodhātuyā nirodho …"
msgstr ""

msgctxt "sn26.9:1.12"
msgid "yo ākāsadhātuyā nirodho …"
msgstr ""

msgctxt "sn26.9:1.13"
msgid ""
"yo viññāṇadhātuyā nirodho vūpasamo atthaṅgamo, dukkhasseso nirodho, rogānaṃ "
"vūpasamo, jarāmaraṇassa atthaṅgamo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn26.9:1.14"
msgid "Navamaṃ."
msgstr ""
