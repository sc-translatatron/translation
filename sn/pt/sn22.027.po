#. <section class="sutta" id="sn22.27"><article><div class="hgroup"><p class="division">
msgctxt "sn22.27:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.27:0.2"
msgid "3. Bhāravagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.27:0.3"
msgid "27. Dutiyaassādasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.29"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.27:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.27:1.2"
msgid "“Rūpassāhaṃ, bhikkhave, assādapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn22.27:1.3"
msgid "Yo rūpassa assādo tadajjhagamaṃ."
msgstr ""

msgctxt "sn22.27:1.4"
msgid "Yāvatā rūpassa assādo paññāya me so sudiṭṭho."
msgstr ""

msgctxt "sn22.27:1.5"
msgid "Rūpassāhaṃ, bhikkhave, ādīnavapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn22.27:1.6"
msgid "Yo rūpassa ādīnavo tadajjhagamaṃ."
msgstr ""

msgctxt "sn22.27:1.7"
msgid "Yāvatā rūpassa ādīnavo paññāya me so sudiṭṭho."
msgstr ""

msgctxt "sn22.27:1.8"
msgid "Rūpassāhaṃ, bhikkhave, nissaraṇapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn22.27:1.9"
msgid "Yaṃ rūpassa nissaraṇaṃ tadajjhagamaṃ."
msgstr ""

msgctxt "sn22.27:1.10"
msgid "Yāvatā rūpassa nissaraṇaṃ paññāya me taṃ sudiṭṭhaṃ."
msgstr ""

msgctxt "sn22.27:1.11"
msgid "Vedanāyāhaṃ, bhikkhave …"
msgstr ""

msgctxt "sn22.27:1.12"
msgid "saññāyāhaṃ, bhikkhave …"
msgstr ""

msgctxt "sn22.27:1.13"
msgid "saṅkhārānāhaṃ, bhikkhave …"
msgstr ""

msgctxt "sn22.27:1.14"
msgid "viññāṇassāhaṃ, bhikkhave, assādapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn22.27:1.15"
msgid "Yo viññāṇassa assādo tadajjhagamaṃ."
msgstr ""

msgctxt "sn22.27:1.16"
msgid "Yāvatā viññāṇassa assādo paññāya me so sudiṭṭho."
msgstr ""

msgctxt "sn22.27:1.17"
msgid "Viññāṇassāhaṃ, bhikkhave, ādīnavapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn22.27:1.18"
msgid "Yo viññāṇassa ādīnavo tadajjhagamaṃ."
msgstr ""

msgctxt "sn22.27:1.19"
msgid "Yāvatā viññāṇassa ādīnavo paññāya me so sudiṭṭho."
msgstr ""

msgctxt "sn22.27:1.20"
msgid "Viññāṇassāhaṃ, bhikkhave, nissaraṇapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn22.27:1.21"
msgid "Yaṃ viññāṇassa nissaraṇaṃ tadajjhagamaṃ."
msgstr ""

msgctxt "sn22.27:1.22"
msgid "Yāvatā viññāṇassa nissaraṇaṃ paññāya me taṃ sudiṭṭhaṃ."
msgstr ""

msgctxt "sn22.27:1.23"
msgid ""
"Yāvakīvañcāhaṃ, bhikkhave, imesaṃ pañcannaṃ upādānakkhandhānaṃ assādañca "
"assādato ādīnavañca ādīnavato nissaraṇañca nissaraṇato yathābhūtaṃ "
"nābbhaññāsiṃ … pe …"
msgstr ""

msgctxt "sn22.27:1.24"
msgid "abbhaññāsiṃ."
msgstr ""

msgctxt "sn22.27:1.25"
msgid "Ñāṇañca pana me dassanaṃ udapādi:"
msgstr ""

#. VAR: vimutti → cetovimutti (bj, pts1, mr)
msgctxt "sn22.27:1.26"
msgid "‘akuppā me vimutti; ayamantimā jāti; natthi dāni punabbhavo’”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.27:1.27"
msgid "Chaṭṭhaṃ."
msgstr ""
