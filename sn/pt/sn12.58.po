#. <section class="sutta" id="sn12.58"><article><div class="hgroup"><p class="division">
msgctxt "sn12.58:0.1"
msgid "Saṃyutta Nikāya 12"
msgstr ""

#. </p><p>
msgctxt "sn12.58:0.2"
msgid "6. Dukkhavagga"
msgstr ""

#. </p><h1>
msgctxt "sn12.58:0.3"
msgid "58. Nāmarūpasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn12.58:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn12.58:1.2"
msgid ""
"“Saṃyojaniyesu, bhikkhave, dhammesu assādānupassino viharato nāmarūpassa "
"avakkanti hoti."
msgstr ""

msgctxt "sn12.58:1.3"
msgid "Nāmarūpapaccayā saḷāyatanaṃ … pe …"
msgstr ""

msgctxt "sn12.58:1.4"
msgid "evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn12.58:2.1"
msgid "Seyyathāpi, bhikkhave, mahārukkho."
msgstr ""

msgctxt "sn12.58:2.2"
msgid ""
"Tassa yāni ceva mūlāni adhogamāni, yāni ca tiriyaṅgamāni, sabbāni tāni "
"uddhaṃ ojaṃ abhiharanti."
msgstr ""

msgctxt "sn12.58:2.3"
msgid ""
"Evañhi so, bhikkhave, mahārukkho tadāhāro tadupādāno ciraṃ dīghamaddhānaṃ "
"tiṭṭheyya."
msgstr ""

msgctxt "sn12.58:2.4"
msgid ""
"Evameva kho, bhikkhave, saṃyojaniyesu dhammesu assādānupassino viharato "
"nāmarūpassa avakkanti hoti … pe …."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.91"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn12.58:3.1"
msgid ""
"Saṃyojaniyesu, bhikkhave, dhammesu ādīnavānupassino viharato nāmarūpassa "
"avakkanti na hoti."
msgstr ""

msgctxt "sn12.58:3.2"
msgid "Nāmarūpanirodhā saḷāyatananirodho … pe …"
msgstr ""

msgctxt "sn12.58:3.3"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn12.58:4.1"
msgid "Seyyathāpi, bhikkhave, mahārukkho."
msgstr ""

msgctxt "sn12.58:4.2"
msgid "Atha puriso āgaccheyya kuddālapiṭakaṃ ādāya … pe …"
msgstr ""

msgctxt "sn12.58:4.3"
msgid "āyatiṃ anuppādadhammo."
msgstr ""

msgctxt "sn12.58:4.4"
msgid ""
"Evameva kho, bhikkhave, saṃyojaniyesu dhammesu ādīnavānupassino viharato "
"nāmarūpassa avakkanti na hoti."
msgstr ""

msgctxt "sn12.58:4.5"
msgid "Nāmarūpanirodhā saḷāyatananirodho … pe …"
msgstr ""

msgctxt "sn12.58:4.6"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.58:4.7"
msgid "Aṭṭhamaṃ."
msgstr ""
