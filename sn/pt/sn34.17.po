#. <section class="sutta" id="sn34.17"><article><div class="hgroup"><p class="division">
msgctxt "sn34.17:0.1"
msgid "Saṃyutta Nikāya 34"
msgstr ""

#. </p><p>
msgctxt "sn34.17:0.2"
msgid "1. Jhānavagga"
msgstr ""

#. </p><h1>
#. VAR: 17. Samāpattimūlakasakkaccasutta → samāpattisakkaccakārīsuttaṃ (bj) | samāpatti-sakkacca (pts1)
msgctxt "sn34.17:0.3"
msgid "17. Samāpattimūlakasakkaccasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn34.17:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn34.17:1.2"
msgid "“Cattārome, bhikkhave, jhāyī."
msgstr ""

msgctxt "sn34.17:1.3"
msgid "Katame cattāro?"
msgstr ""

msgctxt "sn34.17:1.4"
msgid ""
"Idha, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo hoti, na "
"samādhismiṃ sakkaccakārī."
msgstr ""

msgctxt "sn34.17:1.5"
msgid ""
"Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ sakkaccakārī hoti, na "
"samādhismiṃ samāpattikusalo."
msgstr ""

msgctxt "sn34.17:1.6"
msgid ""
"Idha pana, bhikkhave, ekacco jhāyī neva samādhismiṃ samāpattikusalo hoti, na "
"ca samādhismiṃ sakkaccakārī."
msgstr ""

msgctxt "sn34.17:1.7"
msgid ""
"Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ samāpattikusalo ca hoti, "
"samādhismiṃ sakkaccakārī ca."
msgstr ""

msgctxt "sn34.17:1.8"
msgid "Tatra … pe …"
msgstr ""

msgctxt "sn34.17:1.9"
msgid "pavaro cā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn34.17:1.10"
msgid "Sattarasamaṃ."
msgstr ""
