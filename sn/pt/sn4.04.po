#. <section class="sutta" id="sn4.4"><article><div class="hgroup"><p class="division">
msgctxt "sn4.4:0.1"
msgid "Saṃyutta Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "sn4.4:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
#. VAR: 4. Paṭhamamārapāsasutta → paṭhamapāsasuttaṃ (bj, s1-3) | pāsa (1) (pts1)
msgctxt "sn4.4:0.3"
msgid "4. Paṭhamamārapāsasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.105"></a>
#. <a class="pts2ed" id="pts2ed1.235"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn4.4:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn4.4:1.2"
msgid "ekaṃ samayaṃ bhagavā bārāṇasiyaṃ viharati isipatane migadāye."
msgstr ""

msgctxt "sn4.4:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "sn4.4:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "sn4.4:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "sn4.4:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn4.4:2.1"
msgid ""
"“Mayhaṃ kho, bhikkhave, yoniso manasikārā yoniso sammappadhānā anuttarā "
"vimutti anuppattā, anuttarā vimutti sacchikatā."
msgstr ""

msgctxt "sn4.4:2.2"
msgid ""
"Tumhepi, bhikkhave, yoniso manasikārā yoniso sammappadhānā anuttaraṃ "
"vimuttiṃ anupāpuṇātha, anuttaraṃ vimuttiṃ sacchikarothā”ti."
msgstr ""

msgctxt "sn4.4:2.3"
msgid ""
"Atha kho māro pāpimā yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavantaṃ "
"gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn4.4:3.1"
msgid "“Baddhosi mārapāsena,"
msgstr ""

#. <br>
msgctxt "sn4.4:3.2"
msgid "ye dibbā ye ca mānusā;"
msgstr ""

#. <br>
msgctxt "sn4.4:3.3"
msgid "Mārabandhanabaddhosi,"
msgstr ""

#. <br>
msgctxt "sn4.4:3.4"
msgid "na me samaṇa mokkhasī”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.236"></a>
#. <a class="sc" id="sc4"></a>
#. VAR: Muttāhaṃ → muttohaṃ (bj, s1, km, pts1-2)
msgctxt "sn4.4:4.1"
msgid "“Muttāhaṃ mārapāsena,"
msgstr ""

#. <br>
msgctxt "sn4.4:4.2"
msgid "Ye dibbā ye ca mānusā;"
msgstr ""

#. <br>
msgctxt "sn4.4:4.3"
msgid "Mārabandhanamuttomhi,"
msgstr ""

#. <br>
msgctxt "sn4.4:4.4"
msgid "Nihato tvamasi antakā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn4.4:5.1"
msgid "Atha kho māro pāpimā … pe … tatthevantaradhāyīti."
msgstr ""
