#. <section class="sutta" id="sn22.8"><article><div class="hgroup"><p class="division">
msgctxt "sn22.8:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.8:0.2"
msgid "1. Nakulapituvagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.8:0.3"
msgid "8. Dutiyaupādāparitassanāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.8:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.8:1.2"
msgid "“Upādāparitassanañca vo, bhikkhave, desessāmi anupādāaparitassanañca."
msgstr ""

msgctxt "sn22.8:1.3"
msgid "Taṃ suṇātha … pe …"
msgstr ""

msgctxt "sn22.8:1.4"
msgid "kathañca, bhikkhave, upādāparitassanā hoti?"
msgstr ""

msgctxt "sn22.8:1.5"
msgid ""
"Idha, bhikkhave, assutavā puthujjano rūpaṃ ‘etaṃ mama, esohamasmi, eso me "
"attā’ti samanupassati."
msgstr ""

msgctxt "sn22.8:1.6"
msgid "Tassa taṃ rūpaṃ vipariṇamati aññathā hoti."
msgstr ""

msgctxt "sn22.8:1.7"
msgid ""
"Tassa rūpavipariṇāmaññathābhāvā uppajjanti sokaparidevadukkhadomanassupāyāsā."
msgstr ""

msgctxt "sn22.8:1.8"
msgid "Vedanaṃ etaṃ mama … pe …"
msgstr ""

msgctxt "sn22.8:1.9"
msgid "saññaṃ etaṃ mama …"
msgstr ""

msgctxt "sn22.8:1.10"
msgid "saṅkhāre etaṃ mama …"
msgstr ""

msgctxt "sn22.8:1.11"
msgid "viññāṇaṃ ‘etaṃ mama, esohamasmi, eso me attā’ti samanupassati."
msgstr ""

msgctxt "sn22.8:1.12"
msgid "Tassa taṃ viññāṇaṃ vipariṇamati aññathā hoti."
msgstr ""

msgctxt "sn22.8:1.13"
msgid ""
"Tassa viññāṇavipariṇāmaññathābhāvā uppajjanti "
"sokaparidevadukkhadomanassupāyāsā."
msgstr ""

msgctxt "sn22.8:1.14"
msgid "Evaṃ kho, bhikkhave, upādāparitassanā hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.19"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn22.8:2.1"
msgid "Kathañca, bhikkhave, anupādāaparitassanā hoti?"
msgstr ""

msgctxt "sn22.8:2.2"
msgid ""
"Idha, bhikkhave, sutavā ariyasāvako rūpaṃ ‘netaṃ mama, nesohamasmi, na meso "
"attā’ti samanupassati."
msgstr ""

msgctxt "sn22.8:2.3"
msgid "Tassa taṃ rūpaṃ vipariṇamati aññathā hoti."
msgstr ""

msgctxt "sn22.8:2.4"
msgid ""
"Tassa rūpavipariṇāmaññathābhāvā nuppajjanti "
"sokaparidevadukkhadomanassupāyāsā."
msgstr ""

msgctxt "sn22.8:2.5"
msgid "Vedanaṃ netaṃ mama …"
msgstr ""

msgctxt "sn22.8:2.6"
msgid "saññaṃ netaṃ mama …"
msgstr ""

msgctxt "sn22.8:2.7"
msgid "saṅkhāre netaṃ mama …"
msgstr ""

msgctxt "sn22.8:2.8"
msgid "viññāṇaṃ ‘netaṃ mama, nesohamasmi, na meso attā’ti samanupassati."
msgstr ""

msgctxt "sn22.8:2.9"
msgid "Tassa taṃ viññāṇaṃ vipariṇamati aññathā hoti."
msgstr ""

msgctxt "sn22.8:2.10"
msgid ""
"Tassa viññāṇavipariṇāmaññathābhāvā nuppajjanti "
"sokaparidevadukkhadomanassupāyāsā."
msgstr ""

msgctxt "sn22.8:2.11"
msgid "Evaṃ kho, bhikkhave, anupādāaparitassanā hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.8:2.12"
msgid "Aṭṭhamaṃ."
msgstr ""
