#. <section class="sutta" id="sn24.37"><article><div class="hgroup"><p class="division">
msgctxt "sn24.37:0.1"
msgid "Saṃyutta Nikāya 24"
msgstr ""

#. </p><p>
msgctxt "sn24.37:0.2"
msgid "2. Dutiyagamanavagga"
msgstr ""

#. </p><h1>
msgctxt "sn24.37:0.3"
msgid "37. Rūpīattāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.219"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn24.37:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn24.37:1.2"
msgid ""
"“Kismiṃ nu kho, bhikkhave, sati, kiṃ upādāya, kiṃ abhinivissa evaṃ diṭṭhi "
"uppajjati:"
msgstr ""

msgctxt "sn24.37:1.3"
msgid "‘rūpī attā hoti arogo paraṃ maraṇā’”ti?"
msgstr ""

msgctxt "sn24.37:1.4"
msgid "“Bhagavaṃmūlakā no, bhante, dhammā … pe …"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn24.37:2.1"
msgid ""
"“Rūpe kho, bhikkhave, sati, rūpaṃ upādāya, rūpaṃ abhinivissa evaṃ diṭṭhi "
"uppajjati:"
msgstr ""

msgctxt "sn24.37:2.2"
msgid "‘rūpī attā hoti arogo paraṃ maraṇā’ti."
msgstr ""

msgctxt "sn24.37:2.3"
msgid "Vedanāya sati … pe …"
msgstr ""

msgctxt "sn24.37:2.4"
msgid "saññāya sati …"
msgstr ""

msgctxt "sn24.37:2.5"
msgid "saṅkhāresu sati …"
msgstr ""

msgctxt "sn24.37:2.6"
msgid ""
"viññāṇe sati, viññāṇaṃ upādāya, viññāṇaṃ abhinivissa evaṃ diṭṭhi uppajjati:"
msgstr ""

msgctxt "sn24.37:2.7"
msgid "‘rūpī attā hoti arogo paraṃ maraṇā’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn24.37:3.1"
msgid "Taṃ kiṃ maññatha, bhikkhave, rūpaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn24.37:3.2"
msgid "“Aniccaṃ, bhante” … pe …"
msgstr ""

msgctxt "sn24.37:3.3"
msgid "vipariṇāmadhammaṃ, api nu taṃ anupādāya evaṃ diṭṭhi uppajjeyya:"
msgstr ""

msgctxt "sn24.37:3.4"
msgid "‘rūpī attā hoti arogo paraṃ maraṇā’”ti?"
msgstr ""

msgctxt "sn24.37:3.5"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn24.37:3.6"
msgid ""
"“Iti kho, bhikkhave, dukkhe sati, dukkhaṃ upādāya, dukkhaṃ abhinivissa evaṃ "
"diṭṭhi uppajjati:"
msgstr ""

msgctxt "sn24.37:3.7"
msgid "‘rūpī attā hoti arogo paraṃ maraṇā’”ti?"
msgstr ""

msgctxt "sn24.37:3.8"
msgid "“Vedanā … pe …"
msgstr ""

msgctxt "sn24.37:3.9"
msgid "“no hetaṃ, bhante”."
msgstr ""

msgctxt "sn24.37:3.10"
msgid ""
"“Iti kho, bhikkhave, dukkhe sati, dukkhaṃ upādāya, dukkhaṃ abhinivissa evaṃ "
"diṭṭhi uppajjati:"
msgstr ""

msgctxt "sn24.37:3.11"
msgid "‘rūpī attā hoti arogo paraṃ maraṇā’”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn24.37:3.12"
msgid "Ekūnavīsatimaṃ."
msgstr ""
