#. <section class="sutta" id="sn11.8"><article><div class="hgroup"><p class="division">
msgctxt "sn11.8:0.1"
msgid "Saṃyutta Nikāya 11"
msgstr ""

#. </p><p>
msgctxt "sn11.8:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
#. VAR: 8. Verocanaasurindasutta → virocanaasurindasuttaṃ (s1-3, pts2) | virocana-asurindo (attho) (pts1)
msgctxt "sn11.8:0.3"
msgid "8. Verocanaasurindasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts2ed" id="pts2ed1.486"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn11.8:1.1"
msgid "Sāvatthiyaṃ jetavane."
msgstr ""

msgctxt "sn11.8:1.2"
msgid "Tena kho pana samayena bhagavā divāvihāragato hoti paṭisallīno."
msgstr ""

msgctxt "sn11.8:1.3"
msgid ""
"Atha kho sakko ca devānamindo verocano ca asurindo yena bhagavā "
"tenupasaṅkamiṃsu; upasaṅkamitvā paccekaṃ dvārabāhaṃ nissāya aṭṭhaṃsu."
msgstr ""

msgctxt "sn11.8:1.4"
msgid "Atha kho verocano asurindo bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

# Here I’m keeping the masculine forms. they seem to suit Verocana!
#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn11.8:2.1"
msgid "“Vāyametheva puriso,"
msgstr ""

#. <br>
msgctxt "sn11.8:2.2"
msgid "yāva atthassa nipphadā;"
msgstr ""

#. <br>
#. VAR: Nipphannasobhano → nipphannasobhino (bj, pts2) | nippannasobhaṇo (pts1) | nipphannasobhaṇo (mr)
#. VAR: attho → atthā (bj, pts2)
msgctxt "sn11.8:2.3"
msgid "Nipphannasobhano attho,"
msgstr ""

#. <br>
msgctxt "sn11.8:2.4"
msgid "verocanavaco idan”ti."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.226"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn11.8:3.1"
msgid "“Vāyametheva puriso,"
msgstr ""

#. <br>
msgctxt "sn11.8:3.2"
msgid "yāva atthassa nipphadā;"
msgstr ""

#. <br>
#. VAR: Nipphannasobhano attho → nipphannasobhino atthā (bj, s1, km, pts2) | nippannasobhaṇo attho (pts1)
msgctxt "sn11.8:3.3"
msgid "Nipphannasobhano attho,"
msgstr ""

#. <br>
msgctxt "sn11.8:3.4"
msgid "khantyā bhiyyo na vijjatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn11.8:4.1"
msgid "“Sabbe sattā atthajātā,"
msgstr ""

#. <br>
msgctxt "sn11.8:4.2"
msgid "tattha tattha yathārahaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.8:4.3"
msgid "Saṃyogaparamā tveva,"
msgstr ""

#. <br>
msgctxt "sn11.8:4.4"
msgid "sambhogā sabbapāṇinaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.8:4.5"
msgid "Nipphannasobhano attho,"
msgstr ""

#. <br>
msgctxt "sn11.8:4.6"
msgid "verocanavaco idan”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.487"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn11.8:5.1"
msgid "“Sabbe sattā atthajātā,"
msgstr ""

#. <br>
msgctxt "sn11.8:5.2"
msgid "tattha tattha yathārahaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.8:5.3"
msgid "Saṃyogaparamā tveva,"
msgstr ""

#. <br>
msgctxt "sn11.8:5.4"
msgid "sambhogā sabbapāṇinaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.8:5.5"
msgid "Nipphannasobhano attho,"
msgstr ""

#. <br>
msgctxt "sn11.8:5.6"
msgid "khantyā bhiyyo na vijjatī”ti."
msgstr ""
