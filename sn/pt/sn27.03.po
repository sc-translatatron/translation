#. <section class="sutta" id="sn27.3"><article><div class="hgroup"><p class="division">
msgctxt "sn27.3:0.1"
msgid "Saṃyutta Nikāya 27"
msgstr ""

#. </p><p>
msgctxt "sn27.3:0.2"
msgid "1. Kilesavagga"
msgstr ""

#. </p><h1>
msgctxt "sn27.3:0.3"
msgid "3. Viññāṇasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.233"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn27.3:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn27.3:1.2"
msgid "“Yo, bhikkhave, cakkhuviññāṇasmiṃ chandarāgo, cittasseso upakkileso."
msgstr ""

msgctxt "sn27.3:1.3"
msgid "Yo sotaviññāṇasmiṃ …"
msgstr ""

msgctxt "sn27.3:1.4"
msgid "yo ghānaviññāṇasmiṃ …"
msgstr ""

msgctxt "sn27.3:1.5"
msgid "yo jivhāviññāṇasmiṃ …"
msgstr ""

msgctxt "sn27.3:1.6"
msgid "yo kāyaviññāṇasmiṃ …"
msgstr ""

msgctxt "sn27.3:1.7"
msgid "yo manoviññāṇasmiṃ chandarāgo, cittasseso upakkileso."
msgstr ""

msgctxt "sn27.3:1.8"
msgid ""
"Yato kho, bhikkhave, bhikkhuno imesu chasu ṭhānesu cetaso upakkileso pahīno "
"hoti, nekkhammaninnañcassa cittaṃ hoti."
msgstr ""

msgctxt "sn27.3:1.9"
msgid ""
"Nekkhammaparibhāvitaṃ cittaṃ kammaniyaṃ khāyati, abhiññā sacchikaraṇīyesu "
"dhammesū”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn27.3:1.10"
msgid "Tatiyaṃ."
msgstr ""
