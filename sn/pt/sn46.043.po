#. <section class="sutta" id="sn46.43"><article><div class="hgroup"><p class="division">
msgctxt "sn46.43:0.1"
msgid "Saṃyutta Nikāya 46"
msgstr ""

#. </p><p>
msgctxt "sn46.43:0.2"
msgid "5. Cakkavattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn46.43:0.3"
msgid "43. Mārasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn46.43:1.1"
msgid "“Mārasenappamaddanaṃ vo, bhikkhave, maggaṃ desessāmi;"
msgstr ""

msgctxt "sn46.43:1.2"
msgid "taṃ suṇātha."
msgstr ""

msgctxt "sn46.43:1.3"
msgid "Katamo ca, bhikkhave, mārasenappamaddano maggo?"
msgstr ""

msgctxt "sn46.43:1.4"
msgid "Yadidaṃ—"
msgstr ""

msgctxt "sn46.43:1.5"
msgid "satta bojjhaṅgā."
msgstr ""

msgctxt "sn46.43:1.6"
msgid "Katame satta?"
msgstr ""

msgctxt "sn46.43:1.7"
msgid "Satisambojjhaṅgo … pe … upekkhāsambojjhaṅgo—"
msgstr ""

msgctxt "sn46.43:1.8"
msgid "ayaṃ kho, bhikkhave, mārasenappamaddano maggo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn46.43:1.9"
msgid "Tatiyaṃ."
msgstr ""
