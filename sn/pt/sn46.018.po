#. <section class="sutta" id="sn46.18"><article><div class="hgroup"><p class="division">
msgctxt "sn46.18:0.1"
msgid "Saṃyutta Nikāya 46"
msgstr ""

#. </p><p>
msgctxt "sn46.18:0.2"
msgid "2. Gilānavagga"
msgstr ""

#. </p><h1>
#. VAR: 18. Viraddhasutta → āraddhasuttaṃ (bj)
msgctxt "sn46.18:0.3"
msgid "18. Viraddhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn46.18:1.1"
msgid ""
"“Yesaṃ kesañci, bhikkhave, satta bojjhaṅgā viraddhā, viraddho tesaṃ ariyo "
"maggo sammā dukkhakkhayagāmī."
msgstr ""

msgctxt "sn46.18:1.2"
msgid ""
"Yesaṃ kesañci, bhikkhave, satta bojjhaṅgā āraddhā, āraddho tesaṃ ariyo maggo "
"sammā dukkhakkhayagāmī."
msgstr ""

msgctxt "sn46.18:1.3"
msgid "Katame satta?"
msgstr ""

msgctxt "sn46.18:1.4"
msgid "Satisambojjhaṅgo … pe … upekkhāsambojjhaṅgo—"
msgstr ""

msgctxt "sn46.18:1.5"
msgid ""
"yesaṃ kesañci, bhikkhave, ime satta bojjhaṅgā viraddhā, viraddho tesaṃ ariyo "
"maggo sammā dukkhakkhayagāmī."
msgstr ""

msgctxt "sn46.18:1.6"
msgid ""
"Yesaṃ kesañci, bhikkhave, ime satta bojjhaṅgā āraddhā, āraddho tesaṃ ariyo "
"maggo sammā dukkhakkhayagāmī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn46.18:1.7"
msgid "Aṭṭhamaṃ."
msgstr ""
