#. <section class="sutta" id="sn23.17"><article><div class="hgroup"><p class="division">
msgctxt "sn23.17:0.1"
msgid "Saṃyutta Nikāya 23"
msgstr ""

#. </p><p>
msgctxt "sn23.17:0.2"
msgid "2. Dutiyamāravagga"
msgstr ""

#. </p><h1>
#. VAR: 17. Anattasutta → anatta 1 (pts1)
msgctxt "sn23.17:0.3"
msgid "17. Anattasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn23.17:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn23.17:1.2"
msgid "Ekamantaṃ nisinno kho āyasmā rādho bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn23.17:1.3"
msgid "“‘anattā, anattā’ti, bhante, vuccati."
msgstr ""

msgctxt "sn23.17:1.4"
msgid "Katamo nu kho, bhante, anattā”ti?"
msgstr ""

msgctxt "sn23.17:1.5"
msgid ""
"“Rūpaṃ kho, rādha, anattā, vedanā anattā, saññā anattā, saṅkhārā anattā, "
"viññāṇaṃ anattā."
msgstr ""

msgctxt "sn23.17:1.6"
msgid "Evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn23.17:1.7"
msgid "nāparaṃ itthattāyāti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn23.17:1.8"
msgid "Sattamaṃ."
msgstr ""
