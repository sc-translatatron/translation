#. <section class="sutta" id="sn24.2"><article><div class="hgroup"><p class="division">
msgctxt "sn24.2:0.1"
msgid "Saṃyutta Nikāya 24"
msgstr ""

#. </p><p>
msgctxt "sn24.2:0.2"
msgid "1. Sotāpattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn24.2:0.3"
msgid "2. Etaṃmamasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.204"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn24.2:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn24.2:1.2"
msgid ""
"“Kismiṃ nu kho, bhikkhave, sati, kiṃ upādāya, kiṃ abhinivissa evaṃ diṭṭhi "
"uppajjati:"
msgstr ""

msgctxt "sn24.2:1.3"
msgid "‘etaṃ mama, esohamasmi, eso me attā’”ti?"
msgstr ""

msgctxt "sn24.2:1.4"
msgid "“Bhagavaṃmūlakā no, bhante, dhammā … pe …"
msgstr ""

msgctxt "sn24.2:1.5"
msgid ""
"“rūpe kho, bhikkhave, sati, rūpaṃ upādāya, rūpaṃ abhinivissa evaṃ diṭṭhi "
"uppajjati:"
msgstr ""

msgctxt "sn24.2:1.6"
msgid "‘etaṃ mama, esohamasmi, eso me attā’ti."
msgstr ""

msgctxt "sn24.2:1.7"
msgid "Vedanāya sati … pe …"
msgstr ""

msgctxt "sn24.2:1.8"
msgid "saññāya sati …"
msgstr ""

msgctxt "sn24.2:1.9"
msgid "saṅkhāresu sati …"
msgstr ""

msgctxt "sn24.2:1.10"
msgid ""
"viññāṇe sati, viññāṇaṃ upādāya, viññāṇaṃ abhinivissa evaṃ diṭṭhi uppajjati:"
msgstr ""

msgctxt "sn24.2:1.11"
msgid "‘etaṃ mama, esohamasmi, eso me attā’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn24.2:2.1"
msgid "Taṃ kiṃ maññatha, bhikkhave, "
msgstr ""

msgctxt "sn24.2:2.2"
msgid "rūpaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn24.2:2.3"
msgid "“Aniccaṃ, bhante” … pe …"
msgstr ""

msgctxt "sn24.2:2.4"
msgid "“vedanā …"
msgstr ""

msgctxt "sn24.2:2.5"
msgid "saññā …"
msgstr ""

msgctxt "sn24.2:2.6"
msgid "saṅkhārā …"
msgstr ""

msgctxt "sn24.2:2.7"
msgid "viññāṇaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn24.2:2.8"
msgid "“Aniccaṃ, bhante” … pe …"
msgstr ""

msgctxt "sn24.2:2.9"
msgid "api nu taṃ anupādāya evaṃ diṭṭhi uppajjeyya:"
msgstr ""

msgctxt "sn24.2:2.10"
msgid "‘etaṃ mama, esohamasmi, eso me attā’”ti?"
msgstr ""

msgctxt "sn24.2:2.11"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn24.2:2.12"
msgid ""
"“Yampidaṃ diṭṭhaṃ sutaṃ mutaṃ viññātaṃ pattaṃ pariyesitaṃ anuvicaritaṃ "
"manasā tampi niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn24.2:2.13"
msgid "“Aniccaṃ, bhante”."
msgstr ""

msgctxt "sn24.2:2.14"
msgid "“Yaṃ panāniccaṃ dukkhaṃ vā taṃ sukhaṃ vā”ti?"
msgstr ""

msgctxt "sn24.2:2.15"
msgid "“Dukkhaṃ, bhante”."
msgstr ""

msgctxt "sn24.2:2.16"
msgid ""
"“Yaṃ panāniccaṃ dukkhaṃ vipariṇāmadhammaṃ, api nu taṃ anupādāya evaṃ diṭṭhi "
"uppajjeyya:"
msgstr ""

msgctxt "sn24.2:2.17"
msgid "‘etaṃ mama, esohamasmi, eso me attā’”ti?"
msgstr ""

msgctxt "sn24.2:2.18"
msgid "“No hetaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn24.2:3.1"
msgid ""
"“Yato kho, bhikkhave, ariyasāvakassa imesu ca ṭhānesu kaṅkhā pahīnā hoti, "
"dukkhepissa kaṅkhā pahīnā hoti … pe … dukkhanirodhagāminiyā paṭipadāyapissa "
"kaṅkhā pahīnā hoti—"
msgstr ""

msgctxt "sn24.2:3.2"
msgid ""
"ayaṃ vuccati, bhikkhave, ariyasāvako sotāpanno avinipātadhammo niyato "
"sambodhiparāyano”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn24.2:3.3"
msgid "Dutiyaṃ."
msgstr ""
