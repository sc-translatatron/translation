#. <section class="sutta" id="sn1.33"><article><div class="hgroup"><p class="division">
msgctxt "sn1.33:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.33:0.2"
msgid "4. Satullapakāyikavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.33:0.3"
msgid "33. Sādhusutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.20"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.33:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn1.33:1.2"
msgid ""
"Atha kho sambahulā satullapakāyikā devatāyo abhikkantāya rattiyā "
"abhikkantavaṇṇā kevalakappaṃ jetavanaṃ obhāsetvā yena bhagavā "
"tenupasaṅkamiṃsu; upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ aṭṭhaṃsu."
msgstr ""

msgctxt "sn1.33:1.3"
msgid "Ekamantaṃ ṭhitā kho ekā devatā bhagavato santike imaṃ udānaṃ udānesi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.33:2.1"
msgid "“Sādhu kho mārisa dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:2.2"
msgid "maccherā ca pamādā ca;"
msgstr ""

#. <br>
msgctxt "sn1.33:2.3"
msgid "Evaṃ dānaṃ na dīyati,"
msgstr ""

#. <br>
msgctxt "sn1.33:2.4"
msgid "puññaṃ ākaṅkhamānena;"
msgstr ""

#. <br>
msgctxt "sn1.33:2.5"
msgid "Deyyaṃ hoti vijānatā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.33:3.1"
msgid "Atha kho aparā devatā bhagavato santike imaṃ udānaṃ udānesi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.33:4.1"
msgid "“Sādhu kho mārisa dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:4.2"
msgid "Api ca appakasmimpi sāhu dānaṃ."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.44"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn1.33:5.1"
msgid "Appasmeke pavecchanti,"
msgstr ""

#. <br>
msgctxt "sn1.33:5.2"
msgid "bahuneke na dicchare;"
msgstr ""

#. <br>
msgctxt "sn1.33:5.3"
msgid "Appasmā dakkhiṇā dinnā,"
msgstr ""

#. <br>
msgctxt "sn1.33:5.4"
msgid "sahassena samaṃ mitā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn1.33:6.1"
msgid "Atha kho aparā devatā bhagavato santike imaṃ udānaṃ udānesi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn1.33:7.1"
msgid "“Sādhu kho mārisa dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:7.2"
msgid "Appakasmimpi sāhu dānaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.33:7.3"
msgid "Api ca saddhāyapi sāhu dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:7.4"
msgid "Dānañca yuddhañca samānamāhu;"
msgstr ""

#. <br>
msgctxt "sn1.33:7.5"
msgid "Appāpi santā bahuke jinanti,"
msgstr ""

#. <br>
msgctxt "sn1.33:7.6"
msgid "Appampi ce saddahāno dadāti;"
msgstr ""

#. <br>
msgctxt "sn1.33:7.7"
msgid "Teneva so hoti sukhī paratthā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn1.33:8.1"
msgid "Atha kho aparā devatā bhagavato santike imaṃ udānaṃ udānesi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts1ed" id="pts1ed1.21"></a>
#. <a class="sc" id="sc9"></a>
msgctxt "sn1.33:9.1"
msgid "“Sādhu kho mārisa dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:9.2"
msgid "Appakasmimpi sāhu dānaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.33:9.3"
msgid "Saddhāyapi sāhu dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:9.4"
msgid "Api ca dhammaladdhassāpi sāhu dānaṃ."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.45"></a>
#. <a class="sc" id="sc10"></a>
msgctxt "sn1.33:10.1"
msgid "Yo dhammaladdhassa dadāti dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:10.2"
msgid "Uṭṭhānavīriyādhigatassa jantu;"
msgstr ""

#. <br>
msgctxt "sn1.33:10.3"
msgid "Atikkamma so vetaraṇiṃ yamassa,"
msgstr ""

#. <br>
msgctxt "sn1.33:10.4"
msgid "Dibbāni ṭhānāni upeti macco”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn1.33:11.1"
msgid "Atha kho aparā devatā bhagavato santike imaṃ udānaṃ udānesi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn1.33:12.1"
msgid "“Sādhu kho mārisa dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:12.2"
msgid "Appakasmimpi sāhu dānaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.33:12.3"
msgid "Saddhāyapi sāhu dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:12.4"
msgid "Dhammaladdhassāpi sāhu dānaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.33:12.5"
msgid "Api ca viceyya dānampi sāhu dānaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc13"></a>
msgctxt "sn1.33:13.1"
msgid "Viceyya dānaṃ sugatappasatthaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:13.2"
msgid "Ye dakkhiṇeyyā idha jīvaloke;"
msgstr ""

#. <br>
msgctxt "sn1.33:13.3"
msgid "Etesu dinnāni mahapphalāni,"
msgstr ""

#. <br>
msgctxt "sn1.33:13.4"
msgid "Bījāni vuttāni yathā sukhette”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc14"></a>
msgctxt "sn1.33:14.1"
msgid "Atha kho aparā devatā bhagavato santike imaṃ udānaṃ udānesi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.46"></a>
#. <a class="sc" id="sc15"></a>
msgctxt "sn1.33:15.1"
msgid "“Sādhu kho mārisa dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:15.2"
msgid "Appakasmimpi sāhu dānaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.33:15.3"
msgid "Saddhāyapi sāhu dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:15.4"
msgid "Dhammaladdhassāpi sāhu dānaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.33:15.5"
msgid "Viceyya dānampi sāhu dānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:15.6"
msgid "Api ca pāṇesupi sādhu saṃyamo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc16"></a>
#. VAR: pāṇabhūtāni → pāṇabhūtesu (bj, pts1-2)
msgctxt "sn1.33:16.1"
msgid "Yo pāṇabhūtāni aheṭhayaṃ caraṃ,"
msgstr ""

# Both PTS and BJT has karoti here. I don’t understand the sense of this verse.
#. <br>
msgctxt "sn1.33:16.2"
msgid "Parūpavādā na karonti pāpaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.33:16.3"
msgid "Bhīruṃ pasaṃsanti na hi tattha sūraṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:16.4"
msgid "Bhayā hi santo na karonti pāpan”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts1ed" id="pts1ed1.22"></a>
#. <a class="sc" id="sc17"></a>
msgctxt "sn1.33:17.1"
msgid "Atha kho aparā devatā bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn1.33:17.2"
msgid "“kassa nu kho, bhagavā, subhāsitan”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc18"></a>
msgctxt "sn1.33:18.1"
msgid "“Sabbāsaṃ vo subhāsitaṃ pariyāyena, api ca mamapi suṇātha—"
msgstr ""

# See BB for reading addha.
#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc19"></a>
msgctxt "sn1.33:19.1"
msgid "Saddhā hi dānaṃ bahudhā pasatthaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.33:19.2"
msgid "Dānā ca kho dhammapadaṃva seyyo;"
msgstr ""

#. <br>
msgctxt "sn1.33:19.3"
msgid "Pubbe ca hi pubbatare ca santo,"
msgstr ""

#. <br>
msgctxt "sn1.33:19.4"
msgid "Nibbānamevajjhagamuṃ sapaññā”ti."
msgstr ""
