#. <section class="sutta" id="sn1.26"><article><div class="hgroup"><p class="division">
msgctxt "sn1.26:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.26:0.2"
msgid "3. Sattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.26:0.3"
msgid "26. Pajjotasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.26:1.1"
msgid "“Kati lokasmiṃ pajjotā,"
msgstr ""

#. <br>
#. VAR: pakāsati → pabhāsati (si)
msgctxt "sn1.26:1.2"
msgid "Tehi loko pakāsati;"
msgstr ""

#. <br>
#. VAR: Bhagavantaṃ → bhavantaṃ (pts1-2, mr)
msgctxt "sn1.26:1.3"
msgid "Bhagavantaṃ puṭṭhumāgamma,"
msgstr ""

#. <br>
msgctxt "sn1.26:1.4"
msgid "Kathaṃ jānemu taṃ mayan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.26:2.1"
msgid "“Cattāro loke pajjotā,"
msgstr ""

#. <br>
msgctxt "sn1.26:2.2"
msgid "pañcamettha na vijjati;"
msgstr ""

#. <br>
msgctxt "sn1.26:2.3"
msgid "Divā tapati ādicco,"
msgstr ""

#. <br>
msgctxt "sn1.26:2.4"
msgid "rattimābhāti candimā."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.33"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.26:3.1"
msgid "Atha aggi divārattiṃ,"
msgstr ""

#. <br>
msgctxt "sn1.26:3.2"
msgid "tattha tattha pakāsati;"
msgstr ""

#. <br>
msgctxt "sn1.26:3.3"
msgid "Sambuddho tapataṃ seṭṭho,"
msgstr ""

#. <br>
msgctxt "sn1.26:3.4"
msgid "esā ābhā anuttarā”ti."
msgstr ""
