#. <section class="sutta" id="sn47.44"><article><div class="hgroup"><p class="division">
msgctxt "sn47.44:0.1"
msgid "Saṃyutta Nikāya 47"
msgstr ""

#. </p><p>
msgctxt "sn47.44:0.2"
msgid "5. Amatavagga"
msgstr ""

#. </p><h1>
#. VAR: 44. Satisutta → satosuttaṃ (bj) | sato (pts1)
msgctxt "sn47.44:0.3"
msgid "44. Satisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn47.44:1.1"
msgid "“Sato, bhikkhave, bhikkhu vihareyya."
msgstr ""

msgctxt "sn47.44:1.2"
msgid "Ayaṃ vo amhākaṃ anusāsanī."
msgstr ""

msgctxt "sn47.44:1.3"
msgid "Kathañca, bhikkhave, bhikkhu sato hoti?"
msgstr ""

msgctxt "sn47.44:1.4"
msgid ""
"Idha, bhikkhave, bhikkhu kāye kāyānupassī viharati ātāpī sampajāno satimā, "
"vineyya loke abhijjhādomanassaṃ;"
msgstr ""

msgctxt "sn47.44:1.5"
msgid "vedanāsu … pe …"
msgstr ""

msgctxt "sn47.44:1.6"
msgid "citte … pe …"
msgstr ""

msgctxt "sn47.44:1.7"
msgid ""
"dhammesu dhammānupassī viharati ātāpī sampajāno satimā, vineyya loke "
"abhijjhādomanassaṃ."
msgstr ""

msgctxt "sn47.44:1.8"
msgid "Evaṃ kho, bhikkhave, bhikkhu sato hoti."
msgstr ""

msgctxt "sn47.44:1.9"
msgid "Sato, bhikkhave, bhikkhu vihareyya."
msgstr ""

msgctxt "sn47.44:1.10"
msgid "Ayaṃ vo amhākaṃ anusāsanī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn47.44:1.11"
msgid "Catutthaṃ."
msgstr ""
