#. <section class="sutta" id="sn12.57"><article><div class="hgroup"><p class="division">
msgctxt "sn12.57:0.1"
msgid "Saṃyutta Nikāya 12"
msgstr ""

#. </p><p>
msgctxt "sn12.57:0.2"
msgid "6. Dukkhavagga"
msgstr ""

#. </p><h1>
msgctxt "sn12.57:0.3"
msgid "57. Taruṇarukkhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn12.57:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn12.57:1.2"
msgid ""
"“Saṃyojaniyesu, bhikkhave, dhammesu assādānupassino viharato taṇhā "
"pavaḍḍhati."
msgstr ""

msgctxt "sn12.57:1.3"
msgid "Taṇhāpaccayā upādānaṃ … pe …"
msgstr ""

msgctxt "sn12.57:1.4"
msgid "evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn12.57:2.1"
msgid "Seyyathāpi, bhikkhave, taruṇo rukkho."
msgstr ""

#. VAR: palimajjeyya → palisattheyya (bj) | palisajjeyya (s1-3, km, pts1-2) | palipaṭṭheyya (mr) | palibandheyya (ṭīkānurūpaṃ)
msgctxt "sn12.57:2.2"
msgid ""
"Tassa puriso kālena kālaṃ mūlāni palimajjeyya kālena kālaṃ paṃsuṃ dadeyya, "
"kālena kālaṃ udakaṃ dadeyya."
msgstr ""

msgctxt "sn12.57:2.3"
msgid ""
"Evañhi so, bhikkhave, taruṇo rukkho tadāhāro tadupādāno vuddhiṃ virūḷhiṃ "
"vepullaṃ āpajjeyya."
msgstr ""

msgctxt "sn12.57:2.4"
msgid ""
"Evameva kho, bhikkhave, saṃyojaniyesu dhammesu assādānupassino viharato "
"taṇhā pavaḍḍhati."
msgstr ""

msgctxt "sn12.57:2.5"
msgid "Taṇhāpaccayā upādānaṃ … pe …"
msgstr ""

msgctxt "sn12.57:2.6"
msgid "evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn12.57:3.1"
msgid ""
"Saṃyojaniyesu, bhikkhave, dhammesu ādīnavānupassino viharato taṇhā "
"nirujjhati."
msgstr ""

msgctxt "sn12.57:3.2"
msgid "Taṇhānirodhā upādānanirodho … pe …"
msgstr ""

msgctxt "sn12.57:3.3"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.90"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn12.57:4.1"
msgid "Seyyathāpi, bhikkhave, taruṇo rukkho."
msgstr ""

msgctxt "sn12.57:4.2"
msgid "Atha puriso āgaccheyya kuddālapiṭakaṃ ādāya … pe …"
msgstr ""

msgctxt "sn12.57:4.3"
msgid "nadiyā vā sīghasotāya pavāheyya."
msgstr ""

msgctxt "sn12.57:4.4"
msgid ""
"Evañhi so, bhikkhave, taruṇo rukkho ucchinnamūlo assa tālāvatthukato "
"anabhāvaṅkato āyatiṃ anuppādadhammo."
msgstr ""

msgctxt "sn12.57:4.5"
msgid ""
"Evameva kho, bhikkhave, saṃyojaniyesu dhammesu ādīnavānupassino viharato "
"taṇhā nirujjhati."
msgstr ""

msgctxt "sn12.57:4.6"
msgid "Taṇhānirodhā upādānanirodho … pe …"
msgstr ""

msgctxt "sn12.57:4.7"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.57:4.8"
msgid "Sattamaṃ."
msgstr ""
