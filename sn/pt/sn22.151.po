#. <section class="sutta" id="sn22.151"><article><div class="hgroup"><p class="division">
msgctxt "sn22.151:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.151:0.2"
msgid "15. Diṭṭhivagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.151:0.3"
msgid "151. Etaṃmamasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.182"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.151:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.151:1.2"
msgid "“Kismiṃ nu kho, bhikkhave, sati, kiṃ upādāya, kiṃ abhinivissa:"
msgstr ""

msgctxt "sn22.151:1.3"
msgid "‘etaṃ mama, esohamasmi, eso me attā’ti samanupassatī”ti?"
msgstr ""

msgctxt "sn22.151:1.4"
msgid "Bhagavaṃmūlakā no, bhante, dhammā … pe …"
msgstr ""

msgctxt "sn22.151:1.5"
msgid "“rūpe kho, bhikkhave, sati, rūpaṃ upādāya, rūpaṃ abhinivissa … pe …"
msgstr ""

msgctxt "sn22.151:1.6"
msgid "viññāṇe sati, viññāṇaṃ upādāya, viññāṇaṃ abhinivissa:"
msgstr ""

msgctxt "sn22.151:1.7"
msgid "‘etaṃ mama, esohamasmi, eso me attā’ti samanupassati."
msgstr ""

msgctxt "sn22.151:1.8"
msgid "Taṃ kiṃ maññatha, bhikkhave, "
msgstr ""

msgctxt "sn22.151:1.9"
msgid "rūpaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn22.151:1.10"
msgid "“Aniccaṃ, bhante” … pe …"
msgstr ""

msgctxt "sn22.151:1.11"
msgid ""
"vipariṇāmadhammaṃ, api nu taṃ anupādāya etaṃ mama, esohamasmi, eso me attāti "
"samanupasseyyāti?"
msgstr ""

msgctxt "sn22.151:1.12"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn22.151:1.13"
msgid "“Vedanā …"
msgstr ""

msgctxt "sn22.151:1.14"
msgid "saññā …"
msgstr ""

msgctxt "sn22.151:1.15"
msgid "saṅkhārā …"
msgstr ""

msgctxt "sn22.151:1.16"
msgid "viññāṇaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn22.151:1.17"
msgid "“Aniccaṃ, bhante” … pe …"
msgstr ""

msgctxt "sn22.151:1.18"
msgid ""
"vipariṇāmadhammaṃ, api nu taṃ anupādāya etaṃ mama, esohamasmi, eso me attāti "
"samanupasseyyāti?"
msgstr ""

msgctxt "sn22.151:1.19"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn22.151:1.20"
msgid "“Evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn22.151:1.21"
msgid "nāparaṃ itthattāyāti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.151:1.22"
msgid "Dutiyaṃ."
msgstr ""
