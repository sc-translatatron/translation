#. <section class="sutta" id="sn39.16"><article><div class="hgroup"><p class="division">
msgctxt "sn39.16:0.1"
msgid "Saṃyutta Nikāya 39"
msgstr ""

#. </p><p>
msgctxt "sn39.16:0.2"
msgid "1. Sāmaṇḍakavagga"
msgstr ""

#. </p><h1>
msgctxt "sn39.16:0.3"
msgid "16. Dukkarasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn39.16:1.1"
msgid "“Kiṃ nu kho, āvuso sāriputta, imasmiṃ dhammavinaye dukkaran”ti?"
msgstr ""

msgctxt "sn39.16:1.2"
msgid "“Pabbajjā kho, āvuso, imasmiṃ dhammavinaye dukkarā”ti."
msgstr ""

msgctxt "sn39.16:1.3"
msgid "“Pabbajitena panāvuso, kiṃ dukkaran”ti?"
msgstr ""

msgctxt "sn39.16:1.4"
msgid "“Pabbajitena kho, āvuso, abhirati dukkarā”ti."
msgstr ""

msgctxt "sn39.16:1.5"
msgid "“Abhiratena panāvuso, kiṃ dukkaran”ti?"
msgstr ""

msgctxt "sn39.16:1.6"
msgid "“Abhiratena kho, āvuso, dhammānudhammappaṭipatti dukkarā”ti."
msgstr ""

msgctxt "sn39.16:1.7"
msgid "“Kīvaciraṃ panāvuso, dhammānudhammappaṭipanno bhikkhu arahaṃ assā”ti?"
msgstr ""

msgctxt "sn39.16:1.8"
msgid "“Naciraṃ, āvuso”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn39.16:1.9"
msgid "Soḷasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn39.16:1.10"
msgid "(Purimakasadisaṃ uddānaṃ.)"
msgstr ""

#. </p><p class="endsutta">
msgctxt "sn39.16:1.11"
msgid "Sāmaṇḍakasaṃyuttaṃ samattaṃ."
msgstr ""
