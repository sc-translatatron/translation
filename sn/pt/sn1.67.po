#. <section class="sutta" id="sn1.67"><article><div class="hgroup"><p class="division">
msgctxt "sn1.67:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.67:0.2"
msgid "7. Addhavagga"
msgstr ""

#. </p><h1>
#. VAR: 67. Uḍḍitasutta → uddito (pts1)
msgctxt "sn1.67:0.3"
msgid "67. Uḍḍitasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.67:1.1"
msgid "“Kenassu uḍḍito loko,"
msgstr ""

#. <br>
msgctxt "sn1.67:1.2"
msgid "kenassu parivārito;"
msgstr ""

#. <br>
msgctxt "sn1.67:1.3"
msgid "Kenassu pihito loko,"
msgstr ""

#. <br>
msgctxt "sn1.67:1.4"
msgid "kismiṃ loko patiṭṭhito”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.67:2.1"
msgid "“Taṇhāya uḍḍito loko,"
msgstr ""

#. <br>
msgctxt "sn1.67:2.2"
msgid "jarāya parivārito;"
msgstr ""

#. <br>
msgctxt "sn1.67:2.3"
msgid "Maccunā pihito loko,"
msgstr ""

#. <br>
msgctxt "sn1.67:2.4"
msgid "dukkhe loko patiṭṭhito”ti."
msgstr ""
