#. <section class="sutta" id="sn1.10"><article><div class="hgroup"><p class="division">
msgctxt "sn1.10:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.10:0.2"
msgid "1. Naḷavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.10:0.3"
msgid "10. Araññasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.5"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.10:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

# Note the variant pericope here, I don’t know why it is, but it is the same in PTS. BB doesn’t reflect this.
msgctxt "sn1.10:1.2"
msgid "Ekamantaṃ ṭhitā kho sā devatā bhagavantaṃ gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.10:2.1"
msgid "“Araññe viharantānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.10:2.2"
msgid "santānaṃ brahmacārinaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.10:2.3"
msgid "Ekabhattaṃ bhuñjamānānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.10:2.4"
msgid "kena vaṇṇo pasīdatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.10:3.1"
msgid "“Atītaṃ nānusocanti,"
msgstr ""

#. <br>
msgctxt "sn1.10:3.2"
msgid "nappajappanti nāgataṃ;"
msgstr ""

#. <br>
msgctxt "sn1.10:3.3"
msgid "Paccuppannena yāpenti,"
msgstr ""

#. <br>
msgctxt "sn1.10:3.4"
msgid "tena vaṇṇo pasīdati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.10:4.1"
msgid "Anāgatappajappāya,"
msgstr ""

#. <br>
msgctxt "sn1.10:4.2"
msgid "atītassānusocanā;"
msgstr ""

#. <br>
msgctxt "sn1.10:4.3"
msgid "Etena bālā sussanti,"
msgstr ""

#. <br>
msgctxt "sn1.10:4.4"
msgid "naḷova harito luto”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn1.10:4.5"
msgid "Naḷavaggo paṭhamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="pts2ed" id="pts2ed1.10"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn1.10:5.1"
msgid "Oghaṃ nimokkhaṃ upaneyyaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.10:5.2"
msgid "Accenti katichindi ca;"
msgstr ""

#. <br>
msgctxt "sn1.10:5.3"
msgid "Jāgaraṃ appaṭividitā,"
msgstr ""

#. <br>
msgctxt "sn1.10:5.4"
msgid "Susammuṭṭhā mānakāminā;"
msgstr ""

#. <br>
msgctxt "sn1.10:5.5"
msgid "Araññe dasamo vutto,"
msgstr ""

#. <br>
msgctxt "sn1.10:5.6"
msgid "Vaggo tena pavuccati."
msgstr ""
