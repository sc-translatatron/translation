#. <section class="sutta" id="sn35.150"><article><div class="hgroup"><p class="division">
msgctxt "sn35.150:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.150:0.2"
msgid "15. Navapurāṇavagga"
msgstr ""

#. </p><h1>
#. VAR: 150. Nibbānasappāyapaṭipadāsutta → catutthasappāyasuttaṃ (bj) | sappāya 4 (pts1)
msgctxt "sn35.150:0.3"
msgid "150. Nibbānasappāyapaṭipadāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.150:1.1"
msgid "“Nibbānasappāyaṃ vo, bhikkhave, paṭipadaṃ desessāmi."
msgstr ""

msgctxt "sn35.150:1.2"
msgid "Taṃ suṇātha … pe …"
msgstr ""

msgctxt "sn35.150:1.3"
msgid "katamā ca sā, bhikkhave, nibbānasappāyā paṭipadā?"
msgstr ""

msgctxt "sn35.150:1.4"
msgid "Taṃ kiṃ maññatha, bhikkhave, "
msgstr ""

msgctxt "sn35.150:1.5"
msgid "cakkhu niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.150:2.1"
msgid "“Aniccaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn35.150:3.1"
msgid "“Yaṃ panāniccaṃ dukkhaṃ vā taṃ sukhaṃ vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn35.150:4.1"
msgid "“Dukkhaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn35.150:5.1"
msgid ""
"“Yaṃ panāniccaṃ dukkhaṃ vipariṇāmadhammaṃ, kallaṃ nu taṃ samanupassituṃ:"
msgstr ""

msgctxt "sn35.150:5.2"
msgid "‘etaṃ mama, esohamasmi, eso me attā’”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn35.150:6.1"
msgid "“No hetaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn35.150:7.1"
msgid "“Rūpā niccā vā aniccā vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn35.150:8.1"
msgid "“Aniccā, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "sn35.150:9.1"
msgid "“Cakkhuviññāṇaṃ …"
msgstr ""

msgctxt "sn35.150:9.2"
msgid "cakkhusamphasso … pe …"
msgstr ""

msgctxt "sn35.150:9.3"
msgid ""
"yampidaṃ manosamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn35.150:10.1"
msgid "“Aniccaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn35.150:11.1"
msgid "“Yaṃ panāniccaṃ dukkhaṃ vā taṃ sukhaṃ vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn35.150:12.1"
msgid "“Dukkhaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc13"></a>
msgctxt "sn35.150:13.1"
msgid ""
"“Yaṃ panāniccaṃ dukkhaṃ vipariṇāmadhammaṃ, kallaṃ nu taṃ samanupassituṃ:"
msgstr ""

msgctxt "sn35.150:13.2"
msgid "‘etaṃ mama, esohamasmi, eso me attā’”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc14"></a>
msgctxt "sn35.150:14.1"
msgid "“No hetaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.136"></a>
#. <a class="sc" id="sc15"></a>
msgctxt "sn35.150:15.1"
msgid ""
"“Evaṃ passaṃ, bhikkhave, sutavā ariyasāvako cakkhusmimpi nibbindati, "
"rūpesupi nibbindati, cakkhuviññāṇepi nibbindati, cakkhusamphassepi "
"nibbindati … pe …"
msgstr ""

msgctxt "sn35.150:15.2"
msgid ""
"yampidaṃ manosamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tasmimpi nibbindati."
msgstr ""

msgctxt "sn35.150:15.3"
msgid "Nibbindaṃ virajjati; virāgā vimuccati … pe …"
msgstr ""

msgctxt "sn35.150:15.4"
msgid "nāparaṃ itthattāyāti pajānāti."
msgstr ""

msgctxt "sn35.150:15.5"
msgid "Ayaṃ kho sā, bhikkhave, nibbānasappāyā paṭipadā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.150:15.6"
msgid "Pañcamaṃ."
msgstr ""
