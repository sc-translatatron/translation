#. <section class="sutta" id="sn22.118"><article><div class="hgroup"><p class="division">
msgctxt "sn22.118:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.118:0.2"
msgid "12. Dhammakathikavagga"
msgstr ""

#. </p><h1>
#. VAR: 118. Paripucchitasutta → parimucchita 1 (pts1)
msgctxt "sn22.118:0.3"
msgid "118. Paripucchitasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.166"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.118:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.118:1.2"
msgid "“Taṃ kiṃ maññatha, bhikkhave, "
msgstr ""

msgctxt "sn22.118:1.3"
msgid "rūpaṃ ‘etaṃ mama, esohamasmi, eso me attā’ti samanupassathā”ti?"
msgstr ""

msgctxt "sn22.118:1.4"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn22.118:1.5"
msgid "“Sādhu, bhikkhave."
msgstr ""

msgctxt "sn22.118:1.6"
msgid ""
"Rūpaṃ, bhikkhave, ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ "
"yathābhūtaṃ sammappaññāya daṭṭhabbaṃ."
msgstr ""

msgctxt "sn22.118:1.7"
msgid "Vedanaṃ …"
msgstr ""

msgctxt "sn22.118:1.8"
msgid "saññaṃ …"
msgstr ""

msgctxt "sn22.118:1.9"
msgid "saṅkhāre …"
msgstr ""

msgctxt "sn22.118:1.10"
msgid "viññāṇaṃ ‘etaṃ mama, esohamasmi, eso me attā’ti samanupassathā”ti?"
msgstr ""

msgctxt "sn22.118:1.11"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn22.118:1.12"
msgid "“Sādhu, bhikkhave."
msgstr ""

msgctxt "sn22.118:1.13"
msgid ""
"Viññāṇaṃ, bhikkhave, ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ "
"yathābhūtaṃ sammappaññāya daṭṭhabbaṃ … pe …"
msgstr ""

msgctxt "sn22.118:1.14"
msgid "evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn22.118:1.15"
msgid "nāparaṃ itthattāyāti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.118:1.16"
msgid "Chaṭṭhaṃ."
msgstr ""
