#. <section class="sutta" id="sn5.7"><article><div class="hgroup"><p class="division">
msgctxt "sn5.7:0.1"
msgid "Saṃyutta Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "sn5.7:0.2"
msgid "1. Bhikkhunīvagga"
msgstr ""

#. </p><h1>
msgctxt "sn5.7:0.3"
msgid "7. Upacālāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn5.7:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn5.7:1.2"
msgid "Atha kho upacālā bhikkhunī pubbaṇhasamayaṃ nivāsetvā … pe …"
msgstr ""

msgctxt "sn5.7:1.3"
msgid "aññatarasmiṃ rukkhamūle divāvihāraṃ nisīdi."
msgstr ""

msgctxt "sn5.7:1.4"
msgid ""
"Atha kho māro pāpimā yena upacālā bhikkhunī tenupasaṅkami; upasaṅkamitvā "
"upacālaṃ bhikkhuniṃ etadavoca:"
msgstr ""

msgctxt "sn5.7:1.5"
msgid "“kattha nu tvaṃ, bhikkhuni, uppajjitukāmā”ti?"
msgstr ""

msgctxt "sn5.7:1.6"
msgid "“Na khvāhaṃ, āvuso, katthaci uppajjitukāmā”ti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.292"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn5.7:2.1"
msgid "“Tāvatiṃsā ca yāmā ca,"
msgstr ""

#. <br>
msgctxt "sn5.7:2.2"
msgid "tusitā cāpi devatā;"
msgstr ""

#. <br>
msgctxt "sn5.7:2.3"
msgid "Nimmānaratino devā,"
msgstr ""

#. <br>
msgctxt "sn5.7:2.4"
msgid "ye devā vasavattino;"
msgstr ""

#. <br>
msgctxt "sn5.7:2.5"
msgid "Tattha cittaṃ paṇidhehi,"
msgstr ""

#. <br>
msgctxt "sn5.7:2.6"
msgid "ratiṃ paccanubhossasī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn5.7:3.1"
msgid "“Tāvatiṃsā ca yāmā ca,"
msgstr ""

#. <br>
msgctxt "sn5.7:3.2"
msgid "tusitā cāpi devatā;"
msgstr ""

#. <br>
msgctxt "sn5.7:3.3"
msgid "Nimmānaratino devā,"
msgstr ""

#. <br>
msgctxt "sn5.7:3.4"
msgid "ye devā vasavattino;"
msgstr ""

#. <br>
msgctxt "sn5.7:3.5"
msgid "Kāmabandhanabaddhā te,"
msgstr ""

#. <br>
msgctxt "sn5.7:3.6"
msgid "enti māravasaṃ puna."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
#. VAR: Sabbo ādīpito → sabbo ādippito (si) | sabbova āditto (s1-3, km) | sabbo ādipito (pts1-2)
msgctxt "sn5.7:4.1"
msgid "Sabbo ādīpito loko,"
msgstr ""

#. <br>
msgctxt "sn5.7:4.2"
msgid "sabbo loko padhūpito;"
msgstr ""

#. <br>
#. VAR: pajjālito → pajjalito (sabbattha)
msgctxt "sn5.7:4.3"
msgid "Sabbo pajjālito loko,"
msgstr ""

#. <br>
msgctxt "sn5.7:4.4"
msgid "sabbo loko pakampito."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
#. VAR: apajjalitaṃ → ajalitaṃ (bj) | acalitaṃ (s1-3, km, pts1-2)
msgctxt "sn5.7:5.1"
msgid "Akampitaṃ apajjalitaṃ,"
msgstr ""

#. <br>
msgctxt "sn5.7:5.2"
msgid "aputhujjanasevitaṃ;"
msgstr ""

#. <br>
msgctxt "sn5.7:5.3"
msgid "Agati yattha mārassa,"
msgstr ""

#. <br>
msgctxt "sn5.7:5.4"
msgid "tattha me nirato mano”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn5.7:6.1"
msgid ""
"Atha kho māro pāpimā “jānāti maṃ upacālā bhikkhunī”ti dukkhī dummano "
"tatthevantaradhāyīti."
msgstr ""
