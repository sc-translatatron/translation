#. <section class="sutta" id="sn37.34"><article><div class="hgroup"><p class="division">
msgctxt "sn37.34:0.1"
msgid "Saṃyutta Nikāya 37"
msgstr ""

#. </p><p>
msgctxt "sn37.34:0.2"
msgid "3. Balavagga"
msgstr ""

#. </p><h1>
msgctxt "sn37.34:0.3"
msgid "34. Vaḍḍhīsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn37.34:1.1"
msgid ""
"“Pañcahi, bhikkhave, vaḍḍhīhi vaḍḍhamānā ariyasāvikā ariyāya vaḍḍhiyā "
"vaḍḍhati sārādāyinī ca hoti varādāyinī ca kāyassa."
msgstr ""

msgctxt "sn37.34:1.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "sn37.34:1.3"
msgid ""
"Saddhāya vaḍḍhati, sīlena vaḍḍhati, sutena vaḍḍhati, cāgena vaḍḍhati, "
"paññāya vaḍḍhati—"
msgstr ""

msgctxt "sn37.34:1.4"
msgid ""
"imehi kho, bhikkhave, pañcahi vaḍḍhīhi vaḍḍhamānā ariyasāvikā ariyāya "
"vaḍḍhiyā vaḍḍhati, sārādāyinī ca hoti, varādāyinī ca kāyassāti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn37.34:2.1"
msgid "Saddhāya sīlena ca yādha vaḍḍhati,"
msgstr ""

#. <br>
msgctxt "sn37.34:2.2"
msgid "Paññāya cāgena sutena cūbhayaṃ;"
msgstr ""

#. <br>
msgctxt "sn37.34:2.3"
msgid "Sā tādisī sīlavatī upāsikā,"
msgstr ""

#. <br>
msgctxt "sn37.34:2.4"
msgid "Ādīyati sāramidheva attano”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn37.34:2.5"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn37.34:2.6"
msgid "Balavaggo tatiyo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.251"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn37.34:3.1"
msgid "Visāradā pasayha abhibhuyya,"
msgstr ""

#. <br>
msgctxt "sn37.34:3.2"
msgid "Ekaṃ aṅgena pañcamaṃ;"
msgstr ""

#. <br>
msgctxt "sn37.34:3.3"
msgid "Nāsenti hetu ṭhānañca,"
msgstr ""

#. <br>
msgctxt "sn37.34:3.4"
msgid "Visārado vaḍḍhinā dasāti."
msgstr ""

#. </p></blockquote><p class="endsutta">
msgctxt "sn37.34:3.5"
msgid "Mātugāmasaṃyuttaṃ samattaṃ."
msgstr ""
