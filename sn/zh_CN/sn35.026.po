#. <section class="sutta" id="sn35.26"><article><div class="hgroup"><p class="division">
msgctxt "sn35.26:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.26:0.2"
msgid "3. Sabbavagga"
msgstr ""

#. </p><h1>
#. VAR: 26. Paṭhamaaparijānanasutta → aparijānasuttaṃ (bj) | parijānāna (pts1)
msgctxt "sn35.26:0.3"
msgid "26. Paṭhamaaparijānanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.26:1.1"
msgid ""
"“Sabbaṃ, bhikkhave, anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo "
"dukkhakkhayāya."
msgstr ""

# Reading sabba as per PTS.
msgctxt "sn35.26:1.2"
msgid ""
"Kiñca, bhikkhave, anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo "
"dukkhakkhayāya?"
msgstr ""

msgctxt "sn35.26:1.3"
msgid ""
"Cakkhuṃ, bhikkhave, anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:1.4"
msgid ""
"Rūpe anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:1.5"
msgid ""
"Cakkhuviññāṇaṃ anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:1.6"
msgid ""
"Cakkhusamphassaṃ anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:1.7"
msgid ""
"Yampidaṃ cakkhusamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo "
"dukkhakkhayāya … pe …"
msgstr ""

msgctxt "sn35.26:1.8"
msgid ""
"jivhaṃ anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:1.9"
msgid "Rase … pe …"
msgstr ""

msgctxt "sn35.26:1.10"
msgid "jivhāviññāṇaṃ … pe …"
msgstr ""

msgctxt "sn35.26:1.11"
msgid "jivhāsamphassaṃ … pe …"
msgstr ""

msgctxt "sn35.26:1.12"
msgid ""
"yampidaṃ jivhāsamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:1.13"
msgid "Kāyaṃ … pe …"
msgstr ""

msgctxt "sn35.26:1.14"
msgid ""
"manaṃ anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:1.15"
msgid "Dhamme … pe …"
msgstr ""

msgctxt "sn35.26:1.16"
msgid "manoviññāṇaṃ … pe …"
msgstr ""

msgctxt "sn35.26:1.17"
msgid "manosamphassaṃ … pe …"
msgstr ""

msgctxt "sn35.26:1.18"
msgid ""
"yampidaṃ manosamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:1.19"
msgid ""
"Idaṃ kho, bhikkhave, sabbaṃ anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ "
"abhabbo dukkhakkhayāya."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.18"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.26:2.1"
msgid ""
"Sabbañca kho, bhikkhave, abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:2.2"
msgid ""
"Kiñca, bhikkhave, sabbaṃ abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo "
"dukkhakkhayāya?"
msgstr ""

msgctxt "sn35.26:2.3"
msgid ""
"Cakkhuṃ, bhikkhave, abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:2.4"
msgid "Rūpe abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:2.5"
msgid ""
"Cakkhuviññāṇaṃ abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:2.6"
msgid ""
"Cakkhusamphassaṃ abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:2.7"
msgid ""
"Yampidaṃ cakkhusamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo "
"dukkhakkhayāya … pe …"
msgstr ""

msgctxt "sn35.26:2.8"
msgid "jivhaṃ abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:2.9"
msgid "Rase … pe …"
msgstr ""

msgctxt "sn35.26:2.10"
msgid "jivhāviññāṇaṃ … pe …"
msgstr ""

msgctxt "sn35.26:2.11"
msgid "jivhāsamphassaṃ … pe …"
msgstr ""

msgctxt "sn35.26:2.12"
msgid ""
"yampidaṃ jivhāsamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:2.13"
msgid "Kāyaṃ … pe …"
msgstr ""

msgctxt "sn35.26:2.14"
msgid "manaṃ abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:2.15"
msgid "Dhamme … pe …"
msgstr ""

msgctxt "sn35.26:2.16"
msgid "manoviññāṇaṃ … pe …"
msgstr ""

msgctxt "sn35.26:2.17"
msgid "manosamphassaṃ … pe …"
msgstr ""

msgctxt "sn35.26:2.18"
msgid ""
"yampidaṃ manosamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.26:2.19"
msgid ""
"Idaṃ kho, bhikkhave, sabbaṃ abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo "
"dukkhakkhayāyā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.26:2.20"
msgid "Catutthaṃ."
msgstr ""
