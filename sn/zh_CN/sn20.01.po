#. <section class="sutta" id="sn20.1"><article><div class="hgroup"><p class="division">
msgctxt "sn20.1:0.1"
msgid "Saṃyutta Nikāya 20"
msgstr ""

#. </p><p>
msgctxt "sn20.1:0.2"
msgid "1. Opammavagga"
msgstr ""

#. </p><h1>
msgctxt "sn20.1:0.3"
msgid "1. Kūṭasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.263"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn20.1:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn20.1:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "sn20.1:1.3"
msgid "Tatra kho bhagavā … pe … etadavoca:"
msgstr ""

msgctxt "sn20.1:1.4"
msgid ""
"“seyyathāpi, bhikkhave, kūṭāgārassa yā kāci gopānasiyo sabbā tā kūṭaṅgamā "
"kūṭasamosaraṇā kūṭasamugghātā sabbā tā samugghātaṃ gacchanti;"
msgstr ""

msgctxt "sn20.1:1.5"
msgid ""
"evameva kho, bhikkhave, ye keci akusalā dhammā sabbe te avijjāmūlakā "
"avijjāsamosaraṇā avijjāsamugghātā, sabbe te samugghātaṃ gacchanti."
msgstr ""

msgctxt "sn20.1:1.6"
msgid "Tasmātiha, bhikkhave, evaṃ sikkhitabbaṃ:"
msgstr ""

msgctxt "sn20.1:1.7"
msgid "‘appamattā viharissāmā’ti."
msgstr ""

msgctxt "sn20.1:1.8"
msgid "Evañhi vo, bhikkhave, sikkhitabban”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn20.1:1.9"
msgid "Paṭhamaṃ."
msgstr ""
