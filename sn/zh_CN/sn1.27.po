#. <section class="sutta" id="sn1.27"><article><div class="hgroup"><p class="division">
msgctxt "sn1.27:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.27:0.2"
msgid "3. Sattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.27:0.3"
msgid "27. Sarasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.27:1.1"
msgid "“Kuto sarā nivattanti,"
msgstr ""

#. <br>
msgctxt "sn1.27:1.2"
msgid "kattha vaṭṭaṃ na vattati;"
msgstr ""

#. <br>
msgctxt "sn1.27:1.3"
msgid "Kattha nāmañca rūpañca,"
msgstr ""

#. <br>
msgctxt "sn1.27:1.4"
msgid "asesaṃ uparujjhatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.27:2.1"
msgid "“Yattha āpo ca pathavī,"
msgstr ""

#. <br>
msgctxt "sn1.27:2.2"
msgid "tejo vāyo na gādhati;"
msgstr ""

#. <br>
msgctxt "sn1.27:2.3"
msgid "Ato sarā nivattanti,"
msgstr ""

#. <br>
msgctxt "sn1.27:2.4"
msgid "ettha vaṭṭaṃ na vattati;"
msgstr ""

#. <br>
msgctxt "sn1.27:2.5"
msgid "Ettha nāmañca rūpañca,"
msgstr ""

#. <br>
msgctxt "sn1.27:2.6"
msgid "asesaṃ uparujjhatī”ti."
msgstr ""
