#. <section class="sutta" id="sn7.7"><article><div class="hgroup"><p class="division">
msgctxt "sn7.7:0.1"
msgid "Saṃyutta Nikāya 7"
msgstr ""

#. </p><p>
msgctxt "sn7.7:0.2"
msgid "1. Arahantavagga"
msgstr ""

#. </p><h1>
msgctxt "sn7.7:0.3"
msgid "7. Suddhikasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.166"></a>
#. <a class="pts2ed" id="pts2ed1.355"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn7.7:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn7.7:1.2"
msgid ""
"Atha kho suddhikabhāradvājo brāhmaṇo yena bhagavā tenupasaṅkami; "
"upasaṅkamitvā bhagavatā saddhiṃ sammodi."
msgstr ""

msgctxt "sn7.7:1.3"
msgid "Sammodanīyaṃ kathaṃ sāraṇīyaṃ vītisāretvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "sn7.7:1.4"
msgid ""
"Ekamantaṃ nisinno kho suddhikabhāradvājo brāhmaṇo bhagavato santike imaṃ "
"gāthaṃ ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
#. VAR: Na brāhmaṇo → nābrāhmaṇo (?)
msgctxt "sn7.7:2.1"
msgid "“Na brāhmaṇo sujjhati koci,"
msgstr ""

#. <br>
msgctxt "sn7.7:2.2"
msgid "Loke sīlavāpi tapokaraṃ;"
msgstr ""

#. <br>
msgctxt "sn7.7:2.3"
msgid "Vijjācaraṇasampanno,"
msgstr ""

#. <br>
msgctxt "sn7.7:2.4"
msgid "So sujjhati na aññā itarā pajā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn7.7:3.1"
msgid "“Bahumpi palapaṃ jappaṃ,"
msgstr ""

#. <br>
msgctxt "sn7.7:3.2"
msgid "na jaccā hoti brāhmaṇo;"
msgstr ""

#. <br>
msgctxt "sn7.7:3.3"
msgid "Antokasambu saṅkiliṭṭho,"
msgstr ""

#. <br>
msgctxt "sn7.7:3.4"
msgid "kuhanaṃ upanissito."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn7.7:4.1"
msgid "Khattiyo brāhmaṇo vesso,"
msgstr ""

#. <br>
msgctxt "sn7.7:4.2"
msgid "Suddo caṇḍālapukkuso;"
msgstr ""

#. <br>
msgctxt "sn7.7:4.3"
msgid "Āraddhavīriyo pahitatto,"
msgstr ""

#. <br>
msgctxt "sn7.7:4.4"
msgid "Niccaṃ daḷhaparakkamo;"
msgstr ""

#. <br>
msgctxt "sn7.7:4.5"
msgid "Pappoti paramaṃ suddhiṃ,"
msgstr ""

#. <br>
msgctxt "sn7.7:4.6"
msgid "Evaṃ jānāhi brāhmaṇā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts2ed" id="pts2ed1.356"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn7.7:5.1"
msgid "Evaṃ vutte, suddhikabhāradvājo brāhmaṇo bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn7.7:5.2"
msgid "“abhikkantaṃ, bho gotama … pe …"
msgstr ""

msgctxt "sn7.7:5.3"
msgid "aññataro ca panāyasmā bhāradvājo arahataṃ ahosīti."
msgstr ""
