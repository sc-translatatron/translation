#. <section class="sutta" id="sn14.34"><article><div class="hgroup"><p class="division">
msgctxt "sn14.34:0.1"
msgid "Saṃyutta Nikāya 14"
msgstr ""

#. </p><p>
msgctxt "sn14.34:0.2"
msgid "4. Catutthavagga"
msgstr ""

# BB has suffering for dukkha here. But note that the phrase ekantadukkha usually, probably always in fact, refers to painful feelings.
#. </p><h1>
#. VAR: 34. Ekantadukkhasutta → dukkhalaṅkaṇasuttaṃ (bj) | dukkha (pts1-2)
msgctxt "sn14.34:0.3"
msgid "34. Ekantadukkhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn14.34:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

#. VAR: Pathavīdhātu ce → paṭhavīdhātu ca (bj) | paṭhavīdhātuñca (s1-3)
msgctxt "sn14.34:1.2"
msgid ""
"“Pathavīdhātu ce hidaṃ, bhikkhave, ekantadukkhā abhavissa dukkhānupatitā "
"dukkhāvakkantā anavakkantā sukhena, nayidaṃ sattā pathavīdhātuyā sārajjeyyuṃ."
msgstr ""

msgctxt "sn14.34:1.3"
msgid ""
"Yasmā ca kho, bhikkhave, pathavīdhātu sukhā sukhānupatitā sukhāvakkantā "
"anavakkantā dukkhena, tasmā sattā pathavīdhātuyā sārajjanti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.174"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn14.34:2.1"
msgid "Āpodhātu ce hidaṃ, bhikkhave … pe …"
msgstr ""

msgctxt "sn14.34:2.2"
msgid "tejodhātu ce hidaṃ, bhikkhave …"
msgstr ""

msgctxt "sn14.34:2.3"
msgid ""
"vāyodhātu ce hidaṃ, bhikkhave, ekantadukkhā abhavissa dukkhānupatitā "
"dukkhāvakkantā anavakkantā sukhena, nayidaṃ sattā vāyodhātuyā sārajjeyyuṃ."
msgstr ""

msgctxt "sn14.34:2.4"
msgid ""
"Yasmā ca kho, bhikkhave, vāyodhātu sukhā sukhānupatitā sukhāvakkantā "
"anavakkantā dukkhena, tasmā sattā vāyodhātuyā sārajjanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn14.34:3.1"
msgid ""
"Pathavīdhātu ce hidaṃ, bhikkhave, ekantasukhā abhavissa sukhānupatitā "
"sukhāvakkantā anavakkantā dukkhena, nayidaṃ sattā pathavīdhātuyā "
"nibbindeyyuṃ."
msgstr ""

msgctxt "sn14.34:3.2"
msgid ""
"Yasmā ca kho, bhikkhave, pathavīdhātu dukkhā dukkhānupatitā dukkhāvakkantā "
"anavakkantā sukhena, tasmā sattā pathavīdhātuyā nibbindanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn14.34:4.1"
msgid "Āpodhātu ce hidaṃ, bhikkhave … pe …"
msgstr ""

msgctxt "sn14.34:4.2"
msgid "tejodhātu ce hidaṃ, bhikkhave …"
msgstr ""

msgctxt "sn14.34:4.3"
msgid ""
"vāyodhātu ce hidaṃ, bhikkhave, ekantasukhā abhavissa sukhānupatitā "
"sukhāvakkantā anavakkantā dukkhena, nayidaṃ sattā vāyodhātuyā nibbindeyyuṃ."
msgstr ""

msgctxt "sn14.34:4.4"
msgid ""
"Yasmā ca kho, bhikkhave, vāyodhātu dukkhā dukkhānupatitā dukkhāvakkantā "
"anavakkantā sukhena, tasmā sattā vāyodhātuyā nibbindantī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn14.34:4.5"
msgid "Pañcamaṃ."
msgstr ""
