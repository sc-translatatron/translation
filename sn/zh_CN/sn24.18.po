#. <section class="sutta" id="sn24.18"><article><div class="hgroup"><p class="division">
msgctxt "sn24.18:0.1"
msgid "Saṃyutta Nikāya 24"
msgstr ""

#. </p><p>
msgctxt "sn24.18:0.2"
msgid "1. Sotāpattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn24.18:0.3"
msgid "18. Nevahotinanahotitathāgatosutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn24.18:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn24.18:1.2"
msgid ""
"“Kismiṃ nu kho, bhikkhave, sati, kiṃ upādāya, kiṃ abhinivissa evaṃ diṭṭhi "
"uppajjati:"
msgstr ""

msgctxt "sn24.18:1.3"
msgid "‘neva hoti, na na hoti tathāgato paraṃ maraṇā’”ti?"
msgstr ""

msgctxt "sn24.18:1.4"
msgid "“Bhagavaṃmūlakā no, bhante, dhammā … pe …"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn24.18:2.1"
msgid ""
"“Rūpe kho, bhikkhave, sati, rūpaṃ upādāya, rūpaṃ abhinivissa evaṃ diṭṭhi "
"uppajjati:"
msgstr ""

msgctxt "sn24.18:2.2"
msgid "‘neva hoti, na na hoti tathāgato paraṃ maraṇā’”ti … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn24.18:3.1"
msgid "“Taṃ kiṃ maññatha, bhikkhave, "
msgstr ""

msgctxt "sn24.18:3.2"
msgid "rūpaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn24.18:3.3"
msgid "“Aniccaṃ, bhante” … pe …"
msgstr ""

msgctxt "sn24.18:3.4"
msgid "vipariṇāmadhammaṃ, api nu taṃ anupādāya evaṃ diṭṭhi uppajjeyya:"
msgstr ""

msgctxt "sn24.18:3.5"
msgid "‘neva hoti, na na hoti tathāgato paraṃ maraṇā’”ti?"
msgstr ""

msgctxt "sn24.18:3.6"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn24.18:3.7"
msgid ""
"“Yampidaṃ diṭṭhaṃ sutaṃ mutaṃ viññātaṃ pattaṃ pariyesitaṃ anuvicaritaṃ "
"manasā tampi niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn24.18:3.8"
msgid "“Aniccaṃ, bhante”."
msgstr ""

msgctxt "sn24.18:3.9"
msgid "“Yaṃ panāniccaṃ dukkhaṃ vā taṃ sukhaṃ vā”ti?"
msgstr ""

msgctxt "sn24.18:3.10"
msgid "“Dukkhaṃ, bhante”."
msgstr ""

msgctxt "sn24.18:3.11"
msgid ""
"“Yaṃ panāniccaṃ dukkhaṃ vipariṇāmadhammaṃ, api nu taṃ anupādāya evaṃ diṭṭhi "
"uppajjeyya:"
msgstr ""

msgctxt "sn24.18:3.12"
msgid "‘neva hoti, na na hoti tathāgato paraṃ maraṇā’”ti?"
msgstr ""

msgctxt "sn24.18:3.13"
msgid "“No hetaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.217"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn24.18:4.1"
msgid ""
"“Yato kho, bhikkhave, ariyasāvakassa imesu ca ṭhānesu kaṅkhā pahīnā hoti, "
"dukkhepissa kaṅkhā pahīnā hoti, dukkhasamudayepissa kaṅkhā pahīnā hoti, "
"dukkhanirodhepissa kaṅkhā pahīnā hoti, dukkhanirodhagāminiyā paṭipadāyapissa "
"kaṅkhā pahīnā hoti—"
msgstr ""

msgctxt "sn24.18:4.2"
msgid ""
"ayaṃ vuccati, bhikkhave, ariyasāvako sotāpanno avinipātadhammo niyato "
"sambodhiparāyano”ti."
msgstr ""

msgctxt "sn24.18:4.3"
msgid "Aṭṭhārasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn24.18:4.4"
msgid "Sotāpattivaggo."
msgstr ""

#. <br>
msgctxt "sn24.18:4.5"
msgid "Aṭṭhārasaveyyākaraṇaṃ niṭṭhitaṃ."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn24.18:5.1"
msgid "Vātaṃ etaṃ mama,"
msgstr ""

#. <br>
msgctxt "sn24.18:5.2"
msgid "so attā no ca me siyā;"
msgstr ""

#. <br>
msgctxt "sn24.18:5.3"
msgid "Natthi karoto hetu ca,"
msgstr ""

#. <br>
msgctxt "sn24.18:5.4"
msgid "mahādiṭṭhena aṭṭhamaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn24.18:6.1"
msgid "Sassato loko ca,"
msgstr ""

#. <br>
msgctxt "sn24.18:6.2"
msgid "Asassato ca antavā ca;"
msgstr ""

#. <br>
msgctxt "sn24.18:6.3"
msgid "Anantavā ca taṃ jīvaṃ taṃ sarīranti,"
msgstr ""

#. <br>
msgctxt "sn24.18:6.4"
msgid "Aññaṃ jīvaṃ aññaṃ sarīranti ca."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn24.18:7.1"
msgid "Hoti tathāgato paraṃ maraṇāti,"
msgstr ""

#. <br>
msgctxt "sn24.18:7.2"
msgid "Na hoti tathāgato paraṃ maraṇāti;"
msgstr ""

#. <br>
msgctxt "sn24.18:7.3"
msgid "Neva hoti na na hoti tathāgato paraṃ maraṇāti."
msgstr ""
