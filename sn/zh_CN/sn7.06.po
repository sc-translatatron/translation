#. <section class="sutta" id="sn7.6"><article><div class="hgroup"><p class="division">
msgctxt "sn7.6:0.1"
msgid "Saṃyutta Nikāya 7"
msgstr ""

#. </p><p>
msgctxt "sn7.6:0.2"
msgid "1. Arahantavagga"
msgstr ""

#. </p><h1>
msgctxt "sn7.6:0.3"
msgid "6. Jaṭāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn7.6:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn7.6:1.2"
msgid ""
"Atha kho jaṭābhāradvājo brāhmaṇo yena bhagavā tenupasaṅkami; upasaṅkamitvā "
"bhagavatā saddhiṃ sammodi."
msgstr ""

msgctxt "sn7.6:1.3"
msgid "Sammodanīyaṃ kathaṃ sāraṇīyaṃ vītisāretvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "sn7.6:1.4"
msgid ""
"Ekamantaṃ nisinno kho jaṭābhāradvājo brāhmaṇo bhagavantaṃ gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn7.6:2.1"
msgid "“Antojaṭā bahijaṭā,"
msgstr ""

#. <br>
msgctxt "sn7.6:2.2"
msgid "Jaṭāya jaṭitā pajā;"
msgstr ""

#. <br>
msgctxt "sn7.6:2.3"
msgid "Taṃ taṃ gotama pucchāmi,"
msgstr ""

#. <br>
msgctxt "sn7.6:2.4"
msgid "Ko imaṃ vijaṭaye jaṭan”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.354"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn7.6:3.1"
msgid "“Sīle patiṭṭhāya naro sapañño,"
msgstr ""

#. <br>
msgctxt "sn7.6:3.2"
msgid "Cittaṃ paññañca bhāvayaṃ;"
msgstr ""

#. <br>
msgctxt "sn7.6:3.3"
msgid "Ātāpī nipako bhikkhu,"
msgstr ""

#. <br>
msgctxt "sn7.6:3.4"
msgid "So imaṃ vijaṭaye jaṭaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn7.6:4.1"
msgid "Yesaṃ rāgo ca doso ca,"
msgstr ""

#. <br>
msgctxt "sn7.6:4.2"
msgid "avijjā ca virājitā;"
msgstr ""

#. <br>
msgctxt "sn7.6:4.3"
msgid "Khīṇāsavā arahanto,"
msgstr ""

#. <br>
msgctxt "sn7.6:4.4"
msgid "tesaṃ vijaṭitā jaṭā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn7.6:5.1"
msgid "Yattha nāmañca rūpañca,"
msgstr ""

#. <br>
msgctxt "sn7.6:5.2"
msgid "asesaṃ uparujjhati;"
msgstr ""

#. <br>
msgctxt "sn7.6:5.3"
msgid "Paṭighaṃ rūpasaññā ca,"
msgstr ""

#. <br>
msgctxt "sn7.6:5.4"
msgid "etthesā chijjate jaṭā”ti."
msgstr ""

# Note “brahmin” is omitted here.
#. </p></blockquote><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn7.6:6.1"
msgid "Evaṃ vutte, jaṭābhāradvājo bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn7.6:6.2"
msgid "“abhikkantaṃ, bho gotama … pe …"
msgstr ""

msgctxt "sn7.6:6.3"
msgid "aññataro ca panāyasmā bhāradvājo arahataṃ ahosī”ti."
msgstr ""
