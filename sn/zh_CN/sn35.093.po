#. <section class="sutta" id="sn35.93"><article><div class="hgroup"><p class="division">
msgctxt "sn35.93:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.93:0.2"
msgid "9. Channavagga"
msgstr ""

#. </p><h1>
msgctxt "sn35.93:0.3"
msgid "93. Dutiyadvayasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.68"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.93:1.1"
msgid "“Dvayaṃ, bhikkhave, paṭicca viññāṇaṃ sambhoti."
msgstr ""

msgctxt "sn35.93:1.2"
msgid "Kathañca, bhikkhave, dvayaṃ paṭicca viññāṇaṃ sambhoti?"
msgstr ""

msgctxt "sn35.93:1.3"
msgid "Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇaṃ."
msgstr ""

#. VAR: Cakkhu → cakkhuṃ (bj, s1-3)
msgctxt "sn35.93:1.4"
msgid "Cakkhu aniccaṃ vipariṇāmi aññathābhāvi."
msgstr ""

msgctxt "sn35.93:1.5"
msgid "Rūpā aniccā vipariṇāmino aññathābhāvino."
msgstr ""

msgctxt "sn35.93:1.6"
msgid "Itthetaṃ dvayaṃ calañceva byathañca aniccaṃ vipariṇāmi aññathābhāvi."
msgstr ""

msgctxt "sn35.93:1.7"
msgid "Cakkhuviññāṇaṃ aniccaṃ vipariṇāmi aññathābhāvi."
msgstr ""

msgctxt "sn35.93:1.8"
msgid ""
"Yopi hetu yopi paccayo cakkhuviññāṇassa uppādāya, sopi hetu sopi paccayo "
"anicco vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:1.9"
msgid ""
"Aniccaṃ kho pana, bhikkhave, paccayaṃ paṭicca uppannaṃ cakkhuviññāṇaṃ kuto "
"niccaṃ bhavissati."
msgstr ""

# Compare the subtly different usage at MN 28
msgctxt "sn35.93:1.10"
msgid ""
"Yā kho, bhikkhave, imesaṃ tiṇṇaṃ dhammānaṃ saṅgati sannipāto samavāyo, ayaṃ "
"vuccati cakkhusamphasso."
msgstr ""

msgctxt "sn35.93:1.11"
msgid "Cakkhusamphassopi anicco vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:1.12"
msgid ""
"Yopi hetu yopi paccayo cakkhusamphassassa uppādāya, sopi hetu sopi paccayo "
"anicco vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:1.13"
msgid ""
"Aniccaṃ kho pana, bhikkhave, paccayaṃ paṭicca uppanno cakkhusamphasso kuto "
"nicco bhavissati."
msgstr ""

msgctxt "sn35.93:1.14"
msgid "Phuṭṭho, bhikkhave, vedeti, phuṭṭho ceteti, phuṭṭho sañjānāti."
msgstr ""

msgctxt "sn35.93:1.15"
msgid ""
"Itthetepi dhammā calā ceva byathā ca aniccā vipariṇāmino aññathābhāvino … pe "
"…"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.69"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.93:2.1"
msgid "Jivhañca paṭicca rase ca uppajjati jivhāviññāṇaṃ."
msgstr ""

#. VAR: vipariṇāmī aññathābhāvī → vipariṇāminī aññathābhāvinī (?)
msgctxt "sn35.93:2.2"
msgid "Jivhā aniccā vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:2.3"
msgid "Rasā aniccā vipariṇāmino aññathābhāvino."
msgstr ""

msgctxt "sn35.93:2.4"
msgid "Itthetaṃ dvayaṃ calañceva byathañca aniccaṃ vipariṇāmi aññathābhāvi."
msgstr ""

msgctxt "sn35.93:2.5"
msgid "Jivhāviññāṇaṃ aniccaṃ vipariṇāmi aññathābhāvi."
msgstr ""

msgctxt "sn35.93:2.6"
msgid ""
"Yopi hetu yopi paccayo jivhāviññāṇassa uppādāya, sopi hetu sopi paccayo "
"anicco vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:2.7"
msgid ""
"Aniccaṃ kho pana, bhikkhave, paccayaṃ paṭicca uppannaṃ jivhāviññāṇaṃ, kuto "
"niccaṃ bhavissati."
msgstr ""

msgctxt "sn35.93:2.8"
msgid ""
"Yā kho, bhikkhave, imesaṃ tiṇṇaṃ dhammānaṃ saṅgati sannipāto samavāyo, ayaṃ "
"vuccati jivhāsamphasso."
msgstr ""

msgctxt "sn35.93:2.9"
msgid "Jivhāsamphassopi anicco vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:2.10"
msgid ""
"Yopi hetu yopi paccayo jivhāsamphassassa uppādāya, sopi hetu sopi paccayo "
"anicco vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:2.11"
msgid ""
"Aniccaṃ kho pana, bhikkhave, paccayaṃ paṭicca uppanno jivhāsamphasso, kuto "
"nicco bhavissati."
msgstr ""

msgctxt "sn35.93:2.12"
msgid "Phuṭṭho, bhikkhave, vedeti, phuṭṭho ceteti, phuṭṭho sañjānāti."
msgstr ""

msgctxt "sn35.93:2.13"
msgid ""
"Itthetepi dhammā calā ceva byathā ca aniccā vipariṇāmino aññathābhāvino … pe "
"…"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn35.93:3.1"
msgid "Manañca paṭicca dhamme ca uppajjati manoviññāṇaṃ."
msgstr ""

msgctxt "sn35.93:3.2"
msgid "Mano anicco vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:3.3"
msgid "Dhammā aniccā vipariṇāmino aññathābhāvino."
msgstr ""

msgctxt "sn35.93:3.4"
msgid "Itthetaṃ dvayaṃ calañceva byathañca aniccaṃ vipariṇāmi aññathābhāvi."
msgstr ""

msgctxt "sn35.93:3.5"
msgid "Manoviññāṇaṃ aniccaṃ vipariṇāmi aññathābhāvi."
msgstr ""

msgctxt "sn35.93:3.6"
msgid ""
"Yopi hetu yopi paccayo manoviññāṇassa uppādāya, sopi hetu sopi paccayo "
"anicco vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:3.7"
msgid ""
"Aniccaṃ kho pana, bhikkhave, paccayaṃ paṭicca uppannaṃ manoviññāṇaṃ, kuto "
"niccaṃ bhavissati."
msgstr ""

msgctxt "sn35.93:3.8"
msgid ""
"Yā kho, bhikkhave, imesaṃ tiṇṇaṃ dhammānaṃ saṅgati sannipāto samavāyo, ayaṃ "
"vuccati manosamphasso."
msgstr ""

msgctxt "sn35.93:3.9"
msgid "Manosamphassopi anicco vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:3.10"
msgid ""
"Yopi hetu yopi paccayo manosamphassassa uppādāya, sopi hetu sopi paccayo "
"anicco vipariṇāmī aññathābhāvī."
msgstr ""

msgctxt "sn35.93:3.11"
msgid ""
"Aniccaṃ kho pana, bhikkhave, paccayaṃ paṭicca uppanno manosamphasso, kuto "
"nicco bhavissati."
msgstr ""

msgctxt "sn35.93:3.12"
msgid "Phuṭṭho, bhikkhave, vedeti, phuṭṭho ceteti, phuṭṭho sañjānāti."
msgstr ""

msgctxt "sn35.93:3.13"
msgid ""
"Itthetepi dhammā calā ceva byathā ca aniccā vipariṇāmino aññathābhāvino."
msgstr ""

msgctxt "sn35.93:3.14"
msgid "Evaṃ kho, bhikkhave, dvayaṃ paṭicca viññāṇaṃ sambhotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.93:3.15"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.93:3.16"
msgid "Channavaggo catuttho."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn35.93:4.1"
msgid "Palokasuññā saṅkhittaṃ,"
msgstr ""

#. <br>
msgctxt "sn35.93:4.2"
msgid "channo puṇṇo ca bāhiyo;"
msgstr ""

#. <br>
msgctxt "sn35.93:4.3"
msgid "Ejena ca duve vuttā,"
msgstr ""

#. <br>
msgctxt "sn35.93:4.4"
msgid "dvayehi apare duveti."
msgstr ""
