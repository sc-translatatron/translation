#. <section class="sutta" id="sn50.1-12"><article><div class="hgroup"><p class="division">
msgctxt "sn50.1-12:0.1"
msgid "Saṃyutta Nikāya 50"
msgstr ""

#. </p><p>
msgctxt "sn50.1-12:0.2"
msgid "1. Gaṅgāpeyyālavagga"
msgstr ""

#. </p><h1>
#. VAR: 1–​12. Balādisutta → pācīnaninnādisuttāni (bj)
msgctxt "sn50.1-12:0.3"
msgid "1–​12. Balādisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.249"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn50.1-12:1.1"
msgid "“Pañcimāni, bhikkhave, balāni."
msgstr ""

msgctxt "sn50.1-12:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "sn50.1-12:1.3"
msgid "Saddhābalaṃ, vīriyabalaṃ, satibalaṃ, samādhibalaṃ, paññābalaṃ—"
msgstr ""

msgctxt "sn50.1-12:1.4"
msgid "imāni kho, bhikkhave, pañca balānīti."
msgstr ""

msgctxt "sn50.1-12:1.5"
msgid ""
"Seyyathāpi, bhikkhave, gaṅgā nadī pācīnaninnā pācīnapoṇā pācīnapabbhārā;"
msgstr ""

msgctxt "sn50.1-12:1.6"
msgid ""
"evameva kho, bhikkhave, bhikkhu pañca balāni bhāvento pañca balāni "
"bahulīkaronto nibbānaninno hoti nibbānapoṇo nibbānapabbhāro."
msgstr ""

msgctxt "sn50.1-12:1.7"
msgid ""
"Kathañca, bhikkhave, bhikkhu pañca balāni bhāvento pañca balāni "
"bahulīkaronto nibbānaninno hoti nibbānapoṇo nibbānapabbhāro?"
msgstr ""

msgctxt "sn50.1-12:1.8"
msgid ""
"Idha, bhikkhave, bhikkhu saddhābalaṃ bhāveti vivekanissitaṃ virāganissitaṃ "
"nirodhanissitaṃ vossaggapariṇāmiṃ, "
msgstr ""

msgctxt "sn50.1-12:1.9"
msgid "vīriyabalaṃ … pe …"
msgstr ""

msgctxt "sn50.1-12:1.10"
msgid "satibalaṃ …"
msgstr ""

msgctxt "sn50.1-12:1.11"
msgid "samādhibalaṃ …"
msgstr ""

msgctxt "sn50.1-12:1.12"
msgid ""
"paññābalaṃ bhāveti vivekanissitaṃ virāganissitaṃ nirodhanissitaṃ "
"vossaggapariṇāmiṃ."
msgstr ""

msgctxt "sn50.1-12:1.13"
msgid ""
"Evaṃ kho, bhikkhave, bhikkhu pañca balāni bhāvento pañca balāni "
"bahulīkaronto nibbānaninno hoti nibbānapoṇo nibbānapabbhāro”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn50.1-12:1.14"
msgid "Dvādasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn50.1-12:1.15"
msgid "Gaṅgāpeyyālavaggo paṭhamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.250"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn50.1-12:2.1"
msgid "Cha pācīnato ninnā,"
msgstr ""

#. <br>
msgctxt "sn50.1-12:2.2"
msgid "cha ninnā ca samuddato;"
msgstr ""

#. <br>
msgctxt "sn50.1-12:2.3"
msgid "Dvete cha dvādasa honti,"
msgstr ""

#. <br>
msgctxt "sn50.1-12:2.4"
msgid "vaggo tena pavuccatīti."
msgstr ""
