#. <section class="sutta" id="sn35.98"><article><div class="hgroup"><p class="division">
msgctxt "sn35.98:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.98:0.2"
msgid "10. Saḷavagga"
msgstr ""

#. </p><h1>
msgctxt "sn35.98:0.3"
msgid "98. Saṃvarasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.98:1.1"
msgid "“Saṃvarañca vo, bhikkhave, desessāmi, asaṃvarañca."
msgstr ""

msgctxt "sn35.98:1.2"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn35.98:1.3"
msgid "Kathañca, bhikkhave, asaṃvaro hoti?"
msgstr ""

msgctxt "sn35.98:1.4"
msgid ""
"Santi, bhikkhave, cakkhuviññeyyā rūpā iṭṭhā kantā manāpā piyarūpā "
"kāmūpasaṃhitā rajanīyā."
msgstr ""

msgctxt "sn35.98:1.5"
msgid ""
"Tañce bhikkhu abhinandati abhivadati ajjhosāya tiṭṭhati, veditabbametaṃ, "
"bhikkhave, bhikkhunā:"
msgstr ""

msgctxt "sn35.98:1.6"
msgid "‘parihāyāmi kusalehi dhammehi."
msgstr ""

msgctxt "sn35.98:1.7"
msgid "Parihānañhetaṃ vuttaṃ bhagavatā’ti … pe …"
msgstr ""

msgctxt "sn35.98:1.8"
msgid "santi, bhikkhave, jivhāviññeyyā rasā … pe …"
msgstr ""

msgctxt "sn35.98:1.9"
msgid ""
"santi, bhikkhave, manoviññeyyā dhammā iṭṭhā kantā manāpā piyarūpā "
"kāmūpasaṃhitā rajanīyā."
msgstr ""

msgctxt "sn35.98:1.10"
msgid ""
"Tañce bhikkhu abhinandati abhivadati ajjhosāya tiṭṭhati, veditabbametaṃ, "
"bhikkhave, bhikkhunā:"
msgstr ""

msgctxt "sn35.98:1.11"
msgid "‘parihāyāmi kusalehi dhammehi."
msgstr ""

msgctxt "sn35.98:1.12"
msgid "Parihānañhetaṃ vuttaṃ bhagavatā’ti."
msgstr ""

msgctxt "sn35.98:1.13"
msgid "Evaṃ kho, bhikkhave, asaṃvaro hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.80"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.98:2.1"
msgid "Kathañca, bhikkhave, saṃvaro hoti?"
msgstr ""

msgctxt "sn35.98:2.2"
msgid ""
"Santi, bhikkhave, cakkhuviññeyyā rūpā iṭṭhā kantā manāpā piyarūpā "
"kāmūpasaṃhitā rajanīyā."
msgstr ""

msgctxt "sn35.98:2.3"
msgid ""
"Tañce bhikkhu nābhinandati nābhivadati nājjhosāya tiṭṭhati, veditabbametaṃ, "
"bhikkhave, bhikkhunā:"
msgstr ""

msgctxt "sn35.98:2.4"
msgid "‘na parihāyāmi kusalehi dhammehi."
msgstr ""

msgctxt "sn35.98:2.5"
msgid "Aparihānañhetaṃ vuttaṃ bhagavatā’ti … pe …"
msgstr ""

msgctxt "sn35.98:2.6"
msgid "santi, bhikkhave, jivhāviññeyyā rasā … pe …"
msgstr ""

msgctxt "sn35.98:2.7"
msgid ""
"santi, bhikkhave, manoviññeyyā dhammā iṭṭhā kantā manāpā piyarūpā "
"kāmūpasaṃhitā rajanīyā."
msgstr ""

msgctxt "sn35.98:2.8"
msgid ""
"Tañce bhikkhu nābhinandati nābhivadati nājjhosāya tiṭṭhati, veditabbametaṃ "
"bhikkhunā:"
msgstr ""

msgctxt "sn35.98:2.9"
msgid "‘na parihāyāmi kusalehi dhammehi."
msgstr ""

msgctxt "sn35.98:2.10"
msgid "Aparihānañhetaṃ vuttaṃ bhagavatā’ti."
msgstr ""

msgctxt "sn35.98:2.11"
msgid "Evaṃ kho, bhikkhave, saṃvaro hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.98:2.12"
msgid "Pañcamaṃ."
msgstr ""
