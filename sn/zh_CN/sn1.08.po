#. <section class="sutta" id="sn1.8"><article><div class="hgroup"><p class="division">
msgctxt "sn1.8:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.8:0.2"
msgid "1. Naḷavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.8:0.3"
msgid "8. Susammuṭṭhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.8:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn1.8:1.2"
msgid "Ekamantaṃ ṭhitā kho sā devatā bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.8"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.8:2.1"
msgid "“Yesaṃ dhammā susammuṭṭhā,"
msgstr ""

#. <br>
msgctxt "sn1.8:2.2"
msgid "paravādesu nīyare;"
msgstr ""

#. <br>
msgctxt "sn1.8:2.3"
msgid "Suttā te nappabujjhanti,"
msgstr ""

#. <br>
msgctxt "sn1.8:2.4"
msgid "kālo tesaṃ pabujjhitun”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.8:3.1"
msgid "“Yesaṃ dhammā asammuṭṭhā,"
msgstr ""

#. <br>
msgctxt "sn1.8:3.2"
msgid "paravādesu na nīyare;"
msgstr ""

#. <br>
msgctxt "sn1.8:3.3"
msgid "Te sambuddhā sammadaññā,"
msgstr ""

#. <br>
msgctxt "sn1.8:3.4"
msgid "caranti visame saman”ti."
msgstr ""
