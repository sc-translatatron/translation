#. <section class="sutta" id="sn35.86"><article><div class="hgroup"><p class="division">
msgctxt "sn35.86:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.86:0.2"
msgid "9. Channavagga"
msgstr ""

#. </p><h1>
#. VAR: 86. Saṅkhittadhammasutta → saṅkhitta (pts1)
msgctxt "sn35.86:0.3"
msgid "86. Saṅkhittadhammasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.86:1.1"
msgid "Ekamantaṃ nisinno kho āyasmā ānando bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn35.86:1.2"
msgid ""
"“sādhu me, bhante, bhagavā saṃkhittena dhammaṃ desetu, yamahaṃ bhagavato "
"dhammaṃ sutvā eko vūpakaṭṭho appamatto ātāpī pahitatto vihareyyan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.86:2.1"
msgid "“Taṃ kiṃ maññasi, ānanda, "
msgstr ""

msgctxt "sn35.86:2.2"
msgid "cakkhu niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn35.86:3.1"
msgid "“Aniccaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn35.86:4.1"
msgid "“Yaṃ panāniccaṃ dukkhaṃ vā taṃ sukhaṃ vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn35.86:5.1"
msgid "“Dukkhaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn35.86:6.1"
msgid ""
"“Yaṃ panāniccaṃ dukkhaṃ vipariṇāmadhammaṃ, kallaṃ nu taṃ samanupassituṃ:"
msgstr ""

msgctxt "sn35.86:6.2"
msgid "‘etaṃ mama, esohamasmi, eso me attā’”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn35.86:7.1"
msgid "“No hetaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn35.86:8.1"
msgid "“Rūpā niccā vā aniccā vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "sn35.86:9.1"
msgid "“Aniccā, bhante” … pe …."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.55"></a>
#. <a class="sc" id="sc10"></a>
msgctxt "sn35.86:10.1"
msgid "“Cakkhuviññāṇaṃ … pe …"
msgstr ""

msgctxt "sn35.86:10.2"
msgid ""
"yampidaṃ cakkhusamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn35.86:11.1"
msgid "“Aniccaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn35.86:12.1"
msgid "“Yaṃ panāniccaṃ dukkhaṃ vā taṃ sukhaṃ vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc13"></a>
msgctxt "sn35.86:13.1"
msgid "“Dukkhaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc14"></a>
msgctxt "sn35.86:14.1"
msgid ""
"“Yaṃ panāniccaṃ dukkhaṃ vipariṇāmadhammaṃ, kallaṃ nu taṃ samanupassituṃ:"
msgstr ""

msgctxt "sn35.86:14.2"
msgid "‘etaṃ mama, esohamasmi, eso me attā’”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc15"></a>
msgctxt "sn35.86:15.1"
msgid "“No hetaṃ, bhante” … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc16"></a>
msgctxt "sn35.86:16.1"
msgid "“Jivhā niccā vā aniccā vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc17"></a>
msgctxt "sn35.86:17.1"
msgid "“Aniccā, bhante” … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc18"></a>
msgctxt "sn35.86:18.1"
msgid "“Jivhāviññāṇaṃ …"
msgstr ""

msgctxt "sn35.86:18.2"
msgid "jivhāsamphasso … pe …"
msgstr ""

msgctxt "sn35.86:18.3"
msgid ""
"yampidaṃ manosamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tampi niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc19"></a>
msgctxt "sn35.86:19.1"
msgid "“Aniccaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc20"></a>
msgctxt "sn35.86:20.1"
msgid "“Yaṃ panāniccaṃ dukkhaṃ vā taṃ sukhaṃ vā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc21"></a>
msgctxt "sn35.86:21.1"
msgid "“Dukkhaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc22"></a>
msgctxt "sn35.86:22.1"
msgid ""
"“Yaṃ panāniccaṃ dukkhaṃ vipariṇāmadhammaṃ, kallaṃ nu taṃ samanupassituṃ:"
msgstr ""

msgctxt "sn35.86:22.2"
msgid "‘etaṃ mama, esohamasmi, eso me attā’”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc23"></a>
msgctxt "sn35.86:23.1"
msgid "“No hetaṃ, bhante”."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc24"></a>
msgctxt "sn35.86:24.1"
msgid ""
"“Evaṃ passaṃ, ānanda, sutavā ariyasāvako cakkhusmimpi nibbindati … pe … "
"cakkhusamphassepi nibbindati … pe …"
msgstr ""

msgctxt "sn35.86:24.2"
msgid ""
"yampidaṃ manosamphassapaccayā uppajjati vedayitaṃ sukhaṃ vā dukkhaṃ vā "
"adukkhamasukhaṃ vā tasmimpi nibbindati."
msgstr ""

msgctxt "sn35.86:24.3"
msgid ""
"Nibbindaṃ virajjati; virāgā vimuccati; vimuttasmiṃ vimuttamiti ñāṇaṃ hoti."
msgstr ""

msgctxt "sn35.86:24.4"
msgid ""
"‘Khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyā’ti "
"pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.86:24.5"
msgid "Tatiyaṃ."
msgstr ""
