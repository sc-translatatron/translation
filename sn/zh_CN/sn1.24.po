#. <section class="sutta" id="sn1.24"><article><div class="hgroup"><p class="division">
msgctxt "sn1.24:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.24:0.2"
msgid "3. Sattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.24:0.3"
msgid "24. Manonivāraṇasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="pts1ed" id="pts1ed1.14"></a>
#. <a class="pts2ed" id="pts2ed1.30"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.24:1.1"
msgid "“Yato yato mano nivāraye,"
msgstr ""

#. <br>
msgctxt "sn1.24:1.2"
msgid "Na dukkhameti naṃ tato tato;"
msgstr ""

#. <br>
msgctxt "sn1.24:1.3"
msgid "Sa sabbato mano nivāraye,"
msgstr ""

#. <br>
#. VAR: pamuccatī”ti → pamuccati (pts1, c1, c2)
msgctxt "sn1.24:1.4"
msgid "Sa sabbato dukkhā pamuccatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.24:2.1"
msgid "“Na sabbato mano nivāraye,"
msgstr ""

# See BB for readings.
#. <br>
msgctxt "sn1.24:2.2"
msgid "Na mano saṃyatattamāgataṃ;"
msgstr ""

#. <br>
msgctxt "sn1.24:2.3"
msgid "Yato yato ca pāpakaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.24:2.4"
msgid "Tato tato mano nivāraye”ti."
msgstr ""
