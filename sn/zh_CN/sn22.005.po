#. <section class="sutta" id="sn22.5"><article><div class="hgroup"><p class="division">
msgctxt "sn22.5:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.5:0.2"
msgid "1. Nakulapituvagga"
msgstr ""

#. </p><h1>
#. VAR: 5. Samādhisutta → samādhibhāvanāsuttaṃ (bj)
msgctxt "sn22.5:0.3"
msgid "5. Samādhisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.14"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn22.5:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn22.5:1.2"
msgid "… pe … sāvatthiyaṃ …"
msgstr ""

msgctxt "sn22.5:1.3"
msgid "tatra kho … pe … etadavoca:"
msgstr ""

msgctxt "sn22.5:1.4"
msgid "“samādhiṃ, bhikkhave, bhāvetha;"
msgstr ""

msgctxt "sn22.5:1.5"
msgid "samāhito, bhikkhave, bhikkhu yathābhūtaṃ pajānāti."
msgstr ""

msgctxt "sn22.5:1.6"
msgid "Kiñca yathābhūtaṃ pajānāti?"
msgstr ""

msgctxt "sn22.5:1.7"
msgid ""
"Rūpassa samudayañca atthaṅgamañca, vedanāya samudayañca atthaṅgamañca, "
"saññāya samudayañca atthaṅgamañca, saṅkhārānaṃ samudayañca atthaṅgamañca, "
"viññāṇassa samudayañca atthaṅgamañca."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn22.5:2.1"
msgid ""
"Ko ca, bhikkhave, rūpassa samudayo, ko vedanāya samudayo, ko saññāya "
"samudayo, ko saṅkhārānaṃ samudayo, ko viññāṇassa samudayo?"
msgstr ""

msgctxt "sn22.5:2.2"
msgid "Idha, bhikkhave, bhikkhu abhinandati abhivadati ajjhosāya tiṭṭhati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn22.5:3.1"
msgid "Kiñca abhinandati abhivadati ajjhosāya tiṭṭhati?"
msgstr ""

msgctxt "sn22.5:3.2"
msgid "Rūpaṃ abhinandati abhivadati ajjhosāya tiṭṭhati."
msgstr ""

msgctxt "sn22.5:3.3"
msgid "Tassa rūpaṃ abhinandato abhivadato ajjhosāya tiṭṭhato uppajjati nandī."
msgstr ""

msgctxt "sn22.5:3.4"
msgid "Yā rūpe nandī tadupādānaṃ."
msgstr ""

msgctxt "sn22.5:3.5"
msgid "Tassupādānapaccayā bhavo;"
msgstr ""

msgctxt "sn22.5:3.6"
msgid "bhavapaccayā jāti;"
msgstr ""

msgctxt "sn22.5:3.7"
msgid "jātipaccayā jarāmaraṇaṃ sokaparidevadukkhadomanassupāyāsā sambhavanti."
msgstr ""

msgctxt "sn22.5:3.8"
msgid "Evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn22.5:4.1"
msgid "Vedanaṃ abhinandati … pe …"
msgstr ""

msgctxt "sn22.5:4.2"
msgid "saññaṃ abhinandati …"
msgstr ""

msgctxt "sn22.5:4.3"
msgid "saṅkhāre abhinandati …"
msgstr ""

msgctxt "sn22.5:4.4"
msgid "viññāṇaṃ abhinandati abhivadati ajjhosāya tiṭṭhati."
msgstr ""

msgctxt "sn22.5:4.5"
msgid ""
"Tassa viññāṇaṃ abhinandato abhivadato ajjhosāya tiṭṭhato uppajjati nandī."
msgstr ""

msgctxt "sn22.5:4.6"
msgid "Yā viññāṇe nandī tadupādānaṃ."
msgstr ""

msgctxt "sn22.5:4.7"
msgid "Tassupādānapaccayā bhavo;"
msgstr ""

msgctxt "sn22.5:4.8"
msgid "bhavapaccayā jāti;"
msgstr ""

msgctxt "sn22.5:4.9"
msgid "jātipaccayā … pe …"
msgstr ""

msgctxt "sn22.5:4.10"
msgid "evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn22.5:5.1"
msgid "Ayaṃ, bhikkhave, rūpassa samudayo;"
msgstr ""

msgctxt "sn22.5:5.2"
msgid "ayaṃ vedanāya samudayo;"
msgstr ""

msgctxt "sn22.5:5.3"
msgid "ayaṃ saññāya samudayo;"
msgstr ""

msgctxt "sn22.5:5.4"
msgid "ayaṃ saṅkhārānaṃ samudayo;"
msgstr ""

msgctxt "sn22.5:5.5"
msgid "ayaṃ viññāṇassa samudayo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn22.5:6.1"
msgid "Ko ca, bhikkhave, rūpassa atthaṅgamo, ko vedanāya …"
msgstr ""

msgctxt "sn22.5:6.2"
msgid "ko saññāya …"
msgstr ""

msgctxt "sn22.5:6.3"
msgid "ko saṅkhārānaṃ …"
msgstr ""

msgctxt "sn22.5:6.4"
msgid "ko viññāṇassa atthaṅgamo?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn22.5:7.1"
msgid "Idha, bhikkhave, nābhinandati nābhivadati nājjhosāya tiṭṭhati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn22.5:8.1"
msgid "Kiñca nābhinandati nābhivadati nājjhosāya tiṭṭhati?"
msgstr ""

msgctxt "sn22.5:8.2"
msgid "Rūpaṃ nābhinandati nābhivadati nājjhosāya tiṭṭhati."
msgstr ""

msgctxt "sn22.5:8.3"
msgid ""
"Tassa rūpaṃ anabhinandato anabhivadato anajjhosāya tiṭṭhato yā rūpe nandī sā "
"nirujjhati."
msgstr ""

msgctxt "sn22.5:8.4"
msgid "Tassa nandīnirodhā upādānanirodho;"
msgstr ""

msgctxt "sn22.5:8.5"
msgid "upādānanirodhā bhavanirodho … pe …"
msgstr ""

msgctxt "sn22.5:8.6"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.15"></a>
#. <a class="sc" id="sc9"></a>
msgctxt "sn22.5:9.1"
msgid "Vedanaṃ nābhinandati nābhivadati nājjhosāya tiṭṭhati."
msgstr ""

msgctxt "sn22.5:9.2"
msgid ""
"Tassa vedanaṃ anabhinandato anabhivadato anajjhosāya tiṭṭhato yā vedanāya "
"nandī sā nirujjhati."
msgstr ""

msgctxt "sn22.5:9.3"
msgid "Tassa nandīnirodhā upādānanirodho;"
msgstr ""

msgctxt "sn22.5:9.4"
msgid "upādānanirodhā bhavanirodho … pe …"
msgstr ""

msgctxt "sn22.5:9.5"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn22.5:10.1"
msgid "Saññaṃ nābhinandati … pe …"
msgstr ""

msgctxt "sn22.5:10.2"
msgid "saṅkhāre nābhinandati nābhivadati nājjhosāya tiṭṭhati."
msgstr ""

msgctxt "sn22.5:10.3"
msgid ""
"Tassa saṅkhāre anabhinandato anabhivadato anajjhosāya tiṭṭhato yā saṅkhāresu "
"nandī sā nirujjhati."
msgstr ""

msgctxt "sn22.5:10.4"
msgid "Tassa nandīnirodhā upādānanirodho;"
msgstr ""

msgctxt "sn22.5:10.5"
msgid "upādānanirodhā bhavanirodho … pe …"
msgstr ""

msgctxt "sn22.5:10.6"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn22.5:11.1"
msgid "Viññāṇaṃ nābhinandati nābhivadati nājjhosāya tiṭṭhati."
msgstr ""

msgctxt "sn22.5:11.2"
msgid ""
"Tassa viññāṇaṃ anabhinandato anabhivadato anajjhosāya tiṭṭhato yā viññāṇe "
"nandī sā nirujjhati."
msgstr ""

msgctxt "sn22.5:11.3"
msgid "Tassa nandīnirodhā upādānanirodho … pe …"
msgstr ""

msgctxt "sn22.5:11.4"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn22.5:12.1"
msgid ""
"Ayaṃ, bhikkhave, rūpassa atthaṅgamo, ayaṃ vedanāya atthaṅgamo, ayaṃ saññāya "
"atthaṅgamo, ayaṃ saṅkhārānaṃ atthaṅgamo, ayaṃ viññāṇassa atthaṅgamo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.5:12.2"
msgid "Pañcamaṃ."
msgstr ""
