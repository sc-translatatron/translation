#. <section class="sutta" id="sn22.73"><article><div class="hgroup"><p class="division">
msgctxt "sn22.73:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
#. VAR: 8. Khajjanīyavagga → khajjaniyavaggo (pts1)
msgctxt "sn22.73:0.2"
msgid "8. Khajjanīyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.73:0.3"
msgid "73. Assādasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.82"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.73:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.73:1.2"
msgid ""
"“Assutavā, bhikkhave, puthujjano rūpassa assādañca ādīnavañca nissaraṇañca "
"yathābhūtaṃ nappajānāti."
msgstr ""

msgctxt "sn22.73:1.3"
msgid "Vedanāya …"
msgstr ""

msgctxt "sn22.73:1.4"
msgid "saññāya …"
msgstr ""

msgctxt "sn22.73:1.5"
msgid "saṅkhārānaṃ …"
msgstr ""

msgctxt "sn22.73:1.6"
msgid "viññāṇassa assādañca ādīnavañca nissaraṇañca yathābhūtaṃ nappajānāti."
msgstr ""

msgctxt "sn22.73:1.7"
msgid ""
"Sutavā ca kho, bhikkhave, ariyasāvako rūpassa assādañca ādīnavañca "
"nissaraṇañca yathābhūtaṃ pajānāti."
msgstr ""

msgctxt "sn22.73:1.8"
msgid "Vedanāya …"
msgstr ""

msgctxt "sn22.73:1.9"
msgid "saññāya …"
msgstr ""

msgctxt "sn22.73:1.10"
msgid "saṅkhārānaṃ …"
msgstr ""

msgctxt "sn22.73:1.11"
msgid "viññāṇassa assādañca ādīnavañca nissaraṇañca yathābhūtaṃ pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.73:1.12"
msgid "Paṭhamaṃ."
msgstr ""
