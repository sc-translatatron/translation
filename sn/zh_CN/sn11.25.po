#. <section class="sutta" id="sn11.25"><article><div class="hgroup"><p class="division">
msgctxt "sn11.25:0.1"
msgid "Saṃyutta Nikāya 11"
msgstr ""

#. </p><p>
msgctxt "sn11.25:0.2"
msgid "3. Tatiyavagga"
msgstr ""

#. </p><h1>
#. VAR: 25. Akkodhasutta → akodho (avihiṃsā) (pts1)
msgctxt "sn11.25:0.3"
msgid "25. Akkodhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn11.25:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn11.25:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "sn11.25:1.3"
msgid "Tatra kho bhagavā bhikkhū … pe … bhagavā etadavoca:"
msgstr ""

msgctxt "sn11.25:1.4"
msgid ""
"“bhūtapubbaṃ, bhikkhave, sakko devānamindo sudhammāyaṃ sabhāyaṃ deve "
"tāvatiṃse anunayamāno tāyaṃ velāyaṃ imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.515"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn11.25:2.1"
msgid "‘Mā vo kodho ajjhabhavi,"
msgstr ""

#. <br>
msgctxt "sn11.25:2.2"
msgid "mā ca kujjhittha kujjhataṃ;"
msgstr ""

#. <br>
msgctxt "sn11.25:2.3"
msgid "Akkodho avihiṃsā ca,"
msgstr ""

#. <br>
#. VAR: ca paṭipadā → vasati sadā (bj, pts1) | vasatī sadā (s1-3, km, pts2)
msgctxt "sn11.25:2.4"
msgid "ariyesu ca paṭipadā;"
msgstr ""

#. <br>
msgctxt "sn11.25:2.5"
msgid "Atha pāpajanaṃ kodho,"
msgstr ""

#. <br>
msgctxt "sn11.25:2.6"
msgid "pabbatovābhimaddatī’”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn11.25:2.7"
msgid "Tatiyo vaggo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn11.25:3.1"
msgid "Chetvā dubbaṇṇiya māyā,"
msgstr ""

#. <br>
msgctxt "sn11.25:3.2"
msgid "accayena akodhano;"
msgstr ""

#. <br>
msgctxt "sn11.25:3.3"
msgid "Desitaṃ buddhaseṭṭhena,"
msgstr ""

#. <br>
msgctxt "sn11.25:3.4"
msgid "idañhi sakkapañcakanti."
msgstr ""

#. </p></blockquote><p class="endsutta">
msgctxt "sn11.25:3.5"
msgid "Sakkasaṃyuttaṃ samattaṃ."
msgstr ""

#. </p><p class="endsutta">
msgctxt "sn11.25:3.6"
msgid "Sagāthāvaggo paṭhamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="pts2ed" id="pts2ed1.516"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn11.25:4.1"
msgid "Devatā devaputto ca,"
msgstr ""

#. <br>
msgctxt "sn11.25:4.2"
msgid "rājā māro ca bhikkhunī;"
msgstr ""

#. <br>
msgctxt "sn11.25:4.3"
msgid "Brahmā brāhmaṇa vaṅgīso,"
msgstr ""

#. <br>
msgctxt "sn11.25:4.4"
msgid "vanayakkhena vāsavoti."
msgstr ""

#. </p></blockquote><p class="endbook">
msgctxt "sn11.25:4.5"
msgid "Sagāthāvaggasaṃyuttapāḷi niṭṭhitā."
msgstr ""
