#. <section class="sutta" id="sn43.44"><article><div class="hgroup"><p class="division">
msgctxt "sn43.44:0.1"
msgid "Saṃyutta Nikāya 43"
msgstr ""

#. </p><p>
msgctxt "sn43.44:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn43.44:0.3"
msgid "44. Parāyanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.373"></a>
#. <a class="sc" id="sc1"></a>
#. VAR: Parāyanañca → parāyaṇañca (bj)
msgctxt "sn43.44:1.1"
msgid "“Parāyanañca vo, bhikkhave, desessāmi parāyanagāmiñca maggaṃ."
msgstr ""

msgctxt "sn43.44:1.2"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn43.44:1.3"
msgid "Katamañca, bhikkhave, parāyanaṃ?"
msgstr ""

msgctxt "sn43.44:1.4"
msgid "Yo, bhikkhave, rāgakkhayo dosakkhayo mohakkhayo—"
msgstr ""

msgctxt "sn43.44:1.5"
msgid "idaṃ vuccati, bhikkhave, parāyanaṃ."
msgstr ""

msgctxt "sn43.44:1.6"
msgid "Katamo ca, bhikkhave, parāyanagāmī maggo?"
msgstr ""

msgctxt "sn43.44:1.7"
msgid "Kāyagatāsati."
msgstr ""

msgctxt "sn43.44:1.8"
msgid "Ayaṃ vuccati, bhikkhave, parāyanagāmimaggo."
msgstr ""

msgctxt "sn43.44:1.9"
msgid ""
"Iti kho, bhikkhave, desitaṃ vo mayā parāyanaṃ, desito parāyanagāmimaggo."
msgstr ""

msgctxt "sn43.44:1.10"
msgid ""
"Yaṃ, bhikkhave, satthārā karaṇīyaṃ sāvakānaṃ hitesinā anukampakena anukampaṃ "
"upādāya, kataṃ vo taṃ mayā."
msgstr ""

msgctxt "sn43.44:1.11"
msgid ""
"Etāni, bhikkhave, rukkhamūlāni, etāni suññāgārāni. Jhāyatha, bhikkhave, mā "
"pamādattha; mā pacchā vippaṭisārino ahuvattha. Ayaṃ vo amhākaṃ anusāsanī”ti."
msgstr ""

msgctxt "sn43.44:1.12"
msgid "(Yathā asaṅkhataṃ tathā vitthāretabbaṃ.) (33)"
msgstr ""

#. </p><p class="endsection">
msgctxt "sn43.44:1.13"
msgid "Tettiṃsatimaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn43.44:1.14"
msgid "Dutiyo vaggo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn43.44:2.1"
msgid "Asaṅkhataṃ anataṃ anāsavaṃ,"
msgstr ""

#. <br>
msgctxt "sn43.44:2.2"
msgid "Saccañca pāraṃ nipuṇaṃ sududdasaṃ;"
msgstr ""

#. <br>
msgctxt "sn43.44:2.3"
msgid "Ajajjaraṃ dhuvaṃ apalokitaṃ,"
msgstr ""

#. <br>
msgctxt "sn43.44:2.4"
msgid "Anidassanaṃ nippapañca santaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn43.44:3.1"
msgid "Amataṃ paṇītañca sivañca khemaṃ,"
msgstr ""

#. <br>
msgctxt "sn43.44:3.2"
msgid "Taṇhākkhayo acchariyañca abbhutaṃ;"
msgstr ""

#. <br>
msgctxt "sn43.44:3.3"
msgid "Anītikaṃ anītikadhammaṃ,"
msgstr ""

#. <br>
msgctxt "sn43.44:3.4"
msgid "Nibbānametaṃ sugatena desitaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn43.44:4.1"
msgid "Abyābajjho virāgo ca,"
msgstr ""

#. <br>
msgctxt "sn43.44:4.2"
msgid "suddhi mutti anālayo;"
msgstr ""

#. <br>
msgctxt "sn43.44:4.3"
msgid "Dīpo leṇañca tāṇañca,"
msgstr ""

#. <br>
msgctxt "sn43.44:4.4"
msgid "saraṇañca parāyananti."
msgstr ""

#. </p></blockquote><p class="endsutta">
msgctxt "sn43.44:4.5"
msgid "Asaṅkhatasaṃyuttaṃ samattaṃ."
msgstr ""
