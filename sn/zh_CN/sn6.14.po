#. <section class="sutta" id="sn6.14"><article><div class="hgroup"><p class="division">
msgctxt "sn6.14:0.1"
msgid "Saṃyutta Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "sn6.14:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn6.14:0.3"
msgid "14. Aruṇavatīsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.155"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn6.14:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn6.14:1.2"
msgid "ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati … pe …"
msgstr ""

msgctxt "sn6.14:1.3"
msgid "tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "sn6.14:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "sn6.14:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "sn6.14:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.336"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn6.14:2.1"
msgid "“Bhūtapubbaṃ, bhikkhave, rājā ahosi aruṇavā nāma."
msgstr ""

#. VAR: aruṇavatiyaṃ kho pana bhikkhave rājadhāniyaṃ (pā1, ka-ma)
msgctxt "sn6.14:2.2"
msgid "Rañño kho pana, bhikkhave, aruṇavato aruṇavatī nāma rājadhānī ahosi."
msgstr ""

msgctxt "sn6.14:2.3"
msgid ""
"Aruṇavatiṃ kho pana, bhikkhave, rājadhāniṃ sikhī bhagavā arahaṃ "
"sammāsambuddho upanissāya vihāsi."
msgstr ""

msgctxt "sn6.14:2.4"
msgid ""
"Sikhissa kho pana, bhikkhave, bhagavato arahato sammāsambuddhassa "
"abhibhūsambhavaṃ nāma sāvakayugaṃ ahosi aggaṃ bhaddayugaṃ."
msgstr ""

msgctxt "sn6.14:2.5"
msgid ""
"Atha kho, bhikkhave, sikhī bhagavā arahaṃ sammāsambuddho abhibhuṃ bhikkhuṃ "
"āmantesi:"
msgstr ""

msgctxt "sn6.14:2.6"
msgid ""
"‘āyāma, brāhmaṇa, yena aññataro brahmaloko tenupasaṅkamissāma, yāva "
"bhattassa kālo bhavissatī’ti."
msgstr ""

msgctxt "sn6.14:2.7"
msgid ""
"‘Evaṃ, bhante’ti kho bhikkhave, abhibhū bhikkhu sikhissa bhagavato arahato "
"sammāsambuddhassa paccassosi."
msgstr ""

msgctxt "sn6.14:2.8"
msgid ""
"Atha kho, bhikkhave, sikhī bhagavā arahaṃ sammāsambuddho abhibhū ca bhikkhu—"
"seyyathāpi nāma balavā puriso samiñjitaṃ vā bāhaṃ pasāreyya, pasāritaṃ vā "
"bāhaṃ samiñjeyya; evameva—aruṇavatiyā rājadhāniyā antarahitā tasmiṃ "
"brahmaloke pāturahesuṃ."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.156"></a>
#. <a class="pts2ed" id="pts2ed1.337"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn6.14:3.1"
msgid ""
"Atha kho, bhikkhave, sikhī bhagavā arahaṃ sammāsambuddho abhibhuṃ bhikkhuṃ "
"āmantesi:"
msgstr ""

msgctxt "sn6.14:3.2"
msgid ""
"‘paṭibhātu, brāhmaṇa, taṃ brahmuno ca brahmaparisāya ca brahmapārisajjānañca "
"dhammī kathā’ti."
msgstr ""

msgctxt "sn6.14:3.3"
msgid ""
"‘Evaṃ, bhante’ti kho, bhikkhave, abhibhū bhikkhu sikhissa bhagavato arahato "
"sammāsambuddhassa paṭissutvā, brahmānañca brahmaparisañca brahmapārisajje ca "
"dhammiyā kathāya sandassesi samādapesi samuttejesi sampahaṃsesi."
msgstr ""

#. VAR: khiyyanti → khīyanti (bj, s1-3, km, pts1-2)
msgctxt "sn6.14:3.4"
msgid ""
"Tatra sudaṃ, bhikkhave, brahmā ca brahmaparisā ca brahmapārisajjā ca "
"ujjhāyanti khiyyanti vipācenti:"
msgstr ""

msgctxt "sn6.14:3.5"
msgid ""
"‘acchariyaṃ vata, bho, abbhutaṃ vata bho, kathañhi nāma satthari "
"sammukhībhūte sāvako dhammaṃ desessatī’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn6.14:4.1"
msgid ""
"Atha kho, bhikkhave, sikhī bhagavā arahaṃ sammāsambuddho abhibhuṃ bhikkhuṃ "
"āmantesi:"
msgstr ""

msgctxt "sn6.14:4.2"
msgid ""
"‘ujjhāyanti kho te, brāhmaṇa, brahmā ca brahmaparisā ca brahmapārisajjā ca—"
msgstr ""

msgctxt "sn6.14:4.3"
msgid ""
"acchariyaṃ vata bho, abbhutaṃ vata bho, kathañhi nāma satthari sammukhībhūte "
"sāvako dhammaṃ desessatīti."
msgstr ""

msgctxt "sn6.14:4.4"
msgid ""
"Tena hi tvaṃ, brāhmaṇa, bhiyyoso mattāya brahmānañca brahmaparisañca "
"brahmapārisajje ca saṃvejehī’ti."
msgstr ""

msgctxt "sn6.14:4.5"
msgid ""
"‘Evaṃ, bhante’ti kho, bhikkhave, abhibhū bhikkhu sikhissa bhagavato arahato "
"sammāsambuddhassa paṭissutvā dissamānenapi kāyena dhammaṃ desesi, "
"adissamānenapi kāyena dhammaṃ desesi, dissamānenapi heṭṭhimena upaḍḍhakāyena "
"adissamānena uparimena upaḍḍhakāyena dhammaṃ desesi, dissamānenapi uparimena "
"upaḍḍhakāyena adissamānena heṭṭhimena upaḍḍhakāyena dhammaṃ desesi."
msgstr ""

msgctxt "sn6.14:4.6"
msgid ""
"Tatra sudaṃ, bhikkhave, brahmā ca brahmaparisā ca brahmapārisajjā ca "
"acchariyabbhutacittajātā ahesuṃ:"
msgstr ""

msgctxt "sn6.14:4.7"
msgid ""
"‘acchariyaṃ vata bho, abbhutaṃ vata bho, samaṇassa mahiddhikatā "
"mahānubhāvatā’ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.338"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn6.14:5.1"
msgid ""
"Atha kho abhibhū bhikkhu sikhiṃ bhagavantaṃ arahantaṃ sammāsambuddhaṃ "
"etadavoca:"
msgstr ""

msgctxt "sn6.14:5.2"
msgid ""
"‘abhijānāmi khvāhaṃ, bhante, bhikkhusaṃghassa majjhe evarūpiṃ vācaṃ bhāsitā—"
msgstr ""

#. VAR: sahassilokadhātuṃ → sahassīlokadhātuṃ (bj, s1-3, km, pts1-2)
msgctxt "sn6.14:5.3"
msgid ""
"pahomi khvāhaṃ, āvuso, brahmaloke ṭhito sahassilokadhātuṃ sarena "
"viññāpetun’ti."
msgstr ""

msgctxt "sn6.14:5.4"
msgid "‘Etassa, brāhmaṇa, kālo, etassa, brāhmaṇa, kālo;"
msgstr ""

msgctxt "sn6.14:5.5"
msgid ""
"yaṃ tvaṃ, brāhmaṇa, brahmaloke ṭhito sahassilokadhātuṃ sarena "
"viññāpeyyāsī’ti."
msgstr ""

msgctxt "sn6.14:5.6"
msgid ""
"‘Evaṃ, bhante’ti kho, bhikkhave, abhibhū bhikkhu sikhissa bhagavato arahato "
"sammāsambuddhassa paṭissutvā brahmaloke ṭhito imā gāthāyo abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc6"></a>
#. VAR: Ārambhatha → ārabhatha (bj) | ārabbhatha (s1-3, pts1-2)
#. VAR: nikkamatha → nikkhamatha (bj, pts1-2)
msgctxt "sn6.14:6.1"
msgid "‘Ārambhatha nikkamatha,"
msgstr ""

#. <br>
msgctxt "sn6.14:6.2"
msgid "Yuñjatha buddhasāsane;"
msgstr ""

#. <br>
msgctxt "sn6.14:6.3"
msgid "Dhunātha maccuno senaṃ,"
msgstr ""

#. <br>
msgctxt "sn6.14:6.4"
msgid "Naḷāgāraṃva kuñjaro."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.157"></a>
#. <a class="sc" id="sc7"></a>
msgctxt "sn6.14:7.1"
msgid "Yo imasmiṃ dhammavinaye,"
msgstr ""

#. <br>
msgctxt "sn6.14:7.2"
msgid "appamatto vihassati;"
msgstr ""

#. <br>
msgctxt "sn6.14:7.3"
msgid "Pahāya jātisaṃsāraṃ,"
msgstr ""

#. <br>
msgctxt "sn6.14:7.4"
msgid "dukkhassantaṃ karissatī’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts2ed" id="pts2ed1.339"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "sn6.14:8.1"
msgid ""
"Atha kho, bhikkhave, sikhī ca bhagavā arahaṃ sammāsambuddho abhibhū ca "
"bhikkhu brahmānañca brahmaparisañca brahmapārisajje ca saṃvejetvā—"
msgstr ""

msgctxt "sn6.14:8.2"
msgid ""
"seyyathāpi nāma … pe … tasmiṃ brahmaloke antarahitā aruṇavatiyā rājadhāniyā "
"pāturahesuṃ."
msgstr ""

msgctxt "sn6.14:8.3"
msgid ""
"Atha kho, bhikkhave, sikhī bhagavā arahaṃ sammāsambuddho bhikkhū āmantesi:"
msgstr ""

msgctxt "sn6.14:8.4"
msgid ""
"‘assuttha no tumhe, bhikkhave, abhibhussa bhikkhuno brahmaloke ṭhitassa "
"gāthāyo bhāsamānassā’ti?"
msgstr ""

msgctxt "sn6.14:8.5"
msgid ""
"‘Assumha kho mayaṃ, bhante, abhibhussa bhikkhuno brahmaloke ṭhitassa gāthāyo "
"bhāsamānassā’ti."
msgstr ""

msgctxt "sn6.14:8.6"
msgid ""
"‘Yathā kathaṃ pana tumhe, bhikkhave, assuttha abhibhussa bhikkhuno "
"brahmaloke ṭhitassa gāthāyo bhāsamānassā’ti?"
msgstr ""

# MS should have quotes here.
msgctxt "sn6.14:8.7"
msgid ""
"Evaṃ kho mayaṃ, bhante, assumha abhibhussa bhikkhuno brahmaloke ṭhitassa "
"gāthāyo bhāsamānassa:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc9"></a>
msgctxt "sn6.14:9.1"
msgid "‘Ārambhatha nikkamatha,"
msgstr ""

#. <br>
msgctxt "sn6.14:9.2"
msgid "yuñjatha buddhasāsane;"
msgstr ""

#. <br>
msgctxt "sn6.14:9.3"
msgid "Dhunātha maccuno senaṃ,"
msgstr ""

#. <br>
msgctxt "sn6.14:9.4"
msgid "naḷāgāraṃva kuñjaro."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn6.14:10.1"
msgid "Yo imasmiṃ dhammavinaye,"
msgstr ""

#. <br>
msgctxt "sn6.14:10.2"
msgid "appamatto vihassati;"
msgstr ""

#. <br>
msgctxt "sn6.14:10.3"
msgid "Pahāya jātisaṃsāraṃ,"
msgstr ""

#. <br>
msgctxt "sn6.14:10.4"
msgid "dukkhassantaṃ karissatī’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn6.14:11.1"
msgid ""
"‘Evaṃ kho mayaṃ, bhante, assumha abhibhussa bhikkhuno brahmaloke ṭhitassa "
"gāthāyo bhāsamānassā’ti."
msgstr ""

msgctxt "sn6.14:11.2"
msgid "‘Sādhu sādhu, bhikkhave;"
msgstr ""

msgctxt "sn6.14:11.3"
msgid ""
"sādhu kho tumhe, bhikkhave, assuttha abhibhussa bhikkhuno brahmaloke "
"ṭhitassa gāthāyo bhāsamānassā’”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.340"></a>
#. <a class="sc" id="sc12"></a>
msgctxt "sn6.14:12.1"
msgid "Idamavoca bhagavā, attamanā te bhikkhū bhagavato bhāsitaṃ abhinandunti."
msgstr ""
