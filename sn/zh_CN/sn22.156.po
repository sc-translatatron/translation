#. <section class="sutta" id="sn22.156"><article><div class="hgroup"><p class="division">
msgctxt "sn22.156:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.156:0.2"
msgid "15. Diṭṭhivagga"
msgstr ""

#. </p><h1>
#. VAR: 156. Attānudiṭṭhisutta → attānu (pts1)
msgctxt "sn22.156:0.3"
msgid "156. Attānudiṭṭhisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.186"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.156:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.156:1.2"
msgid ""
"“Kismiṃ nu kho, bhikkhave, sati, kiṃ upādāya, kiṃ abhinivissa attānudiṭṭhi "
"uppajjatī”ti?"
msgstr ""

msgctxt "sn22.156:1.3"
msgid "Bhagavaṃmūlakā no, bhante, dhammā … pe …"
msgstr ""

msgctxt "sn22.156:1.4"
msgid ""
"“rūpe kho, bhikkhave, sati, rūpaṃ upādāya, rūpaṃ abhinivissa attānudiṭṭhi "
"uppajjati."
msgstr ""

msgctxt "sn22.156:1.5"
msgid "Vedanāya sati …"
msgstr ""

msgctxt "sn22.156:1.6"
msgid "saññāya sati …"
msgstr ""

msgctxt "sn22.156:1.7"
msgid "saṅkhāresu sati …"
msgstr ""

msgctxt "sn22.156:1.8"
msgid ""
"viññāṇe sati, viññāṇaṃ upādāya, viññāṇaṃ abhinivissa attānudiṭṭhi uppajjati."
msgstr ""

msgctxt "sn22.156:1.9"
msgid "Taṃ kiṃ maññatha, bhikkhave, "
msgstr ""

msgctxt "sn22.156:1.10"
msgid "rūpaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn22.156:1.11"
msgid "“Aniccaṃ, bhante”."
msgstr ""

msgctxt "sn22.156:1.12"
msgid "“Yaṃ panāniccaṃ … pe …"
msgstr ""

msgctxt "sn22.156:1.13"
msgid "api nu taṃ anupādāya attānudiṭṭhi uppajjeyyā”ti?"
msgstr ""

msgctxt "sn22.156:1.14"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn22.156:1.15"
msgid "“Vedanā …"
msgstr ""

msgctxt "sn22.156:1.16"
msgid "saññā …"
msgstr ""

msgctxt "sn22.156:1.17"
msgid "saṅkhārā …"
msgstr ""

msgctxt "sn22.156:1.18"
msgid "viññāṇaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn22.156:1.19"
msgid "“Aniccaṃ, bhante”."
msgstr ""

msgctxt "sn22.156:1.20"
msgid "“Yaṃ panāniccaṃ … pe …"
msgstr ""

msgctxt "sn22.156:1.21"
msgid "api nu taṃ anupādāya attānudiṭṭhi uppajjeyyā”ti?"
msgstr ""

msgctxt "sn22.156:1.22"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn22.156:1.23"
msgid "“Evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn22.156:1.24"
msgid "nāparaṃ itthattāyāti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.156:1.25"
msgid "Sattamaṃ."
msgstr ""
