#. <section class="sutta" id="sn11.17"><article><div class="hgroup"><p class="division">
msgctxt "sn11.17:0.1"
msgid "Saṃyutta Nikāya 11"
msgstr ""

#. </p><p>
msgctxt "sn11.17:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
#. VAR: 17. Buddhavandanāsutta → vandanāsuttaṃ (bj) | vandanasuttaṃ (s1-3) | vandanā (pts1)
msgctxt "sn11.17:0.3"
msgid "17. Buddhavandanāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn11.17:1.1"
msgid "Sāvatthiyaṃ jetavane."
msgstr ""

msgctxt "sn11.17:1.2"
msgid "Tena kho pana samayena bhagavā divāvihāragato hoti paṭisallīno."
msgstr ""

msgctxt "sn11.17:1.3"
msgid ""
"Atha kho sakko ca devānamindo brahmā ca sahampati yena bhagavā "
"tenupasaṅkamiṃsu; upasaṅkamitvā paccekaṃ dvārabāhaṃ nissāya aṭṭhaṃsu."
msgstr ""

msgctxt "sn11.17:1.4"
msgid "Atha kho sakko devānamindo bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn11.17:2.1"
msgid "“Uṭṭhehi vīra vijitasaṅgāma,"
msgstr ""

#. <br>
msgctxt "sn11.17:2.2"
msgid "Pannabhāra anaṇa vicara loke;"
msgstr ""

#. <br>
msgctxt "sn11.17:2.3"
msgid "Cittañca te suvimuttaṃ,"
msgstr ""

#. <br>
msgctxt "sn11.17:2.4"
msgid "Cando yathā pannarasāya rattin”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts1ed" id="pts1ed1.234"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn11.17:3.1"
msgid "“Na kho, devānaminda, tathāgatā evaṃ vanditabbā."
msgstr ""

msgctxt "sn11.17:3.2"
msgid "Evañca kho, devānaminda, tathāgatā vanditabbā:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.502"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn11.17:4.1"
msgid "‘Uṭṭhehi vīra vijitasaṅgāma,"
msgstr ""

#. <br>
msgctxt "sn11.17:4.2"
msgid "Satthavāha anaṇa vicara loke;"
msgstr ""

#. <br>
msgctxt "sn11.17:4.3"
msgid "Desassu bhagavā dhammaṃ,"
msgstr ""

#. <br>
msgctxt "sn11.17:4.4"
msgid "Aññātāro bhavissantī’”ti."
msgstr ""
