#. <section class="sutta" id="sn3.16"><article><div class="hgroup"><p class="division">
msgctxt "sn3.16:0.1"
msgid "Saṃyutta Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "sn3.16:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
#. VAR: 16. Mallikāsutta → dhītusuttaṃ (bj, s1-3, pts2) | dhitā (pts1)
msgctxt "sn3.16:0.3"
msgid "16. Mallikāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.86"></a>
#. <a class="pts2ed" id="pts2ed1.194"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn3.16:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn3.16:1.2"
msgid ""
"Atha kho rājā pasenadi kosalo yena bhagavā tenupasaṅkami; upasaṅkamitvā "
"bhagavantaṃ abhivādetvā ekamantaṃ nisīdi."
msgstr ""

msgctxt "sn3.16:1.3"
msgid ""
"Atha kho aññataro puriso yena rājā pasenadi kosalo tenupasaṅkami; "
"upasaṅkamitvā rañño pasenadissa kosalassa upakaṇṇake ārocesi:"
msgstr ""

msgctxt "sn3.16:1.4"
msgid "“mallikā, deva, devī dhītaraṃ vijātā”ti."
msgstr ""

msgctxt "sn3.16:1.5"
msgid "Evaṃ vutte, rājā pasenadi kosalo anattamano ahosi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn3.16:2.1"
msgid ""
"Atha kho bhagavā rājānaṃ pasenadiṃ kosalaṃ anattamanataṃ viditvā tāyaṃ "
"velāyaṃ imā gāthāyo abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn3.16:3.1"
msgid "“Itthīpi hi ekacciyā,"
msgstr ""

#. <br>
msgctxt "sn3.16:3.2"
msgid "seyyā posa janādhipa;"
msgstr ""

#. <br>
msgctxt "sn3.16:3.3"
msgid "Medhāvinī sīlavatī,"
msgstr ""

#. <br>
msgctxt "sn3.16:3.4"
msgid "sassudevā patibbatā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn3.16:4.1"
msgid "Tassā yo jāyati poso,"
msgstr ""

#. <br>
msgctxt "sn3.16:4.2"
msgid "sūro hoti disampati;"
msgstr ""

#. <br>
#. VAR: subhagiyā → subhariyā (s1-3, pts1, mr)
msgctxt "sn3.16:4.3"
msgid "Tādisā subhagiyā putto,"
msgstr ""

#. <br>
msgctxt "sn3.16:4.4"
msgid "rajjampi anusāsatī”ti."
msgstr ""
