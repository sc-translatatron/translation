#. <section class="sutta" id="sn1.14"><article><div class="hgroup"><p class="division">
msgctxt "sn1.14:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.14:0.2"
msgid "2. Nandanavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.14:0.3"
msgid "14. Khattiyasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.13"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.14:1.1"
msgid "“Khattiyo dvipadaṃ seṭṭho,"
msgstr ""

#. <br>
#. VAR: balībaddo → balivaddo (bj, pts1-2) | balibaddho (s1-3, km) | balibaddo (mr)
msgctxt "sn1.14:1.2"
msgid "balībaddo catuppadaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.14:1.3"
msgid "Komārī seṭṭhā bhariyānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.14:1.4"
msgid "yo ca puttāna pubbajo”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.14:2.1"
msgid "“Sambuddho dvipadaṃ seṭṭho,"
msgstr ""

#. <br>
msgctxt "sn1.14:2.2"
msgid "ājānīyo catuppadaṃ;"
msgstr ""

# BB’s “obedient”, while etymologically sound, finds little support in the suttas. sussusati and its forms are commonly found in the standard description of mendicants listening to the teaching, where it clearly means “listens well, pays attention”, not “follows orders”.
#. <br>
msgctxt "sn1.14:2.3"
msgid "Sussūsā seṭṭhā bhariyānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.14:2.4"
msgid "yo ca puttānamassavo”ti."
msgstr ""
