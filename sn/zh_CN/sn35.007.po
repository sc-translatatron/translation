#. <section class="sutta" id="sn35.7"><article><div class="hgroup"><p class="division">
msgctxt "sn35.7:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.7:0.2"
msgid "1. Aniccavagga"
msgstr ""

#. </p><h1>
#. VAR: 7. Ajjhattāniccātītānāgatasutta → dutiyaajjhattāniccasuttaṃ (bj) | aniccaṃ 3; ajjhataṃ (pts1)
msgctxt "sn35.7:0.3"
msgid "7. Ajjhattāniccātītānāgatasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.4"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.7:1.1"
msgid "“Cakkhuṃ, bhikkhave, aniccaṃ atītānāgataṃ;"
msgstr ""

msgctxt "sn35.7:1.2"
msgid "ko pana vādo paccuppannassa."
msgstr ""

msgctxt "sn35.7:1.3"
msgid ""
"Evaṃ passaṃ, bhikkhave, sutavā ariyasāvako atītasmiṃ cakkhusmiṃ anapekkho "
"hoti;"
msgstr ""

msgctxt "sn35.7:1.4"
msgid "anāgataṃ cakkhuṃ nābhinandati;"
msgstr ""

msgctxt "sn35.7:1.5"
msgid "paccuppannassa cakkhussa nibbidāya virāgāya nirodhāya paṭipanno hoti."
msgstr ""

msgctxt "sn35.7:1.6"
msgid "Sotaṃ aniccaṃ …"
msgstr ""

msgctxt "sn35.7:1.7"
msgid "ghānaṃ aniccaṃ …"
msgstr ""

msgctxt "sn35.7:1.8"
msgid "jivhā aniccā atītānāgatā;"
msgstr ""

msgctxt "sn35.7:1.9"
msgid "ko pana vādo paccuppannāya."
msgstr ""

msgctxt "sn35.7:1.10"
msgid ""
"Evaṃ passaṃ, bhikkhave, sutavā ariyasāvako atītāya jivhāya anapekkho hoti;"
msgstr ""

msgctxt "sn35.7:1.11"
msgid "anāgataṃ jivhaṃ nābhinandati;"
msgstr ""

msgctxt "sn35.7:1.12"
msgid "paccuppannāya jivhāya nibbidāya virāgāya nirodhāya paṭipanno hoti."
msgstr ""

msgctxt "sn35.7:1.13"
msgid "Kāyo anicco … pe …"
msgstr ""

msgctxt "sn35.7:1.14"
msgid "mano anicco atītānāgato;"
msgstr ""

msgctxt "sn35.7:1.15"
msgid "ko pana vādo paccuppannassa."
msgstr ""

msgctxt "sn35.7:1.16"
msgid ""
"Evaṃ passaṃ, bhikkhave, sutavā ariyasāvako atītasmiṃ manasmiṃ anapekkho hoti;"
msgstr ""

msgctxt "sn35.7:1.17"
msgid "anāgataṃ manaṃ nābhinandati;"
msgstr ""

msgctxt "sn35.7:1.18"
msgid "paccuppannassa manassa nibbidāya virāgāya nirodhāya paṭipanno hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.7:1.19"
msgid "Sattamaṃ."
msgstr ""
