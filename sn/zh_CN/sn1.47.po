#. <section class="sutta" id="sn1.47"><article><div class="hgroup"><p class="division">
msgctxt "sn1.47:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.47:0.2"
msgid "5. Ādittavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.47:0.3"
msgid "47. Vanaropasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.47:1.1"
msgid "“Kesaṃ divā ca ratto ca,"
msgstr ""

#. <br>
msgctxt "sn1.47:1.2"
msgid "sadā puññaṃ pavaḍḍhati;"
msgstr ""

#. <br>
msgctxt "sn1.47:1.3"
msgid "Dhammaṭṭhā sīlasampannā,"
msgstr ""

#. <br>
msgctxt "sn1.47:1.4"
msgid "ke janā saggagāmino”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.47:2.1"
msgid "“Ārāmaropā vanaropā,"
msgstr ""

#. <br>
msgctxt "sn1.47:2.2"
msgid "ye janā setukārakā;"
msgstr ""

#. <br>
msgctxt "sn1.47:2.3"
msgid "Papañca udapānañca,"
msgstr ""

#. <br>
msgctxt "sn1.47:2.4"
msgid "ye dadanti upassayaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.47:3.1"
msgid "Tesaṃ divā ca ratto ca,"
msgstr ""

#. <br>
msgctxt "sn1.47:3.2"
msgid "sadā puññaṃ pavaḍḍhati;"
msgstr ""

#. <br>
msgctxt "sn1.47:3.3"
msgid "Dhammaṭṭhā sīlasampannā,"
msgstr ""

#. <br>
msgctxt "sn1.47:3.4"
msgid "te janā saggagāmino”ti."
msgstr ""
