#. <section class="sutta" id="sn2.4"><article><div class="hgroup"><p class="division">
msgctxt "sn2.4:0.1"
msgid "Saṃyutta Nikāya 2"
msgstr ""

#. </p><p>
msgctxt "sn2.4:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
msgctxt "sn2.4:0.3"
msgid "4. Māgadhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn2.4:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn2.4:1.2"
msgid "Ekamantaṃ ṭhito kho māgadho devaputto bhagavantaṃ gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.107"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn2.4:2.1"
msgid "“Kati lokasmiṃ pajjotā,"
msgstr ""

#. <br>
msgctxt "sn2.4:2.2"
msgid "yehi loko pakāsati;"
msgstr ""

#. <br>
msgctxt "sn2.4:2.3"
msgid "Bhavantaṃ puṭṭhumāgamma,"
msgstr ""

#. <br>
msgctxt "sn2.4:2.4"
msgid "kathaṃ jānemu taṃ mayan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn2.4:3.1"
msgid "“Cattāro loke pajjotā,"
msgstr ""

#. <br>
msgctxt "sn2.4:3.2"
msgid "pañcamettha na vijjati;"
msgstr ""

#. <br>
msgctxt "sn2.4:3.3"
msgid "Divā tapati ādicco,"
msgstr ""

#. <br>
msgctxt "sn2.4:3.4"
msgid "rattimābhāti candimā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn2.4:4.1"
msgid "Atha aggi divārattiṃ,"
msgstr ""

#. <br>
msgctxt "sn2.4:4.2"
msgid "tattha tattha pakāsati;"
msgstr ""

#. <br>
msgctxt "sn2.4:4.3"
msgid "Sambuddho tapataṃ seṭṭho,"
msgstr ""

#. <br>
msgctxt "sn2.4:4.4"
msgid "esā ābhā anuttarā”ti."
msgstr ""
