#. <section class="sutta" id="sn37.25"><article><div class="hgroup"><p class="division">
msgctxt "sn37.25:0.1"
msgid "Saṃyutta Nikāya 37"
msgstr ""

#. </p><p>
msgctxt "sn37.25:0.2"
msgid "3. Balavagga"
msgstr ""

#. </p><h1>
msgctxt "sn37.25:0.3"
msgid "25. Visāradasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.246"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn37.25:1.1"
msgid "“Pañcimāni, bhikkhave, mātugāmassa balāni."
msgstr ""

msgctxt "sn37.25:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "sn37.25:1.3"
msgid "Rūpabalaṃ, bhogabalaṃ, ñātibalaṃ, puttabalaṃ, sīlabalaṃ—"
msgstr ""

msgctxt "sn37.25:1.4"
msgid "imāni kho, bhikkhave, pañca mātugāmassa balāni."
msgstr ""

msgctxt "sn37.25:1.5"
msgid ""
"Imehi kho, bhikkhave, pañcahi balehi samannāgato mātugāmo visārado agāraṃ "
"ajjhāvasatī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn37.25:1.6"
msgid "Paṭhamaṃ."
msgstr ""
