#. <section class="sutta" id="sn18.10"><article><div class="hgroup"><p class="division">
msgctxt "sn18.10:0.1"
msgid "Saṃyutta Nikāya 18"
msgstr ""

#. </p><p>
msgctxt "sn18.10:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
msgctxt "sn18.10:0.3"
msgid "10. Khandhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn18.10:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn18.10:1.2"
msgid "“Taṃ kiṃ maññasi, rāhula, "
msgstr ""

msgctxt "sn18.10:1.3"
msgid "rūpaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn18.10:1.4"
msgid "“Aniccaṃ, bhante” …"
msgstr ""

msgctxt "sn18.10:1.5"
msgid "“vedanā … pe …"
msgstr ""

msgctxt "sn18.10:1.6"
msgid "saññā …"
msgstr ""

msgctxt "sn18.10:1.7"
msgid "saṅkhārā …"
msgstr ""

msgctxt "sn18.10:1.8"
msgid "viññāṇaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn18.10:1.9"
msgid "“Aniccaṃ, bhante” …"
msgstr ""

msgctxt "sn18.10:1.10"
msgid ""
"“evaṃ passaṃ, rāhula, sutavā ariyasāvako rūpasmimpi nibbindati … pe … "
"vedanāyapi nibbindati … saññāyapi nibbindati … saṅkhāresupi nibbindati … "
"viññāṇasmimpi nibbindati;"
msgstr ""

msgctxt "sn18.10:1.11"
msgid ""
"nibbindaṃ virajjati; virāgā vimuccati; vimuttasmiṃ vimuttamiti ñāṇaṃ hoti."
msgstr ""

msgctxt "sn18.10:1.12"
msgid ""
"‘Khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyā’ti "
"pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn18.10:1.13"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn18.10:1.14"
msgid "Paṭhamo vaggo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn18.10:2.1"
msgid "Cakkhu rūpañca viññāṇaṃ,"
msgstr ""

#. <br>
msgctxt "sn18.10:2.2"
msgid "samphasso vedanāya ca;"
msgstr ""

#. <br>
msgctxt "sn18.10:2.3"
msgid "Saññā sañcetanā taṇhā,"
msgstr ""

#. <br>
msgctxt "sn18.10:2.4"
msgid "dhātu khandhena te dasāti."
msgstr ""
