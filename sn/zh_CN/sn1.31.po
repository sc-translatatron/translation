#. <section class="sutta" id="sn1.31"><article><div class="hgroup"><p class="division">
msgctxt "sn1.31:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.31:0.2"
msgid "4. Satullapakāyikavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.31:0.3"
msgid "31. Sabbhisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.17"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn1.31:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn1.31:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "sn1.31:1.3"
msgid ""
"Atha kho sambahulā satullapakāyikā devatāyo abhikkantāya rattiyā "
"abhikkantavaṇṇā kevalakappaṃ jetavanaṃ obhāsetvā yena bhagavā "
"tenupasaṅkamiṃsu; upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ aṭṭhaṃsu."
msgstr ""

msgctxt "sn1.31:1.4"
msgid "Ekamantaṃ ṭhitā kho ekā devatā bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.31:2.1"
msgid "“Sabbhireva samāsetha,"
msgstr ""

#. <br>
#. VAR: kubbetha → krubbetha (bj, mr)
msgctxt "sn1.31:2.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.31:2.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn1.31:2.4"
msgid "seyyo hoti na pāpiyo”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.31:3.1"
msgid "Atha kho aparā devatā bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.38"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.31:4.1"
msgid "“Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn1.31:4.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.31:4.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
#. VAR: paññā labbhati → paññaṃ labhati (s1-3, km)
msgctxt "sn1.31:4.4"
msgid "paññā labbhati nāññato”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn1.31:5.1"
msgid "Atha kho aparā devatā bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn1.31:6.1"
msgid "“Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn1.31:6.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.31:6.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn1.31:6.4"
msgid "sokamajjhe na socatī”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn1.31:7.1"
msgid "Atha kho aparā devatā bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn1.31:8.1"
msgid "“Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn1.31:8.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.31:8.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn1.31:8.4"
msgid "ñātimajjhe virocatī”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc9"></a>
msgctxt "sn1.31:9.1"
msgid "Atha kho aparā devatā bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.39"></a>
#. <a class="sc" id="sc10"></a>
msgctxt "sn1.31:10.1"
msgid "“Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn1.31:10.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.31:10.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn1.31:10.4"
msgid "sattā gacchanti suggatin”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn1.31:11.1"
msgid "Atha kho aparā devatā bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn1.31:12.1"
msgid "“Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn1.31:12.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.31:12.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn1.31:12.4"
msgid "sattā tiṭṭhanti sātatan”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc13"></a>
msgctxt "sn1.31:13.1"
msgid "Atha kho aparā devatā bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn1.31:13.2"
msgid "“kassa nu kho, bhagavā, subhāsitan”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc14"></a>
msgctxt "sn1.31:14.1"
msgid "“Sabbāsaṃ vo subhāsitaṃ pariyāyena, api ca mamapi suṇātha—"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts1ed" id="pts1ed1.18"></a>
#. <a class="sc" id="sc15"></a>
msgctxt "sn1.31:15.1"
msgid "Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn1.31:15.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.31:15.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn1.31:15.4"
msgid "sabbadukkhā pamuccatī”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc16"></a>
msgctxt "sn1.31:16.1"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "sn1.31:16.2"
msgid ""
"Attamanā tā devatāyo bhagavantaṃ abhivādetvā padakkhiṇaṃ katvā "
"tatthevantaradhāyiṃsūti."
msgstr ""
