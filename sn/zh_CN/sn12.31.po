#. <section class="sutta" id="sn12.31"><article><div class="hgroup"><p class="division">
msgctxt "sn12.31:0.1"
msgid "Saṃyutta Nikāya 12"
msgstr ""

#. </p><p>
msgctxt "sn12.31:0.2"
msgid "4. Kaḷārakhattiyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn12.31:0.3"
msgid "31. Bhūtasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn12.31:1.1"
msgid "Ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn12.31:1.2"
msgid "Tatra kho bhagavā āyasmantaṃ sāriputtaṃ āmantesi:"
msgstr ""

#. VAR: pārāyane → pārāyaṇe (bj) | parāyane (pts1-2)
msgctxt "sn12.31:1.3"
msgid "“vuttamidaṃ, sāriputta, pārāyane ajitapañhe:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn12.31:2.1"
msgid "‘Ye ca saṅkhātadhammāse,"
msgstr ""

#. <br>
msgctxt "sn12.31:2.2"
msgid "ye ca sekkhā puthū idha;"
msgstr ""

#. <br>
msgctxt "sn12.31:2.3"
msgid "Tesaṃ me nipako iriyaṃ,"
msgstr ""

#. <br>
msgctxt "sn12.31:2.4"
msgid "puṭṭho pabrūhi mārisā’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn12.31:3.1"
msgid ""
"Imassa nu kho, sāriputta, saṅkhittena bhāsitassa kathaṃ vitthārena attho "
"daṭṭhabbo”ti?"
msgstr ""

msgctxt "sn12.31:3.2"
msgid "Evaṃ vutte, āyasmā sāriputto tuṇhī ahosi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn12.31:4.1"
msgid "Dutiyampi kho bhagavā āyasmantaṃ sāriputtaṃ āmantesi … pe …"
msgstr ""

msgctxt "sn12.31:4.2"
msgid "dutiyampi kho āyasmā sāriputto tuṇhī ahosi."
msgstr ""

msgctxt "sn12.31:4.3"
msgid "Tatiyampi kho bhagavā āyasmantaṃ sāriputtaṃ āmantesi:"
msgstr ""

msgctxt "sn12.31:4.4"
msgid "“vuttamidaṃ, sāriputta, pārāyane ajitapañhe:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn12.31:5.1"
msgid "‘Ye ca saṅkhātadhammāse,"
msgstr ""

#. <br>
msgctxt "sn12.31:5.2"
msgid "ye ca sekkhā puthū idha;"
msgstr ""

#. <br>
msgctxt "sn12.31:5.3"
msgid "Tesaṃ me nipako iriyaṃ,"
msgstr ""

#. <br>
msgctxt "sn12.31:5.4"
msgid "puṭṭho pabrūhi mārisā’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.48"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "sn12.31:6.1"
msgid ""
"Imassa nu kho, sāriputta, saṅkhittena bhāsitassa kathaṃ vitthārena attho "
"daṭṭhabbo”ti?"
msgstr ""

msgctxt "sn12.31:6.2"
msgid "Tatiyampi kho āyasmā sāriputto tuṇhī ahosi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn12.31:7.1"
msgid "“Bhūtamidanti, sāriputta, passasī”ti?"
msgstr ""

msgctxt "sn12.31:7.2"
msgid "“Bhūtamidanti, bhante, yathābhūtaṃ sammappaññāya passati."
msgstr ""

msgctxt "sn12.31:7.3"
msgid ""
"Bhūtamidanti yathābhūtaṃ sammappaññāya disvā bhūtassa nibbidāya virāgāya "
"nirodhāya paṭipanno hoti."
msgstr ""

msgctxt "sn12.31:7.4"
msgid "Tadāhārasambhavanti yathābhūtaṃ sammappaññāya passati."
msgstr ""

msgctxt "sn12.31:7.5"
msgid ""
"Tadāhārasambhavanti yathābhūtaṃ sammappaññāya disvā āhārasambhavassa "
"nibbidāya virāgāya nirodhāya paṭipanno hoti."
msgstr ""

msgctxt "sn12.31:7.6"
msgid ""
"Tadāhāranirodhā yaṃ bhūtaṃ taṃ nirodhadhammanti yathābhūtaṃ sammappaññāya "
"passati."
msgstr ""

msgctxt "sn12.31:7.7"
msgid ""
"Tadāhāranirodhā yaṃ bhūtaṃ taṃ nirodhadhammanti yathābhūtaṃ sammappaññāya "
"disvā nirodhadhammassa nibbidāya virāgāya nirodhāya paṭipanno hoti."
msgstr ""

msgctxt "sn12.31:7.8"
msgid "Evaṃ kho, bhante, sekkho hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.49"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "sn12.31:8.1"
msgid "Kathañca, bhante, saṅkhātadhammo hoti?"
msgstr ""

msgctxt "sn12.31:8.2"
msgid "Bhūtamidanti, bhante, yathābhūtaṃ sammappaññāya passati."
msgstr ""

msgctxt "sn12.31:8.3"
msgid ""
"Bhūtamidanti yathābhūtaṃ sammappaññāya disvā bhūtassa nibbidā virāgā nirodhā "
"anupādā vimutto hoti."
msgstr ""

msgctxt "sn12.31:8.4"
msgid "Tadāhārasambhavanti yathābhūtaṃ sammappaññāya passati."
msgstr ""

msgctxt "sn12.31:8.5"
msgid ""
"Tadāhārasambhavanti yathābhūtaṃ sammappaññāya disvā āhārasambhavassa nibbidā "
"virāgā nirodhā anupādā vimutto hoti."
msgstr ""

msgctxt "sn12.31:8.6"
msgid ""
"Tadāhāranirodhā yaṃ bhūtaṃ taṃ nirodhadhammanti yathābhūtaṃ sammappaññāya "
"passati."
msgstr ""

msgctxt "sn12.31:8.7"
msgid ""
"Tadāhāranirodhā yaṃ bhūtaṃ taṃ nirodhadhammanti yathābhūtaṃ sammappaññāya "
"disvā nirodhadhammassa nibbidā virāgā nirodhā anupādā vimutto hoti."
msgstr ""

msgctxt "sn12.31:8.8"
msgid "Evaṃ kho, bhante, saṅkhātadhammo hoti."
msgstr ""

msgctxt "sn12.31:8.9"
msgid "Iti kho, bhante, yaṃ taṃ vuttaṃ pārāyane ajitapañhe:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc9"></a>
msgctxt "sn12.31:9.1"
msgid "‘Ye ca saṅkhātadhammāse,"
msgstr ""

#. <br>
msgctxt "sn12.31:9.2"
msgid "ye ca sekkhā puthū idha;"
msgstr ""

#. <br>
msgctxt "sn12.31:9.3"
msgid "Tesaṃ me nipako iriyaṃ,"
msgstr ""

#. <br>
msgctxt "sn12.31:9.4"
msgid "puṭṭho pabrūhi mārisā’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn12.31:10.1"
msgid ""
"Imassa khvāhaṃ, bhante, saṅkhittena bhāsitassa evaṃ vitthārena atthaṃ "
"ājānāmī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn12.31:11.1"
msgid ""
"“Sādhu sādhu, sāriputta, bhūtamidanti, sāriputta, yathābhūtaṃ sammappaññāya "
"passati."
msgstr ""

msgctxt "sn12.31:11.2"
msgid ""
"Bhūtamidanti yathābhūtaṃ sammappaññāya disvā bhūtassa nibbidāya virāgāya "
"nirodhāya paṭipanno hoti."
msgstr ""

msgctxt "sn12.31:11.3"
msgid "Tadāhārasambhavanti yathābhūtaṃ sammappaññāya passati."
msgstr ""

msgctxt "sn12.31:11.4"
msgid ""
"Tadāhārasambhavanti yathābhūtaṃ sammappaññāya disvā āhārasambhavassa "
"nibbidāya virāgāya nirodhāya paṭipanno hoti."
msgstr ""

msgctxt "sn12.31:11.5"
msgid ""
"Tadāhāranirodhā yaṃ bhūtaṃ taṃ nirodhadhammanti yathābhūtaṃ sammappaññāya "
"passati."
msgstr ""

msgctxt "sn12.31:11.6"
msgid ""
"Tadāhāranirodhā yaṃ bhūtaṃ taṃ nirodhadhammanti yathābhūtaṃ sammappaññāya "
"disvā nirodhadhammassa nibbidāya virāgāya nirodhāya paṭipanno hoti."
msgstr ""

msgctxt "sn12.31:11.7"
msgid "Evaṃ kho, sāriputta, sekkho hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.50"></a>
#. <a class="sc" id="sc12"></a>
msgctxt "sn12.31:12.1"
msgid "Kathañca, sāriputta, saṅkhātadhammo hoti?"
msgstr ""

msgctxt "sn12.31:12.2"
msgid "Bhūtamidanti, sāriputta, yathābhūtaṃ sammappaññāya passati."
msgstr ""

msgctxt "sn12.31:12.3"
msgid ""
"Bhūtamidanti yathābhūtaṃ sammappaññāya disvā bhūtassa nibbidā virāgā nirodhā "
"anupādā vimutto hoti."
msgstr ""

msgctxt "sn12.31:12.4"
msgid "Tadāhārasambhavanti yathābhūtaṃ sammappaññāya passati."
msgstr ""

msgctxt "sn12.31:12.5"
msgid ""
"Tadāhārasambhavanti yathābhūtaṃ sammappaññāya disvā āhārasambhavassa nibbidā "
"virāgā nirodhā anupādā vimutto hoti."
msgstr ""

msgctxt "sn12.31:12.6"
msgid ""
"Tadāhāranirodhā yaṃ bhūtaṃ taṃ nirodhadhammanti yathābhūtaṃ sammappaññāya "
"passati."
msgstr ""

msgctxt "sn12.31:12.7"
msgid ""
"Tadāhāranirodhā yaṃ bhūtaṃ taṃ nirodhadhammanti yathābhūtaṃ sammappaññā "
"disvā nirodhadhammassa nibbidā virāgā nirodhā anupādā vimutto hoti."
msgstr ""

msgctxt "sn12.31:12.8"
msgid "Evaṃ kho, sāriputta, saṅkhātadhammo hoti."
msgstr ""

msgctxt "sn12.31:12.9"
msgid "Iti kho, sāriputta, yaṃ taṃ vuttaṃ pārāyane ajitapañhe:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc13"></a>
msgctxt "sn12.31:13.1"
msgid "‘Ye ca saṅkhātadhammāse,"
msgstr ""

#. <br>
msgctxt "sn12.31:13.2"
msgid "ye ca sekkhā puthū idha;"
msgstr ""

#. <br>
msgctxt "sn12.31:13.3"
msgid "Tesaṃ me nipako iriyaṃ,"
msgstr ""

#. <br>
msgctxt "sn12.31:13.4"
msgid "puṭṭho pabrūhi mārisā’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc14"></a>
msgctxt "sn12.31:14.1"
msgid ""
"Imassa kho, sāriputta, saṅkhittena bhāsitassa evaṃ vitthārena attho "
"daṭṭhabbo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.31:14.2"
msgid "Paṭhamaṃ."
msgstr ""
