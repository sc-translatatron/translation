#. <section class="sutta" id="sn22.37"><article><div class="hgroup"><p class="division">
msgctxt "sn22.37:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.37:0.2"
msgid "4. Natumhākavagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.37:0.3"
msgid "37. Ānandasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.37:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.37:1.2"
msgid "Atha kho āyasmā ānando … pe …"
msgstr ""

msgctxt "sn22.37:1.3"
msgid "ekamantaṃ nisinnaṃ kho āyasmantaṃ ānandaṃ bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.38"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn22.37:2.1"
msgid "“Sace taṃ, ānanda, evaṃ puccheyyuṃ:"
msgstr ""

#. VAR: ṭhitassa → ṭhitānaṃ (s1-3, km, pts1, mr)
msgctxt "sn22.37:2.2"
msgid ""
"‘katamesaṃ, āvuso ānanda, dhammānaṃ uppādo paññāyati, vayo paññāyati, "
"ṭhitassa aññathattaṃ paññāyatī’ti?"
msgstr ""

msgctxt "sn22.37:2.3"
msgid "Evaṃ puṭṭho tvaṃ, ānanda, kinti byākareyyāsī”ti?"
msgstr ""

msgctxt "sn22.37:2.4"
msgid "“Sace maṃ, bhante, evaṃ puccheyyuṃ:"
msgstr ""

msgctxt "sn22.37:2.5"
msgid ""
"‘katamesaṃ, āvuso ānanda, dhammānaṃ uppādo paññāyati, vayo paññāyati, "
"ṭhitassa aññathattaṃ paññāyatī’ti?"
msgstr ""

msgctxt "sn22.37:2.6"
msgid "Evaṃ puṭṭhohaṃ, bhante, evaṃ byākareyyaṃ:"
msgstr ""

msgctxt "sn22.37:2.7"
msgid ""
"‘rūpassa kho, āvuso, uppādo paññāyati, vayo paññāyati, ṭhitassa aññathattaṃ "
"paññāyati."
msgstr ""

msgctxt "sn22.37:2.8"
msgid "Vedanāya …"
msgstr ""

msgctxt "sn22.37:2.9"
msgid "saññāya …"
msgstr ""

msgctxt "sn22.37:2.10"
msgid "saṅkhārānaṃ …"
msgstr ""

msgctxt "sn22.37:2.11"
msgid ""
"viññāṇassa uppādo paññāyati, vayo paññāyati, ṭhitassa aññathattaṃ paññāyati."
msgstr ""

msgctxt "sn22.37:2.12"
msgid ""
"Imesaṃ kho, āvuso, dhammānaṃ uppādo paññāyati, vayo paññāyati, ṭhitassa "
"aññathattaṃ paññāyatī’ti."
msgstr ""

msgctxt "sn22.37:2.13"
msgid "Evaṃ puṭṭhohaṃ, bhante, evaṃ byākareyyan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn22.37:3.1"
msgid "“Sādhu sādhu, ānanda."
msgstr ""

msgctxt "sn22.37:3.2"
msgid ""
"Rūpassa kho, ānanda, uppādo paññāyati, vayo paññāyati, ṭhitassa aññathattaṃ "
"paññāyati."
msgstr ""

msgctxt "sn22.37:3.3"
msgid "Vedanāya …"
msgstr ""

msgctxt "sn22.37:3.4"
msgid "saññāya …"
msgstr ""

msgctxt "sn22.37:3.5"
msgid "saṅkhārānaṃ …"
msgstr ""

msgctxt "sn22.37:3.6"
msgid ""
"viññāṇassa uppādo paññāyati, vayo paññāyati, ṭhitassa aññathattaṃ paññāyati."
msgstr ""

msgctxt "sn22.37:3.7"
msgid ""
"Imesaṃ kho, ānanda, dhammānaṃ uppādo paññāyati, vayo paññāyati, ṭhitassa "
"aññathattaṃ paññāyatīti."
msgstr ""

msgctxt "sn22.37:3.8"
msgid "Evaṃ puṭṭho tvaṃ, ānanda, evaṃ byākareyyāsī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.37:3.9"
msgid "Pañcamaṃ."
msgstr ""
