#. <section class="sutta" id="sn2.21"><article><div class="hgroup"><p class="division">
msgctxt "sn2.21:0.1"
msgid "Saṃyutta Nikāya 2"
msgstr ""

#. </p><p>
msgctxt "sn2.21:0.2"
msgid "3. Nānātitthiyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn2.21:0.3"
msgid "21. Sivasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn2.21:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn2.21:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "sn2.21:1.3"
msgid ""
"Atha kho sivo devaputto abhikkantāya rattiyā abhikkantavaṇṇo kevalakappaṃ "
"jetavanaṃ obhāsetvā yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavantaṃ "
"abhivādetvā ekamantaṃ aṭṭhāsi."
msgstr ""

msgctxt "sn2.21:1.4"
msgid ""
"Ekamantaṃ ṭhito kho sivo devaputto bhagavato santike imā gāthāyo abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn2.21:2.1"
msgid "“Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn2.21:2.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn2.21:2.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.21:2.4"
msgid "seyyo hoti na pāpiyo."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.130"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn2.21:3.1"
msgid "Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn2.21:3.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn2.21:3.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.21:3.4"
msgid "paññā labbhati nāññato."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn2.21:4.1"
msgid "Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn2.21:4.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn2.21:4.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.21:4.4"
msgid "sokamajjhe na socati."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.57"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn2.21:5.1"
msgid "Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn2.21:5.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn2.21:5.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.21:5.4"
msgid "ñātimajjhe virocati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn2.21:6.1"
msgid "Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn2.21:6.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn2.21:6.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.21:6.4"
msgid "sattā gacchanti suggatiṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn2.21:7.1"
msgid "Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn2.21:7.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn2.21:7.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.21:7.4"
msgid "sattā tiṭṭhanti sātatan”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts2ed" id="pts2ed1.131"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "sn2.21:8.1"
msgid "Atha kho bhagavā sivaṃ devaputtaṃ gāthāya paccabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc9"></a>
msgctxt "sn2.21:9.1"
msgid "“Sabbhireva samāsetha,"
msgstr ""

#. <br>
msgctxt "sn2.21:9.2"
msgid "sabbhi kubbetha santhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn2.21:9.3"
msgid "Sataṃ saddhammamaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.21:9.4"
msgid "sabbadukkhā pamuccatī”ti."
msgstr ""
