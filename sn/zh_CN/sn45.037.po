#. <section class="sutta" id="sn45.37"><article><div class="hgroup"><p class="division">
msgctxt "sn45.37:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.37:0.2"
msgid "4. Paṭipattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.37:0.3"
msgid "37. Paṭhamabrahmaññasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.26"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.37:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.37:1.2"
msgid "“Brahmaññañca vo, bhikkhave, desessāmi, brahmaññaphalāni ca."
msgstr ""

msgctxt "sn45.37:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn45.37:1.4"
msgid "Katamañca kho, bhikkhave, brahmaññaṃ?"
msgstr ""

msgctxt "sn45.37:1.5"
msgid "Ayameva ariyo aṭṭhaṅgiko maggo, seyyathidaṃ—"
msgstr ""

msgctxt "sn45.37:1.6"
msgid "sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

msgctxt "sn45.37:1.7"
msgid "Idaṃ vuccati, bhikkhave, brahmaññaṃ."
msgstr ""

msgctxt "sn45.37:1.8"
msgid "Katamāni ca, bhikkhave, brahmaññaphalāni?"
msgstr ""

msgctxt "sn45.37:1.9"
msgid "Sotāpattiphalaṃ, sakadāgāmiphalaṃ, anāgāmiphalaṃ, arahattaphalaṃ—"
msgstr ""

msgctxt "sn45.37:1.10"
msgid "imāni vuccanti, bhikkhave, brahmaññaphalānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.37:1.11"
msgid "Sattamaṃ."
msgstr ""
