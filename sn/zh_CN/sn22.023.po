#. <section class="sutta" id="sn22.23"><article><div class="hgroup"><p class="division">
msgctxt "sn22.23:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.23:0.2"
msgid "3. Bhāravagga"
msgstr ""

#. </p><h1>
#. VAR: 23. Pariññasutta → pariññāsuttaṃ (bj)
msgctxt "sn22.23:0.3"
msgid "23. Pariññasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.23:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.23:1.2"
msgid "“Pariññeyye ca, bhikkhave, dhamme desessāmi pariññañca."
msgstr ""

msgctxt "sn22.23:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn22.23:1.4"
msgid "Katame ca, bhikkhave, pariññeyyā dhammā?"
msgstr ""

msgctxt "sn22.23:1.5"
msgid ""
"Rūpaṃ, bhikkhave, pariññeyyo dhammo, vedanā pariññeyyo dhammo, saññā "
"pariññeyyo dhammo, saṅkhārā pariññeyyo dhammo, viññāṇaṃ pariññeyyo dhammo."
msgstr ""

msgctxt "sn22.23:1.6"
msgid "Ime vuccanti, bhikkhave, pariññeyyā dhammā."
msgstr ""

msgctxt "sn22.23:1.7"
msgid "Katamā ca, bhikkhave, pariññā?"
msgstr ""

msgctxt "sn22.23:1.8"
msgid "Yo, bhikkhave, rāgakkhayo dosakkhayo mohakkhayo."
msgstr ""

msgctxt "sn22.23:1.9"
msgid "Ayaṃ vuccati, bhikkhave, pariññā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.23:1.10"
msgid "Dutiyaṃ."
msgstr ""
