#. <section class="sutta" id="sn22.153"><article><div class="hgroup"><p class="division">
msgctxt "sn22.153:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.153:0.2"
msgid "15. Diṭṭhivagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.153:0.3"
msgid "153. Nocamesiyāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.184"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.153:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.153:1.2"
msgid ""
"“Kismiṃ nu kho, bhikkhave, sati, kiṃ upādāya, kiṃ abhinivissa evaṃ diṭṭhi "
"uppajjati:"
msgstr ""

msgctxt "sn22.153:1.3"
msgid "‘no cassaṃ, no ca me siyā, nābhavissa, na me bhavissatī’”ti?"
msgstr ""

msgctxt "sn22.153:1.4"
msgid "Bhagavaṃmūlakā no, bhante, dhammā … pe …"
msgstr ""

msgctxt "sn22.153:1.5"
msgid ""
"“rūpe kho, bhikkhave, sati, rūpaṃ upādāya, rūpaṃ abhinivissa evaṃ diṭṭhi "
"uppajjati:"
msgstr ""

msgctxt "sn22.153:1.6"
msgid "‘no cassaṃ, no ca me siyā, nābhavissa, na me bhavissatī’ti."
msgstr ""

msgctxt "sn22.153:1.7"
msgid "Vedanāya sati …"
msgstr ""

msgctxt "sn22.153:1.8"
msgid "saññāya sati …"
msgstr ""

msgctxt "sn22.153:1.9"
msgid "saṅkhāresu sati …"
msgstr ""

msgctxt "sn22.153:1.10"
msgid ""
"viññāṇe sati, viññāṇaṃ upādāya, viññāṇaṃ abhinivissa, evaṃ diṭṭhi uppajjati:"
msgstr ""

msgctxt "sn22.153:1.11"
msgid "‘no cassaṃ, no ca me siyā, nābhavissa, na me bhavissatī’ti."
msgstr ""

msgctxt "sn22.153:1.12"
msgid "Taṃ kiṃ maññatha, bhikkhave, "
msgstr ""

msgctxt "sn22.153:1.13"
msgid "rūpaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn22.153:1.14"
msgid "“Aniccaṃ, bhante”."
msgstr ""

msgctxt "sn22.153:1.15"
msgid "“Yaṃ panāniccaṃ dukkhaṃ vā taṃ sukhaṃ vā”ti?"
msgstr ""

msgctxt "sn22.153:1.16"
msgid "“Dukkhaṃ, bhante”."
msgstr ""

msgctxt "sn22.153:1.17"
msgid ""
"“Yaṃ panāniccaṃ dukkhaṃ vipariṇāmadhammaṃ, api nu taṃ anupādāya evaṃ diṭṭhi "
"uppajjeyya:"
msgstr ""

msgctxt "sn22.153:1.18"
msgid "‘no cassaṃ, no ca me siyā, nābhavissa, na me bhavissatī’”ti?"
msgstr ""

msgctxt "sn22.153:1.19"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn22.153:1.20"
msgid "“Vedanā …"
msgstr ""

msgctxt "sn22.153:1.21"
msgid "saññā …"
msgstr ""

msgctxt "sn22.153:1.22"
msgid "saṅkhārā …"
msgstr ""

msgctxt "sn22.153:1.23"
msgid "viññāṇaṃ niccaṃ vā aniccaṃ vā”ti?"
msgstr ""

msgctxt "sn22.153:1.24"
msgid "“Aniccaṃ, bhante”."
msgstr ""

msgctxt "sn22.153:1.25"
msgid "“Yaṃ panāniccaṃ dukkhaṃ vā taṃ sukhaṃ vā”ti?"
msgstr ""

msgctxt "sn22.153:1.26"
msgid "“Dukkhaṃ, bhante”."
msgstr ""

msgctxt "sn22.153:1.27"
msgid ""
"“Yaṃ panāniccaṃ dukkhaṃ vipariṇāmadhammaṃ, api nu taṃ anupādāya evaṃ diṭṭhi "
"uppajjeyya:"
msgstr ""

msgctxt "sn22.153:1.28"
msgid "‘no cassaṃ, no ca me siyā, nābhavissa, na me bhavissatī’”ti?"
msgstr ""

msgctxt "sn22.153:1.29"
msgid "“No hetaṃ, bhante”."
msgstr ""

msgctxt "sn22.153:1.30"
msgid "“Evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn22.153:1.31"
msgid "nāparaṃ itthattāyāti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.153:1.32"
msgid "Catutthaṃ."
msgstr ""
