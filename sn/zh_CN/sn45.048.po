#. <section class="sutta" id="sn45.48"><article><div class="hgroup"><p class="division">
msgctxt "sn45.48:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.48:0.2"
msgid "5. Aññatitthiyapeyyālavagga"
msgstr ""

#. </p><h1>
#. VAR: 48. Anupādāparinibbānasutta → anupādasuttaṃ (bj) | anupādāya (pts1)
msgctxt "sn45.48:0.3"
msgid "48. Anupādāparinibbānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.29"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.48:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.48:1.2"
msgid "“Sace vo, bhikkhave, aññatitthiyā paribbājakā evaṃ puccheyyuṃ:"
msgstr ""

msgctxt "sn45.48:1.3"
msgid ""
"‘kimatthiyaṃ, āvuso, samaṇe gotame brahmacariyaṃ vussatī’ti, evaṃ puṭṭhā "
"tumhe, bhikkhave, tesaṃ aññatitthiyānaṃ paribbājakānaṃ evaṃ byākareyyātha:"
msgstr ""

msgctxt "sn45.48:1.4"
msgid ""
"‘anupādāparinibbānatthaṃ kho, āvuso, bhagavati brahmacariyaṃ vussatī’ti."
msgstr ""

msgctxt "sn45.48:1.5"
msgid "Sace pana vo, bhikkhave, aññatitthiyā paribbājakā evaṃ puccheyyuṃ:"
msgstr ""

msgctxt "sn45.48:1.6"
msgid ""
"‘atthi panāvuso, maggo, atthi paṭipadā anupādāparinibbānāyā’ti, evaṃ puṭṭhā "
"tumhe, bhikkhave, tesaṃ aññatitthiyānaṃ paribbājakānaṃ evaṃ byākareyyātha:"
msgstr ""

msgctxt "sn45.48:1.7"
msgid "‘atthi kho, āvuso, maggo, atthi paṭipadā anupādāparinibbānāyā’ti."
msgstr ""

msgctxt "sn45.48:1.8"
msgid "Katamo ca, bhikkhave, maggo, katamā ca paṭipadā anupādāparinibbānāya?"
msgstr ""

msgctxt "sn45.48:1.9"
msgid "Ayameva ariyo aṭṭhaṅgiko maggo, seyyathidaṃ—"
msgstr ""

msgctxt "sn45.48:1.10"
msgid "sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

msgctxt "sn45.48:1.11"
msgid "Ayaṃ, bhikkhave, maggo, ayaṃ paṭipadā anupādāparinibbānāyāti."
msgstr ""

msgctxt "sn45.48:1.12"
msgid ""
"Evaṃ puṭṭhā tumhe, bhikkhave, tesaṃ aññatitthiyānaṃ paribbājakānaṃ evaṃ "
"byākareyyāthā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.48:1.13"
msgid "Aṭṭhamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.48:1.14"
msgid "Aññatitthiyapeyyālaṃ."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn45.48:2.1"
msgid "Virāgasaṃyojanaṃ anusayaṃ,"
msgstr ""

#. <br>
msgctxt "sn45.48:2.2"
msgid "Addhānaṃ āsavā khayā;"
msgstr ""

#. <br>
msgctxt "sn45.48:2.3"
msgid "Vijjāvimuttiñāṇañca,"
msgstr ""

#. <br>
msgctxt "sn45.48:2.4"
msgid "Anupādāya aṭṭhamī."
msgstr ""
