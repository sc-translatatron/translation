#. <section class="sutta" id="sn37.3"><article><div class="hgroup"><p class="division">
msgctxt "sn37.3:0.1"
msgid "Saṃyutta Nikāya 37"
msgstr ""

#. </p><p>
msgctxt "sn37.3:0.2"
msgid "1. Paṭhamapeyyālavagga"
msgstr ""

#. </p><h1>
#. VAR: 3. Āveṇikadukkhasutta → āveṇikā (pts1)
msgctxt "sn37.3:0.3"
msgid "3. Āveṇikadukkhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn37.3:1.1"
msgid ""
"“Pañcimāni, bhikkhave, mātugāmassa āveṇikāni dukkhāni, yāni mātugāmo "
"paccanubhoti, aññatreva purisehi."
msgstr ""

msgctxt "sn37.3:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "sn37.3:1.3"
msgid ""
"Idha, bhikkhave, mātugāmo daharova samāno patikulaṃ gacchati, ñātakehi vinā "
"hoti."
msgstr ""

msgctxt "sn37.3:1.4"
msgid ""
"Idaṃ, bhikkhave, mātugāmassa paṭhamaṃ āveṇikaṃ dukkhaṃ, yaṃ mātugāmo "
"paccanubhoti, aññatreva purisehi."
msgstr ""

# See https://discourse.suttacentral.net/t/menstruation-and-fertility-in-the-pali-texts/3042
msgctxt "sn37.3:1.5"
msgid "Puna caparaṃ, bhikkhave, mātugāmo utunī hoti."
msgstr ""

msgctxt "sn37.3:1.6"
msgid ""
"Idaṃ, bhikkhave, mātugāmassa dutiyaṃ āveṇikaṃ dukkhaṃ, yaṃ mātugāmo "
"paccanubhoti, aññatreva purisehi."
msgstr ""

msgctxt "sn37.3:1.7"
msgid "Puna caparaṃ, bhikkhave, mātugāmo gabbhinī hoti."
msgstr ""

msgctxt "sn37.3:1.8"
msgid ""
"Idaṃ, bhikkhave, mātugāmassa tatiyaṃ āveṇikaṃ dukkhaṃ, yaṃ mātugāmo "
"paccanubhoti, aññatreva purisehi."
msgstr ""

msgctxt "sn37.3:1.9"
msgid "Puna caparaṃ, bhikkhave, mātugāmo vijāyati."
msgstr ""

msgctxt "sn37.3:1.10"
msgid ""
"Idaṃ, bhikkhave, mātugāmassa catutthaṃ āveṇikaṃ dukkhaṃ, yaṃ mātugāmo "
"paccanubhoti, aññatreva purisehi."
msgstr ""

msgctxt "sn37.3:1.11"
msgid "Puna caparaṃ, bhikkhave, mātugāmo purisassa pāricariyaṃ upeti."
msgstr ""

msgctxt "sn37.3:1.12"
msgid ""
"Idaṃ kho, bhikkhave, mātugāmassa pañcamaṃ āveṇikaṃ dukkhaṃ, yaṃ mātugāmo "
"paccanubhoti, aññatreva purisehi."
msgstr ""

msgctxt "sn37.3:1.13"
msgid ""
"Imāni kho, bhikkhave, pañca mātugāmassa āveṇikāni dukkhāni, yāni mātugāmo "
"paccanubhoti, aññatreva purisehī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn37.3:1.14"
msgid "Tatiyaṃ."
msgstr ""
