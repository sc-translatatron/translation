#. <section class="sutta" id="sn51.2"><article><div class="hgroup"><p class="division">
msgctxt "sn51.2:0.1"
msgid "Saṃyutta Nikāya 51"
msgstr ""

#. </p><p>
msgctxt "sn51.2:0.2"
msgid "1. Cāpālavagga"
msgstr ""

#. </p><h1>
msgctxt "sn51.2:0.3"
msgid "2. Viraddhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.255"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn51.2:1.1"
msgid ""
"“Yesaṃ kesañci, bhikkhave, cattāro iddhipādā viraddhā, viraddho tesaṃ ariyo "
"maggo sammā dukkhakkhayagāmī."
msgstr ""

msgctxt "sn51.2:1.2"
msgid ""
"Yesaṃ kesañci, bhikkhave, cattāro iddhipādā āraddhā, āraddho tesaṃ ariyo "
"maggo sammā dukkhakkhayagāmī."
msgstr ""

msgctxt "sn51.2:1.3"
msgid "Katame cattāro?"
msgstr ""

msgctxt "sn51.2:1.4"
msgid ""
"Idha, bhikkhave, bhikkhu chandasamādhippadhānasaṅkhārasamannāgataṃ "
"iddhipādaṃ bhāveti, "
msgstr ""

msgctxt "sn51.2:1.5"
msgid "vīriyasamādhi … pe …"
msgstr ""

msgctxt "sn51.2:1.6"
msgid "cittasamādhi … pe …"
msgstr ""

msgctxt "sn51.2:1.7"
msgid "vīmaṃsāsamādhippadhānasaṅkhārasamannāgataṃ iddhipādaṃ bhāveti."
msgstr ""

msgctxt "sn51.2:1.8"
msgid ""
"Yesaṃ kesañci, bhikkhave, ime cattāro iddhipādā viraddhā, viraddho tesaṃ "
"ariyo maggo sammā dukkhakkhayagāmī."
msgstr ""

msgctxt "sn51.2:1.9"
msgid ""
"Yesaṃ kesañci, bhikkhave, ime cattāro iddhipādā āraddhā, āraddho tesaṃ ariyo "
"maggo sammā dukkhakkhayagāmī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn51.2:1.10"
msgid "Dutiyaṃ."
msgstr ""
