#. <section class="sutta" id="sn1.80"><article><div class="hgroup"><p class="division">
msgctxt "sn1.80:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.80:0.2"
msgid "8. Chetvāvagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.80:0.3"
msgid "80. Pajjotasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.80:1.1"
msgid "“Kiṃsu lokasmi pajjoto,"
msgstr ""

#. <br>
msgctxt "sn1.80:1.2"
msgid "kiṃsu lokasmi jāgaro;"
msgstr ""

#. <br>
msgctxt "sn1.80:1.3"
msgid "Kiṃsu kamme sajīvānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.80:1.4"
msgid "kimassa iriyāpatho."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.101"></a>
#. <a class="sc" id="sc2"></a>
#. VAR: alasaṃ analasañca → kiṃ ālasyānālasyañca (mr)
msgctxt "sn1.80:2.1"
msgid "Kiṃsu alasaṃ analasañca,"
msgstr ""

#. <br>
msgctxt "sn1.80:2.2"
msgid "Mātā puttaṃva posati;"
msgstr ""

#. <br>
msgctxt "sn1.80:2.3"
msgid "Kiṃ bhūtā upajīvanti,"
msgstr ""

#. <br>
msgctxt "sn1.80:2.4"
msgid "Ye pāṇā pathavissitā”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.102"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.80:3.1"
msgid "“Paññā lokasmi pajjoto,"
msgstr ""

#. <br>
msgctxt "sn1.80:3.2"
msgid "sati lokasmi jāgaro;"
msgstr ""

#. <br>
msgctxt "sn1.80:3.3"
msgid "Gāvo kamme sajīvānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.80:3.4"
msgid "sītassa iriyāpatho."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.80:4.1"
msgid "Vuṭṭhi alasaṃ analasañca,"
msgstr ""

#. <br>
msgctxt "sn1.80:4.2"
msgid "Mātā puttaṃva posati;"
msgstr ""

#. <br>
msgctxt "sn1.80:4.3"
msgid "Vuṭṭhiṃ bhūtā upajīvanti,"
msgstr ""

#. <br>
msgctxt "sn1.80:4.4"
msgid "Ye pāṇā pathavissitā”ti."
msgstr ""
