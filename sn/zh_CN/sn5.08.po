#. <section class="sutta" id="sn5.8"><article><div class="hgroup"><p class="division">
msgctxt "sn5.8:0.1"
msgid "Saṃyutta Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "sn5.8:0.2"
msgid "1. Bhikkhunīvagga"
msgstr ""

#. </p><h1>
msgctxt "sn5.8:0.3"
msgid "8. Sīsupacālāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts2ed" id="pts2ed1.293"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn5.8:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

#. VAR: sīsupacālā → sīsūpacālā (bj)
msgctxt "sn5.8:1.2"
msgid "Atha kho sīsupacālā bhikkhunī pubbaṇhasamayaṃ nivāsetvā … pe …"
msgstr ""

msgctxt "sn5.8:1.3"
msgid "aññatarasmiṃ rukkhamūle divāvihāraṃ nisīdi."
msgstr ""

msgctxt "sn5.8:1.4"
msgid ""
"Atha kho māro pāpimā yena sīsupacālā bhikkhunī tenupasaṅkami; upasaṅkamitvā "
"sīsupacālaṃ bhikkhuniṃ etadavoca:"
msgstr ""

msgctxt "sn5.8:1.5"
msgid "“kassa nu tvaṃ, bhikkhuni, pāsaṇḍaṃ rocesī”ti?"
msgstr ""

msgctxt "sn5.8:1.6"
msgid "“Na khvāhaṃ, āvuso, kassaci pāsaṇḍaṃ rocemī”ti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn5.8:2.1"
msgid "“Kaṃ nu uddissa muṇḍāsi,"
msgstr ""

#. <br>
msgctxt "sn5.8:2.2"
msgid "Samaṇī viya dissasi;"
msgstr ""

#. <br>
msgctxt "sn5.8:2.3"
msgid "Na ca rocesi pāsaṇḍaṃ,"
msgstr ""

#. <br>
msgctxt "sn5.8:2.4"
msgid "Kimiva carasi momūhā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn5.8:3.1"
msgid "“Ito bahiddhā pāsaṇḍā,"
msgstr ""

#. <br>
msgctxt "sn5.8:3.2"
msgid "diṭṭhīsu pasīdanti te;"
msgstr ""

#. <br>
msgctxt "sn5.8:3.3"
msgid "Na tesaṃ dhammaṃ rocemi,"
msgstr ""

#. <br>
msgctxt "sn5.8:3.4"
msgid "te dhammassa akovidā."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.134"></a>
#. <a class="pts2ed" id="pts2ed1.294"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn5.8:4.1"
msgid "Atthi sakyakule jāto,"
msgstr ""

#. <br>
msgctxt "sn5.8:4.2"
msgid "buddho appaṭipuggalo;"
msgstr ""

#. <br>
msgctxt "sn5.8:4.3"
msgid "Sabbābhibhū māranudo,"
msgstr ""

#. <br>
msgctxt "sn5.8:4.4"
msgid "sabbatthamaparājito."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn5.8:5.1"
msgid "Sabbattha mutto asito,"
msgstr ""

#. <br>
msgctxt "sn5.8:5.2"
msgid "sabbaṃ passati cakkhumā;"
msgstr ""

#. <br>
msgctxt "sn5.8:5.3"
msgid "Sabbakammakkhayaṃ patto,"
msgstr ""

#. <br>
msgctxt "sn5.8:5.4"
msgid "vimutto upadhisaṅkhaye;"
msgstr ""

#. <br>
msgctxt "sn5.8:5.5"
msgid "So mayhaṃ bhagavā satthā,"
msgstr ""

#. <br>
msgctxt "sn5.8:5.6"
msgid "tassa rocemi sāsanan”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn5.8:6.1"
msgid ""
"Atha kho māro pāpimā “jānāti maṃ sīsupacālā bhikkhunī”ti dukkhī dummano "
"tatthevantaradhāyīti."
msgstr ""
