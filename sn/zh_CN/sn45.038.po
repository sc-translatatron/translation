#. <section class="sutta" id="sn45.38"><article><div class="hgroup"><p class="division">
msgctxt "sn45.38:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.38:0.2"
msgid "4. Paṭipattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.38:0.3"
msgid "38. Dutiyabrahmaññasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.38:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.38:1.2"
msgid "“Brahmaññañca vo, bhikkhave, desessāmi, brahmaññatthañca."
msgstr ""

msgctxt "sn45.38:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn45.38:1.4"
msgid "Katamañca, bhikkhave, brahmaññaṃ?"
msgstr ""

msgctxt "sn45.38:1.5"
msgid "Ayameva ariyo aṭṭhaṅgiko maggo, seyyathidaṃ—"
msgstr ""

msgctxt "sn45.38:1.6"
msgid "sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

msgctxt "sn45.38:1.7"
msgid "Idaṃ vuccati, bhikkhave, brahmaññaṃ."
msgstr ""

msgctxt "sn45.38:1.8"
msgid "Katamo ca, bhikkhave, brahmaññattho?"
msgstr ""

msgctxt "sn45.38:1.9"
msgid "Yo kho, bhikkhave, rāgakkhayo dosakkhayo mohakkhayo—"
msgstr ""

msgctxt "sn45.38:1.10"
msgid "ayaṃ vuccati, bhikkhave, brahmaññattho”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.38:1.11"
msgid "Aṭṭhamaṃ."
msgstr ""
