#. <section class="sutta" id="sn14.28"><article><div class="hgroup"><p class="division">
msgctxt "sn14.28:0.1"
msgid "Saṃyutta Nikāya 14"
msgstr ""

#. </p><p>
msgctxt "sn14.28:0.2"
msgid "3. Kammapathavagga"
msgstr ""

#. </p><h1>
msgctxt "sn14.28:0.3"
msgid "28. Aṭṭhaṅgikasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn14.28:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn14.28:1.2"
msgid "“Dhātusova, bhikkhave, sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.28:1.3"
msgid "Micchādiṭṭhikā micchādiṭṭhikehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.28:1.4"
msgid "micchāsaṅkappā … pe …"
msgstr ""

msgctxt "sn14.28:1.5"
msgid "micchāvācā …"
msgstr ""

msgctxt "sn14.28:1.6"
msgid "micchākammantā …"
msgstr ""

msgctxt "sn14.28:1.7"
msgid "micchāājīvā …"
msgstr ""

msgctxt "sn14.28:1.8"
msgid "micchāvāyāmā …"
msgstr ""

msgctxt "sn14.28:1.9"
msgid "micchāsatino …"
msgstr ""

msgctxt "sn14.28:1.10"
msgid "micchāsamādhino micchāsamādhīhi saddhiṃ saṃsandanti samenti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn14.28:2.1"
msgid "Sammādiṭṭhikā sammādiṭṭhikehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.28:2.2"
msgid "sammāsaṅkappā … pe …"
msgstr ""

msgctxt "sn14.28:2.3"
msgid "sammāvācā …"
msgstr ""

msgctxt "sn14.28:2.4"
msgid "sammākammantā …"
msgstr ""

msgctxt "sn14.28:2.5"
msgid "sammāājīvā …"
msgstr ""

msgctxt "sn14.28:2.6"
msgid "sammāvāyāmā …"
msgstr ""

msgctxt "sn14.28:2.7"
msgid "sammāsatino …"
msgstr ""

msgctxt "sn14.28:2.8"
msgid "sammāsamādhino sammāsamādhīhi saddhiṃ saṃsandanti samentī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn14.28:2.9"
msgid "Chaṭṭhaṃ."
msgstr ""
