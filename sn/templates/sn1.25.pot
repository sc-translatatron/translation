#. <section class="sutta" id="sn1.25"><article><div class="hgroup"><p class="division">
msgctxt "sn1.25:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.25:0.2"
msgid "3. Sattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.25:0.3"
msgid "25. Arahantasutta"
msgstr ""

# Katavi seems to be used in the EBTs only here and MN 82.
#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.25:1.1"
msgid "“Yo hoti bhikkhu arahaṃ katāvī,"
msgstr ""

#. <br>
msgctxt "sn1.25:1.2"
msgid "Khīṇāsavo antimadehadhārī;"
msgstr ""

#. <br>
msgctxt "sn1.25:1.3"
msgid "Ahaṃ vadāmītipi so vadeyya,"
msgstr ""

#. <br>
msgctxt "sn1.25:1.4"
msgid "Mamaṃ vadantītipi so vadeyyā”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.31"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.25:2.1"
msgid "“Yo hoti bhikkhu arahaṃ katāvī,"
msgstr ""

#. <br>
msgctxt "sn1.25:2.2"
msgid "Khīṇāsavo antimadehadhārī;"
msgstr ""

#. <br>
msgctxt "sn1.25:2.3"
msgid "Ahaṃ vadāmītipi so vadeyya,"
msgstr ""

#. <br>
msgctxt "sn1.25:2.4"
msgid "Mamaṃ vadantītipi so vadeyya;"
msgstr ""

#. <br>
msgctxt "sn1.25:2.5"
msgid "Loke samaññaṃ kusalo viditvā,"
msgstr ""

#. <br>
#. VAR: so → sa (pts2)
msgctxt "sn1.25:2.6"
msgid "Vohāramattena so vohareyyā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.25:3.1"
msgid "“Yo hoti bhikkhu arahaṃ katāvī,"
msgstr ""

#. <br>
msgctxt "sn1.25:3.2"
msgid "Khīṇāsavo antimadehadhārī;"
msgstr ""

#. <br>
msgctxt "sn1.25:3.3"
msgid "Mānaṃ nu kho so upagamma bhikkhu,"
msgstr ""

#. <br>
msgctxt "sn1.25:3.4"
msgid "Ahaṃ vadāmītipi so vadeyya;"
msgstr ""

#. <br>
msgctxt "sn1.25:3.5"
msgid "Mamaṃ vadantītipi so vadeyyā”ti."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.15"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.25:4.1"
msgid "“Pahīnamānassa na santi ganthā,"
msgstr ""

#. <br>
msgctxt "sn1.25:4.2"
msgid "Vidhūpitā mānaganthassa sabbe;"
msgstr ""

#. <br>
#. VAR: maññataṃ → maññanaṃ (bj) | yamataṃ (s1-3, pts1-2) | maññitaṃ (?)
msgctxt "sn1.25:4.3"
msgid "Sa vītivatto maññataṃ sumedho,"
msgstr ""

#. <br>
msgctxt "sn1.25:4.4"
msgid "Ahaṃ vadāmītipi so vadeyya."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.32"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn1.25:5.1"
msgid "Mamaṃ vadantītipi so vadeyya,"
msgstr ""

#. <br>
msgctxt "sn1.25:5.2"
msgid "Loke samaññaṃ kusalo viditvā;"
msgstr ""

#. <br>
msgctxt "sn1.25:5.3"
msgid "Vohāramattena so vohareyyā”ti."
msgstr ""
