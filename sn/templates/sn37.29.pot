#. <section class="sutta" id="sn37.29"><article><div class="hgroup"><p class="division">
msgctxt "sn37.29:0.1"
msgid "Saṃyutta Nikāya 37"
msgstr ""

#. </p><p>
msgctxt "sn37.29:0.2"
msgid "3. Balavagga"
msgstr ""

#. </p><h1>
msgctxt "sn37.29:0.3"
msgid "29. Aṅgasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.247"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn37.29:1.1"
msgid "“Pañcimāni, bhikkhave, mātugāmassa balāni."
msgstr ""

msgctxt "sn37.29:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "sn37.29:1.3"
msgid "Rūpabalaṃ, bhogabalaṃ, ñātibalaṃ, puttabalaṃ, sīlabalaṃ."
msgstr ""

msgctxt "sn37.29:1.4"
msgid "Rūpabalena ca, bhikkhave, mātugāmo samannāgato hoti, na ca bhogabalena—"
msgstr ""

msgctxt "sn37.29:1.5"
msgid "evaṃ so tenaṅgena aparipūro hoti."
msgstr ""

msgctxt "sn37.29:1.6"
msgid ""
"Yato ca kho, bhikkhave, mātugāmo rūpabalena ca samannāgato hoti, bhogabalena "
"ca—"
msgstr ""

msgctxt "sn37.29:1.7"
msgid "evaṃ so tenaṅgena paripūro hoti."
msgstr ""

msgctxt "sn37.29:1.8"
msgid ""
"Rūpabalena ca, bhikkhave, mātugāmo samannāgato hoti, bhogabalena ca, na ca "
"ñātibalena—"
msgstr ""

msgctxt "sn37.29:1.9"
msgid "evaṃ so tenaṅgena aparipūro hoti."
msgstr ""

msgctxt "sn37.29:1.10"
msgid ""
"Yato ca kho, bhikkhave, mātugāmo rūpabalena ca samannāgato hoti, bhogabalena "
"ca, ñātibalena ca—"
msgstr ""

msgctxt "sn37.29:1.11"
msgid "evaṃ so tenaṅgena paripūro hoti."
msgstr ""

msgctxt "sn37.29:1.12"
msgid ""
"Rūpabalena ca, bhikkhave, mātugāmo samannāgato hoti, bhogabalena ca, "
"ñātibalena ca, na ca puttabalena—"
msgstr ""

msgctxt "sn37.29:1.13"
msgid "evaṃ so tenaṅgena aparipūro hoti."
msgstr ""

msgctxt "sn37.29:1.14"
msgid ""
"Yato ca kho, bhikkhave, mātugāmo rūpabalena ca samannāgato hoti, bhogabalena "
"ca, ñātibalena ca, puttabalena ca—"
msgstr ""

msgctxt "sn37.29:1.15"
msgid "evaṃ so tenaṅgena paripūro hoti."
msgstr ""

msgctxt "sn37.29:1.16"
msgid ""
"Rūpabalena ca, bhikkhave, mātugāmo samannāgato hoti, bhogabalena ca, "
"ñātibalena ca, puttabalena ca, na ca sīlabalena—"
msgstr ""

msgctxt "sn37.29:1.17"
msgid "evaṃ so tenaṅgena aparipūro hoti."
msgstr ""

msgctxt "sn37.29:1.18"
msgid ""
"Yato ca kho, bhikkhave, mātugāmo rūpabalena ca samannāgato hoti, bhogabalena "
"ca, ñātibalena ca, puttabalena ca, sīlabalena ca—"
msgstr ""

msgctxt "sn37.29:1.19"
msgid "evaṃ so tenaṅgena paripūro hoti."
msgstr ""

msgctxt "sn37.29:1.20"
msgid "Imāni kho, bhikkhave, pañca mātugāmassa balānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn37.29:1.21"
msgid "Pañcamaṃ."
msgstr ""
