#. <section class="sutta" id="sn21.11"><article><div class="hgroup"><p class="division">
msgctxt "sn21.11:0.1"
msgid "Saṃyutta Nikāya 21"
msgstr ""

#. </p><p>
msgctxt "sn21.11:0.2"
msgid "1. Bhikkhuvagga"
msgstr ""

#. </p><h1>
#. VAR: 11. Mahākappinasutta → kappino (pts1-2)
msgctxt "sn21.11:0.3"
msgid "11. Mahākappinasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn21.11:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn21.11:1.2"
msgid "Atha kho āyasmā mahākappino yena bhagavā tenupasaṅkami."
msgstr ""

msgctxt "sn21.11:1.3"
msgid "Addasā kho bhagavā āyasmantaṃ mahākappinaṃ dūratova āgacchantaṃ."
msgstr ""

msgctxt "sn21.11:1.4"
msgid "Disvāna bhikkhū āmantesi:"
msgstr ""

msgctxt "sn21.11:1.5"
msgid ""
"“passatha no tumhe, bhikkhave, etaṃ bhikkhuṃ āgacchantaṃ odātakaṃ tanukaṃ "
"tuṅganāsikan”ti?"
msgstr ""

msgctxt "sn21.11:1.6"
msgid "“Evaṃ, bhante”."
msgstr ""

msgctxt "sn21.11:1.7"
msgid "“Eso kho, bhikkhave, bhikkhu mahiddhiko mahānubhāvo."
msgstr ""

msgctxt "sn21.11:1.8"
msgid "Na ca sā samāpatti sulabharūpā yā tena bhikkhunā asamāpannapubbā."
msgstr ""

msgctxt "sn21.11:1.9"
msgid ""
"Yassa catthāya kulaputtā sammadeva agārasmā anagāriyaṃ pabbajanti "
"tadanuttaraṃ brahmacariyapariyosānaṃ diṭṭheva dhamme sayaṃ abhiññā "
"sacchikatvā upasampajja viharatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn21.11:2.1"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "sn21.11:2.2"
msgid "Idaṃ vatvāna sugato athāparaṃ etadavoca satthā:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn21.11:3.1"
msgid "“Khattiyo seṭṭho janetasmiṃ,"
msgstr ""

#. <br>
msgctxt "sn21.11:3.2"
msgid "ye gottapaṭisārino;"
msgstr ""

#. <br>
msgctxt "sn21.11:3.3"
msgid "Vijjācaraṇasampanno,"
msgstr ""

#. <br>
msgctxt "sn21.11:3.4"
msgid "so seṭṭho devamānuse."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn21.11:4.1"
msgid "Divā tapati ādicco,"
msgstr ""

#. <br>
msgctxt "sn21.11:4.2"
msgid "rattimābhāti candimā;"
msgstr ""

#. <br>
msgctxt "sn21.11:4.3"
msgid "Sannaddho khattiyo tapati,"
msgstr ""

#. <br>
msgctxt "sn21.11:4.4"
msgid "jhāyī tapati brāhmaṇo;"
msgstr ""

#. <br>
#. VAR: Atha sabbamahorattiṃ → atha sabbamahorattaṃ (bj, s1-3, km)
msgctxt "sn21.11:4.5"
msgid "Atha sabbamahorattiṃ,"
msgstr ""

#. <br>
msgctxt "sn21.11:4.6"
msgid "buddho tapati tejasā”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn21.11:4.7"
msgid "Ekādasamaṃ."
msgstr ""
