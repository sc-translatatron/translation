#. <section class="sutta" id="sn45.110"><article><div class="hgroup"><p class="division">
msgctxt "sn45.110-114:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.110-114:0.2"
msgid "10. Dutiyagaṅgāpeyyālavagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.110-114:0.3"
msgid "110–​114. Dutiyādisamuddaninnasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.110-114:1.1"
msgid ""
"“Seyyathāpi, bhikkhave, yamunā nadī samuddaninnā samuddapoṇā samuddapabbhārā;"
msgstr ""

msgctxt "sn45.110-114:1.2"
msgid "evameva kho, bhikkhave, bhikkhu … pe …"
msgstr ""

msgctxt "sn45.110-114:1.3"
msgid "dutiyaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn45.110-114:2.1"
msgid ""
"“Seyyathāpi, bhikkhave, aciravatī nadī samuddaninnā samuddapoṇā "
"samuddapabbhārā;"
msgstr ""

msgctxt "sn45.110-114:2.2"
msgid "evameva kho, bhikkhave, bhikkhu … pe …"
msgstr ""

msgctxt "sn45.110-114:2.3"
msgid "tatiyaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn45.110-114:3.1"
msgid ""
"“Seyyathāpi, bhikkhave, sarabhū nadī samuddaninnā samuddapoṇā "
"samuddapabbhārā;"
msgstr ""

msgctxt "sn45.110-114:3.2"
msgid "evameva kho, bhikkhave, bhikkhu … pe …"
msgstr ""

msgctxt "sn45.110-114:3.3"
msgid "catutthaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn45.110-114:4.1"
msgid ""
"“Seyyathāpi, bhikkhave, mahī nadī samuddaninnā samuddapoṇā samuddapabbhārā;"
msgstr ""

msgctxt "sn45.110-114:4.2"
msgid "evameva kho, bhikkhave, bhikkhu … pe …"
msgstr ""

msgctxt "sn45.110-114:4.3"
msgid "pañcamaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn45.110-114:5.1"
msgid "“Seyyathāpi, bhikkhave, yā kācimā mahānadiyo, seyyathidaṃ—"
msgstr ""

msgctxt "sn45.110-114:5.2"
msgid ""
"gaṅgā, yamunā, aciravatī, sarabhū, mahī, sabbā tā samuddaninnā samuddapoṇā "
"samuddapabbhārā;"
msgstr ""

msgctxt "sn45.110-114:5.3"
msgid ""
"evameva kho, bhikkhave, bhikkhu ariyaṃ aṭṭhaṅgikaṃ maggaṃ bhāvento ariyaṃ "
"aṭṭhaṅgikaṃ maggaṃ bahulīkaronto nibbānaninno hoti nibbānapoṇo "
"nibbānapabbhāro."
msgstr ""

msgctxt "sn45.110-114:5.4"
msgid ""
"Kathañca, bhikkhave, bhikkhu ariyaṃ aṭṭhaṅgikaṃ maggaṃ bhāvento ariyaṃ "
"aṭṭhaṅgikaṃ maggaṃ bahulīkaronto nibbānaninno hoti nibbānapoṇo "
"nibbānapabbhāro?"
msgstr ""

msgctxt "sn45.110-114:5.5"
msgid ""
"Idha, bhikkhave, bhikkhu sammādiṭṭhiṃ bhāveti rāgavinayapariyosānaṃ "
"dosavinayapariyosānaṃ mohavinayapariyosānaṃ … pe … sammāsamādhiṃ bhāveti "
"rāgavinayapariyosānaṃ dosavinayapariyosānaṃ mohavinayapariyosānaṃ."
msgstr ""

msgctxt "sn45.110-114:5.6"
msgid ""
"Evaṃ kho, bhikkhave, bhikkhu ariyaṃ aṭṭhaṅgikaṃ maggaṃ bhāvento ariyaṃ "
"aṭṭhaṅgikaṃ maggaṃ bahulīkaronto nibbānaninno hoti nibbānapoṇo "
"nibbānapabbhāro”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.110-114:5.7"
msgid "Chaṭṭhaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.110-114:5.8"
msgid "(Rāgavinayadvādasakī dutiyakī samuddaninnanti.)"
msgstr ""
