#. <section class="sutta" id="sn35.180"><article><div class="hgroup"><p class="division">
msgctxt "sn35.180-182:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.180-182:0.2"
msgid "17. Saṭṭhipeyyālavagga"
msgstr ""

#. </p><h1>
msgctxt "sn35.180-182:0.3"
msgid "180–​182. Bāhiradukkhachandādisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.180-182:1.1"
msgid ""
"“Yaṃ, bhikkhave, dukkhaṃ, tatra vo chando pahātabbo, rāgo pahātabbo, "
"chandarāgo pahātabbo."
msgstr ""

msgctxt "sn35.180-182:1.2"
msgid "Kiñca, bhikkhave, dukkhaṃ?"
msgstr ""

msgctxt "sn35.180-182:1.3"
msgid ""
"Rūpā, bhikkhave, dukkhā; tatra vo chando pahātabbo, rāgo pahātabbo, "
"chandarāgo pahātabbo."
msgstr ""

msgctxt "sn35.180-182:1.4"
msgid "Saddā …"
msgstr ""

msgctxt "sn35.180-182:1.5"
msgid "gandhā …"
msgstr ""

msgctxt "sn35.180-182:1.6"
msgid "rasā …"
msgstr ""

msgctxt "sn35.180-182:1.7"
msgid "phoṭṭhabbā …"
msgstr ""

msgctxt "sn35.180-182:1.8"
msgid ""
"dhammā dukkhā; tatra vo chando pahātabbo, rāgo pahātabbo, chandarāgo "
"pahātabbo."
msgstr ""

msgctxt "sn35.180-182:1.9"
msgid ""
"Yaṃ, bhikkhave, dukkhaṃ, tatra vo chando pahātabbo, rāgo pahātabbo, "
"chandarāgo pahātabbo”ti."
msgstr ""
