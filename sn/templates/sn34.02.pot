#. <section class="sutta" id="sn34.2"><article><div class="hgroup"><p class="division">
msgctxt "sn34.2:0.1"
msgid "Saṃyutta Nikāya 34"
msgstr ""

#. </p><p>
msgctxt "sn34.2:0.2"
msgid "1. Jhānavagga"
msgstr ""

#. </p><h1>
#. VAR: 2. Samādhimūlakaṭhitisutta → ṭhitikusalasuttaṃ (bj) | ṭhiti (pts1)
msgctxt "sn34.2:0.3"
msgid "2. Samādhimūlakaṭhitisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.265"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn34.2:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn34.2:1.2"
msgid "“Cattārome, bhikkhave, jhāyī."
msgstr ""

msgctxt "sn34.2:1.3"
msgid "Katame cattāro?"
msgstr ""

msgctxt "sn34.2:1.4"
msgid ""
"Idha, bhikkhave, ekacco jhāyī samādhismiṃ samādhikusalo hoti, na samādhismiṃ "
"ṭhitikusalo."
msgstr ""

msgctxt "sn34.2:1.5"
msgid ""
"Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ ṭhitikusalo hoti, na "
"samādhismiṃ samādhikusalo."
msgstr ""

msgctxt "sn34.2:1.6"
msgid ""
"Idha pana, bhikkhave, ekacco jhāyī neva samādhismiṃ samādhikusalo hoti, na "
"ca samādhismiṃ ṭhitikusalo."
msgstr ""

msgctxt "sn34.2:1.7"
msgid ""
"Idha pana, bhikkhave, ekacco jhāyī samādhismiṃ samādhikusalo ca hoti, "
"samādhismiṃ ṭhitikusalo ca."
msgstr ""

msgctxt "sn34.2:1.8"
msgid ""
"Tatra, bhikkhave, yvāyaṃ jhāyī samādhismiṃ samādhikusalo ca hoti samādhismiṃ "
"ṭhitikusalo ca ayaṃ imesaṃ catunnaṃ jhāyīnaṃ aggo ca seṭṭho ca mokkho ca "
"uttamo ca pavaro ca."
msgstr ""

msgctxt "sn34.2:1.9"
msgid ""
"Seyyathāpi, bhikkhave, gavā khīraṃ, khīramhā dadhi, dadhimhā navanītaṃ, "
"navanītamhā sappi, sappimhā sappimaṇḍo tatra aggamakkhāyati;"
msgstr ""

msgctxt "sn34.2:1.10"
msgid ""
"evameva kho, bhikkhave, yvāyaṃ jhāyī samādhismiṃ samādhikusalo ca hoti "
"samādhismiṃ ṭhitikusalo ca ayaṃ imesaṃ catunnaṃ jhāyīnaṃ aggo ca seṭṭho ca "
"mokkho ca uttamo ca pavaro cā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn34.2:1.11"
msgid "Dutiyaṃ."
msgstr ""
