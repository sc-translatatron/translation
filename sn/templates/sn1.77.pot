#. <section class="sutta" id="sn1.77"><article><div class="hgroup"><p class="division">
msgctxt "sn1.77:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.77:0.2"
msgid "8. Chetvāvagga"
msgstr ""

#. </p><h1>
#. VAR: 77. Issariyasutta → issarasuttaṃ (bj, s1-3) | issaraṃ (pts1)
msgctxt "sn1.77:0.3"
msgid "77. Issariyasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.77:1.1"
msgid "“Kiṃsu issariyaṃ loke,"
msgstr ""

#. <br>
msgctxt "sn1.77:1.2"
msgid "kiṃsu bhaṇḍānamuttamaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.77:1.3"
msgid "Kiṃsu satthamalaṃ loke,"
msgstr ""

#. <br>
msgctxt "sn1.77:1.4"
msgid "kiṃsu lokasmimabbudaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.77:2.1"
msgid "Kiṃsu harantaṃ vārenti,"
msgstr ""

#. <br>
msgctxt "sn1.77:2.2"
msgid "haranto pana ko piyo;"
msgstr ""

#. <br>
msgctxt "sn1.77:2.3"
msgid "Kiṃsu punappunāyantaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.77:2.4"
msgid "abhinandanti paṇḍitā”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.99"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.77:3.1"
msgid "“Vaso issariyaṃ loke,"
msgstr ""

#. <br>
msgctxt "sn1.77:3.2"
msgid "itthī bhaṇḍānamuttamaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.77:3.3"
msgid "Kodho satthamalaṃ loke,"
msgstr ""

#. <br>
msgctxt "sn1.77:3.4"
msgid "corā lokasmimabbudā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.77:4.1"
msgid "Coraṃ harantaṃ vārenti,"
msgstr ""

#. <br>
msgctxt "sn1.77:4.2"
msgid "haranto samaṇo piyo;"
msgstr ""

#. <br>
msgctxt "sn1.77:4.3"
msgid "Samaṇaṃ punappunāyantaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.77:4.4"
msgid "abhinandanti paṇḍitā”ti."
msgstr ""
