#. <section class="sutta" id="sn21.12"><article><div class="hgroup"><p class="division">
msgctxt "sn21.12:0.1"
msgid "Saṃyutta Nikāya 21"
msgstr ""

#. </p><p>
msgctxt "sn21.12:0.2"
msgid "1. Bhikkhuvagga"
msgstr ""

#. </p><h1>
#. VAR: 12. Sahāyakasutta → sahāya (pts1-2)
msgctxt "sn21.12:0.3"
msgid "12. Sahāyakasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.285"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn21.12:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn21.12:1.2"
msgid ""
"Atha kho dve bhikkhū sahāyakā āyasmato mahākappinassa saddhivihārino yena "
"bhagavā tenupasaṅkamiṃsu."
msgstr ""

msgctxt "sn21.12:1.3"
msgid "Addasā kho bhagavā te bhikkhū dūratova āgacchante."
msgstr ""

msgctxt "sn21.12:1.4"
msgid "Disvāna bhikkhū āmantesi:"
msgstr ""

msgctxt "sn21.12:1.5"
msgid ""
"“passatha no tumhe, bhikkhave, ete bhikkhū sahāyake āgacchante kappinassa "
"saddhivihārino”ti?"
msgstr ""

msgctxt "sn21.12:1.6"
msgid "“Evaṃ, bhante”."
msgstr ""

msgctxt "sn21.12:1.7"
msgid "“Ete kho te bhikkhū mahiddhikā mahānubhāvā."
msgstr ""

msgctxt "sn21.12:1.8"
msgid "Na ca sā samāpatti sulabharūpā, yā tehi bhikkhūhi asamāpannapubbā."
msgstr ""

msgctxt "sn21.12:1.9"
msgid ""
"Yassa catthāya kulaputtā sammadeva agārasmā anagāriyaṃ pabbajanti "
"tadanuttaraṃ brahmacariyapariyosānaṃ diṭṭheva dhamme sayaṃ abhiññā "
"sacchikatvā upasampajja viharantī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn21.12:2.1"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "sn21.12:2.2"
msgid "Idaṃ vatvāna sugato athāparaṃ etadavoca satthā:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn21.12:3.1"
msgid "“Sahāyāvatime bhikkhū,"
msgstr ""

#. <br>
msgctxt "sn21.12:3.2"
msgid "cirarattaṃ sametikā;"
msgstr ""

#. <br>
msgctxt "sn21.12:3.3"
msgid "Sameti nesaṃ saddhammo,"
msgstr ""

#. <br>
msgctxt "sn21.12:3.4"
msgid "dhamme buddhappavedite."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn21.12:4.1"
msgid "Suvinītā kappinena,"
msgstr ""

#. <br>
msgctxt "sn21.12:4.2"
msgid "dhamme ariyappavedite;"
msgstr ""

#. <br>
msgctxt "sn21.12:4.3"
msgid "Dhārenti antimaṃ dehaṃ,"
msgstr ""

#. <br>
msgctxt "sn21.12:4.4"
msgid "jetvā māraṃ savāhinin”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn21.12:4.5"
msgid "Dvādasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn21.12:4.6"
msgid "Bhikkhuvaggo paṭhamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.286"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn21.12:5.1"
msgid "Kolito upatisso ca,"
msgstr ""

#. <br>
msgctxt "sn21.12:5.2"
msgid "ghaṭo cāpi pavuccati;"
msgstr ""

#. <br>
msgctxt "sn21.12:5.3"
msgid "Navo sujāto bhaddi ca,"
msgstr ""

#. <br>
msgctxt "sn21.12:5.4"
msgid "visākho nando tisso ca;"
msgstr ""

#. <br>
msgctxt "sn21.12:5.5"
msgid "Theranāmo ca kappino,"
msgstr ""

#. <br>
msgctxt "sn21.12:5.6"
msgid "sahāyena ca dvādasāti."
msgstr ""

#. </p></blockquote><p class="endsutta">
msgctxt "sn21.12:5.7"
msgid "Bhikkhusaṃyuttaṃ samattaṃ."
msgstr ""

#. </p><p class="endsutta">
msgctxt "sn21.12:5.8"
msgid "Nidānavaggo dutiyo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn21.12:6.1"
msgid "Nidānābhisamayadhātu,"
msgstr ""

#. <br>
msgctxt "sn21.12:6.2"
msgid "anamataggena kassapaṃ;"
msgstr ""

#. <br>
msgctxt "sn21.12:6.3"
msgid "Sakkārarāhulalakkhaṇo,"
msgstr ""

#. <br>
msgctxt "sn21.12:6.4"
msgid "opammabhikkhunā vaggo."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn21.12:6.5"
msgid "Dutiyo tena pavuccatīti."
msgstr ""

#. </p><p class="endbook">
msgctxt "sn21.12:6.6"
msgid "Nidānavaggasaṃyuttapāḷi niṭṭhitā."
msgstr ""
