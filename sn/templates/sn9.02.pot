#. <section class="sutta" id="sn9.2"><article><div class="hgroup"><p class="division">
msgctxt "sn9.2:0.1"
msgid "Saṃyutta Nikāya 9"
msgstr ""

#. </p><p>
msgctxt "sn9.2:0.2"
msgid "1. Vanavagga"
msgstr ""

# BB has “rousing”, evidently reading utthana, which makes sense.
#. </p><h1>
msgctxt "sn9.2:0.3"
msgid "2. Upaṭṭhānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.198"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn9.2:1.1"
msgid "Ekaṃ samayaṃ aññataro bhikkhu kosalesu viharati aññatarasmiṃ vanasaṇḍe."
msgstr ""

msgctxt "sn9.2:1.2"
msgid "Tena kho pana samayena so bhikkhu divāvihāragato supati."
msgstr ""

msgctxt "sn9.2:1.3"
msgid ""
"Atha kho yā tasmiṃ vanasaṇḍe adhivatthā devatā tassa bhikkhuno anukampikā "
"atthakāmā taṃ bhikkhuṃ saṃvejetukāmā yena so bhikkhu tenupasaṅkami; "
"upasaṅkamitvā taṃ bhikkhuṃ gāthāhi ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.426"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn9.2:2.1"
msgid "“Uṭṭhehi bhikkhu kiṃ sesi,"
msgstr ""

#. <br>
#. VAR: supitena → supinena (bj, pts2)
msgctxt "sn9.2:2.2"
msgid "ko attho supitena te;"
msgstr ""

#. <br>
msgctxt "sn9.2:2.3"
msgid "Āturassa hi kā niddā,"
msgstr ""

#. <br>
msgctxt "sn9.2:2.4"
msgid "sallaviddhassa ruppato."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
#. VAR: Yāya saddhāya pabbajito → yāya saddhāpabbajito (bj, s1-3, km, pts2)
msgctxt "sn9.2:3.1"
msgid "Yāya saddhāya pabbajito,"
msgstr ""

#. <br>
msgctxt "sn9.2:3.2"
msgid "agārasmānagāriyaṃ;"
msgstr ""

#. <br>
msgctxt "sn9.2:3.3"
msgid "Tameva saddhaṃ brūhehi,"
msgstr ""

#. <br>
msgctxt "sn9.2:3.4"
msgid "mā niddāya vasaṃ gamī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn9.2:4.1"
msgid "“Aniccā addhuvā kāmā,"
msgstr ""

#. <br>
msgctxt "sn9.2:4.2"
msgid "yesu mandova mucchito;"
msgstr ""

#. <br>
#. VAR: Baddhesu → khandhesu (bj) | bandhesu (pts1-2)
msgctxt "sn9.2:4.3"
msgid "Baddhesu muttaṃ asitaṃ,"
msgstr ""

#. <br>
msgctxt "sn9.2:4.4"
msgid "kasmā pabbajitaṃ tape."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn9.2:5.1"
msgid "Chandarāgassa vinayā,"
msgstr ""

#. <br>
msgctxt "sn9.2:5.2"
msgid "avijjāsamatikkamā;"
msgstr ""

# BB seems to omit parama here, I’m not sure why.
#. <br>
#. VAR: paramodānaṃ → paramavodānaṃ (bj) | pariyodātaṃ (si, pts1-2) | paramodātaṃ (s1-3, km)
msgctxt "sn9.2:5.3"
msgid "Taṃ ñāṇaṃ paramodānaṃ,"
msgstr ""

#. <br>
msgctxt "sn9.2:5.4"
msgid "kasmā pabbajitaṃ tape."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.427"></a>
#. <a class="sc" id="sc6"></a>
#. VAR: Chetvā → bhetvā (bj, s1-3, km, pts1-2)
msgctxt "sn9.2:6.1"
msgid "Chetvā avijjaṃ vijjāya,"
msgstr ""

#. <br>
msgctxt "sn9.2:6.2"
msgid "āsavānaṃ parikkhayā;"
msgstr ""

#. <br>
msgctxt "sn9.2:6.3"
msgid "Asokaṃ anupāyāsaṃ,"
msgstr ""

#. <br>
msgctxt "sn9.2:6.4"
msgid "kasmā pabbajitaṃ tape."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn9.2:7.1"
msgid "Āraddhavīriyaṃ pahitattaṃ,"
msgstr ""

#. <br>
msgctxt "sn9.2:7.2"
msgid "Niccaṃ daḷhaparakkamaṃ;"
msgstr ""

#. <br>
msgctxt "sn9.2:7.3"
msgid "Nibbānaṃ abhikaṅkhantaṃ,"
msgstr ""

#. <br>
msgctxt "sn9.2:7.4"
msgid "Kasmā pabbajitaṃ tape”ti."
msgstr ""
