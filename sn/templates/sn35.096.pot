#. <section class="sutta" id="sn35.96"><article><div class="hgroup"><p class="division">
msgctxt "sn35.96:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.96:0.2"
msgid "10. Saḷavagga"
msgstr ""

#. </p><h1>
msgctxt "sn35.96:0.3"
msgid "96. Parihānadhammasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.96:1.1"
msgid ""
"“Parihānadhammañca vo, bhikkhave, desessāmi aparihānadhammañca cha ca "
"abhibhāyatanāni."
msgstr ""

msgctxt "sn35.96:1.2"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn35.96:1.3"
msgid "Kathañca, bhikkhave, parihānadhammo hoti?"
msgstr ""

#. VAR: akusalā sarasaṅkappā → akusalā dhammā sarasaṅkappā (s1-3, km, pts1, mr)
msgctxt "sn35.96:1.4"
msgid ""
"Idha, bhikkhave, bhikkhuno cakkhunā rūpaṃ disvā uppajjanti pāpakā akusalā "
"sarasaṅkappā saṃyojaniyā."
msgstr ""

#. VAR: byantīkaroti → vyantī karoti (bj) | vyantikaroti (pts1) | byantiṃ karoti (mr)
msgctxt "sn35.96:1.5"
msgid ""
"Tañce bhikkhu adhivāseti nappajahati na vinodeti na byantīkaroti na "
"anabhāvaṃ gameti, veditabbametaṃ, bhikkhave, bhikkhunā:"
msgstr ""

msgctxt "sn35.96:1.6"
msgid "‘parihāyāmi kusalehi dhammehi’."
msgstr ""

msgctxt "sn35.96:1.7"
msgid "Parihānañhetaṃ vuttaṃ bhagavatāti … pe …."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.77"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.96:2.1"
msgid ""
"Puna caparaṃ, bhikkhave, bhikkhuno jivhāya rasaṃ sāyitvā uppajjanti … pe …"
msgstr ""

msgctxt "sn35.96:2.2"
msgid ""
"puna caparaṃ, bhikkhave, bhikkhuno manasā dhammaṃ viññāya uppajjanti pāpakā "
"akusalā sarasaṅkappā saṃyojaniyā."
msgstr ""

msgctxt "sn35.96:2.3"
msgid ""
"Tañce bhikkhu adhivāseti nappajahati na vinodeti na byantīkaroti na "
"anabhāvaṃ gameti, veditabbametaṃ, bhikkhave, bhikkhunā:"
msgstr ""

msgctxt "sn35.96:2.4"
msgid "‘parihāyāmi kusalehi dhammehi’."
msgstr ""

msgctxt "sn35.96:2.5"
msgid "Parihānañhetaṃ vuttaṃ bhagavatāti."
msgstr ""

msgctxt "sn35.96:2.6"
msgid "Evaṃ kho, bhikkhave, parihānadhammo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn35.96:3.1"
msgid "Kathañca, bhikkhave, aparihānadhammo hoti?"
msgstr ""

msgctxt "sn35.96:3.2"
msgid ""
"Idha, bhikkhave, bhikkhuno cakkhunā rūpaṃ disvā uppajjanti pāpakā akusalā "
"sarasaṅkappā saṃyojaniyā."
msgstr ""

msgctxt "sn35.96:3.3"
msgid ""
"Tañce bhikkhu nādhivāseti pajahati vinodeti byantīkaroti anabhāvaṃ gameti, "
"veditabbametaṃ, bhikkhave, bhikkhunā:"
msgstr ""

msgctxt "sn35.96:3.4"
msgid "‘na parihāyāmi kusalehi dhammehi’."
msgstr ""

msgctxt "sn35.96:3.5"
msgid "Aparihānañhetaṃ vuttaṃ bhagavatāti … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn35.96:4.1"
msgid ""
"Puna caparaṃ, bhikkhave, bhikkhuno jivhāya rasaṃ sāyitvā uppajjanti … pe …"
msgstr ""

msgctxt "sn35.96:4.2"
msgid ""
"puna caparaṃ, bhikkhave, bhikkhuno manasā dhammaṃ viññāya uppajjanti pāpakā "
"akusalā sarasaṅkappā saṃyojaniyā."
msgstr ""

msgctxt "sn35.96:4.3"
msgid ""
"Tañce bhikkhu nādhivāseti pajahati vinodeti byantīkaroti anabhāvaṃ gameti, "
"veditabbametaṃ, bhikkhave, bhikkhunā:"
msgstr ""

msgctxt "sn35.96:4.4"
msgid "‘na parihāyāmi kusalehi dhammehi’."
msgstr ""

msgctxt "sn35.96:4.5"
msgid "Aparihānañhetaṃ vuttaṃ bhagavatāti."
msgstr ""

msgctxt "sn35.96:4.6"
msgid "Evaṃ kho, bhikkhave, aparihānadhammo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn35.96:5.1"
msgid "Katamāni ca, bhikkhave, cha abhibhāyatanāni?"
msgstr ""

msgctxt "sn35.96:5.2"
msgid ""
"Idha, bhikkhave, bhikkhuno cakkhunā rūpaṃ disvā nuppajjanti pāpakā akusalā "
"sarasaṅkappā saṃyojaniyā."
msgstr ""

msgctxt "sn35.96:5.3"
msgid "Veditabbametaṃ, bhikkhave, bhikkhunā:"
msgstr ""

msgctxt "sn35.96:5.4"
msgid "‘abhibhūtametaṃ āyatanaṃ’."
msgstr ""

msgctxt "sn35.96:5.5"
msgid "Abhibhāyatanañhetaṃ vuttaṃ bhagavatāti … pe …"
msgstr ""

msgctxt "sn35.96:5.6"
msgid ""
"puna caparaṃ, bhikkhave, bhikkhuno manasā dhammaṃ viññāya nuppajjanti pāpakā "
"akusalā dhammā sarasaṅkappā saṃyojaniyā."
msgstr ""

msgctxt "sn35.96:5.7"
msgid "Veditabbametaṃ, bhikkhave, bhikkhunā:"
msgstr ""

msgctxt "sn35.96:5.8"
msgid "‘abhibhūtametaṃ āyatanaṃ’."
msgstr ""

msgctxt "sn35.96:5.9"
msgid "Abhibhāyatanañhetaṃ vuttaṃ bhagavatāti."
msgstr ""

msgctxt "sn35.96:5.10"
msgid "Imāni vuccanti, bhikkhave, cha abhibhāyatanānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.96:5.11"
msgid "Tatiyaṃ."
msgstr ""
