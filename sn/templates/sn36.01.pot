#. <section class="sutta" id="sn36.1"><article><div class="hgroup"><p class="division">
msgctxt "sn36.1:0.1"
msgid "Saṃyutta Nikāya 36"
msgstr ""

#. </p><p>
#. VAR: 1. Sagāthāvagga → paṭhamakasagāthavaggo (s1-3) | paṭhamasagāthavaggo (pts1)
msgctxt "sn36.1:0.2"
msgid "1. Sagāthāvagga"
msgstr ""

#. </p><h1>
msgctxt "sn36.1:0.3"
msgid "1. Samādhisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn36.1:1.1"
msgid "“Tisso imā, bhikkhave, vedanā."
msgstr ""

msgctxt "sn36.1:1.2"
msgid "Katamā tisso?"
msgstr ""

msgctxt "sn36.1:1.3"
msgid "Sukhā vedanā, dukkhā vedanā, adukkhamasukhā vedanā—"
msgstr ""

msgctxt "sn36.1:1.4"
msgid "imā kho, bhikkhave, tisso vedanāti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn36.1:2.1"
msgid "Samāhito sampajāno,"
msgstr ""

#. <br>
msgctxt "sn36.1:2.2"
msgid "sato buddhassa sāvako;"
msgstr ""

#. <br>
msgctxt "sn36.1:2.3"
msgid "Vedanā ca pajānāti,"
msgstr ""

#. <br>
msgctxt "sn36.1:2.4"
msgid "vedanānañca sambhavaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn36.1:3.1"
msgid "Yattha cetā nirujjhanti,"
msgstr ""

#. <br>
msgctxt "sn36.1:3.2"
msgid "maggañca khayagāminaṃ;"
msgstr ""

#. <br>
msgctxt "sn36.1:3.3"
msgid "Vedanānaṃ khayā bhikkhu,"
msgstr ""

#. <br>
msgctxt "sn36.1:3.4"
msgid "nicchāto parinibbuto”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn36.1:3.5"
msgid "Paṭhamaṃ."
msgstr ""
