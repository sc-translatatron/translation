msgctxt "sn48.22:0.1"
msgid "Saṃyutta Nikāya 48"
msgstr ""

#. </p><p>
msgctxt "sn48.22:0.2"
msgid "3. Chaḷindriyavagga"
msgstr ""

#. </p><h1>
#. VAR: 22. Jīvitindriyasutta → jīvita (pts1)
msgctxt "sn48.22:0.3"
msgid "22. Jīvitindriyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn48.22:1.1"
msgid "“Tīṇimāni, bhikkhave, indriyāni."
msgstr ""

msgctxt "sn48.22:1.2"
msgid "Katamāni tīṇi?"
msgstr ""

msgctxt "sn48.22:1.3"
msgid "Itthindriyaṃ, purisindriyaṃ, jīvitindriyaṃ—"
msgstr ""

msgctxt "sn48.22:1.4"
msgid "imāni kho, bhikkhave, tīṇi indriyānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn48.22:1.5"
msgid "Dutiyaṃ."
msgstr ""
