#. <section class="sutta" id="sn2.6"><article><div class="hgroup"><p class="division">
msgctxt "sn2.6:0.1"
msgid "Saṃyutta Nikāya 2"
msgstr ""

#. </p><p>
msgctxt "sn2.6:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
msgctxt "sn2.6:0.3"
msgid "6. Kāmadasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts2ed" id="pts2ed1.109"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn2.6:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn2.6:1.2"
msgid "Ekamantaṃ ṭhito kho kāmado devaputto bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn2.6:1.3"
msgid "“dukkaraṃ, bhagavā, sudukkaraṃ, bhagavā”ti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn2.6:2.1"
msgid "“Dukkaraṃ vāpi karonti, (kāmadāti bhagavā)"
msgstr ""

# See my note on AN 8.59 for silasamahita
#. <br>
msgctxt "sn2.6:2.2"
msgid "Sekhā sīlasamāhitā;"
msgstr ""

#. <br>
msgctxt "sn2.6:2.3"
msgid "Ṭhitattā anagāriyupetassa,"
msgstr ""

#. <br>
msgctxt "sn2.6:2.4"
msgid "Tuṭṭhi hoti sukhāvahā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn2.6:3.1"
msgid "“Dullabhā, bhagavā, yadidaṃ tuṭṭhī”ti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn2.6:4.1"
msgid "“Dullabhaṃ vāpi labhanti, (kāmadāti bhagavā)"
msgstr ""

#. <br>
msgctxt "sn2.6:4.2"
msgid "Cittavūpasame ratā;"
msgstr ""

#. <br>
msgctxt "sn2.6:4.3"
msgid "Yesaṃ divā ca ratto ca,"
msgstr ""

#. <br>
msgctxt "sn2.6:4.4"
msgid "Bhāvanāya rato mano”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn2.6:5.1"
msgid "“Dussamādahaṃ, bhagavā, yadidaṃ cittan”ti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn2.6:6.1"
msgid "“Dussamādahaṃ vāpi samādahanti, (kāmadāti bhagavā)"
msgstr ""

#. <br>
msgctxt "sn2.6:6.2"
msgid "Indriyūpasame ratā;"
msgstr ""

#. <br>
msgctxt "sn2.6:6.3"
msgid "Te chetvā maccuno jālaṃ,"
msgstr ""

#. <br>
msgctxt "sn2.6:6.4"
msgid "Ariyā gacchanti kāmadā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn2.6:7.1"
msgid "“Duggamo, bhagavā, visamo maggo”ti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.110"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "sn2.6:8.1"
msgid "“Duggame visame vāpi,"
msgstr ""

#. <br>
msgctxt "sn2.6:8.2"
msgid "Ariyā gacchanti kāmada;"
msgstr ""

#. <br>
msgctxt "sn2.6:8.3"
msgid "Anariyā visame magge,"
msgstr ""

#. <br>
msgctxt "sn2.6:8.4"
msgid "Papatanti avaṃsirā;"
msgstr ""

#. <br>
msgctxt "sn2.6:8.5"
msgid "Ariyānaṃ samo maggo,"
msgstr ""

#. <br>
msgctxt "sn2.6:8.6"
msgid "Ariyā hi visame samā”ti."
msgstr ""
