#. <section class="sutta" id="sn6.9"><article><div class="hgroup"><p class="division">
msgctxt "sn6.9:0.1"
msgid "Saṃyutta Nikāya 6"
msgstr ""

#. </p><p>
msgctxt "sn6.9:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
#. VAR: 9. Turūbrahmasutta → tudubrahmasuttaṃ (bj, s1-3, pts2) | tudu brahmā (pts1)
msgctxt "sn6.9:0.3"
msgid "9. Turūbrahmasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts2ed" id="pts2ed1.324"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn6.9:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn6.9:1.2"
msgid ""
"Tena kho pana samayena kokāliko bhikkhu ābādhiko hoti dukkhito bāḷhagilāno."
msgstr ""

#. VAR: turū → tudu (bj, s1-3, km, pts1-2)
msgctxt "sn6.9:1.3"
msgid ""
"Atha kho turū paccekabrahmā abhikkantāya rattiyā abhikkantavaṇṇo "
"kevalakappaṃ jetavanaṃ obhāsetvā yena kokāliko bhikkhu tenupasaṅkami; "
"upasaṅkamitvā vehāsaṃ ṭhito kokālikaṃ bhikkhuṃ etadavoca:"
msgstr ""

msgctxt "sn6.9:1.4"
msgid "“pasādehi, kokālika, sāriputtamoggallānesu cittaṃ."
msgstr ""

msgctxt "sn6.9:1.5"
msgid "Pesalā sāriputtamoggallānā”ti."
msgstr ""

msgctxt "sn6.9:1.6"
msgid "“Kosi tvaṃ, āvuso”ti?"
msgstr ""

msgctxt "sn6.9:1.7"
msgid "“Ahaṃ turū paccekabrahmā”ti."
msgstr ""

msgctxt "sn6.9:1.8"
msgid "“Nanu tvaṃ, āvuso, bhagavatā anāgāmī byākato, atha kiñcarahi idhāgato?"
msgstr ""

msgctxt "sn6.9:1.9"
msgid "Passa, yāvañca te idaṃ aparaddhan”ti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn6.9:2.1"
msgid "“Purisassa hi jātassa,"
msgstr ""

#. <br>
#. VAR: kuṭhārī → kudhārī (s1-3, km, mr)
msgctxt "sn6.9:2.2"
msgid "kuṭhārī jāyate mukhe;"
msgstr ""

#. <br>
msgctxt "sn6.9:2.3"
msgid "Yāya chindati attānaṃ,"
msgstr ""

#. <br>
msgctxt "sn6.9:2.4"
msgid "bālo dubbhāsitaṃ bhaṇaṃ."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.325"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn6.9:3.1"
msgid "Yo nindiyaṃ pasaṃsati,"
msgstr ""

#. <br>
msgctxt "sn6.9:3.2"
msgid "Taṃ vā nindati yo pasaṃsiyo;"
msgstr ""

#. <br>
msgctxt "sn6.9:3.3"
msgid "Vicināti mukhena so kaliṃ,"
msgstr ""

#. <br>
msgctxt "sn6.9:3.4"
msgid "Kalinā tena sukhaṃ na vindati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn6.9:4.1"
msgid "Appamattako ayaṃ kali,"
msgstr ""

#. <br>
msgctxt "sn6.9:4.2"
msgid "Yo akkhesu dhanaparājayo;"
msgstr ""

#. <br>
msgctxt "sn6.9:4.3"
msgid "Sabbassāpi sahāpi attanā,"
msgstr ""

#. <br>
msgctxt "sn6.9:4.4"
msgid "Ayameva mahantataro kali;"
msgstr ""

#. <br>
msgctxt "sn6.9:4.5"
msgid "Yo sugatesu manaṃ padosaye."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn6.9:5.1"
msgid "Sataṃ sahassānaṃ nirabbudānaṃ,"
msgstr ""

#. <br>
msgctxt "sn6.9:5.2"
msgid "Chattiṃsati pañca ca abbudāni;"
msgstr ""

#. <br>
#. VAR: Yamariyagarahī → yamariye garahī (s1-3, km) | yamariyaṃ garahaṃ (mr)
msgctxt "sn6.9:5.3"
msgid "Yamariyagarahī nirayaṃ upeti,"
msgstr ""

#. <br>
msgctxt "sn6.9:5.4"
msgid "Vācaṃ manañca paṇidhāya pāpakan”ti."
msgstr ""
