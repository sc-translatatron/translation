#. <section class="sutta" id="sn55.33"><article><div class="hgroup"><p class="division">
msgctxt "sn55.33:0.1"
msgid "Saṃyutta Nikāya 55"
msgstr ""

#. </p><p>
msgctxt "sn55.33:0.2"
msgid "4. Puññābhisandavagga"
msgstr ""

#. </p><h1>
#. VAR: 33. Tatiyapuññābhisandasutta → tatiyābhisandasuttaṃ (bj) | abhisanda 3 (pts1)
msgctxt "sn55.33:0.3"
msgid "33. Tatiyapuññābhisandasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn55.33:1.1"
msgid "“Cattārome, bhikkhave, puññābhisandā kusalābhisandā sukhassāhārā."
msgstr ""

msgctxt "sn55.33:1.2"
msgid "Katame cattāro?"
msgstr ""

msgctxt "sn55.33:1.3"
msgid "Idha, bhikkhave, ariyasāvako buddhe aveccappasādena samannāgato hoti—"
msgstr ""

msgctxt "sn55.33:1.4"
msgid "itipi so bhagavā … pe … satthā devamanussānaṃ buddho bhagavāti."
msgstr ""

msgctxt "sn55.33:1.5"
msgid "Ayaṃ paṭhamo puññābhisando kusalābhisando sukhassāhāro."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn55.33:2.1"
msgid "Puna caparaṃ, bhikkhave, ariyasāvako dhamme … pe …"
msgstr ""

msgctxt "sn55.33:2.2"
msgid "saṅghe … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn55.33:3.1"
msgid ""
"Puna caparaṃ, bhikkhave, ariyasāvako paññavā hoti udayatthagāminiyā paññāya "
"samannāgato ariyāya nibbedhikāya sammā dukkhakkhayagāminiyā."
msgstr ""

msgctxt "sn55.33:3.2"
msgid "Ayaṃ catuttho puññābhisando kusalābhisando sukhassāhāro."
msgstr ""

msgctxt "sn55.33:3.3"
msgid ""
"Ime kho, bhikkhave, cattāro puññābhisandā kusalābhisandā sukhassāhārā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn55.33:3.4"
msgid "Tatiyaṃ."
msgstr ""
