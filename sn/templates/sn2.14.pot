#. <section class="sutta" id="sn2.14"><article><div class="hgroup"><p class="division">
msgctxt "sn2.14:0.1"
msgid "Saṃyutta Nikāya 2"
msgstr ""

#. </p><p>
msgctxt "sn2.14:0.2"
msgid "2. Anāthapiṇḍikavagga"
msgstr ""

#. </p><h1>
msgctxt "sn2.14:0.3"
msgid "14. Nandanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn2.14:1.1"
msgid "Ekamantaṃ ṭhito kho nandano devaputto bhagavantaṃ gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts1ed" id="pts1ed1.53"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn2.14:2.1"
msgid "“Pucchāmi taṃ gotama bhūripañña,"
msgstr ""

#. <br>
msgctxt "sn2.14:2.2"
msgid "Anāvaṭaṃ bhagavato ñāṇadassanaṃ;"
msgstr ""

#. <br>
msgctxt "sn2.14:2.3"
msgid "Kathaṃvidhaṃ sīlavantaṃ vadanti,"
msgstr ""

#. <br>
msgctxt "sn2.14:2.4"
msgid "Kathaṃvidhaṃ paññavantaṃ vadanti;"
msgstr ""

#. <br>
msgctxt "sn2.14:2.5"
msgid "Kathaṃvidho dukkhamaticca iriyati,"
msgstr ""

#. <br>
msgctxt "sn2.14:2.6"
msgid "Kathaṃvidhaṃ devatā pūjayantī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn2.14:3.1"
msgid "“Yo sīlavā paññavā bhāvitatto,"
msgstr ""

#. <br>
msgctxt "sn2.14:3.2"
msgid "Samāhito jhānarato satīmā;"
msgstr ""

#. <br>
msgctxt "sn2.14:3.3"
msgid "Sabbassa sokā vigatā pahīnā,"
msgstr ""

#. <br>
msgctxt "sn2.14:3.4"
msgid "Khīṇāsavo antimadehadhārī."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.121"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn2.14:4.1"
msgid "Tathāvidhaṃ sīlavantaṃ vadanti,"
msgstr ""

#. <br>
msgctxt "sn2.14:4.2"
msgid "Tathāvidhaṃ paññavantaṃ vadanti;"
msgstr ""

#. <br>
msgctxt "sn2.14:4.3"
msgid "Tathāvidho dukkhamaticca iriyati,"
msgstr ""

#. <br>
msgctxt "sn2.14:4.4"
msgid "Tathāvidhaṃ devatā pūjayantī”ti."
msgstr ""
