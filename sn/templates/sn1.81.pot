#. <section class="sutta" id="sn1.81"><article><div class="hgroup"><p class="division">
msgctxt "sn1.81:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.81:0.2"
msgid "8. Chetvāvagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.81:0.3"
msgid "81. Araṇasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.81:1.1"
msgid "“Kesūdha araṇā loke,"
msgstr ""

#. <br>
msgctxt "sn1.81:1.2"
msgid "kesaṃ vusitaṃ na nassati;"
msgstr ""

#. <br>
msgctxt "sn1.81:1.3"
msgid "Kedha icchaṃ parijānanti,"
msgstr ""

#. <br>
msgctxt "sn1.81:1.4"
msgid "kesaṃ bhojissiyaṃ sadā."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.45"></a>
#. <a class="pts2ed" id="pts2ed1.103"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.81:2.1"
msgid "Kiṃsu mātā pitā bhātā,"
msgstr ""

#. <br>
msgctxt "sn1.81:2.2"
msgid "vandanti naṃ patiṭṭhitaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.81:2.3"
msgid "Kiṃsu idha jātihīnaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.81:2.4"
msgid "abhivādenti khattiyā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.81:3.1"
msgid "“Samaṇīdha araṇā loke,"
msgstr ""

#. <br>
msgctxt "sn1.81:3.2"
msgid "Samaṇānaṃ vusitaṃ na nassati;"
msgstr ""

#. <br>
msgctxt "sn1.81:3.3"
msgid "Samaṇā icchaṃ parijānanti,"
msgstr ""

#. <br>
msgctxt "sn1.81:3.4"
msgid "Samaṇānaṃ bhojissiyaṃ sadā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.81:4.1"
msgid "Samaṇaṃ mātā pitā bhātā,"
msgstr ""

#. <br>
msgctxt "sn1.81:4.2"
msgid "vandanti naṃ patiṭṭhitaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.81:4.3"
msgid "Samaṇīdha jātihīnaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.81:4.4"
msgid "abhivādenti khattiyā”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn1.81:4.5"
msgid "Chetvāvaggo aṭṭhamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn1.81:5.1"
msgid "Chetvā rathañca cittañca,"
msgstr ""

#. <br>
msgctxt "sn1.81:5.2"
msgid "vuṭṭhi bhītā najīrati;"
msgstr ""

#. <br>
msgctxt "sn1.81:5.3"
msgid "Issaraṃ kāmaṃ pātheyyaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.81:5.4"
msgid "pajjoto araṇena cāti."
msgstr ""

#. </p></blockquote><p class="endsutta">
msgctxt "sn1.81:5.5"
msgid "Devatāsaṃyuttaṃ samattaṃ."
msgstr ""
