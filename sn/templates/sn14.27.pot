#. <section class="sutta" id="sn14.27"><article><div class="hgroup"><p class="division">
msgctxt "sn14.27:0.1"
msgid "Saṃyutta Nikāya 14"
msgstr ""

#. </p><p>
msgctxt "sn14.27:0.2"
msgid "3. Kammapathavagga"
msgstr ""

#. </p><h1>
msgctxt "sn14.27:0.3"
msgid "27. Dasakammapathasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.168"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn14.27:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn14.27:1.2"
msgid "“Dhātusova, bhikkhave, sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.27:1.3"
msgid "Pāṇātipātino pāṇātipātīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.27:1.4"
msgid "adinnādāyino … pe …"
msgstr ""

msgctxt "sn14.27:1.5"
msgid "kāmesumicchācārino …"
msgstr ""

msgctxt "sn14.27:1.6"
msgid "musāvādino …"
msgstr ""

msgctxt "sn14.27:1.7"
msgid "pisuṇavācā …"
msgstr ""

msgctxt "sn14.27:1.8"
msgid "pharusavācā …"
msgstr ""

msgctxt "sn14.27:1.9"
msgid "samphappalāpino samphappalāpīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.27:1.10"
msgid "abhijjhāluno abhijjhālūhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.27:1.11"
msgid "byāpannacittā byāpannacittehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.27:1.12"
msgid "micchādiṭṭhikā micchādiṭṭhikehi saddhiṃ saṃsandanti samenti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn14.27:2.1"
msgid ""
"Pāṇātipātā paṭiviratā pāṇātipātā paṭiviratehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.27:2.2"
msgid "adinnādānā paṭiviratā … pe …"
msgstr ""

msgctxt "sn14.27:2.3"
msgid "kāmesumicchācārā paṭiviratā …"
msgstr ""

msgctxt "sn14.27:2.4"
msgid "musāvādā paṭiviratā …"
msgstr ""

msgctxt "sn14.27:2.5"
msgid "pisuṇāya vācāya …"
msgstr ""

msgctxt "sn14.27:2.6"
msgid "pharusāya vācāya …"
msgstr ""

msgctxt "sn14.27:2.7"
msgid ""
"samphappalāpā paṭiviratā samphappalāpā paṭiviratehi saddhiṃ saṃsandanti "
"samenti;"
msgstr ""

msgctxt "sn14.27:2.8"
msgid "anabhijjhāluno anabhijjhālūhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.27:2.9"
msgid "abyāpannacittā abyāpannacittehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.27:2.10"
msgid "sammādiṭṭhikā sammādiṭṭhikehi saddhiṃ saṃsandanti samentī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn14.27:2.11"
msgid "Pañcamaṃ."
msgstr ""
