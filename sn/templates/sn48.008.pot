#. <section class="sutta" id="sn48.8"><article><div class="hgroup"><p class="division">
msgctxt "sn48.8:0.1"
msgid "Saṃyutta Nikāya 48"
msgstr ""

#. </p><p>
msgctxt "sn48.8:0.2"
msgid "1. Suddhikavagga"
msgstr ""

#. </p><h1>
msgctxt "sn48.8:0.3"
msgid "8. Daṭṭhabbasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn48.8:1.1"
msgid "“Pañcimāni, bhikkhave, indriyāni."
msgstr ""

msgctxt "sn48.8:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "sn48.8:1.3"
msgid "Saddhindriyaṃ … pe … paññindriyaṃ."
msgstr ""

msgctxt "sn48.8:1.4"
msgid "Kattha ca, bhikkhave, saddhindriyaṃ daṭṭhabbaṃ?"
msgstr ""

msgctxt "sn48.8:1.5"
msgid "Catūsu sotāpattiyaṅgesu—"
msgstr ""

msgctxt "sn48.8:1.6"
msgid "ettha saddhindriyaṃ daṭṭhabbaṃ."
msgstr ""

msgctxt "sn48.8:1.7"
msgid "Kattha ca, bhikkhave, vīriyindriyaṃ daṭṭhabbaṃ?"
msgstr ""

msgctxt "sn48.8:1.8"
msgid "Catūsu sammappadhānesu—"
msgstr ""

msgctxt "sn48.8:1.9"
msgid "ettha vīriyindriyaṃ daṭṭhabbaṃ."
msgstr ""

msgctxt "sn48.8:1.10"
msgid "Kattha ca, bhikkhave, satindriyaṃ daṭṭhabbaṃ?"
msgstr ""

msgctxt "sn48.8:1.11"
msgid "Catūsu satipaṭṭhānesu—"
msgstr ""

msgctxt "sn48.8:1.12"
msgid "ettha satindriyaṃ daṭṭhabbaṃ."
msgstr ""

msgctxt "sn48.8:1.13"
msgid "Kattha ca, bhikkhave, samādhindriyaṃ daṭṭhabbaṃ?"
msgstr ""

msgctxt "sn48.8:1.14"
msgid "Catūsu jhānesu—"
msgstr ""

msgctxt "sn48.8:1.15"
msgid "ettha samādhindriyaṃ daṭṭhabbaṃ."
msgstr ""

msgctxt "sn48.8:1.16"
msgid "Kattha ca, bhikkhave, paññindriyaṃ daṭṭhabbaṃ?"
msgstr ""

msgctxt "sn48.8:1.17"
msgid "Catūsu ariyasaccesu—"
msgstr ""

msgctxt "sn48.8:1.18"
msgid "ettha paññindriyaṃ daṭṭhabbaṃ."
msgstr ""

msgctxt "sn48.8:1.19"
msgid "Imāni kho, bhikkhave, pañcindriyānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn48.8:1.20"
msgid "Aṭṭhamaṃ."
msgstr ""
