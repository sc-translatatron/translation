#. <section class="sutta" id="sn48.46"><article><div class="hgroup"><p class="division">
msgctxt "sn48.46:0.1"
msgid "Saṃyutta Nikāya 48"
msgstr ""

#. </p><p>
msgctxt "sn48.46:0.2"
msgid "5. Jarāvagga"
msgstr ""

#. </p><h1>
msgctxt "sn48.46:0.3"
msgid "46. Dutiyapubbārāmasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn48.46:1.1"
msgid "Taṃyeva nidānaṃ."
msgstr ""

msgctxt "sn48.46:1.2"
msgid ""
"“Katinaṃ nu kho, bhikkhave, indriyānaṃ bhāvitattā bahulīkatattā khīṇāsavo "
"bhikkhu aññaṃ byākaroti:"
msgstr ""

msgctxt "sn48.46:1.3"
msgid ""
"‘khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyā’ti "
"pajānāmī”ti?"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.223"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn48.46:2.1"
msgid "“Bhagavaṃmūlakā no, bhante, dhammā … pe …"
msgstr ""

msgctxt "sn48.46:2.2"
msgid ""
"“dvinnaṃ kho, bhikkhave, indriyānaṃ bhāvitattā bahulīkatattā khīṇāsavo "
"bhikkhu aññaṃ byākaroti:"
msgstr ""

msgctxt "sn48.46:2.3"
msgid ""
"‘khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyā’ti "
"pajānāmīti."
msgstr ""

msgctxt "sn48.46:2.4"
msgid "Katamesaṃ dvinnaṃ?"
msgstr ""

msgctxt "sn48.46:2.5"
msgid "Ariyāya ca paññāya, ariyāya ca vimuttiyā."
msgstr ""

msgctxt "sn48.46:2.6"
msgid "Yā hissa, bhikkhave, ariyā paññā tadassa paññindriyaṃ."
msgstr ""

msgctxt "sn48.46:2.7"
msgid "Yā hissa, bhikkhave, ariyā vimutti tadassa samādhindriyaṃ."
msgstr ""

msgctxt "sn48.46:2.8"
msgid ""
"Imesaṃ kho, bhikkhave, dvinnaṃ indriyānaṃ bhāvitattā bahulīkatattā khīṇāsavo "
"bhikkhu aññaṃ byākaroti:"
msgstr ""

msgctxt "sn48.46:2.9"
msgid ""
"‘khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyā’ti "
"pajānāmī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn48.46:2.10"
msgid "Chaṭṭhaṃ."
msgstr ""
