#. <section class="sutta" id="sn14.17"><article><div class="hgroup"><p class="division">
msgctxt "sn14.17:0.1"
msgid "Saṃyutta Nikāya 14"
msgstr ""

#. </p><p>
msgctxt "sn14.17:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
#. VAR: 17. Assaddhasaṃsandanasutta → assaddhasuttaṃ (bj) | asaddha (pts1-2)
msgctxt "sn14.17:0.3"
msgid "17. Assaddhasaṃsandanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.159"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn14.17:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn14.17:1.2"
msgid "“Dhātusova, bhikkhave, sattā saṃsandanti samenti."
msgstr ""

#. VAR: Assaddhā → asaddhā (pts1-2)
msgctxt "sn14.17:1.3"
msgid "Assaddhā assaddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:1.4"
msgid "ahirikā ahirikehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:1.5"
msgid "anottappino anottappīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:1.6"
msgid "appassutā appassutehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:1.7"
msgid "kusītā kusītehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:1.8"
msgid "muṭṭhassatino muṭṭhassatīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:1.9"
msgid "duppaññā duppaññehi saddhiṃ saṃsandanti samenti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn14.17:2.1"
msgid "Atītampi kho, bhikkhave, addhānaṃ dhātusova sattā saṃsandiṃsu samiṃsu."
msgstr ""

msgctxt "sn14.17:2.2"
msgid "Assaddhā assaddhehi saddhiṃ saṃsandiṃsu samiṃsu;"
msgstr ""

msgctxt "sn14.17:2.3"
msgid "ahirikā ahirikehi saddhiṃ saṃsandiṃsu samiṃsu;"
msgstr ""

msgctxt "sn14.17:2.4"
msgid "anottappino anottappīhi saddhiṃ saṃsandiṃsu samiṃsu;"
msgstr ""

msgctxt "sn14.17:2.5"
msgid "appassutā appassutehi saddhiṃ saṃsandiṃsu samiṃsu;"
msgstr ""

msgctxt "sn14.17:2.6"
msgid "kusītā kusītehi saddhiṃ saṃsandiṃsu samiṃsu;"
msgstr ""

msgctxt "sn14.17:2.7"
msgid "muṭṭhassatino muṭṭhassatīhi saddhiṃ saṃsandiṃsu samiṃsu;"
msgstr ""

msgctxt "sn14.17:2.8"
msgid "duppaññā duppaññehi saddhiṃ saṃsandiṃsu samiṃsu."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.160"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn14.17:3.1"
msgid ""
"Anāgatampi kho, bhikkhave, addhānaṃ dhātusova sattā saṃsandissanti "
"samessanti."
msgstr ""

msgctxt "sn14.17:3.2"
msgid "Assaddhā assaddhehi saddhiṃ saṃsandissanti samessanti;"
msgstr ""

msgctxt "sn14.17:3.3"
msgid "ahirikā ahirikehi saddhiṃ saṃsandissanti samessanti;"
msgstr ""

msgctxt "sn14.17:3.4"
msgid "anottappino anottappīhi saddhiṃ … pe …"
msgstr ""

msgctxt "sn14.17:3.5"
msgid "appassutā appassutehi saddhiṃ … pe …"
msgstr ""

msgctxt "sn14.17:3.6"
msgid "kusītā kusītehi saddhiṃ … pe …"
msgstr ""

msgctxt "sn14.17:3.7"
msgid "muṭṭhassatino muṭṭhassatīhi saddhiṃ … pe …"
msgstr ""

msgctxt "sn14.17:3.8"
msgid "duppaññā duppaññehi saddhiṃ saṃsandissanti samessanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn14.17:4.1"
msgid ""
"Etarahipi kho, bhikkhave, paccuppannaṃ addhānaṃ dhātusova sattā saṃsandanti "
"samenti."
msgstr ""

msgctxt "sn14.17:4.2"
msgid "Assaddhā assaddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:4.3"
msgid "ahirikā ahirikehi saddhiṃ … pe …"
msgstr ""

msgctxt "sn14.17:4.4"
msgid "anottappino anottappīhi saddhiṃ … pe …"
msgstr ""

msgctxt "sn14.17:4.5"
msgid "appassutā appassutehi saddhiṃ … pe …"
msgstr ""

msgctxt "sn14.17:4.6"
msgid "kusītā kusītehi saddhiṃ … pe …"
msgstr ""

msgctxt "sn14.17:4.7"
msgid "muṭṭhassatino muṭṭhassatīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:4.8"
msgid "duppaññā duppaññehi saddhiṃ saṃsandanti samenti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn14.17:5.1"
msgid "Dhātusova, bhikkhave, sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.17:5.2"
msgid "Saddhā saddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:5.3"
msgid "hirimanā hirimanehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:5.4"
msgid "ottappino ottappīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:5.5"
msgid "bahussutā bahussutehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:5.6"
msgid "āraddhavīriyā āraddhavīriyehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:5.7"
msgid "upaṭṭhitassatino upaṭṭhitassatīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.17:5.8"
msgid "paññavanto paññavantehi saddhiṃ saṃsandanti samenti."
msgstr ""

msgctxt "sn14.17:5.9"
msgid "Atītampi kho, bhikkhave, addhānaṃ … pe …"
msgstr ""

msgctxt "sn14.17:5.10"
msgid "anāgatampi kho, bhikkhave … pe …"
msgstr ""

msgctxt "sn14.17:5.11"
msgid ""
"etarahipi kho, bhikkhave, paccuppannaṃ addhānaṃ dhātusova sattā saṃsandanti "
"samenti."
msgstr ""

msgctxt "sn14.17:5.12"
msgid "Saddhā saddhehi saddhiṃ … pe …"
msgstr ""

msgctxt "sn14.17:5.13"
msgid "paññavanto paññavantehi saddhiṃ saṃsandanti samentī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn14.17:5.14"
msgid "Sattamaṃ."
msgstr ""
