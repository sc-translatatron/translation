#. <section class="sutta" id="sn45.141"><article><div class="hgroup"><p class="division">
msgctxt "sn45.141-145:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.141-145:0.2"
msgid "11. Appamādapeyyālavagga"
msgstr ""

#. </p><h1>
#. VAR: 141–​145. Kūṭādisutta → rājasuttāni (bj) | rājā (pts1)
msgctxt "sn45.141-145:0.3"
msgid "141–​145. Kūṭādisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.141-145:1.1"
msgid ""
"“Seyyathāpi, bhikkhave, kūṭāgārassa yā kāci gopānasiyo sabbā tā kūṭaṅgamā "
"kūṭaninnā kūṭasamosaraṇā; kūṭaṃ tāsaṃ aggamakkhāyati;"
msgstr ""

msgctxt "sn45.141-145:1.2"
msgid "evameva kho, bhikkhave … pe …"
msgstr ""

msgctxt "sn45.141-145:1.3"
msgid "(yathā heṭṭhimasuttantaṃ, evaṃ vitthāretabbaṃ.)"
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.141-145:1.4"
msgid "Tatiyaṃ."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.44"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn45.141-145:2.1"
msgid ""
"“Seyyathāpi, bhikkhave, ye keci mūlagandhā, kāḷānusāriyaṃ tesaṃ "
"aggamakkhāyati;"
msgstr ""

msgctxt "sn45.141-145:2.2"
msgid "evameva kho, bhikkhave … pe …"
msgstr ""

msgctxt "sn45.141-145:2.3"
msgid "catutthaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn45.141-145:3.1"
msgid ""
"“Seyyathāpi, bhikkhave, ye keci sāragandhā, lohitacandanaṃ tesaṃ "
"aggamakkhāyati;"
msgstr ""

msgctxt "sn45.141-145:3.2"
msgid "evameva kho, bhikkhave … pe …"
msgstr ""

msgctxt "sn45.141-145:3.3"
msgid "pañcamaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn45.141-145:4.1"
msgid ""
"“Seyyathāpi, bhikkhave, ye keci pupphagandhā, vassikaṃ tesaṃ aggamakkhāyati;"
msgstr ""

msgctxt "sn45.141-145:4.2"
msgid "evameva kho, bhikkhave … pe …"
msgstr ""

msgctxt "sn45.141-145:4.3"
msgid "chaṭṭhaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn45.141-145:5.1"
msgid ""
"“Seyyathāpi, bhikkhave, ye keci kuṭṭarājāno, sabbe te rañño cakkavattissa "
"anuyantā bhavanti, rājā tesaṃ cakkavatti aggamakkhāyati;"
msgstr ""

msgctxt "sn45.141-145:5.2"
msgid "evameva kho, bhikkhave … pe …"
msgstr ""

msgctxt "sn45.141-145:5.3"
msgid "sattamaṃ."
msgstr ""
