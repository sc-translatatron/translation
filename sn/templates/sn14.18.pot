#. <section class="sutta" id="sn14.18"><article><div class="hgroup"><p class="division">
msgctxt "sn14.18:0.1"
msgid "Saṃyutta Nikāya 14"
msgstr ""

#. </p><p>
msgctxt "sn14.18:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
#. VAR: 18. Assaddhamūlakasutta → assaddhamūlakatikapañcakasuttaṃ (bj) | asaddhamūlakāpañca (pts1-2)
msgctxt "sn14.18:0.3"
msgid "18. Assaddhamūlakasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.161"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn14.18:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn14.18:1.2"
msgid "“Dhātusova, bhikkhave, sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.18:1.3"
msgid "Assaddhā assaddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:1.4"
msgid "ahirikā ahirikehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:1.5"
msgid "duppaññā duppaññehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:1.6"
msgid "saddhā saddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:1.7"
msgid "hirimanā hirimanehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:1.8"
msgid "paññavanto paññavantehi saddhiṃ saṃsandanti samenti."
msgstr ""

msgctxt "sn14.18:1.9"
msgid ""
"Atītampi kho, bhikkhave, addhānaṃ dhātusova sattā saṃsandiṃsu samiṃsu … pe …"
msgstr ""

msgctxt "sn14.18:1.10"
msgid ""
"anāgatampi kho, bhikkhave, addhānaṃ dhātusova sattā saṃsandissanti "
"samessanti … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn14.18:2.1"
msgid ""
"Etarahipi kho, bhikkhave, paccuppannaṃ addhānaṃ dhātusova sattā saṃsandanti "
"samenti."
msgstr ""

msgctxt "sn14.18:2.2"
msgid "Assaddhā assaddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:2.3"
msgid ""
"ahirikā ahirikehi saddhiṃ saṃsandanti samenti, duppaññā duppaññehi saddhiṃ "
"saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:2.4"
msgid "saddhā saddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:2.5"
msgid "hirimanā hirimanehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:2.6"
msgid "paññavanto paññavantehi saddhiṃ saṃsandanti samentīti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn14.18:3.1"
msgid "Dhātusova, bhikkhave, sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.18:3.2"
msgid "Assaddhā assaddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:3.3"
msgid "anottappino anottappīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:3.4"
msgid "duppaññā duppaññehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:3.5"
msgid "saddhā saddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:3.6"
msgid "ottappino ottappīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:3.7"
msgid "paññavanto paññavantehi saddhiṃ saṃsandanti samenti … pe …"
msgstr ""

msgctxt "sn14.18:3.8"
msgid "paṭhamavāro viya vitthāretabbo. (2)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.162"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn14.18:4.1"
msgid "Dhātusova, bhikkhave … pe …"
msgstr ""

msgctxt "sn14.18:4.2"
msgid "assaddhā assaddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:4.3"
msgid "appassutā appassutehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:4.4"
msgid "duppaññā duppaññehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:4.5"
msgid ""
"saddhā saddhehi saddhiṃ saṃsandanti samenti; bahussutā bahussutehi saddhiṃ "
"saṃsandanti samenti, paññavanto paññavantehi saddhiṃ saṃsandanti samenti … "
"pe …. (3)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn14.18:5.1"
msgid "Dhātusova, bhikkhave … pe …"
msgstr ""

msgctxt "sn14.18:5.2"
msgid "assaddhā assaddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:5.3"
msgid "kusītā kusītehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:5.4"
msgid "duppaññā duppaññehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:5.5"
msgid "saddhā saddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:5.6"
msgid ""
"āraddhavīriyā āraddhavīriyehi saddhiṃ saṃsandanti samenti; paññavanto "
"paññavantehi saddhiṃ saṃsandanti samenti … pe …. (4)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn14.18:6.1"
msgid "Dhātusova, bhikkhave … pe …"
msgstr ""

msgctxt "sn14.18:6.2"
msgid "assaddhā assaddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:6.3"
msgid "muṭṭhassatino muṭṭhassatīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:6.4"
msgid "duppaññā duppaññehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:6.5"
msgid "saddhā saddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.18:6.6"
msgid ""
"upaṭṭhitassatino upaṭṭhitassatīhi saddhiṃ saṃsandanti samenti; paññavanto "
"paññavantehi saddhiṃ saṃsandanti samentī”ti … pe …. (5)"
msgstr ""

#. </p><p class="endsection">
msgctxt "sn14.18:6.7"
msgid "Aṭṭhamaṃ."
msgstr ""
