#. <section class="sutta" id="sn45.40"><article><div class="hgroup"><p class="division">
msgctxt "sn45.40:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.40:0.2"
msgid "4. Paṭipattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.40:0.3"
msgid "40. Dutiyabrahmacariyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.27"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.40:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.40:1.2"
msgid "“Brahmacariyañca vo, bhikkhave, desessāmi, brahmacariyatthañca."
msgstr ""

msgctxt "sn45.40:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn45.40:1.4"
msgid "Katamañca, bhikkhave, brahmacariyaṃ?"
msgstr ""

msgctxt "sn45.40:1.5"
msgid "Ayameva ariyo aṭṭhaṅgiko maggo, seyyathidaṃ—"
msgstr ""

msgctxt "sn45.40:1.6"
msgid "sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

msgctxt "sn45.40:1.7"
msgid "Idaṃ vuccati, bhikkhave, brahmacariyaṃ."
msgstr ""

msgctxt "sn45.40:1.8"
msgid "Katamo ca, bhikkhave, brahmacariyattho?"
msgstr ""

msgctxt "sn45.40:1.9"
msgid "Yo kho, bhikkhave, rāgakkhayo dosakkhayo mohakkhayo—"
msgstr ""

msgctxt "sn45.40:1.10"
msgid "ayaṃ vuccati, bhikkhave, brahmacariyattho”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.40:1.11"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.40:1.12"
msgid "Paṭipattivaggo catuttho."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn45.40:2.1"
msgid "Paṭipatti paṭipanno ca,"
msgstr ""

#. <br>
msgctxt "sn45.40:2.2"
msgid "viraddhañca pāraṅgamā;"
msgstr ""

#. <br>
msgctxt "sn45.40:2.3"
msgid "Sāmaññena ca dve vuttā,"
msgstr ""

#. <br>
msgctxt "sn45.40:2.4"
msgid "brahmaññā apare duve;"
msgstr ""

#. <br>
msgctxt "sn45.40:2.5"
msgid "Brahmacariyena dve vuttā,"
msgstr ""

#. <br>
msgctxt "sn45.40:2.6"
msgid "vaggo tena pavuccatīti."
msgstr ""
