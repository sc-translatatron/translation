#. <section class="sutta" id="sn45.34"><article><div class="hgroup"><p class="division">
msgctxt "sn45.34:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.34:0.2"
msgid "4. Paṭipattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.34:0.3"
msgid "34. Pāraṅgamasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.34:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.34:1.2"
msgid ""
"“Aṭṭhime, bhikkhave, dhammā bhāvitā bahulīkatā apārā pāraṃ gamanāya "
"saṃvattanti."
msgstr ""

msgctxt "sn45.34:1.3"
msgid "Katame aṭṭha?"
msgstr ""

msgctxt "sn45.34:1.4"
msgid "Seyyathidaṃ—sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

msgctxt "sn45.34:1.5"
msgid ""
"Ime kho, bhikkhave, aṭṭha dhammā bhāvitā bahulīkatā apārā pāraṃ gamanāya "
"saṃvattantī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn45.34:2.1"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "sn45.34:2.2"
msgid "Idaṃ vatvāna sugato athāparaṃ etadavoca satthā:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn45.34:3.1"
msgid "“Appakā te manussesu,"
msgstr ""

#. <br>
msgctxt "sn45.34:3.2"
msgid "ye janā pāragāmino;"
msgstr ""

#. <br>
msgctxt "sn45.34:3.3"
msgid "Athāyaṃ itarā pajā,"
msgstr ""

#. <br>
msgctxt "sn45.34:3.4"
msgid "tīramevānudhāvati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn45.34:4.1"
msgid "Ye ca kho sammadakkhāte,"
msgstr ""

#. <br>
msgctxt "sn45.34:4.2"
msgid "dhamme dhammānuvattino;"
msgstr ""

#. <br>
msgctxt "sn45.34:4.3"
msgid "Te janā pāramessanti,"
msgstr ""

#. <br>
msgctxt "sn45.34:4.4"
msgid "maccudheyyaṃ suduttaraṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn45.34:5.1"
msgid "Kaṇhaṃ dhammaṃ vippahāya,"
msgstr ""

#. <br>
msgctxt "sn45.34:5.2"
msgid "sukkaṃ bhāvetha paṇḍito;"
msgstr ""

#. <br>
msgctxt "sn45.34:5.3"
msgid "Okā anokamāgamma,"
msgstr ""

#. <br>
msgctxt "sn45.34:5.4"
msgid "viveke yattha dūramaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn45.34:6.1"
msgid "Tatrābhiratimiccheyya,"
msgstr ""

#. <br>
msgctxt "sn45.34:6.2"
msgid "hitvā kāme akiñcano;"
msgstr ""

#. <br>
msgctxt "sn45.34:6.3"
msgid "Pariyodapeyya attānaṃ,"
msgstr ""

#. <br>
msgctxt "sn45.34:6.4"
msgid "cittaklesehi paṇḍito."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn45.34:7.1"
msgid "Yesaṃ sambodhiyaṅgesu,"
msgstr ""

#. <br>
msgctxt "sn45.34:7.2"
msgid "sammā cittaṃ subhāvitaṃ;"
msgstr ""

#. <br>
msgctxt "sn45.34:7.3"
msgid "Ādānapaṭinissagge,"
msgstr ""

#. <br>
msgctxt "sn45.34:7.4"
msgid "anupādāya ye ratā;"
msgstr ""

#. <br>
msgctxt "sn45.34:7.5"
msgid "Khīṇāsavā jutimanto,"
msgstr ""

#. <br>
msgctxt "sn45.34:7.6"
msgid "te loke parinibbutā”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn45.34:7.7"
msgid "Catutthaṃ."
msgstr ""
