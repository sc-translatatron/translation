#. <section class="sutta" id="sn9.6"><article><div class="hgroup"><p class="division">
msgctxt "sn9.6:0.1"
msgid "Saṃyutta Nikāya 9"
msgstr ""

#. </p><p>
msgctxt "sn9.6:0.2"
msgid "1. Vanavagga"
msgstr ""

#. </p><h1>
msgctxt "sn9.6:0.3"
msgid "6. Anuruddhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn9.6:1.1"
msgid "Ekaṃ samayaṃ āyasmā anuruddho kosalesu viharati aññatarasmiṃ vanasaṇḍe."
msgstr ""

msgctxt "sn9.6:1.2"
msgid ""
"Atha kho aññatarā tāvatiṃsakāyikā devatā jālinī nāma āyasmato anuruddhassa "
"purāṇadutiyikā yenāyasmā anuruddho tenupasaṅkami; upasaṅkamitvā āyasmantaṃ "
"anuruddhaṃ gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.431"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn9.6:2.1"
msgid "“Tattha cittaṃ paṇidhehi,"
msgstr ""

#. <br>
msgctxt "sn9.6:2.2"
msgid "yattha te vusitaṃ pure;"
msgstr ""

#. <br>
msgctxt "sn9.6:2.3"
msgid "Tāvatiṃsesu devesu,"
msgstr ""

#. <br>
msgctxt "sn9.6:2.4"
msgid "sabbakāmasamiddhisu;"
msgstr ""

#. <br>
msgctxt "sn9.6:2.5"
msgid "Purakkhato parivuto,"
msgstr ""

#. <br>
msgctxt "sn9.6:2.6"
msgid "devakaññāhi sobhasī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn9.6:3.1"
msgid "“Duggatā devakaññāyo,"
msgstr ""

#. <br>
msgctxt "sn9.6:3.2"
msgid "sakkāyasmiṃ patiṭṭhitā;"
msgstr ""

#. <br>
msgctxt "sn9.6:3.3"
msgid "Te cāpi duggatā sattā,"
msgstr ""

# See BB for reading.
#. <br>
msgctxt "sn9.6:3.4"
msgid "devakaññāhi patthitā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn9.6:4.1"
msgid "“Na te sukhaṃ pajānanti,"
msgstr ""

#. <br>
msgctxt "sn9.6:4.2"
msgid "ye na passanti nandanaṃ;"
msgstr ""

#. <br>
msgctxt "sn9.6:4.3"
msgid "Āvāsaṃ naradevānaṃ,"
msgstr ""

#. <br>
msgctxt "sn9.6:4.4"
msgid "tidasānaṃ yasassinan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn9.6:5.1"
msgid "“Na tvaṃ bāle vijānāsi,"
msgstr ""

#. <br>
msgctxt "sn9.6:5.2"
msgid "yathā arahataṃ vaco;"
msgstr ""

#. <br>
msgctxt "sn9.6:5.3"
msgid "Aniccā sabbasaṅkhārā,"
msgstr ""

#. <br>
msgctxt "sn9.6:5.4"
msgid "uppādavayadhammino;"
msgstr ""

#. <br>
msgctxt "sn9.6:5.5"
msgid "Uppajjitvā nirujjhanti,"
msgstr ""

#. <br>
msgctxt "sn9.6:5.6"
msgid "tesaṃ vūpasamo sukho."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.432"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "sn9.6:6.1"
msgid "Natthi dāni punāvāso,"
msgstr ""

#. <br>
msgctxt "sn9.6:6.2"
msgid "devakāyasmi jālini;"
msgstr ""

#. <br>
msgctxt "sn9.6:6.3"
msgid "Vikkhīṇo jātisaṃsāro,"
msgstr ""

#. <br>
msgctxt "sn9.6:6.4"
msgid "natthi dāni punabbhavo”ti."
msgstr ""
