#. <section class="sutta" id="sn37.30"><article><div class="hgroup"><p class="division">
msgctxt "sn37.30:0.1"
msgid "Saṃyutta Nikāya 37"
msgstr ""

#. </p><p>
msgctxt "sn37.30:0.2"
msgid "3. Balavagga"
msgstr ""

#. </p><h1>
#. VAR: 30. Nāsentisutta → nāsitabbasuttaṃ (bj)
msgctxt "sn37.30:0.3"
msgid "30. Nāsentisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.248"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn37.30:1.1"
msgid "“Pañcimāni, bhikkhave, mātugāmassa balāni."
msgstr ""

msgctxt "sn37.30:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "sn37.30:1.3"
msgid "Rūpabalaṃ, bhogabalaṃ, ñātibalaṃ, puttabalaṃ, sīlabalaṃ."
msgstr ""

msgctxt "sn37.30:1.4"
msgid ""
"Rūpabalena ca, bhikkhave, mātugāmo samannāgato hoti, na ca sīlabalena, "
"nāsenteva naṃ, kule na vāsenti."
msgstr ""

msgctxt "sn37.30:1.5"
msgid ""
"Rūpabalena ca, bhikkhave, mātugāmo samannāgato hoti, bhogabalena ca, na ca "
"sīlabalena, nāsenteva naṃ, kule na vāsenti."
msgstr ""

msgctxt "sn37.30:1.6"
msgid ""
"Rūpabalena ca, bhikkhave, mātugāmo samannāgato hoti, bhogabalena ca, "
"ñātibalena ca, na ca sīlabalena, nāsenteva naṃ, kule na vāsenti."
msgstr ""

msgctxt "sn37.30:1.7"
msgid ""
"Rūpabalena ca, bhikkhave, mātugāmo samannāgato hoti, bhogabalena ca, "
"ñātibalena ca, puttabalena ca, na ca sīlabalena, nāsenteva naṃ, kule na "
"vāsenti."
msgstr ""

msgctxt "sn37.30:1.8"
msgid ""
"Sīlabalena ca, bhikkhave, mātugāmo samannāgato hoti, na ca rūpabalena, "
"vāsenteva naṃ, kule na nāsenti."
msgstr ""

msgctxt "sn37.30:1.9"
msgid ""
"Sīlabalena ca, bhikkhave, mātugāmo samannāgato hoti, na ca bhogabalena, "
"vāsenteva naṃ, kule na nāsenti."
msgstr ""

msgctxt "sn37.30:1.10"
msgid ""
"Sīlabalena ca, bhikkhave, mātugāmo samannāgato hoti, na ca ñātibalena, "
"vāsenteva naṃ, kule na nāsenti."
msgstr ""

msgctxt "sn37.30:1.11"
msgid ""
"Sīlabalena ca, bhikkhave, mātugāmo samannāgato hoti, na ca puttabalena, "
"vāsenteva naṃ, kule na nāsenti."
msgstr ""

msgctxt "sn37.30:1.12"
msgid "Imāni kho, bhikkhave, pañca mātugāmassa balānī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn37.30:1.13"
msgid "Chaṭṭhaṃ."
msgstr ""
