#. <section class="sutta" id="sn4.16"><article><div class="hgroup"><p class="division">
msgctxt "sn4.16:0.1"
msgid "Saṃyutta Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "sn4.16:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn4.16:0.3"
msgid "16. Pattasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.112"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn4.16:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

#. VAR: samādapeti → samādāpeti (?)
msgctxt "sn4.16:1.2"
msgid ""
"Tena kho pana samayena bhagavā pañcannaṃ upādānakkhandhānaṃ upādāya "
"bhikkhūnaṃ dhammiyā kathāya sandasseti samādapeti samuttejeti sampahaṃseti."
msgstr ""

#. VAR: aṭṭhiṃ katvā → aṭṭhikatvā (bj, s1-3, km, pts1-2)
#. VAR: sabbacetasā → sabbacetaso (si, s1-3, km, pts1) | sabbaṃ cetasā (mr)
msgctxt "sn4.16:1.3"
msgid ""
"Te ca bhikkhū aṭṭhiṃ katvā manasi katvā sabbacetasā samannāharitvā ohitasotā "
"dhammaṃ suṇanti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.250"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn4.16:2.1"
msgid "Atha kho mārassa pāpimato etadahosi:"
msgstr ""

msgctxt "sn4.16:2.2"
msgid ""
"“ayaṃ kho samaṇo gotamo pañcannaṃ upādānakkhandhānaṃ upādāya bhikkhūnaṃ "
"dhammiyā kathāya sandasseti samādapeti samuttejeti sampahaṃseti."
msgstr ""

msgctxt "sn4.16:2.3"
msgid ""
"Te ca bhikkhū aṭṭhiṃ katvā manasi katvā sabbacetasā samannāharitvā ohitasotā "
"dhammaṃ suṇanti."
msgstr ""

msgctxt "sn4.16:2.4"
msgid "Yannūnāhaṃ yena samaṇo gotamo tenupasaṅkameyyaṃ vicakkhukammāyā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn4.16:3.1"
msgid "Tena kho pana samayena sambahulā pattā abbhokāse nikkhittā honti."
msgstr ""

msgctxt "sn4.16:3.2"
msgid ""
"Atha kho māro pāpimā balībaddavaṇṇaṃ abhinimminitvā yena te pattā "
"tenupasaṅkami."
msgstr ""

msgctxt "sn4.16:3.3"
msgid "Atha kho aññataro bhikkhu aññataraṃ bhikkhuṃ etadavoca:"
msgstr ""

msgctxt "sn4.16:3.4"
msgid "“bhikkhu bhikkhu, eso balībaddo patte bhindeyyā”ti."
msgstr ""

msgctxt "sn4.16:3.5"
msgid "Evaṃ vutte, bhagavā taṃ bhikkhuṃ etadavoca:"
msgstr ""

msgctxt "sn4.16:3.6"
msgid "“na so, bhikkhu, balībaddo."
msgstr ""

msgctxt "sn4.16:3.7"
msgid "Māro eso pāpimā tumhākaṃ vicakkhukammāya āgato”ti."
msgstr ""

msgctxt "sn4.16:3.8"
msgid ""
"Atha kho bhagavā “māro ayaṃ pāpimā” iti viditvā māraṃ pāpimantaṃ gāthāya "
"ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn4.16:4.1"
msgid "“Rūpaṃ vedayitaṃ saññā,"
msgstr ""

#. <br>
msgctxt "sn4.16:4.2"
msgid "viññāṇaṃ yañca saṅkhataṃ;"
msgstr ""

#. <br>
msgctxt "sn4.16:4.3"
msgid "Nesohamasmi netaṃ me,"
msgstr ""

#. <br>
msgctxt "sn4.16:4.4"
msgid "evaṃ tattha virajjati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn4.16:5.1"
msgid "Evaṃ virattaṃ khemattaṃ,"
msgstr ""

#. <br>
msgctxt "sn4.16:5.2"
msgid "sabbasaṃyojanātigaṃ;"
msgstr ""

#. <br>
msgctxt "sn4.16:5.3"
msgid "Anvesaṃ sabbaṭṭhānesu,"
msgstr ""

#. <br>
msgctxt "sn4.16:5.4"
msgid "mārasenāpi nājjhagā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts2ed" id="pts2ed1.251"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "sn4.16:6.1"
msgid "Atha kho māro pāpimā … pe … tatthevantaradhāyīti."
msgstr ""
