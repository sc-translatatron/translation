msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.4:0.2"
msgid "1. Naḷavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.4:0.3"
msgid "4. Accentisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.3"></a>
#. <a class="pts2ed" id="pts2ed1.5"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.4:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn1.4:1.2"
msgid "Ekamantaṃ ṭhitā kho sā devatā bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.4:2.1"
msgid "“Accenti kālā tarayanti rattiyo,"
msgstr ""

#. <br>
msgctxt "sn1.4:2.2"
msgid "Vayoguṇā anupubbaṃ jahanti;"
msgstr ""

#. <br>
msgctxt "sn1.4:2.3"
msgid "Etaṃ bhayaṃ maraṇe pekkhamāno,"
msgstr ""

#. <br>
msgctxt "sn1.4:2.4"
msgid "Puññāni kayirātha sukhāvahānī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.4:3.1"
msgid "“Accenti kālā tarayanti rattiyo,"
msgstr ""

#. <br>
msgctxt "sn1.4:3.2"
msgid "Vayoguṇā anupubbaṃ jahanti;"
msgstr ""

#. <br>
msgctxt "sn1.4:3.3"
msgid "Etaṃ bhayaṃ maraṇe pekkhamāno,"
msgstr ""

#. <br>
msgctxt "sn1.4:3.4"
msgid "Lokāmisaṃ pajahe santipekkho”ti."
msgstr ""
