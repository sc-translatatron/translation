#. <section class="sutta" id="sn45.24"><article><div class="hgroup"><p class="division">
msgctxt "sn45.24:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.24:0.2"
msgid "3. Micchattavagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.24:0.3"
msgid "24. Dutiyapaṭipadāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.19"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.24:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.24:1.2"
msgid "“Gihino vāhaṃ, bhikkhave, pabbajitassa vā micchāpaṭipadaṃ na vaṇṇemi."
msgstr ""

msgctxt "sn45.24:1.3"
msgid ""
"Gihi vā, bhikkhave, pabbajito vā micchāpaṭipanno "
"micchāpaṭipattādhikaraṇahetu nārādhako hoti ñāyaṃ dhammaṃ kusalaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn45.24:2.1"
msgid "Katamā ca, bhikkhave, micchāpaṭipadā?"
msgstr ""

msgctxt "sn45.24:2.2"
msgid "Seyyathidaṃ—micchādiṭṭhi … pe … micchāsamādhi."
msgstr ""

msgctxt "sn45.24:2.3"
msgid "Ayaṃ vuccati, bhikkhave, micchāpaṭipadā."
msgstr ""

msgctxt "sn45.24:2.4"
msgid "Gihino vāhaṃ, bhikkhave, pabbajitassa vā micchāpaṭipadaṃ na vaṇṇemi."
msgstr ""

msgctxt "sn45.24:2.5"
msgid ""
"Gihi vā, bhikkhave, pabbajito vā micchāpaṭipanno "
"micchāpaṭipattādhikaraṇahetu nārādhako hoti ñāyaṃ dhammaṃ kusalaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn45.24:3.1"
msgid "Gihino vāhaṃ, bhikkhave, pabbajitassa vā sammāpaṭipadaṃ vaṇṇemi."
msgstr ""

msgctxt "sn45.24:3.2"
msgid ""
"Gihi vā, bhikkhave, pabbajito vā sammāpaṭipanno sammāpaṭipattādhikaraṇahetu "
"ārādhako hoti ñāyaṃ dhammaṃ kusalaṃ."
msgstr ""

msgctxt "sn45.24:3.3"
msgid "Katamā ca, bhikkhave, sammāpaṭipadā?"
msgstr ""

msgctxt "sn45.24:3.4"
msgid "Seyyathidaṃ—sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

msgctxt "sn45.24:3.5"
msgid "Ayaṃ vuccati, bhikkhave, sammāpaṭipadā."
msgstr ""

msgctxt "sn45.24:3.6"
msgid "Gihino vāhaṃ, bhikkhave, pabbajitassa vā sammāpaṭipadaṃ vaṇṇemi."
msgstr ""

msgctxt "sn45.24:3.7"
msgid ""
"Gihi vā, bhikkhave, pabbajito vā sammāpaṭipanno sammāpaṭipattādhikaraṇahetu "
"ārādhako hoti ñāyaṃ dhammaṃ kusalan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.24:3.8"
msgid "Catutthaṃ."
msgstr ""
