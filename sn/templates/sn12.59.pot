#. <section class="sutta" id="sn12.59"><article><div class="hgroup"><p class="division">
msgctxt "sn12.59:0.1"
msgid "Saṃyutta Nikāya 12"
msgstr ""

#. </p><p>
msgctxt "sn12.59:0.2"
msgid "6. Dukkhavagga"
msgstr ""

#. </p><h1>
msgctxt "sn12.59:0.3"
msgid "59. Viññāṇasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn12.59:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn12.59:1.2"
msgid ""
"“Saṃyojaniyesu, bhikkhave, dhammesu assādānupassino viharato viññāṇassa "
"avakkanti hoti."
msgstr ""

msgctxt "sn12.59:1.3"
msgid "Viññāṇapaccayā nāmarūpaṃ … pe …"
msgstr ""

msgctxt "sn12.59:1.4"
msgid "evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn12.59:2.1"
msgid "Seyyathāpi, bhikkhave, mahārukkho."
msgstr ""

msgctxt "sn12.59:2.2"
msgid "Tassa yāni ceva mūlāni … pe …"
msgstr ""

msgctxt "sn12.59:2.3"
msgid ""
"evameva kho, bhikkhave, saṃyojaniyesu dhammesu assādānupassino viharato "
"viññāṇassa avakkanti hoti … pe …."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn12.59:3.1"
msgid ""
"Saṃyojaniyesu, bhikkhave, dhammesu ādīnavānupassino viharato viññāṇassa "
"avakkanti na hoti."
msgstr ""

msgctxt "sn12.59:3.2"
msgid "Viññāṇanirodhā nāmarūpanirodho … pe …"
msgstr ""

msgctxt "sn12.59:3.3"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn12.59:4.1"
msgid "Seyyathāpi, bhikkhave, mahārukkho."
msgstr ""

msgctxt "sn12.59:4.2"
msgid "Atha puriso āgaccheyya kuddālapiṭakaṃ ādāya … pe …"
msgstr ""

msgctxt "sn12.59:4.3"
msgid "āyatiṃ anuppādadhammo."
msgstr ""

msgctxt "sn12.59:4.4"
msgid ""
"Evameva kho, bhikkhave, saṃyojaniyesu dhammesu ādīnavānupassino viharato "
"viññāṇassa avakkanti na hoti."
msgstr ""

msgctxt "sn12.59:4.5"
msgid "Viññāṇassa nirodhā nāmarūpanirodho … pe …"
msgstr ""

msgctxt "sn12.59:4.6"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.59:4.7"
msgid "Navamaṃ."
msgstr ""
