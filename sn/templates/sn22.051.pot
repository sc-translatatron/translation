#. <section class="sutta" id="sn22.51"><article><div class="hgroup"><p class="division">
msgctxt "sn22.51:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.51:0.2"
msgid "5. Attadīpavagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.51:0.3"
msgid "51. Nandikkhayasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.51:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.51:1.2"
msgid ""
"“Aniccaññeva, bhikkhave, bhikkhu rūpaṃ aniccanti passati. Sāssa hoti "
"sammādiṭṭhi."
msgstr ""

msgctxt "sn22.51:1.3"
msgid "Sammā passaṃ nibbindati."
msgstr ""

msgctxt "sn22.51:1.4"
msgid "Nandikkhayā rāgakkhayo, rāgakkhayā nandikkhayo."
msgstr ""

msgctxt "sn22.51:1.5"
msgid "Nandirāgakkhayā cittaṃ vimuttaṃ suvimuttanti vuccati."
msgstr ""

msgctxt "sn22.51:1.6"
msgid ""
"Aniccaññeva, bhikkhave, bhikkhu vedanaṃ aniccanti passati. Sāssa hoti "
"sammādiṭṭhi."
msgstr ""

msgctxt "sn22.51:1.7"
msgid "Sammā passaṃ nibbindati."
msgstr ""

msgctxt "sn22.51:1.8"
msgid "Nandikkhayā rāgakkhayo, rāgakkhayā nandikkhayo."
msgstr ""

msgctxt "sn22.51:1.9"
msgid "Nandirāgakkhayā cittaṃ vimuttaṃ suvimuttanti vuccati."
msgstr ""

msgctxt "sn22.51:1.10"
msgid "Aniccaññeva, bhikkhave, bhikkhu saññaṃ aniccanti passati … pe …"
msgstr ""

msgctxt "sn22.51:1.11"
msgid ""
"anicceyeva bhikkhave, bhikkhu saṅkhāre aniccāti passati. Sāssa hoti "
"sammādiṭṭhi."
msgstr ""

msgctxt "sn22.51:1.12"
msgid "Sammā passaṃ nibbindati."
msgstr ""

msgctxt "sn22.51:1.13"
msgid "Nandikkhayā rāgakkhayo, rāgakkhayā nandikkhayo."
msgstr ""

msgctxt "sn22.51:1.14"
msgid "Nandirāgakkhayā cittaṃ vimuttaṃ suvimuttanti vuccati."
msgstr ""

msgctxt "sn22.51:1.15"
msgid ""
"Aniccaññeva, bhikkhave, bhikkhu viññāṇaṃ aniccanti passati. Sāssa hoti "
"sammādiṭṭhi."
msgstr ""

msgctxt "sn22.51:1.16"
msgid "Sammā passaṃ nibbindati."
msgstr ""

msgctxt "sn22.51:1.17"
msgid "Nandikkhayā rāgakkhayo, rāgakkhayā nandikkhayo."
msgstr ""

msgctxt "sn22.51:1.18"
msgid "Nandirāgakkhayā cittaṃ vimuttaṃ suvimuttanti vuccatī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.51:1.19"
msgid "Navamaṃ."
msgstr ""
