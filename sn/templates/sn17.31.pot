#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.240"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn17.31:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn17.31:1.2"
msgid "“Dāruṇo, bhikkhave, lābhasakkārasiloko."
msgstr ""

msgctxt "sn17.31:1.3"
msgid ""
"Lābhasakkārasilokena abhibhūto pariyādiṇṇacitto, bhikkhave, devadatto "
"saṃghaṃ bhindi."
msgstr ""

msgctxt "sn17.31:1.4"
msgid "Evaṃ dāruṇo kho, bhikkhave, lābhasakkārasiloko … pe …"
msgstr ""

msgctxt "sn17.31:1.5"
msgid "sikkhitabban”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn17.31:1.6"
msgid "Paṭhamaṃ."
msgstr ""
