#. <section class="sutta" id="sn22.52"><article><div class="hgroup"><p class="division">
msgctxt "sn22.52:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.52:0.2"
msgid "5. Attadīpavagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.52:0.3"
msgid "52. Dutiyanandikkhayasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.52"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.52:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.52:1.2"
msgid ""
"“Rūpaṃ, bhikkhave, yoniso manasi karotha, rūpāniccatañca yathābhūtaṃ "
"samanupassatha."
msgstr ""

msgctxt "sn22.52:1.3"
msgid ""
"Rūpaṃ, bhikkhave, bhikkhu yoniso manasi karonto, rūpāniccatañca yathābhūtaṃ "
"samanupassanto rūpasmiṃ nibbindati."
msgstr ""

msgctxt "sn22.52:1.4"
msgid "Nandikkhayā rāgakkhayo, rāgakkhayā nandikkhayo."
msgstr ""

msgctxt "sn22.52:1.5"
msgid "Nandirāgakkhayā cittaṃ vimuttaṃ suvimuttanti vuccati."
msgstr ""

msgctxt "sn22.52:1.6"
msgid ""
"Vedanaṃ, bhikkhave, yoniso manasi karotha, vedanāniccatañca yathābhūtaṃ "
"samanupassatha."
msgstr ""

msgctxt "sn22.52:1.7"
msgid ""
"Vedanaṃ, bhikkhave, bhikkhu yoniso manasi karonto, vedanāniccatañca "
"yathābhūtaṃ samanupassanto vedanāya nibbindati."
msgstr ""

msgctxt "sn22.52:1.8"
msgid "Nandikkhayā rāgakkhayo, rāgakkhayā nandikkhayo."
msgstr ""

msgctxt "sn22.52:1.9"
msgid "Nandirāgakkhayā cittaṃ vimuttaṃ suvimuttanti vuccati."
msgstr ""

msgctxt "sn22.52:1.10"
msgid "Saññaṃ bhikkhave …"
msgstr ""

msgctxt "sn22.52:1.11"
msgid ""
"saṅkhāre, bhikkhave, yoniso manasi karotha, saṅkhārāniccatañca yathābhūtaṃ "
"samanupassatha."
msgstr ""

msgctxt "sn22.52:1.12"
msgid ""
"Saṅkhāre, bhikkhave, bhikkhu yoniso manasi karonto, saṅkhārāniccataṃ "
"yathābhūtaṃ samanupassanto saṅkhāresu nibbindati."
msgstr ""

msgctxt "sn22.52:1.13"
msgid "Nandikkhayā rāgakkhayo, rāgakkhayā nandikkhayo."
msgstr ""

msgctxt "sn22.52:1.14"
msgid "Nandirāgakkhayā cittaṃ vimuttaṃ suvimuttanti vuccati."
msgstr ""

msgctxt "sn22.52:1.15"
msgid ""
"Viññāṇaṃ, bhikkhave, yoniso manasi karotha, viññāṇāniccatañca yathābhūtaṃ "
"samanupassatha."
msgstr ""

msgctxt "sn22.52:1.16"
msgid ""
"Viññāṇaṃ, bhikkhave, bhikkhu yoniso manasi karonto, viññāṇāniccatañca "
"yathābhūtaṃ samanupassanto viññāṇasmiṃ nibbindati."
msgstr ""

msgctxt "sn22.52:1.17"
msgid "Nandikkhayā rāgakkhayo, rāgakkhayā nandikkhayo."
msgstr ""

msgctxt "sn22.52:1.18"
msgid "Nandirāgakkhayā cittaṃ vimuttaṃ suvimuttanti vuccatī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.52:1.19"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.52:1.20"
msgid "Attadīpavaggo pañcamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn22.52:2.1"
msgid "Attadīpā paṭipadā,"
msgstr ""

#. <br>
msgctxt "sn22.52:2.2"
msgid "Dve ca honti aniccatā;"
msgstr ""

#. <br>
msgctxt "sn22.52:2.3"
msgid "Samanupassanā khandhā,"
msgstr ""

#. <br>
msgctxt "sn22.52:2.4"
msgid "Dve soṇā dve nandikkhayena cāti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn22.52:2.5"
msgid "Mūlapaṇṇāsako samatto."
msgstr ""

#. </p><blockquote class="vagguddanagatha"><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.53"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn22.52:3.1"
msgid "Nakulapitā anicco ca,"
msgstr ""

#. <br>
msgctxt "sn22.52:3.2"
msgid "Bhāro natumhākena ca;"
msgstr ""

#. <br>
msgctxt "sn22.52:3.3"
msgid "Attadīpena paññāso,"
msgstr ""

#. <br>
msgctxt "sn22.52:3.4"
msgid "Paṭhamo tena pavuccatīti."
msgstr ""
