#. <section class="sutta" id="sn43.12"><article><div class="hgroup"><p class="division">
msgctxt "sn43.12:0.1"
msgid "Saṃyutta Nikāya 43"
msgstr ""

#. </p><p>
#. VAR: 2. Dutiyavagga → aasaṅkhatavaggo (bj)
msgctxt "sn43.12:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn43.12:0.3"
msgid "12. Asaṅkhatasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.362"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn43.12:1.1"
msgid "“Asaṅkhatañca vo, bhikkhave, desessāmi asaṅkhatagāmiñca maggaṃ."
msgstr ""

msgctxt "sn43.12:1.2"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn43.12:1.3"
msgid "Katamañca, bhikkhave, asaṅkhataṃ?"
msgstr ""

msgctxt "sn43.12:1.4"
msgid "Yo, bhikkhave, rāgakkhayo dosakkhayo mohakkhayo—"
msgstr ""

msgctxt "sn43.12:1.5"
msgid "idaṃ vuccati, bhikkhave, asaṅkhataṃ."
msgstr ""

msgctxt "sn43.12:1.6"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:1.7"
msgid "Samatho."
msgstr ""

msgctxt "sn43.12:1.8"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo."
msgstr ""

msgctxt "sn43.12:1.9"
msgid ""
"Iti kho, bhikkhave, desitaṃ vo mayā asaṅkhataṃ, desito asaṅkhatagāmimaggo."
msgstr ""

msgctxt "sn43.12:1.10"
msgid ""
"Yaṃ, bhikkhave, satthārā karaṇīyaṃ sāvakānaṃ hitesinā anukampakena anukampaṃ "
"upādāya, kataṃ vo taṃ mayā."
msgstr ""

msgctxt "sn43.12:1.11"
msgid ""
"Etāni, bhikkhave, rukkhamūlāni, etāni suññāgārāni. Jhāyatha, bhikkhave, mā "
"pamādattha; mā pacchā vippaṭisārino ahuvattha. Ayaṃ vo amhākaṃ anusāsanīti. "
"(1.1)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn43.12:2.1"
msgid "Asaṅkhatañca vo, bhikkhave, desessāmi asaṅkhatagāmiñca maggaṃ."
msgstr ""

msgctxt "sn43.12:2.2"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn43.12:2.3"
msgid "Katamañca, bhikkhave, asaṅkhataṃ?"
msgstr ""

msgctxt "sn43.12:2.4"
msgid "Yo, bhikkhave, rāgakkhayo dosakkhayo mohakkhayo—"
msgstr ""

msgctxt "sn43.12:2.5"
msgid "idaṃ vuccati, bhikkhave, asaṅkhataṃ."
msgstr ""

msgctxt "sn43.12:2.6"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:2.7"
msgid "Vipassanā."
msgstr ""

msgctxt "sn43.12:2.8"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo."
msgstr ""

msgctxt "sn43.12:2.9"
msgid "Iti kho, bhikkhave, desitaṃ vo mayā asaṅkhataṃ … pe …"
msgstr ""

msgctxt "sn43.12:2.10"
msgid "ayaṃ vo amhākaṃ anusāsanīti. (1.2)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.363"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn43.12:3.1"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:3.2"
msgid "Savitakko savicāro samādhi."
msgstr ""

msgctxt "sn43.12:3.3"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:3.4"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:3.5"
msgid "Avitakko vicāramatto samādhi."
msgstr ""

msgctxt "sn43.12:3.6"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:3.7"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:3.8"
msgid ""
"Avitakko avicāro samādhi. Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
". (1.3–5.)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn43.12:4.1"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:4.2"
msgid "Suññato samādhi."
msgstr ""

msgctxt "sn43.12:4.3"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:4.4"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:4.5"
msgid "Animitto samādhi."
msgstr ""

msgctxt "sn43.12:4.6"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:4.7"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:4.8"
msgid ""
"Appaṇihito samādhi. Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …. "
"(1.6–8.)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.364"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn43.12:5.1"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:5.2"
msgid ""
"Idha, bhikkhave, bhikkhu kāye kāyānupassī viharati ātāpī sampajāno satimā "
"vineyya loke abhijjhādomanassaṃ."
msgstr ""

msgctxt "sn43.12:5.3"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:5.4"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:5.5"
msgid "Idha, bhikkhave, bhikkhu vedanāsu vedanānupassī viharati … pe …"
msgstr ""

msgctxt "sn43.12:5.6"
msgid "ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:5.7"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:5.8"
msgid "Idha, bhikkhave, bhikkhu citte cittānupassī viharati … pe …"
msgstr ""

msgctxt "sn43.12:5.9"
msgid "ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:5.10"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:5.11"
msgid ""
"Idha, bhikkhave, bhikkhu dhammesu dhammānupassī viharati … pe … ayaṃ "
"vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …. (1.9–12.)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.365"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "sn43.12:6.1"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:6.2"
msgid ""
"Idha, bhikkhave, bhikkhu anuppannānaṃ pāpakānaṃ akusalānaṃ dhammānaṃ "
"anuppādāya chandaṃ janeti vāyamati vīriyaṃ ārabhati cittaṃ paggaṇhāti "
"padahati."
msgstr ""

msgctxt "sn43.12:6.3"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:6.4"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:6.5"
msgid ""
"Idha, bhikkhave, bhikkhu uppannānaṃ pāpakānaṃ akusalānaṃ dhammānaṃ pahānāya "
"chandaṃ janeti vāyamati vīriyaṃ ārabhati cittaṃ paggaṇhāti padahati."
msgstr ""

msgctxt "sn43.12:6.6"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:6.7"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:6.8"
msgid ""
"Idha, bhikkhave, bhikkhu anuppannānaṃ kusalānaṃ dhammānaṃ uppādāya chandaṃ "
"janeti vāyamati vīriyaṃ ārabhati cittaṃ paggaṇhāti padahati."
msgstr ""

msgctxt "sn43.12:6.9"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:6.10"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:6.11"
msgid ""
"Idha, bhikkhave, bhikkhu uppannānaṃ kusalānaṃ dhammānaṃ ṭhitiyā asammosāya "
"bhiyyobhāvāya vepullāya bhāvanāya pāripūriyā chandaṃ janeti vāyamati vīriyaṃ "
"ārabhati cittaṃ paggaṇhāti padahati. Ayaṃ vuccati, bhikkhave, "
"asaṅkhatagāmimaggo … pe …. (1.13–16.)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn43.12:7.1"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:7.2"
msgid ""
"Idha, bhikkhave, bhikkhu chandasamādhipadhānasaṅkhārasamannāgataṃ iddhipādaṃ "
"bhāveti."
msgstr ""

msgctxt "sn43.12:7.3"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:7.4"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:7.5"
msgid ""
"Idha, bhikkhave, bhikkhu vīriyasamādhipadhānasaṅkhārasamannāgataṃ iddhipādaṃ "
"bhāveti."
msgstr ""

msgctxt "sn43.12:7.6"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:7.7"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:7.8"
msgid ""
"Idha, bhikkhave, bhikkhu cittasamādhipadhānasaṅkhārasamannāgataṃ iddhipādaṃ "
"bhāveti."
msgstr ""

msgctxt "sn43.12:7.9"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:7.10"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:7.11"
msgid ""
"Idha, bhikkhave, bhikkhu vīmaṃsasamādhipadhānasaṅkhārasamannāgataṃ "
"iddhipādaṃ bhāveti. Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …. "
"(1.17–20.)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.366"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "sn43.12:8.1"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:8.2"
msgid ""
"Idha, bhikkhave, bhikkhu saddhindriyaṃ bhāveti vivekanissitaṃ virāganissitaṃ "
"nirodhanissitaṃ vossaggapariṇāmiṃ."
msgstr ""

msgctxt "sn43.12:8.3"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:8.4"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:8.5"
msgid "Idha, bhikkhave, bhikkhu vīriyindriyaṃ bhāveti vivekanissitaṃ … pe …"
msgstr ""

msgctxt "sn43.12:8.6"
msgid "ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:8.7"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:8.8"
msgid "Idha, bhikkhave, bhikkhu satindriyaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:8.9"
msgid "ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:8.10"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:8.11"
msgid "Idha, bhikkhave, bhikkhu samādhindriyaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:8.12"
msgid "ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:8.13"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:8.14"
msgid ""
"Idha, bhikkhave, bhikkhu paññindriyaṃ bhāveti vivekanissitaṃ virāganissitaṃ "
"nirodhanissitaṃ vossaggapariṇāmiṃ. Ayaṃ vuccati, bhikkhave, "
"asaṅkhatagāmimaggo … pe …. (1.21–25.)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.367"></a>
#. <a class="sc" id="sc9"></a>
msgctxt "sn43.12:9.1"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:9.2"
msgid "Idha, bhikkhave, bhikkhu saddhābalaṃ bhāveti vivekanissitaṃ … pe …"
msgstr ""

msgctxt "sn43.12:9.3"
msgid "ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:9.4"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:9.5"
msgid "Idha, bhikkhave, bhikkhu vīriyabalaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:9.6"
msgid "ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:9.7"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:9.8"
msgid "Idha, bhikkhave, bhikkhu satibalaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:9.9"
msgid "ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:9.10"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:9.11"
msgid "Idha, bhikkhave, bhikkhu samādhibalaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:9.12"
msgid "ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:9.13"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:9.14"
msgid ""
"Idha, bhikkhave, bhikkhu paññābalaṃ bhāveti vivekanissitaṃ virāganissitaṃ "
"nirodhanissitaṃ vossaggapariṇāmiṃ. Ayaṃ vuccati, bhikkhave, "
"asaṅkhatagāmimaggo … pe …. (1.26–30.)"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn43.12:10.1"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:10.2"
msgid ""
"Idha, bhikkhave, bhikkhu satisambojjhaṅgaṃ bhāveti vivekanissitaṃ … pe …"
msgstr ""

msgctxt "sn43.12:10.3"
msgid "ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:10.4"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:10.5"
msgid "Idha, bhikkhave, bhikkhu dhammavicayasambojjhaṅgaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:10.6"
msgid "vīriyasambojjhaṅgaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:10.7"
msgid "pītisambojjhaṅgaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:10.8"
msgid "passaddhisambojjhaṅgaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:10.9"
msgid "samādhisambojjhaṅgaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:10.10"
msgid ""
"upekkhāsambojjhaṅgaṃ bhāveti vivekanissitaṃ virāganissitaṃ nirodhanissitaṃ "
"vossaggapariṇāmiṃ. Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …. "
"(1.31–37.)"
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.368"></a>
#. <a class="sc" id="sc11"></a>
msgctxt "sn43.12:11.1"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:11.2"
msgid ""
"Idha, bhikkhave, bhikkhu sammādiṭṭhiṃ bhāveti vivekanissitaṃ virāganissitaṃ "
"nirodhanissitaṃ vossaggapariṇāmiṃ."
msgstr ""

msgctxt "sn43.12:11.3"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo … pe …"
msgstr ""

msgctxt "sn43.12:11.4"
msgid "katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:11.5"
msgid "Idha, bhikkhave, bhikkhu sammāsaṅkappaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:11.6"
msgid "sammāvācaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:11.7"
msgid "sammākammantaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:11.8"
msgid "sammāājīvaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:11.9"
msgid "sammāvāyāmaṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:11.10"
msgid "sammāsatiṃ bhāveti … pe …"
msgstr ""

msgctxt "sn43.12:11.11"
msgid "asaṅkhatañca vo, bhikkhave, desessāmi asaṅkhatagāmiñca maggaṃ."
msgstr ""

msgctxt "sn43.12:11.12"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn43.12:11.13"
msgid "Katamañca, bhikkhave, asaṅkhataṃ … pe …?"
msgstr ""

msgctxt "sn43.12:11.14"
msgid "Katamo ca, bhikkhave, asaṅkhatagāmimaggo?"
msgstr ""

msgctxt "sn43.12:11.15"
msgid ""
"Idha, bhikkhave, bhikkhu sammāsamādhiṃ bhāveti vivekanissitaṃ virāganissitaṃ "
"nirodhanissitaṃ vossaggapariṇāmiṃ."
msgstr ""

msgctxt "sn43.12:11.16"
msgid "Ayaṃ vuccati, bhikkhave, asaṅkhatagāmimaggo."
msgstr ""

msgctxt "sn43.12:11.17"
msgid ""
"Iti kho, bhikkhave, desitaṃ vo mayā asaṅkhataṃ, desito asaṅkhatagāmimaggo."
msgstr ""

msgctxt "sn43.12:11.18"
msgid ""
"Yaṃ, bhikkhave, satthārā karaṇīyaṃ sāvakānaṃ hitesinā anukampakena anukampaṃ "
"upādāya, kataṃ vo taṃ mayā."
msgstr ""

msgctxt "sn43.12:11.19"
msgid ""
"Etāni, bhikkhave, rukkhamūlāni, etāni suññāgārāni. Jhāyatha, bhikkhave, mā "
"pamādattha; mā pacchā vippaṭisārino ahuvattha. Ayaṃ vo amhākaṃ anusāsanī”ti. "
"(1.38–45)"
msgstr ""

#. </p><p class="endsection">
msgctxt "sn43.12:11.20"
msgid "Paṭhamaṃ."
msgstr ""
