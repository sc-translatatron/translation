#. <section class="sutta" id="sn8.5"><article><div class="hgroup"><p class="division">
msgctxt "sn8.5:0.1"
msgid "Saṃyutta Nikāya 8"
msgstr ""

#. </p><p>
msgctxt "sn8.5:0.2"
msgid "1. Vaṅgīsavagga"
msgstr ""

#. </p><h1>
msgctxt "sn8.5:0.3"
msgid "5. Subhāsitasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn8.5:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn8.5:1.2"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "sn8.5:1.3"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "sn8.5:1.4"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "sn8.5:1.5"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.189"></a>
#. <a class="pts2ed" id="pts2ed1.407"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn8.5:2.1"
msgid ""
"“Catūhi, bhikkhave, aṅgehi samannāgatā vācā subhāsitā hoti, no dubbhāsitā; "
"anavajjā ca ananuvajjā ca viññūnaṃ."
msgstr ""

msgctxt "sn8.5:2.2"
msgid "Katamehi catūhi?"
msgstr ""

msgctxt "sn8.5:2.3"
msgid ""
"Idha, bhikkhave, bhikkhu subhāsitaṃyeva bhāsati no dubbhāsitaṃ, dhammaṃyeva "
"bhāsati no adhammaṃ, piyaṃyeva bhāsati no appiyaṃ, saccaṃyeva bhāsati no "
"alikaṃ."
msgstr ""

msgctxt "sn8.5:2.4"
msgid ""
"Imehi kho, bhikkhave, catūhi aṅgehi samannāgatā vācā subhāsitā hoti, no "
"dubbhāsitā, anavajjā ca ananuvajjā ca viññūnan”ti."
msgstr ""

msgctxt "sn8.5:2.5"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "sn8.5:2.6"
msgid "Idaṃ vatvāna sugato athāparaṃ etadavoca satthā:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn8.5:3.1"
msgid "“Subhāsitaṃ uttamamāhu santo,"
msgstr ""

#. <br>
msgctxt "sn8.5:3.2"
msgid "Dhammaṃ bhaṇe nādhammaṃ taṃ dutiyaṃ;"
msgstr ""

#. <br>
msgctxt "sn8.5:3.3"
msgid "Piyaṃ bhaṇe nāppiyaṃ taṃ tatiyaṃ,"
msgstr ""

#. <br>
msgctxt "sn8.5:3.4"
msgid "Saccaṃ bhaṇe nālikaṃ taṃ catutthan”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn8.5:4.1"
msgid ""
"Atha kho āyasmā vaṅgīso uṭṭhāyāsanā ekaṃsaṃ uttarāsaṅgaṃ karitvā yena "
"bhagavā tenañjaliṃ paṇāmetvā bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn8.5:4.2"
msgid "“paṭibhāti maṃ, bhagavā, paṭibhāti maṃ, sugatā”ti."
msgstr ""

msgctxt "sn8.5:4.3"
msgid "“Paṭibhātu taṃ, vaṅgīsā”ti bhagavā avoca."
msgstr ""

msgctxt "sn8.5:4.4"
msgid ""
"Atha kho āyasmā vaṅgīso bhagavantaṃ sammukhā sāruppāhi gāthāhi abhitthavi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn8.5:5.1"
msgid "“Tameva vācaṃ bhāseyya,"
msgstr ""

#. <br>
msgctxt "sn8.5:5.2"
msgid "yāyattānaṃ na tāpaye;"
msgstr ""

#. <br>
msgctxt "sn8.5:5.3"
msgid "Pare ca na vihiṃseyya,"
msgstr ""

#. <br>
msgctxt "sn8.5:5.4"
msgid "sā ve vācā subhāsitā."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.408"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "sn8.5:6.1"
msgid "Piyavācaṃva bhāseyya,"
msgstr ""

#. <br>
msgctxt "sn8.5:6.2"
msgid "yā vācā paṭinanditā;"
msgstr ""

#. <br>
msgctxt "sn8.5:6.3"
msgid "Yaṃ anādāya pāpāni,"
msgstr ""

#. <br>
msgctxt "sn8.5:6.4"
msgid "paresaṃ bhāsate piyaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn8.5:7.1"
msgid "Saccaṃ ve amatā vācā,"
msgstr ""

#. <br>
msgctxt "sn8.5:7.2"
msgid "esa dhammo sanantano;"
msgstr ""

#. <br>
msgctxt "sn8.5:7.3"
msgid "Sacce atthe ca dhamme ca,"
msgstr ""

#. <br>
msgctxt "sn8.5:7.4"
msgid "āhu santo patiṭṭhitā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn8.5:8.1"
msgid "Yaṃ buddho bhāsate vācaṃ,"
msgstr ""

#. <br>
msgctxt "sn8.5:8.2"
msgid "khemaṃ nibbānapattiyā;"
msgstr ""

#. <br>
msgctxt "sn8.5:8.3"
msgid "Dukkhassantakiriyāya,"
msgstr ""

#. <br>
msgctxt "sn8.5:8.4"
msgid "sā ve vācānamuttamā”ti."
msgstr ""
