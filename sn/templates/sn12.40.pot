#. <section class="sutta" id="sn12.40"><article><div class="hgroup"><p class="division">
msgctxt "sn12.40:0.1"
msgid "Saṃyutta Nikāya 12"
msgstr ""

#. </p><p>
msgctxt "sn12.40:0.2"
msgid "4. Kaḷārakhattiyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn12.40:0.3"
msgid "40. Tatiyacetanāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.67"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn12.40:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn12.40:1.2"
msgid ""
"“Yañca, bhikkhave, ceteti yañca pakappeti yañca anuseti ārammaṇametaṃ hoti "
"viññāṇassa ṭhitiyā."
msgstr ""

msgctxt "sn12.40:1.3"
msgid "Ārammaṇe sati patiṭṭhā viññāṇassa hoti."
msgstr ""

msgctxt "sn12.40:1.4"
msgid "Tasmiṃ patiṭṭhite viññāṇe virūḷhe nati hoti."
msgstr ""

msgctxt "sn12.40:1.5"
msgid "Natiyā sati āgatigati hoti."
msgstr ""

msgctxt "sn12.40:1.6"
msgid "Āgatigatiyā sati cutūpapāto hoti."
msgstr ""

msgctxt "sn12.40:1.7"
msgid ""
"Cutūpapāte sati āyatiṃ jāti jarāmaraṇaṃ sokaparidevadukkhadomanassupāyāsā "
"sambhavanti."
msgstr ""

msgctxt "sn12.40:1.8"
msgid "Evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn12.40:2.1"
msgid ""
"No ce, bhikkhave, ceteti no ce pakappeti atha ce anuseti, ārammaṇametaṃ hoti "
"viññāṇassa ṭhitiyā."
msgstr ""

msgctxt "sn12.40:2.2"
msgid "Ārammaṇe sati patiṭṭhā viññāṇassa hoti."
msgstr ""

msgctxt "sn12.40:2.3"
msgid "Tasmiṃ patiṭṭhite viññāṇe virūḷhe nati hoti."
msgstr ""

msgctxt "sn12.40:2.4"
msgid "Natiyā sati āgatigati hoti."
msgstr ""

msgctxt "sn12.40:2.5"
msgid "Āgatigatiyā sati cutūpapāto hoti."
msgstr ""

msgctxt "sn12.40:2.6"
msgid ""
"Cutūpapāte sati āyatiṃ jāti jarāmaraṇaṃ sokaparidevadukkhadomanassupāyāsā "
"sambhavanti."
msgstr ""

msgctxt "sn12.40:2.7"
msgid "Evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn12.40:3.1"
msgid ""
"Yato ca kho, bhikkhave, no ceva ceteti no ca pakappeti no ca anuseti, "
"ārammaṇametaṃ na hoti viññāṇassa ṭhitiyā."
msgstr ""

msgctxt "sn12.40:3.2"
msgid "Ārammaṇe asati patiṭṭhā viññāṇassa na hoti."
msgstr ""

msgctxt "sn12.40:3.3"
msgid "Tadappatiṭṭhite viññāṇe avirūḷhe nati na hoti."
msgstr ""

msgctxt "sn12.40:3.4"
msgid "Natiyā asati āgatigati na hoti."
msgstr ""

msgctxt "sn12.40:3.5"
msgid "Āgatigatiyā asati cutūpapāto na hoti."
msgstr ""

msgctxt "sn12.40:3.6"
msgid ""
"Cutūpapāte asati āyatiṃ jāti jarāmaraṇaṃ sokaparidevadukkhadomanassupāyāsā "
"nirujjhanti."
msgstr ""

msgctxt "sn12.40:3.7"
msgid "Evametassa kevalassa dukkhakkhandhassa nirodho hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.40:3.8"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.40:3.9"
msgid "Kaḷārakhattiyavaggo catuttho."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.68"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn12.40:4.1"
msgid "Bhūtamidaṃ kaḷārañca,"
msgstr ""

#. <br>
msgctxt "sn12.40:4.2"
msgid "duve ca ñāṇavatthūni;"
msgstr ""

#. <br>
msgctxt "sn12.40:4.3"
msgid "Avijjāpaccayā ca dve,"
msgstr ""

#. <br>
msgctxt "sn12.40:4.4"
msgid "natumhā cetanā tayoti."
msgstr ""
