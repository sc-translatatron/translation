#. <section class="sutta" id="sn46.17"><article><div class="hgroup"><p class="division">
msgctxt "sn46.17:0.1"
msgid "Saṃyutta Nikāya 46"
msgstr ""

#. </p><p>
msgctxt "sn46.17:0.2"
msgid "2. Gilānavagga"
msgstr ""

#. </p><h1>
#. VAR: 17. Pāraṅgamasutta → apārasuttaṃ (bj) | pāraṅgāmi (pts1)
msgctxt "sn46.17:0.3"
msgid "17. Pāraṅgamasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn46.17:1.1"
msgid ""
"“Sattime, bhikkhave, bojjhaṅgā bhāvitā bahulīkatā apārā pāraṃ gamanāya "
"saṃvattanti."
msgstr ""

msgctxt "sn46.17:1.2"
msgid "Katame satta?"
msgstr ""

msgctxt "sn46.17:1.3"
msgid "Satisambojjhaṅgo … pe … upekkhāsambojjhaṅgo—"
msgstr ""

msgctxt "sn46.17:1.4"
msgid ""
"ime kho, bhikkhave, satta bojjhaṅgā bhāvitā bahulīkatā apārā pāraṃ gamanāya "
"saṃvattantīti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.82"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn46.17:2.1"
msgid "Appakā te manussesu,"
msgstr ""

#. <br>
msgctxt "sn46.17:2.2"
msgid "ye janā pāragāmino;"
msgstr ""

#. <br>
msgctxt "sn46.17:2.3"
msgid "Athāyaṃ itarā pajā,"
msgstr ""

#. <br>
msgctxt "sn46.17:2.4"
msgid "tīramevānudhāvati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn46.17:3.1"
msgid "Ye ca kho sammadakkhāte,"
msgstr ""

#. <br>
msgctxt "sn46.17:3.2"
msgid "dhamme dhammānuvattino;"
msgstr ""

#. <br>
msgctxt "sn46.17:3.3"
msgid "Te janā pāramessanti,"
msgstr ""

#. <br>
msgctxt "sn46.17:3.4"
msgid "maccudheyyaṃ suduttaraṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn46.17:4.1"
msgid "Kaṇhaṃ dhammaṃ vippahāya,"
msgstr ""

#. <br>
msgctxt "sn46.17:4.2"
msgid "sukkaṃ bhāvetha paṇḍito;"
msgstr ""

#. <br>
msgctxt "sn46.17:4.3"
msgid "Okā anokamāgamma,"
msgstr ""

#. <br>
msgctxt "sn46.17:4.4"
msgid "viveke yattha dūramaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn46.17:5.1"
msgid "Tatrābhiratimiccheyya,"
msgstr ""

#. <br>
msgctxt "sn46.17:5.2"
msgid "hitvā kāme akiñcano;"
msgstr ""

#. <br>
msgctxt "sn46.17:5.3"
msgid "Pariyodapeyya attānaṃ,"
msgstr ""

#. <br>
msgctxt "sn46.17:5.4"
msgid "cittaklesehi paṇḍito."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn46.17:6.1"
msgid "Yesaṃ sambodhiyaṅgesu,"
msgstr ""

#. <br>
msgctxt "sn46.17:6.2"
msgid "sammā cittaṃ subhāvitaṃ;"
msgstr ""

#. <br>
msgctxt "sn46.17:6.3"
msgid "Ādānappaṭinissagge,"
msgstr ""

#. <br>
msgctxt "sn46.17:6.4"
msgid "anupādāya ye ratā;"
msgstr ""

#. <br>
msgctxt "sn46.17:6.5"
msgid "Khīṇāsavā jutimanto,"
msgstr ""

#. <br>
msgctxt "sn46.17:6.6"
msgid "te loke parinibbutā”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn46.17:6.7"
msgid "Sattamaṃ."
msgstr ""
