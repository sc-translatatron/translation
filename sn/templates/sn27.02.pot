#. <section class="sutta" id="sn27.2"><article><div class="hgroup"><p class="division">
msgctxt "sn27.2:0.1"
msgid "Saṃyutta Nikāya 27"
msgstr ""

#. </p><p>
msgctxt "sn27.2:0.2"
msgid "1. Kilesavagga"
msgstr ""

#. </p><h1>
msgctxt "sn27.2:0.3"
msgid "2. Rūpasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn27.2:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn27.2:1.2"
msgid "“Yo, bhikkhave, rūpesu chandarāgo, cittasseso upakkileso."
msgstr ""

msgctxt "sn27.2:1.3"
msgid "Yo saddesu …"
msgstr ""

msgctxt "sn27.2:1.4"
msgid "yo gandhesu …"
msgstr ""

msgctxt "sn27.2:1.5"
msgid "yo rasesu …"
msgstr ""

msgctxt "sn27.2:1.6"
msgid "yo phoṭṭhabbesu …"
msgstr ""

msgctxt "sn27.2:1.7"
msgid "yo dhammesu chandarāgo, cittasseso upakkileso."
msgstr ""

msgctxt "sn27.2:1.8"
msgid ""
"Yato kho, bhikkhave, bhikkhuno imesu chasu ṭhānesu cetaso upakkileso pahīno "
"hoti, nekkhammaninnañcassa cittaṃ hoti."
msgstr ""

msgctxt "sn27.2:1.9"
msgid ""
"Nekkhammaparibhāvitaṃ cittaṃ kammaniyaṃ khāyati, abhiññā sacchikaraṇīyesu "
"dhammesū”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn27.2:1.10"
msgid "Dutiyaṃ."
msgstr ""
