#. <section class="sutta" id="sn11.16"><article><div class="hgroup"><p class="division">
msgctxt "sn11.16:0.1"
msgid "Saṃyutta Nikāya 11"
msgstr ""

#. </p><p>
msgctxt "sn11.16:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn11.16:0.3"
msgid "16. Yajamānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn11.16:1.1"
msgid "Ekaṃ samayaṃ bhagavā rājagahe viharati gijjhakūṭe pabbate."
msgstr ""

msgctxt "sn11.16:1.2"
msgid ""
"Atha kho sakko devānamindo yena bhagavā tenupasaṅkami; upasaṅkamitvā "
"bhagavantaṃ abhivādetvā ekamantaṃ aṭṭhāsi."
msgstr ""

msgctxt "sn11.16:1.3"
msgid "Ekamantaṃ ṭhito kho sakko devānamindo bhagavantaṃ gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn11.16:2.1"
msgid "“Yajamānānaṃ manussānaṃ,"
msgstr ""

#. <br>
msgctxt "sn11.16:2.2"
msgid "puññapekkhāna pāṇinaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.16:2.3"
msgid "Karotaṃ opadhikaṃ puññaṃ,"
msgstr ""

#. <br>
msgctxt "sn11.16:2.4"
msgid "kattha dinnaṃ mahapphalan”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.501"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn11.16:3.1"
msgid "“Cattāro ca paṭipannā,"
msgstr ""

#. <br>
msgctxt "sn11.16:3.2"
msgid "cattāro ca phale ṭhitā;"
msgstr ""

#. <br>
msgctxt "sn11.16:3.3"
msgid "Esa saṅgho ujubhūto,"
msgstr ""

#. <br>
msgctxt "sn11.16:3.4"
msgid "paññāsīlasamāhito."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn11.16:4.1"
msgid "Yajamānānaṃ manussānaṃ,"
msgstr ""

#. <br>
msgctxt "sn11.16:4.2"
msgid "Puññapekkhāna pāṇinaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.16:4.3"
msgid "Karotaṃ opadhikaṃ puññaṃ,"
msgstr ""

#. <br>
msgctxt "sn11.16:4.4"
msgid "Saṅghe dinnaṃ mahapphalan”ti."
msgstr ""
