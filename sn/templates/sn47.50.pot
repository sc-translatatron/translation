#. <section class="sutta" id="sn47.50"><article><div class="hgroup"><p class="division">
msgctxt "sn47.50:0.1"
msgid "Saṃyutta Nikāya 47"
msgstr ""

#. </p><p>
msgctxt "sn47.50:0.2"
msgid "5. Amatavagga"
msgstr ""

#. </p><h1>
msgctxt "sn47.50:0.3"
msgid "50. Āsavasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.190"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn47.50:1.1"
msgid "“Tayome, bhikkhave āsavā."
msgstr ""

msgctxt "sn47.50:1.2"
msgid "Katame tayo?"
msgstr ""

msgctxt "sn47.50:1.3"
msgid "Kāmāsavo, bhavāsavo, avijjāsavo—"
msgstr ""

msgctxt "sn47.50:1.4"
msgid "ime kho, bhikkhave, tayo āsavā."
msgstr ""

msgctxt "sn47.50:1.5"
msgid ""
"Imesaṃ kho, bhikkhave, tiṇṇannaṃ āsavānaṃ pahānāya cattāro satipaṭṭhānā "
"bhāvetabbā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn47.50:2.1"
msgid "Katame cattāro?"
msgstr ""

msgctxt "sn47.50:2.2"
msgid ""
"Idha, bhikkhave, bhikkhu kāye kāyānupassī viharati ātāpī sampajāno satimā, "
"vineyya loke abhijjhādomanassaṃ;"
msgstr ""

msgctxt "sn47.50:2.3"
msgid "vedanāsu … pe …"
msgstr ""

msgctxt "sn47.50:2.4"
msgid "citte … pe …"
msgstr ""

msgctxt "sn47.50:2.5"
msgid ""
"dhammesu dhammānupassī viharati ātāpī sampajāno satimā, vineyya loke "
"abhijjhādomanassaṃ."
msgstr ""

msgctxt "sn47.50:2.6"
msgid ""
"Imesaṃ kho, bhikkhave, tiṇṇannaṃ āsavānaṃ pahānāya ime cattāro satipaṭṭhānā "
"bhāvetabbā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn47.50:2.7"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn47.50:2.8"
msgid "Amatavaggo pañcamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn47.50:3.1"
msgid "Amataṃ samudayo maggo,"
msgstr ""

#. <br>
msgctxt "sn47.50:3.2"
msgid "Sati kusalarāsi ca;"
msgstr ""

#. <br>
msgctxt "sn47.50:3.3"
msgid "Pātimokkhaṃ duccaritaṃ,"
msgstr ""

#. <br>
msgctxt "sn47.50:3.4"
msgid "Mittavedanā āsavena cāti."
msgstr ""
