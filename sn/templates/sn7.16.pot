#. <section class="sutta" id="sn7.16"><article><div class="hgroup"><p class="division">
msgctxt "sn7.16:0.1"
msgid "Saṃyutta Nikāya 7"
msgstr ""

#. </p><p>
msgctxt "sn7.16:0.2"
msgid "2. Upāsakavagga"
msgstr ""

#. </p><h1>
msgctxt "sn7.16:0.3"
msgid "16. Paccanīkasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.179"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn7.16:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn7.16:1.2"
msgid ""
"Tena kho pana samayena paccanīkasāto nāma brāhmaṇo sāvatthiyaṃ paṭivasati."
msgstr ""

msgctxt "sn7.16:1.3"
msgid "Atha kho paccanīkasātassa brāhmaṇassa etadahosi:"
msgstr ""

msgctxt "sn7.16:1.4"
msgid "“yannūnāhaṃ yena samaṇo gotamo tenupasaṅkameyyaṃ."
msgstr ""

#. VAR: tadevassāhaṃ → tadeva sāhaṃ (mr)
#. VAR: paccanīkāssan”ti → paccanikāssanti (s1-3) | paccanīkassanti (pts1) | paccanīkasātanti (mr)
msgctxt "sn7.16:1.5"
msgid "Yaṃ yadeva samaṇo gotamo bhāsissati taṃ tadevassāhaṃ paccanīkāssan”ti."
msgstr ""

msgctxt "sn7.16:1.6"
msgid "Tena kho pana samayena bhagavā abbhokāse caṅkamati."
msgstr ""

msgctxt "sn7.16:1.7"
msgid ""
"Atha kho paccanīkasāto brāhmaṇo yena bhagavā tenupasaṅkami; upasaṅkamitvā "
"bhagavantaṃ caṅkamantaṃ etadavoca:"
msgstr ""

msgctxt "sn7.16:1.8"
msgid "“bhaṇa samaṇadhamman”ti."
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.386"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn7.16:2.1"
msgid "“Na paccanīkasātena,"
msgstr ""

#. <br>
msgctxt "sn7.16:2.2"
msgid "suvijānaṃ subhāsitaṃ;"
msgstr ""

#. <br>
msgctxt "sn7.16:2.3"
msgid "Upakkiliṭṭhacittena,"
msgstr ""

#. <br>
msgctxt "sn7.16:2.4"
msgid "sārambhabahulena ca."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn7.16:3.1"
msgid "Yo ca vineyya sārambhaṃ,"
msgstr ""

#. <br>
msgctxt "sn7.16:3.2"
msgid "appasādañca cetaso;"
msgstr ""

#. <br>
msgctxt "sn7.16:3.3"
msgid "Āghātaṃ paṭinissajja,"
msgstr ""

#. <br>
#. VAR: sa ve → sace (s1-3, km, mr)
msgctxt "sn7.16:3.4"
msgid "sa ve jaññā subhāsitan”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn7.16:4.1"
msgid "Evaṃ vutte, paccanīkasāto brāhmaṇo bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn7.16:4.2"
msgid "“abhikkantaṃ, bho gotama, abhikkantaṃ, bho gotama … pe …"
msgstr ""

msgctxt "sn7.16:4.3"
msgid ""
"upāsakaṃ maṃ bhavaṃ gotamo dhāretu ajjatagge pāṇupetaṃ saraṇaṃ gatan”ti."
msgstr ""
