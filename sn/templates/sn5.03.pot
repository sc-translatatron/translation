#. <section class="sutta" id="sn5.3"><article><div class="hgroup"><p class="division">
msgctxt "sn5.3:0.1"
msgid "Saṃyutta Nikāya 5"
msgstr ""

#. </p><p>
msgctxt "sn5.3:0.2"
msgid "1. Bhikkhunīvagga"
msgstr ""

#. </p><h1>
#. VAR: 3. Kisāgotamīsutta → gotamīsuttaṃ (bj, s1-3) | gotamī (pts1)
msgctxt "sn5.3:0.3"
msgid "3. Kisāgotamīsutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.130"></a>
#. <a class="pts2ed" id="pts2ed1.285"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn5.3:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn5.3:1.2"
msgid ""
"Atha kho kisāgotamī bhikkhunī pubbaṇhasamayaṃ nivāsetvā pattacīvaramādāya "
"sāvatthiṃ piṇḍāya pāvisi."
msgstr ""

msgctxt "sn5.3:1.3"
msgid ""
"Sāvatthiyaṃ piṇḍāya caritvā pacchābhattaṃ piṇḍapātapaṭikkantā yena "
"andhavanaṃ tenupasaṅkami, upasaṅkamitvā divāvihārāya."
msgstr ""

msgctxt "sn5.3:1.4"
msgid "Andhavanaṃ ajjhogāhetvā aññatarasmiṃ rukkhamūle divāvihāraṃ nisīdi."
msgstr ""

msgctxt "sn5.3:1.5"
msgid ""
"Atha kho māro pāpimā kisāgotamiyā bhikkhuniyā bhayaṃ chambhitattaṃ "
"lomahaṃsaṃ uppādetukāmo samādhimhā cāvetukāmo yena kisāgotamī bhikkhunī "
"tenupasaṅkami; upasaṅkamitvā kisāgotamiṃ bhikkhuniṃ gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn5.3:2.1"
msgid "“Kiṃ nu tvaṃ mataputtāva,"
msgstr ""

#. <br>
msgctxt "sn5.3:2.2"
msgid "ekamāsi rudammukhī;"
msgstr ""

#. <br>
msgctxt "sn5.3:2.3"
msgid "Vanamajjhagatā ekā,"
msgstr ""

#. <br>
msgctxt "sn5.3:2.4"
msgid "purisaṃ nu gavesasī”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn5.3:3.1"
msgid "Atha kho kisāgotamiyā bhikkhuniyā etadahosi:"
msgstr ""

msgctxt "sn5.3:3.2"
msgid "“ko nu khvāyaṃ manusso vā amanusso vā gāthaṃ bhāsatī”ti?"
msgstr ""

msgctxt "sn5.3:3.3"
msgid "Atha kho kisāgotamiyā bhikkhuniyā etadahosi:"
msgstr ""

msgctxt "sn5.3:3.4"
msgid ""
"“māro kho ayaṃ pāpimā mama bhayaṃ chambhitattaṃ lomahaṃsaṃ uppādetukāmo "
"samādhimhā cāvetukāmo gāthaṃ bhāsatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn5.3:4.1"
msgid ""
"Atha kho kisāgotamī bhikkhunī “māro ayaṃ pāpimā” iti viditvā māraṃ "
"pāpimantaṃ gāthāhi paccabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn5.3:5.1"
msgid "“Accantaṃ mataputtāmhi,"
msgstr ""

#. <br>
msgctxt "sn5.3:5.2"
msgid "purisā etadantikā;"
msgstr ""

#. <br>
msgctxt "sn5.3:5.3"
msgid "Na socāmi na rodāmi,"
msgstr ""

#. <br>
msgctxt "sn5.3:5.4"
msgid "na taṃ bhāyāmi āvuso."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.286"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "sn5.3:6.1"
msgid "Sabbattha vihatā nandī,"
msgstr ""

#. <br>
msgctxt "sn5.3:6.2"
msgid "tamokkhandho padālito;"
msgstr ""

#. <br>
#. VAR: Jetvāna maccuno → jetvā namucino (si)
msgctxt "sn5.3:6.3"
msgid "Jetvāna maccuno senaṃ,"
msgstr ""

#. <br>
msgctxt "sn5.3:6.4"
msgid "viharāmi anāsavā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn5.3:7.1"
msgid ""
"Atha kho māro pāpimā “jānāti maṃ kisāgotamī bhikkhunī”ti dukkhī dummano "
"tatthevantaradhāyīti."
msgstr ""
