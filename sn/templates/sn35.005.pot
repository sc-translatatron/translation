#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.5:1.1"
msgid "“Rūpā, bhikkhave, dukkhā."
msgstr ""

msgctxt "sn35.5:1.2"
msgid "Yaṃ dukkhaṃ tadanattā;"
msgstr ""

msgctxt "sn35.5:1.3"
msgid ""
"yadanattā taṃ ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ yathābhūtaṃ "
"sammappaññāya daṭṭhabbaṃ."
msgstr ""

msgctxt "sn35.5:1.4"
msgid "Saddā …"
msgstr ""

msgctxt "sn35.5:1.5"
msgid "gandhā …"
msgstr ""

msgctxt "sn35.5:1.6"
msgid "rasā …"
msgstr ""

msgctxt "sn35.5:1.7"
msgid "phoṭṭhabbā …"
msgstr ""

msgctxt "sn35.5:1.8"
msgid "dhammā dukkhā."
msgstr ""

msgctxt "sn35.5:1.9"
msgid "Yaṃ dukkhaṃ tadanattā."
msgstr ""

msgctxt "sn35.5:1.10"
msgid ""
"Yadanattā taṃ ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ yathābhūtaṃ "
"sammappaññāya daṭṭhabbaṃ."
msgstr ""

msgctxt "sn35.5:1.11"
msgid "Evaṃ passaṃ … pe …"
