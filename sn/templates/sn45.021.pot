#. <section class="sutta" id="sn45.21"><article><div class="hgroup"><p class="division">
msgctxt "sn45.21:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.21:0.2"
msgid "3. Micchattavagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.21:0.3"
msgid "21. Micchattasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.18"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.21:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.21:1.2"
msgid "“Micchattañca vo, bhikkhave, desessāmi, sammattañca."
msgstr ""

msgctxt "sn45.21:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn45.21:1.4"
msgid "Katamañca, bhikkhave, micchattaṃ?"
msgstr ""

msgctxt "sn45.21:1.5"
msgid "Seyyathidaṃ—micchādiṭṭhi … pe … micchāsamādhi."
msgstr ""

msgctxt "sn45.21:1.6"
msgid "Idaṃ vuccati, bhikkhave, micchattaṃ."
msgstr ""

msgctxt "sn45.21:1.7"
msgid "Katamañca, bhikkhave, sammattaṃ?"
msgstr ""

msgctxt "sn45.21:1.8"
msgid "Seyyathidaṃ—sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

msgctxt "sn45.21:1.9"
msgid "Idaṃ vuccati, bhikkhave, sammattan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.21:1.10"
msgid "Paṭhamaṃ."
msgstr ""
