#. <section class="sutta" id="sn45.27"><article><div class="hgroup"><p class="division">
msgctxt "sn45.27:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.27:0.2"
msgid "3. Micchattavagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.27:0.3"
msgid "27. Kumbhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.21"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.27:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.27:1.2"
msgid ""
"“Seyyathāpi, bhikkhave, kumbho anādhāro suppavattiyo hoti, sādhāro "
"duppavattiyo hoti;"
msgstr ""

msgctxt "sn45.27:1.3"
msgid ""
"evameva kho, bhikkhave, cittaṃ anādhāraṃ suppavattiyaṃ hoti, sādhāraṃ "
"duppavattiyaṃ hoti."
msgstr ""

msgctxt "sn45.27:1.4"
msgid "Ko ca, bhikkhave, cittassa ādhāro?"
msgstr ""

msgctxt "sn45.27:1.5"
msgid "Ayameva ariyo aṭṭhaṅgiko maggo, seyyathidaṃ—"
msgstr ""

msgctxt "sn45.27:1.6"
msgid "sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

msgctxt "sn45.27:1.7"
msgid "Ayaṃ cittassa ādhāro."
msgstr ""

msgctxt "sn45.27:1.8"
msgid ""
"Seyyathāpi, bhikkhave, kumbho anādhāro suppavattiyo hoti, sādhāro "
"duppavattiyo hoti;"
msgstr ""

msgctxt "sn45.27:1.9"
msgid ""
"evameva kho, bhikkhave, cittaṃ anādhāraṃ suppavattiyaṃ hoti, sādhāraṃ "
"duppavattiyaṃ hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.27:1.10"
msgid "Sattamaṃ."
msgstr ""
