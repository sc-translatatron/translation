#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.16:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.16:1.2"
msgid "“Rūpaṃ, bhikkhave, dukkhaṃ."
msgstr ""

msgctxt "sn22.16:1.3"
msgid "Yaṃ dukkhaṃ tadanattā;"
msgstr ""

msgctxt "sn22.16:1.4"
msgid ""
"yadanattā taṃ ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ yathābhūtaṃ "
"sammappaññāya daṭṭhabbaṃ."
msgstr ""

msgctxt "sn22.16:1.5"
msgid "Vedanā dukkhā …"
msgstr ""

msgctxt "sn22.16:1.6"
msgid "saññā dukkhā …"
msgstr ""

msgctxt "sn22.16:1.7"
msgid "saṅkhārā dukkhā …"
msgstr ""

msgctxt "sn22.16:1.8"
msgid "viññāṇaṃ dukkhaṃ."
msgstr ""

msgctxt "sn22.16:1.9"
msgid "Yaṃ dukkhaṃ tadanattā;"
msgstr ""

msgctxt "sn22.16:1.10"
msgid ""
"yadanattā taṃ ‘netaṃ mama, nesohamasmi, na meso attā’ti evametaṃ yathābhūtaṃ "
"sammappaññāya daṭṭhabbaṃ."
msgstr ""

msgctxt "sn22.16:1.11"
msgid "Evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn22.16:1.12"
msgid "nāparaṃ itthattāyā’ti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
