#. <section class="sutta" id="sn22.10"><article><div class="hgroup"><p class="division">
msgctxt "sn22.10:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.10:0.2"
msgid "1. Nakulapituvagga"
msgstr ""

#. </p><h1>
#. VAR: 10. Kālattayadukkhasutta → dutiyaatītānāgatasuttaṃ (bj) | atītānāgatapaccuppanna 2 (pts1)
msgctxt "sn22.10:0.3"
msgid "10. Kālattayadukkhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.20"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.10:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.10:1.2"
msgid "“Rūpaṃ, bhikkhave, dukkhaṃ atītānāgataṃ;"
msgstr ""

msgctxt "sn22.10:1.3"
msgid "ko pana vādo paccuppannassa."
msgstr ""

msgctxt "sn22.10:1.4"
msgid ""
"Evaṃ passaṃ, bhikkhave, sutavā ariyasāvako atītasmiṃ rūpasmiṃ anapekkho hoti;"
msgstr ""

msgctxt "sn22.10:1.5"
msgid "anāgataṃ rūpaṃ nābhinandati;"
msgstr ""

msgctxt "sn22.10:1.6"
msgid "paccuppannassa rūpassa nibbidāya virāgāya nirodhāya paṭipanno hoti."
msgstr ""

msgctxt "sn22.10:1.7"
msgid "Vedanā dukkhā …"
msgstr ""

msgctxt "sn22.10:1.8"
msgid "saññā dukkhā …"
msgstr ""

msgctxt "sn22.10:1.9"
msgid "saṅkhārā dukkhā …"
msgstr ""

msgctxt "sn22.10:1.10"
msgid "viññāṇaṃ dukkhaṃ atītānāgataṃ;"
msgstr ""

msgctxt "sn22.10:1.11"
msgid "ko pana vādo paccuppannassa."
msgstr ""

msgctxt "sn22.10:1.12"
msgid ""
"Evaṃ passaṃ, bhikkhave, sutavā ariyasāvako atītasmiṃ viññāṇasmiṃ anapekkho "
"hoti;"
msgstr ""

msgctxt "sn22.10:1.13"
msgid "anāgataṃ viññāṇaṃ nābhinandati;"
msgstr ""

msgctxt "sn22.10:1.14"
msgid ""
"paccuppannassa viññāṇassa nibbidāya virāgāya nirodhāya paṭipanno hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.10:1.15"
msgid "Dasamaṃ."
msgstr ""
