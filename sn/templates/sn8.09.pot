#. <section class="sutta" id="sn8.9"><article><div class="hgroup"><p class="division">
msgctxt "sn8.9:0.1"
msgid "Saṃyutta Nikāya 8"
msgstr ""

#. </p><p>
msgctxt "sn8.9:0.2"
msgid "1. Vaṅgīsavagga"
msgstr ""

#. </p><h1>
msgctxt "sn8.9:0.3"
msgid "9. Koṇḍaññasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.194"></a>
#. <a class="pts2ed" id="pts2ed1.418"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn8.9:1.1"
msgid "Ekaṃ samayaṃ bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

#. VAR: aññāsikoṇḍañño → aññākoṇḍañño (bj, s1-3, km, pts2)
msgctxt "sn8.9:1.2"
msgid ""
"Atha kho āyasmā aññāsikoṇḍañño sucirasseva yena bhagavā tenupasaṅkami; "
"upasaṅkamitvā bhagavato pādesu sirasā nipatitvā bhagavato pādāni mukhena ca "
"paricumbati, pāṇīhi ca parisambāhati, nāmañca sāveti:"
msgstr ""

msgctxt "sn8.9:1.3"
msgid "“koṇḍaññohaṃ, bhagavā, koṇḍaññohaṃ, sugatā”ti."
msgstr ""

msgctxt "sn8.9:1.4"
msgid "Atha kho āyasmato vaṅgīsassa etadahosi:"
msgstr ""

msgctxt "sn8.9:1.5"
msgid ""
"“ayaṃ kho āyasmā aññāsikoṇḍañño sucirasseva yena bhagavā tenupasaṅkami; "
"upasaṅkamitvā bhagavato pādesu sirasā nipatitvā bhagavato pādāni mukhena ca "
"paricumbati, pāṇīhi ca parisambāhati, nāmañca sāveti:"
msgstr ""

msgctxt "sn8.9:1.6"
msgid "‘koṇḍaññohaṃ, bhagavā, koṇḍaññohaṃ, sugatā’ti."
msgstr ""

msgctxt "sn8.9:1.7"
msgid ""
"Yannūnāhaṃ āyasmantaṃ aññāsikoṇḍaññaṃ bhagavato sammukhā sāruppāhi gāthāhi "
"abhitthaveyyan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn8.9:2.1"
msgid ""
"Atha kho āyasmā vaṅgīso uṭṭhāyāsanā ekaṃsaṃ uttarāsaṅgaṃ karitvā yena "
"bhagavā tenañjaliṃ paṇāmetvā bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn8.9:2.2"
msgid "“paṭibhāti maṃ, bhagavā, paṭibhāti maṃ, sugatā”ti."
msgstr ""

msgctxt "sn8.9:2.3"
msgid "“Paṭibhātu taṃ, vaṅgīsā”ti bhagavā avoca."
msgstr ""

msgctxt "sn8.9:2.4"
msgid ""
"Atha kho āyasmā vaṅgīso āyasmantaṃ aññāsikoṇḍaññaṃ bhagavato sammukhā "
"sāruppāhi gāthāhi abhitthavi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn8.9:3.1"
msgid "“Buddhānubuddho so thero,"
msgstr ""

#. <br>
msgctxt "sn8.9:3.2"
msgid "koṇḍañño tibbanikkamo;"
msgstr ""

#. <br>
msgctxt "sn8.9:3.3"
msgid "Lābhī sukhavihārānaṃ,"
msgstr ""

#. <br>
msgctxt "sn8.9:3.4"
msgid "vivekānaṃ abhiṇhaso."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.419"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn8.9:4.1"
msgid "Yaṃ sāvakena pattabbaṃ,"
msgstr ""

#. <br>
msgctxt "sn8.9:4.2"
msgid "satthusāsanakārinā;"
msgstr ""

#. <br>
msgctxt "sn8.9:4.3"
msgid "Sabbassa taṃ anuppattaṃ,"
msgstr ""

#. <br>
msgctxt "sn8.9:4.4"
msgid "appamattassa sikkhato."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn8.9:5.1"
msgid "Mahānubhāvo tevijjo,"
msgstr ""

#. <br>
msgctxt "sn8.9:5.2"
msgid "cetopariyāyakovido;"
msgstr ""

#. <br>
#. VAR: buddhadāyādo → buddhasāvako (pts1)
msgctxt "sn8.9:5.3"
msgid "Koṇḍañño buddhadāyādo,"
msgstr ""

#. <br>
msgctxt "sn8.9:5.4"
msgid "pāde vandati satthuno”ti."
msgstr ""
