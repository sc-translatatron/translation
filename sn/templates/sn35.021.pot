#. <section class="sutta" id="sn35.21"><article><div class="hgroup"><p class="division">
msgctxt "sn35.21:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.21:0.2"
msgid "2. Yamakavagga"
msgstr ""

#. </p><h1>
#. VAR: 21. Paṭhamadukkhuppādasutta → uppādasuttaṃ (bj) | uppādena 1 (pts1)
msgctxt "sn35.21:0.3"
msgid "21. Paṭhamadukkhuppādasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.21:1.1"
msgid ""
"“Yo, bhikkhave, cakkhussa uppādo ṭhiti abhinibbatti pātubhāvo, dukkhasseso "
"uppādo, rogānaṃ ṭhiti, jarāmaraṇassa pātubhāvo."
msgstr ""

msgctxt "sn35.21:1.2"
msgid "Yo sotassa … pe …"
msgstr ""

msgctxt "sn35.21:1.3"
msgid "yo ghānassa …"
msgstr ""

msgctxt "sn35.21:1.4"
msgid "yo jivhāya …"
msgstr ""

msgctxt "sn35.21:1.5"
msgid "yo kāyassa …"
msgstr ""

msgctxt "sn35.21:1.6"
msgid ""
"yo manassa uppādo ṭhiti abhinibbatti pātubhāvo, dukkhasseso uppādo, rogānaṃ "
"ṭhiti, jarāmaraṇassa pātubhāvo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.21:2.1"
msgid ""
"Yo ca kho, bhikkhave, cakkhussa nirodho vūpasamo atthaṅgamo, dukkhasseso "
"nirodho, rogānaṃ vūpasamo, jarāmaraṇassa atthaṅgamo."
msgstr ""

msgctxt "sn35.21:2.2"
msgid "Yo sotassa …"
msgstr ""

msgctxt "sn35.21:2.3"
msgid "yo ghānassa …"
msgstr ""

msgctxt "sn35.21:2.4"
msgid "yo jivhāya …"
msgstr ""

msgctxt "sn35.21:2.5"
msgid "yo kāyassa …"
msgstr ""

msgctxt "sn35.21:2.6"
msgid ""
"yo manassa nirodho vūpasamo atthaṅgamo, dukkhasseso nirodho, rogānaṃ "
"vūpasamo, jarāmaraṇassa atthaṅgamo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.21:2.7"
msgid "Navamaṃ."
msgstr ""
