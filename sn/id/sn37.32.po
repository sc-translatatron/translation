#. <section class="sutta" id="sn37.32"><article><div class="hgroup"><p class="division">
msgctxt "sn37.32:0.1"
msgid "Saṃyutta Nikāya 37"
msgstr ""

#. </p><p>
msgctxt "sn37.32:0.2"
msgid "3. Balavagga"
msgstr ""

#. </p><h1>
msgctxt "sn37.32:0.3"
msgid "32. Ṭhānasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.249"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn37.32:1.1"
msgid "“Pañcimāni, bhikkhave, ṭhānāni dullabhāni akatapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:1.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "sn37.32:1.3"
msgid "Patirūpe kule jāyeyyanti—"
msgstr ""

msgctxt "sn37.32:1.4"
msgid "idaṃ, bhikkhave, paṭhamaṃ ṭhānaṃ dullabhaṃ akatapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:1.5"
msgid "Patirūpe kule jāyitvā patirūpaṃ kulaṃ gaccheyyanti—"
msgstr ""

msgctxt "sn37.32:1.6"
msgid "idaṃ, bhikkhave, dutiyaṃ ṭhānaṃ dullabhaṃ akatapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:1.7"
msgid ""
"Patirūpe kule jāyitvā, patirūpaṃ kulaṃ gantvā, asapatti agāraṃ "
"ajjhāvaseyyanti—"
msgstr ""

msgctxt "sn37.32:1.8"
msgid "idaṃ, bhikkhave, tatiyaṃ ṭhānaṃ dullabhaṃ akatapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:1.9"
msgid ""
"Patirūpe kule jāyitvā, patirūpaṃ kulaṃ gantvā, asapatti agāraṃ ajjhāvasantī "
"puttavatī assanti—"
msgstr ""

msgctxt "sn37.32:1.10"
msgid "idaṃ, bhikkhave, catutthaṃ ṭhānaṃ dullabhaṃ akatapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:1.11"
msgid ""
"Patirūpe kule jāyitvā, patirūpaṃ kulaṃ gantvā, asapatti agāraṃ ajjhāvasantī "
"puttavatī samānā sāmikaṃ abhibhuyya vatteyyanti—"
msgstr ""

msgctxt "sn37.32:1.12"
msgid "idaṃ, bhikkhave, pañcamaṃ ṭhānaṃ dullabhaṃ akatapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:1.13"
msgid ""
"Imāni kho, bhikkhave, pañca ṭhānāni dullabhāni akatapuññena mātugāmenāti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.250"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn37.32:2.1"
msgid "Pañcimāni, bhikkhave, ṭhānāni sulabhāni katapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:2.2"
msgid "Katamāni pañca?"
msgstr ""

msgctxt "sn37.32:2.3"
msgid "Patirūpe kule jāyeyyanti—"
msgstr ""

msgctxt "sn37.32:2.4"
msgid "idaṃ, bhikkhave, paṭhamaṃ ṭhānaṃ sulabhaṃ katapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:2.5"
msgid "Patirūpe kule jāyitvā patirūpaṃ kulaṃ gaccheyyanti—"
msgstr ""

msgctxt "sn37.32:2.6"
msgid "idaṃ, bhikkhave, dutiyaṃ ṭhānaṃ sulabhaṃ katapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:2.7"
msgid ""
"Patirūpe kule jāyitvā patirūpaṃ kulaṃ gantvā asapatti agāraṃ ajjhāvaseyyanti—"
msgstr ""

msgctxt "sn37.32:2.8"
msgid "idaṃ, bhikkhave, tatiyaṃ ṭhānaṃ sulabhaṃ katapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:2.9"
msgid ""
"Patirūpe kule jāyitvā patirūpaṃ kulaṃ gantvā asapatti agāraṃ ajjhāvasantī "
"puttavatī assanti—"
msgstr ""

msgctxt "sn37.32:2.10"
msgid "idaṃ, bhikkhave, catutthaṃ ṭhānaṃ sulabhaṃ katapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:2.11"
msgid ""
"Patirūpe kule jāyitvā patirūpaṃ kulaṃ gantvā asapatti agāraṃ ajjhāvasantī "
"puttavatī samānā sāmikaṃ abhibhuyya vatteyyanti—"
msgstr ""

msgctxt "sn37.32:2.12"
msgid "idaṃ, bhikkhave, pañcamaṃ ṭhānaṃ sulabhaṃ katapuññena mātugāmena."
msgstr ""

msgctxt "sn37.32:2.13"
msgid ""
"Imāni kho, bhikkhave, pañca ṭhānāni sulabhāni katapuññena mātugāmenā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn37.32:2.14"
msgid "Aṭṭhamaṃ."
msgstr ""
