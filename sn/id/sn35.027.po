#. <section class="sutta" id="sn35.27"><article><div class="hgroup"><p class="division">
msgctxt "sn35.27:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.27:0.2"
msgid "3. Sabbavagga"
msgstr ""

#. </p><h1>
#. VAR: 27. Dutiyaaparijānanasutta → dutiyaaparijānasuttaṃ (bj)
msgctxt "sn35.27:0.3"
msgid "27. Dutiyaaparijānanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.19"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.27:1.1"
msgid ""
"“Sabbaṃ, bhikkhave, anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.27:1.2"
msgid ""
"Kiñca, bhikkhave, sabbaṃ anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ abhabbo "
"dukkhakkhayāya?"
msgstr ""

msgctxt "sn35.27:1.3"
msgid ""
"Yañca, bhikkhave, cakkhu, ye ca rūpā, yañca cakkhuviññāṇaṃ, ye ca "
"cakkhuviññāṇaviññātabbā dhammā … pe …"
msgstr ""

msgctxt "sn35.27:1.4"
msgid ""
"yā ca jivhā, ye ca rasā, yañca jivhāviññāṇaṃ, ye ca jivhāviññāṇaviññātabbā "
"dhammā;"
msgstr ""

msgctxt "sn35.27:1.5"
msgid ""
"yo ca kāyo, ye ca phoṭṭhabbā, yañca kāyaviññāṇaṃ, ye ca "
"kāyaviññāṇaviññātabbā dhammā;"
msgstr ""

msgctxt "sn35.27:1.6"
msgid ""
"yo ca mano, ye ca dhammā, yañca manoviññāṇaṃ, ye ca manoviññāṇaviññātabbā "
"dhammā—"
msgstr ""

msgctxt "sn35.27:1.7"
msgid ""
"idaṃ kho, bhikkhave, sabbaṃ anabhijānaṃ aparijānaṃ avirājayaṃ appajahaṃ "
"abhabbo dukkhakkhayāya."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.27:2.1"
msgid ""
"Sabbaṃ, bhikkhave, abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo "
"dukkhakkhayāya."
msgstr ""

msgctxt "sn35.27:2.2"
msgid ""
"Kiñca, bhikkhave, sabbaṃ abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo "
"dukkhakkhayāya?"
msgstr ""

#. VAR: cakkhu → cakkhuṃ (bj, s1-3, pts1)
msgctxt "sn35.27:2.3"
msgid ""
"Yañca, bhikkhave, cakkhu, ye ca rūpā, yañca cakkhuviññāṇaṃ, ye ca "
"cakkhuviññāṇaviññātabbā dhammā … pe …"
msgstr ""

msgctxt "sn35.27:2.4"
msgid ""
"yā ca jivhā, ye ca rasā, yañca jivhāviññāṇaṃ, ye ca jivhāviññāṇaviññātabbā "
"dhammā;"
msgstr ""

msgctxt "sn35.27:2.5"
msgid ""
"yo ca kāyo, ye ca phoṭṭhabbā, yañca kāyaviññāṇaṃ, ye ca "
"kāyaviññāṇaviññātabbā dhammā;"
msgstr ""

msgctxt "sn35.27:2.6"
msgid ""
"yo ca mano, ye ca dhammā, yañca manoviññāṇaṃ, ye ca manoviññāṇaviññātabbā "
"dhammā—"
msgstr ""

msgctxt "sn35.27:2.7"
msgid ""
"idaṃ kho, bhikkhave, sabbaṃ abhijānaṃ parijānaṃ virājayaṃ pajahaṃ bhabbo "
"dukkhakkhayāyā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.27:2.8"
msgid "Pañcamaṃ."
msgstr ""
