#. <section class="sutta" id="sn1.68"><article><div class="hgroup"><p class="division">
msgctxt "sn1.68:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.68:0.2"
msgid "7. Addhavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.68:0.3"
msgid "68. Pihitasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.91"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.68:1.1"
msgid "“Kenassu pihito loko,"
msgstr ""

#. <br>
msgctxt "sn1.68:1.2"
msgid "kismiṃ loko patiṭṭhito;"
msgstr ""

#. <br>
msgctxt "sn1.68:1.3"
msgid "Kenassu uḍḍito loko,"
msgstr ""

#. <br>
msgctxt "sn1.68:1.4"
msgid "kenassu parivārito”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.68:2.1"
msgid "“Maccunā pihito loko,"
msgstr ""

#. <br>
msgctxt "sn1.68:2.2"
msgid "dukkhe loko patiṭṭhito;"
msgstr ""

#. <br>
msgctxt "sn1.68:2.3"
msgid "Taṇhāya uḍḍito loko,"
msgstr ""

#. <br>
msgctxt "sn1.68:2.4"
msgid "jarāya parivārito”ti."
msgstr ""
