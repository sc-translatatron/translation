#. <section class="sutta" id="sn1.59"><article><div class="hgroup"><p class="division">
msgctxt "sn1.59:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.59:0.2"
msgid "6. Jarāvagga"
msgstr ""

#. </p><h1>
#. VAR: 59. Dutiyasutta → dutiyāsuttaṃ (bj, pts2) | dutiyauppathasuttaṃ (si)
msgctxt "sn1.59:0.3"
msgid "59. Dutiyasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
#. VAR: dutiyā → dutiyaṃ (s1-3, km, pts1-2)
msgctxt "sn1.59:1.1"
msgid "“Kiṃsu dutiyā purisassa hoti,"
msgstr ""

#. <br>
msgctxt "sn1.59:1.2"
msgid "Kiṃsu cenaṃ pasāsati;"
msgstr ""

#. <br>
msgctxt "sn1.59:1.3"
msgid "Kissa cābhirato macco,"
msgstr ""

#. <br>
msgctxt "sn1.59:1.4"
msgid "Sabbadukkhā pamuccatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.59:2.1"
msgid "“Saddhā dutiyā purisassa hoti,"
msgstr ""

#. <br>
msgctxt "sn1.59:2.2"
msgid "Paññā cenaṃ pasāsati;"
msgstr ""

#. <br>
msgctxt "sn1.59:2.3"
msgid "Nibbānābhirato macco,"
msgstr ""

#. <br>
msgctxt "sn1.59:2.4"
msgid "Sabbadukkhā pamuccatī”ti."
msgstr ""
