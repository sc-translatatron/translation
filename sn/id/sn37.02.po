#. <section class="sutta" id="sn37.2"><article><div class="hgroup"><p class="division">
msgctxt "sn37.2:0.1"
msgid "Saṃyutta Nikāya 37"
msgstr ""

#. </p><p>
msgctxt "sn37.2:0.2"
msgid "1. Paṭhamapeyyālavagga"
msgstr ""

#. </p><h1>
#. VAR: 2. Purisasutta → manāpā amanāpā (pts1)
msgctxt "sn37.2:0.3"
msgid "2. Purisasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.239"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn37.2:1.1"
msgid ""
"“Pañcahi, bhikkhave, aṅgehi samannāgato puriso ekantaamanāpo hoti "
"mātugāmassa."
msgstr ""

msgctxt "sn37.2:1.2"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "sn37.2:1.3"
msgid ""
"Na ca rūpavā hoti, na ca bhogavā hoti, na ca sīlavā hoti, alaso ca hoti, "
"pajañcassa na labhati—"
msgstr ""

msgctxt "sn37.2:1.4"
msgid ""
"imehi kho, bhikkhave, pañcahi aṅgehi samannāgato puriso ekantaamanāpo hoti "
"mātugāmassa."
msgstr ""

msgctxt "sn37.2:1.5"
msgid ""
"Pañcahi, bhikkhave, aṅgehi samannāgato puriso ekantamanāpo hoti mātugāmassa."
msgstr ""

msgctxt "sn37.2:1.6"
msgid "Katamehi pañcahi?"
msgstr ""

msgctxt "sn37.2:1.7"
msgid ""
"Rūpavā ca hoti, bhogavā ca hoti, sīlavā ca hoti, dakkho ca hoti analaso, "
"pajañcassa labhati—"
msgstr ""

msgctxt "sn37.2:1.8"
msgid ""
"imehi kho, bhikkhave, pañcahi aṅgehi samannāgato puriso ekantamanāpo hoti "
"mātugāmassā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn37.2:1.9"
msgid "Dutiyaṃ."
msgstr ""
