#. <section class="sutta" id="sn1.52"><article><div class="hgroup"><p class="division">
msgctxt "sn1.52:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.52:0.2"
msgid "6. Jarāvagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.52:0.3"
msgid "52. Ajarasāsutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.80"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.52:1.1"
msgid "“Kiṃsu ajarasā sādhu,"
msgstr ""

#. <br>
msgctxt "sn1.52:1.2"
msgid "kiṃsu sādhu adhiṭṭhitaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.52:1.3"
msgid "Kiṃsu narānaṃ ratanaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.52:1.4"
msgid "kiṃsu corehyahāriyan”ti."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.37"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.52:2.1"
msgid "“Sīlaṃ ajarasā sādhu,"
msgstr ""

#. <br>
msgctxt "sn1.52:2.2"
msgid "saddhā sādhu adhiṭṭhitā;"
msgstr ""

#. <br>
msgctxt "sn1.52:2.3"
msgid "Paññā narānaṃ ratanaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.52:2.4"
msgid "puññaṃ corehyahāriyan”ti."
msgstr ""
