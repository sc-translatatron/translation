#. <section class="sutta" id="sn22.31"><article><div class="hgroup"><p class="division">
msgctxt "sn22.31:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.31:0.2"
msgid "3. Bhāravagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.31:0.3"
msgid "31. Aghamūlasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.31:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.31:1.2"
msgid "“Aghañca, bhikkhave, desessāmi aghamūlañca."
msgstr ""

msgctxt "sn22.31:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn22.31:1.4"
msgid "Katamañca, bhikkhave, aghaṃ?"
msgstr ""

msgctxt "sn22.31:1.5"
msgid ""
"Rūpaṃ, bhikkhave, aghaṃ, vedanā aghaṃ, saññā aghaṃ, saṅkhārā aghaṃ, viññāṇaṃ "
"aghaṃ."
msgstr ""

msgctxt "sn22.31:1.6"
msgid "Idaṃ vuccati, bhikkhave, aghaṃ."
msgstr ""

msgctxt "sn22.31:1.7"
msgid "Katamañca, bhikkhave, aghamūlaṃ?"
msgstr ""

#. VAR: nandīrāgasahagatā → nandirāgasahagatā (sabbattha)
msgctxt "sn22.31:1.8"
msgid "Yāyaṃ taṇhā ponobhavikā nandīrāgasahagatā tatratatrābhinandinī;"
msgstr ""

msgctxt "sn22.31:1.9"
msgid "Seyyathidaṃ—kāmataṇhā, bhavataṇhā, vibhavataṇhā."
msgstr ""

msgctxt "sn22.31:1.10"
msgid "Idaṃ vuccati, bhikkhave, aghamūlan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.31:1.11"
msgid "Dasamaṃ."
msgstr ""
