#. <section class="sutta" id="sn17.30"><article><div class="hgroup"><p class="division">
msgctxt "sn17.30:0.1"
msgid "Saṃyutta Nikāya 17"
msgstr ""

#. </p><p>
msgctxt "sn17.30:0.2"
msgid "3. Tatiyavagga"
msgstr ""

#. </p><h1>
#. VAR: 30. Bhikkhusutta → khīṇāsavabhikkhusuttaṃ (bj)
msgctxt "sn17.30:0.3"
msgid "30. Bhikkhusutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.239"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn17.30:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn17.30:1.2"
msgid ""
"“Yopi so, bhikkhave, bhikkhu arahaṃ khīṇāsavo tassapāhaṃ lābhasakkārasiloko "
"antarāyāya vadāmī”ti."
msgstr ""

msgctxt "sn17.30:1.3"
msgid "Evaṃ vutte, āyasmā ānando bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn17.30:1.4"
msgid ""
"“kissa pana, bhante, khīṇāsavassa bhikkhuno lābhasakkārasiloko antarāyāyā”ti?"
msgstr ""

msgctxt "sn17.30:1.5"
msgid ""
"“Yā hissa sā, ānanda, akuppā cetovimutti nāhaṃ tassā lābhasakkārasilokaṃ "
"antarāyāya vadāmi."
msgstr ""

msgctxt "sn17.30:1.6"
msgid ""
"Ye ca khvassa, ānanda, appamattassa ātāpino pahitattassa viharato "
"diṭṭhadhammasukhavihārā adhigatā tesāhamassa lābhasakkārasilokaṃ antarāyāya "
"vadāmi."
msgstr ""

msgctxt "sn17.30:1.7"
msgid ""
"Evaṃ dāruṇo kho, ānanda, lābhasakkārasiloko kaṭuko pharuso antarāyiko "
"anuttarassa yogakkhemassa adhigamāya."
msgstr ""

msgctxt "sn17.30:1.8"
msgid "Tasmātihānanda, evaṃ sikkhitabbaṃ:"
msgstr ""

msgctxt "sn17.30:1.9"
msgid ""
"‘uppannaṃ lābhasakkārasilokaṃ pajahissāma, na ca no uppanno "
"lābhasakkārasiloko cittaṃ pariyādāya ṭhassatī’ti."
msgstr ""

msgctxt "sn17.30:1.10"
msgid "Evañhi vo, ānanda, sikkhitabban”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn17.30:1.11"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn17.30:1.12"
msgid "Tatiyo vaggo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn17.30:2.1"
msgid "Mātugāmo ca kalyāṇī,"
msgstr ""

#. <br>
msgctxt "sn17.30:2.2"
msgid "putto ca ekadhītu ca;"
msgstr ""

#. <br>
msgctxt "sn17.30:2.3"
msgid "Samaṇabrāhmaṇā tīṇi,"
msgstr ""

#. <br>
msgctxt "sn17.30:2.4"
msgid "chavi rajju ca bhikkhunāti."
msgstr ""
