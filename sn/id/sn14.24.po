#. <section class="sutta" id="sn14.24"><article><div class="hgroup"><p class="division">
msgctxt "sn14.24:0.1"
msgid "Saṃyutta Nikāya 14"
msgstr ""

#. </p><p>
msgctxt "sn14.24:0.2"
msgid "3. Kammapathavagga"
msgstr ""

#. </p><h1>
msgctxt "sn14.24:0.3"
msgid "24. Dussīlasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn14.24:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn14.24:1.2"
msgid "“Dhātusova, bhikkhave, sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.24:1.3"
msgid "Assaddhā assaddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.24:1.4"
msgid "ahirikā ahirikehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.24:1.5"
msgid "anottappino anottappīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.24:1.6"
msgid "dussīlā dussīlehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.24:1.7"
msgid "duppaññā duppaññehi saddhiṃ saṃsandanti samenti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.167"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn14.24:2.1"
msgid "Saddhā saddhehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.24:2.2"
msgid "hirimanā hirimanehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.24:2.3"
msgid "ottappino ottappīhi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.24:2.4"
msgid "sīlavanto sīlavantehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.24:2.5"
msgid "paññavanto paññavantehi saddhiṃ saṃsandanti samentī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn14.24:2.6"
msgid "Dutiyaṃ."
msgstr ""
