#. <section class="sutta" id="sn12.43"><article><div class="hgroup"><p class="division">
msgctxt "sn12.43:0.1"
msgid "Saṃyutta Nikāya 12"
msgstr ""

#. </p><p>
msgctxt "sn12.43:0.2"
msgid "5. Gahapativagga"
msgstr ""

#. </p><h1>
msgctxt "sn12.43:0.3"
msgid "43. Dukkhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.72"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn12.43:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn12.43:1.2"
msgid "“Dukkhassa, bhikkhave, samudayañca atthaṅgamañca desessāmi."
msgstr ""

msgctxt "sn12.43:1.3"
msgid "Taṃ suṇātha, sādhukaṃ manasi karotha, bhāsissāmī”ti."
msgstr ""

msgctxt "sn12.43:1.4"
msgid "“Evaṃ, bhante”ti kho te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "sn12.43:1.5"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn12.43:2.1"
msgid "“Katamo ca, bhikkhave, dukkhassa samudayo?"
msgstr ""

msgctxt "sn12.43:2.2"
msgid ""
"Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇaṃ. Tiṇṇaṃ saṅgati phasso."
msgstr ""

msgctxt "sn12.43:2.3"
msgid "Phassapaccayā vedanā;"
msgstr ""

msgctxt "sn12.43:2.4"
msgid "vedanāpaccayā taṇhā."
msgstr ""

msgctxt "sn12.43:2.5"
msgid "Ayaṃ kho, bhikkhave, dukkhassa samudayo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn12.43:3.1"
msgid "Sotañca paṭicca sadde ca uppajjati sotaviññāṇaṃ … pe …"
msgstr ""

msgctxt "sn12.43:3.2"
msgid "ghānañca paṭicca gandhe ca … pe …"
msgstr ""

msgctxt "sn12.43:3.3"
msgid "jivhañca paṭicca rase ca … pe …"
msgstr ""

msgctxt "sn12.43:3.4"
msgid "kāyañca paṭicca phoṭṭhabbe ca … pe …"
msgstr ""

msgctxt "sn12.43:3.5"
msgid ""
"manañca paṭicca dhamme ca uppajjati manoviññāṇaṃ. Tiṇṇaṃ saṅgati phasso."
msgstr ""

msgctxt "sn12.43:3.6"
msgid "Phassapaccayā vedanā;"
msgstr ""

msgctxt "sn12.43:3.7"
msgid "vedanāpaccayā taṇhā."
msgstr ""

msgctxt "sn12.43:3.8"
msgid "Ayaṃ kho, bhikkhave, dukkhassa samudayo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn12.43:4.1"
msgid "Katamo ca, bhikkhave, dukkhassa atthaṅgamo?"
msgstr ""

msgctxt "sn12.43:4.2"
msgid ""
"Cakkhuñca paṭicca rūpe ca uppajjati cakkhuviññāṇaṃ. Tiṇṇaṃ saṅgati phasso."
msgstr ""

msgctxt "sn12.43:4.3"
msgid "Phassapaccayā vedanā;"
msgstr ""

msgctxt "sn12.43:4.4"
msgid "vedanāpaccayā taṇhā."
msgstr ""

msgctxt "sn12.43:4.5"
msgid "Tassāyeva taṇhāya asesavirāganirodhā upādānanirodho;"
msgstr ""

msgctxt "sn12.43:4.6"
msgid "upādānanirodhā bhavanirodho;"
msgstr ""

msgctxt "sn12.43:4.7"
msgid "bhavanirodhā jātinirodho;"
msgstr ""

msgctxt "sn12.43:4.8"
msgid "jātinirodhā jarāmaraṇaṃ sokaparidevadukkhadomanassupāyāsā nirujjhanti."
msgstr ""

msgctxt "sn12.43:4.9"
msgid "Evametassa kevalassa dukkhakkhandhassa nirodho hoti."
msgstr ""

msgctxt "sn12.43:4.10"
msgid "Ayaṃ kho, bhikkhave, dukkhassa atthaṅgamo."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.73"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn12.43:5.1"
msgid "Sotañca paṭicca sadde ca uppajjati sotaviññāṇaṃ … pe …"
msgstr ""

msgctxt "sn12.43:5.2"
msgid "ghānañca paṭicca gandhe ca … pe …"
msgstr ""

msgctxt "sn12.43:5.3"
msgid "jivhañca paṭicca rase ca … pe …"
msgstr ""

msgctxt "sn12.43:5.4"
msgid "kāyañca paṭicca phoṭṭhabbe ca … pe …"
msgstr ""

msgctxt "sn12.43:5.5"
msgid ""
"manañca paṭicca dhamme ca uppajjati manoviññāṇaṃ. Tiṇṇaṃ saṅgati phasso."
msgstr ""

msgctxt "sn12.43:5.6"
msgid "Phassapaccayā vedanā;"
msgstr ""

msgctxt "sn12.43:5.7"
msgid "vedanāpaccayā taṇhā."
msgstr ""

msgctxt "sn12.43:5.8"
msgid "Tassāyeva taṇhāya asesavirāganirodhā upādānanirodho;"
msgstr ""

msgctxt "sn12.43:5.9"
msgid "upādānanirodhā bhavanirodho;"
msgstr ""

msgctxt "sn12.43:5.10"
msgid "bhavanirodhā jātinirodho;"
msgstr ""

msgctxt "sn12.43:5.11"
msgid "jātinirodhā jarāmaraṇaṃ sokaparidevadukkhadomanassupāyāsā nirujjhanti."
msgstr ""

msgctxt "sn12.43:5.12"
msgid "Evametassa kevalassa dukkhakkhandhassa nirodho hoti."
msgstr ""

msgctxt "sn12.43:5.13"
msgid "Ayaṃ kho, bhikkhave, dukkhassa atthaṅgamo”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.43:5.14"
msgid "Tatiyaṃ."
msgstr ""
