#. <section class="sutta" id="sn1.28"><article><div class="hgroup"><p class="division">
msgctxt "sn1.28:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.28:0.2"
msgid "3. Sattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.28:0.3"
msgid "28. Mahaddhanasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.34"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.28:1.1"
msgid "“Mahaddhanā mahābhogā,"
msgstr ""

#. <br>
msgctxt "sn1.28:1.2"
msgid "raṭṭhavantopi khattiyā;"
msgstr ""

#. <br>
msgctxt "sn1.28:1.3"
msgid "Aññamaññābhigijjhanti,"
msgstr ""

#. <br>
msgctxt "sn1.28:1.4"
msgid "kāmesu analaṅkatā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.28:2.1"
msgid "Tesu ussukkajātesu,"
msgstr ""

#. <br>
msgctxt "sn1.28:2.2"
msgid "bhavasotānusārisu;"
msgstr ""

#. <br>
#. VAR: Kedha taṇhaṃ → rodhataṇhaṃ (s1-3, km) | gedhataṇhaṃ (pts1-2, mr)
#. VAR: pajahiṃsu → pavāhiṃsu (s1-3, km, mr)
msgctxt "sn1.28:2.3"
msgid "Kedha taṇhaṃ pajahiṃsu,"
msgstr ""

#. <br>
msgctxt "sn1.28:2.4"
msgid "ke lokasmiṃ anussukā”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.35"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.28:3.1"
msgid "“Hitvā agāraṃ pabbajitā,"
msgstr ""

#. <br>
msgctxt "sn1.28:3.2"
msgid "hitvā puttaṃ pasuṃ piyaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.28:3.3"
msgid "Hitvā rāgañca dosañca,"
msgstr ""

#. <br>
msgctxt "sn1.28:3.4"
msgid "avijjañca virājiya;"
msgstr ""

#. <br>
msgctxt "sn1.28:3.5"
msgid "Khīṇāsavā arahanto,"
msgstr ""

#. <br>
msgctxt "sn1.28:3.6"
msgid "te lokasmiṃ anussukā”ti."
msgstr ""
