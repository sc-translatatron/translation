#. <section class="sutta" id="sn31.1"><article><div class="hgroup"><p class="division">
msgctxt "sn31.1:0.1"
msgid "Saṃyutta Nikāya 31"
msgstr ""

#. </p><p>
msgctxt "sn31.1:0.2"
msgid "1. Gandhabbavagga"
msgstr ""

#. </p><h1>
msgctxt "sn31.1:0.3"
msgid "1. Suddhikasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.250"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn31.1:1.1"
msgid ""
"Ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme … "
"pe …"
msgstr ""

msgctxt "sn31.1:1.2"
msgid "bhagavā etadavoca:"
msgstr ""

msgctxt "sn31.1:1.3"
msgid "“gandhabbakāyike vo, bhikkhave, deve desessāmi."
msgstr ""

msgctxt "sn31.1:1.4"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn31.1:1.5"
msgid "Katamā ca, bhikkhave, gandhabbakāyikā devā?"
msgstr ""

msgctxt "sn31.1:1.6"
msgid "Santi, bhikkhave, mūlagandhe adhivatthā devā."
msgstr ""

msgctxt "sn31.1:1.7"
msgid "Santi, bhikkhave, sāragandhe adhivatthā devā."
msgstr ""

msgctxt "sn31.1:1.8"
msgid "Santi, bhikkhave, pheggugandhe adhivatthā devā."
msgstr ""

msgctxt "sn31.1:1.9"
msgid "Santi, bhikkhave, tacagandhe adhivatthā devā."
msgstr ""

msgctxt "sn31.1:1.10"
msgid "Santi, bhikkhave, papaṭikagandhe adhivatthā devā."
msgstr ""

msgctxt "sn31.1:1.11"
msgid "Santi, bhikkhave, pattagandhe adhivatthā devā."
msgstr ""

msgctxt "sn31.1:1.12"
msgid "Santi, bhikkhave, pupphagandhe adhivatthā devā."
msgstr ""

msgctxt "sn31.1:1.13"
msgid "Santi, bhikkhave, phalagandhe adhivatthā devā."
msgstr ""

msgctxt "sn31.1:1.14"
msgid "Santi, bhikkhave, rasagandhe adhivatthā devā."
msgstr ""

msgctxt "sn31.1:1.15"
msgid "Santi, bhikkhave, gandhagandhe adhivatthā devā."
msgstr ""

msgctxt "sn31.1:1.16"
msgid "Ime vuccanti, bhikkhave, gandhabbakāyikā devā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn31.1:1.17"
msgid "Paṭhamaṃ."
msgstr ""
