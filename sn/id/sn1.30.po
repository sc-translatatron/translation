#. <section class="sutta" id="sn1.30"><article><div class="hgroup"><p class="division">
msgctxt "sn1.30:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.30:0.2"
msgid "3. Sattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.30:0.3"
msgid "30. Eṇijaṅghasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.36"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.30:1.1"
msgid "“Eṇijaṅghaṃ kisaṃ vīraṃ,"
msgstr ""

#. <br>
msgctxt "sn1.30:1.2"
msgid "appāhāraṃ alolupaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.30:1.3"
msgid "Sīhaṃvekacaraṃ nāgaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.30:1.4"
msgid "kāmesu anapekkhinaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.30:1.5"
msgid "Upasaṅkamma pucchāma,"
msgstr ""

#. <br>
msgctxt "sn1.30:1.6"
msgid "kathaṃ dukkhā pamuccatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.30:2.1"
msgid "“Pañca kāmaguṇā loke,"
msgstr ""

#. <br>
msgctxt "sn1.30:2.2"
msgid "manochaṭṭhā paveditā;"
msgstr ""

#. <br>
msgctxt "sn1.30:2.3"
msgid "Ettha chandaṃ virājetvā,"
msgstr ""

#. <br>
msgctxt "sn1.30:2.4"
msgid "evaṃ dukkhā pamuccatī”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn1.30:2.5"
msgid "Sattivaggo tatiyo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="pts2ed" id="pts2ed1.37"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.30:3.1"
msgid "Sattiyā phusati ceva,"
msgstr ""

#. <br>
msgctxt "sn1.30:3.2"
msgid "jaṭā manonivāraṇā;"
msgstr ""

#. <br>
msgctxt "sn1.30:3.3"
msgid "Arahantena pajjoto,"
msgstr ""

#. <br>
msgctxt "sn1.30:3.4"
msgid "sarā mahaddhanena ca;"
msgstr ""

#. <br>
msgctxt "sn1.30:3.5"
msgid "Catucakkena navamaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.30:3.6"
msgid "eṇijaṅghena te dasāti."
msgstr ""
