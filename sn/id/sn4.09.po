#. <section class="sutta" id="sn4.9"><article><div class="hgroup"><p class="division">
msgctxt "sn4.9:0.1"
msgid "Saṃyutta Nikāya 4"
msgstr ""

#. </p><p>
msgctxt "sn4.9:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
msgctxt "sn4.9:0.3"
msgid "9. Paṭhamaāyusutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn4.9:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn4.9:1.2"
msgid "ekaṃ samayaṃ bhagavā rājagahe viharati veḷuvane kalandakanivāpe."
msgstr ""

msgctxt "sn4.9:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "sn4.9:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "sn4.9:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "sn4.9:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn4.9:2.1"
msgid "“Appamidaṃ, bhikkhave, manussānaṃ āyu."
msgstr ""

msgctxt "sn4.9:2.2"
msgid "Gamanīyo samparāyo, kattabbaṃ kusalaṃ, caritabbaṃ brahmacariyaṃ."
msgstr ""

msgctxt "sn4.9:2.3"
msgid "Natthi jātassa amaraṇaṃ."
msgstr ""

msgctxt "sn4.9:2.4"
msgid "Yo, bhikkhave, ciraṃ jīvati, so vassasataṃ appaṃ vā bhiyyo”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn4.9:3.1"
msgid ""
"Atha kho māro pāpimā yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavantaṃ "
"gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn4.9:4.1"
msgid "“Dīghamāyu manussānaṃ,"
msgstr ""

#. <br>
msgctxt "sn4.9:4.2"
msgid "na naṃ hīḷe suporiso;"
msgstr ""

#. <br>
msgctxt "sn4.9:4.3"
msgid "Careyya khīramattova,"
msgstr ""

#. <br>
msgctxt "sn4.9:4.4"
msgid "natthi maccussa āgamo”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.242"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn4.9:5.1"
msgid "“Appamāyu manussānaṃ,"
msgstr ""

#. <br>
msgctxt "sn4.9:5.2"
msgid "hīḷeyya naṃ suporiso;"
msgstr ""

#. <br>
msgctxt "sn4.9:5.3"
msgid "Careyyādittasīsova,"
msgstr ""

#. <br>
msgctxt "sn4.9:5.4"
msgid "natthi maccussa nāgamo”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn4.9:6.1"
msgid "Atha kho māro … pe … tatthevantaradhāyīti."
msgstr ""
