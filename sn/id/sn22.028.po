#. <section class="sutta" id="sn22.28"><article><div class="hgroup"><p class="division">
msgctxt "sn22.28:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.28:0.2"
msgid "3. Bhāravagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.28:0.3"
msgid "28. Tatiyaassādasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.30"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.28:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.28:1.2"
msgid ""
"“No cedaṃ, bhikkhave, rūpassa assādo abhavissa nayidaṃ sattā rūpasmiṃ "
"sārajjeyyuṃ."
msgstr ""

msgctxt "sn22.28:1.3"
msgid ""
"Yasmā ca kho, bhikkhave, atthi rūpassa assādo, tasmā sattā rūpasmiṃ "
"sārajjanti."
msgstr ""

msgctxt "sn22.28:1.4"
msgid ""
"No cedaṃ, bhikkhave, rūpassa ādīnavo abhavissa nayidaṃ sattā rūpasmiṃ "
"nibbindeyyuṃ."
msgstr ""

msgctxt "sn22.28:1.5"
msgid ""
"Yasmā ca kho, bhikkhave, atthi rūpassa ādīnavo, tasmā sattā rūpasmiṃ "
"nibbindanti."
msgstr ""

msgctxt "sn22.28:1.6"
msgid ""
"No cedaṃ, bhikkhave, rūpassa nissaraṇaṃ abhavissa nayidaṃ sattā rūpasmā "
"nissareyyuṃ."
msgstr ""

msgctxt "sn22.28:1.7"
msgid ""
"Yasmā ca kho, bhikkhave, atthi rūpassa nissaraṇaṃ, tasmā sattā rūpasmā "
"nissaranti."
msgstr ""

msgctxt "sn22.28:1.8"
msgid "No cedaṃ, bhikkhave, vedanāya … pe …"
msgstr ""

msgctxt "sn22.28:1.9"
msgid "no cedaṃ, bhikkhave, saññāya …"
msgstr ""

msgctxt "sn22.28:1.10"
msgid ""
"no cedaṃ, bhikkhave, saṅkhārānaṃ nissaraṇaṃ abhavissa, nayidaṃ sattā "
"saṅkhārehi nissareyyuṃ."
msgstr ""

msgctxt "sn22.28:1.11"
msgid ""
"Yasmā ca kho, bhikkhave, atthi saṅkhārānaṃ nissaraṇaṃ, tasmā sattā "
"saṅkhārehi nissaranti."
msgstr ""

msgctxt "sn22.28:1.12"
msgid ""
"No cedaṃ, bhikkhave, viññāṇassa assādo abhavissa, nayidaṃ sattā viññāṇasmiṃ "
"sārajjeyyuṃ."
msgstr ""

msgctxt "sn22.28:1.13"
msgid ""
"Yasmā ca kho, bhikkhave, atthi viññāṇassa assādo, tasmā sattā viññāṇasmiṃ "
"sārajjanti."
msgstr ""

msgctxt "sn22.28:1.14"
msgid ""
"No cedaṃ, bhikkhave, viññāṇassa ādīnavo abhavissa, nayidaṃ sattā viññāṇasmiṃ "
"nibbindeyyuṃ."
msgstr ""

msgctxt "sn22.28:1.15"
msgid ""
"Yasmā ca kho, bhikkhave, atthi viññāṇassa ādīnavo, tasmā sattā viññāṇasmiṃ "
"nibbindanti."
msgstr ""

msgctxt "sn22.28:1.16"
msgid ""
"No cedaṃ, bhikkhave, viññāṇassa nissaraṇaṃ abhavissa, nayidaṃ sattā "
"viññāṇasmā nissareyyuṃ."
msgstr ""

msgctxt "sn22.28:1.17"
msgid ""
"Yasmā ca kho, bhikkhave, atthi viññāṇassa nissaraṇaṃ, tasmā sattā viññāṇasmā "
"nissaranti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.31"></a>
#. <a class="sc" id="sc2"></a>
#. VAR: nābbhaññaṃsu → na abbhaññiṃsu (s1-3)
msgctxt "sn22.28:2.1"
msgid ""
"Yāvakīvañca, bhikkhave, sattā imesaṃ pañcannaṃ upādānakkhandhānaṃ assādañca "
"assādato ādīnavañca ādīnavato nissaraṇañca nissaraṇato yathābhūtaṃ "
"nābbhaññaṃsu;"
msgstr ""

msgctxt "sn22.28:2.2"
msgid ""
"neva tāva, bhikkhave, sattā sadevakā lokā samārakā sabrahmakā "
"sassamaṇabrāhmaṇiyā pajāya sadevamanussāya nissaṭā visaṃyuttā vippamuttā "
"vimariyādīkatena cetasā vihariṃsu."
msgstr ""

msgctxt "sn22.28:2.3"
msgid ""
"Yato ca kho, bhikkhave, sattā imesaṃ pañcannaṃ upādānakkhandhānaṃ assādañca "
"assādato ādīnavañca ādīnavato nissaraṇañca nissaraṇato yathābhūtaṃ "
"abbhaññaṃsu;"
msgstr ""

msgctxt "sn22.28:2.4"
msgid ""
"atha, bhikkhave, sattā sadevakā lokā samārakā sabrahmakā sassamaṇabrāhmaṇiyā "
"pajāya sadevamanussāya nissaṭā visaṃyuttā vippamuttā vimariyādīkatena cetasā "
"viharanti”."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.28:2.5"
msgid "Sattamaṃ."
msgstr ""
