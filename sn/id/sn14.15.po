#. <section class="sutta" id="sn14.15"><article><div class="hgroup"><p class="division">
msgctxt "sn14.15:0.1"
msgid "Saṃyutta Nikāya 14"
msgstr ""

#. </p><p>
msgctxt "sn14.15:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
#. VAR: 15. Caṅkamasutta → kammaṃ (pts1-2)
msgctxt "sn14.15:0.3"
msgid "15. Caṅkamasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn14.15:1.1"
msgid "Ekaṃ samayaṃ bhagavā rājagahe viharati gijjhakūṭe pabbate."
msgstr ""

msgctxt "sn14.15:1.2"
msgid ""
"Tena kho pana samayena āyasmā sāriputto sambahulehi bhikkhūhi saddhiṃ "
"bhagavato avidūre caṅkamati;"
msgstr ""

msgctxt "sn14.15:1.3"
msgid ""
"āyasmāpi kho mahāmoggallāno sambahulehi bhikkhūhi saddhiṃ bhagavato avidūre "
"caṅkamati;"
msgstr ""

msgctxt "sn14.15:1.4"
msgid ""
"āyasmāpi kho mahākassapo sambahulehi bhikkhūhi saddhiṃ bhagavato avidūre "
"caṅkamati;"
msgstr ""

msgctxt "sn14.15:1.5"
msgid ""
"āyasmāpi kho anuruddho sambahulehi bhikkhūhi saddhiṃ bhagavato avidūre "
"caṅkamati;"
msgstr ""

#. VAR: puṇṇo mantāniputto → puṇṇo mantānīputto (s1-3)
msgctxt "sn14.15:1.6"
msgid ""
"āyasmāpi kho puṇṇo mantāniputto sambahulehi bhikkhūhi saddhiṃ bhagavato "
"avidūre caṅkamati;"
msgstr ""

msgctxt "sn14.15:1.7"
msgid ""
"āyasmāpi kho upāli sambahulehi bhikkhūhi saddhiṃ bhagavato avidūre caṅkamati;"
msgstr ""

msgctxt "sn14.15:1.8"
msgid ""
"āyasmāpi kho ānando sambahulehi bhikkhūhi saddhiṃ bhagavato avidūre "
"caṅkamati;"
msgstr ""

msgctxt "sn14.15:1.9"
msgid ""
"devadattopi kho sambahulehi bhikkhūhi saddhiṃ bhagavato avidūre caṅkamati."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.156"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn14.15:2.1"
msgid "Atha kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "sn14.15:2.2"
msgid ""
"“passatha no tumhe, bhikkhave, sāriputtaṃ sambahulehi bhikkhūhi saddhiṃ "
"caṅkamantan”ti?"
msgstr ""

msgctxt "sn14.15:2.3"
msgid "“Evaṃ, bhante”."
msgstr ""

msgctxt "sn14.15:2.4"
msgid "“Sabbe kho ete, bhikkhave, bhikkhū mahāpaññā."
msgstr ""

msgctxt "sn14.15:2.5"
msgid ""
"Passatha no tumhe, bhikkhave, moggallānaṃ sambahulehi bhikkhūhi saddhiṃ "
"caṅkamantan”ti?"
msgstr ""

msgctxt "sn14.15:2.6"
msgid "“Evaṃ, bhante”."
msgstr ""

msgctxt "sn14.15:2.7"
msgid "“Sabbe kho ete, bhikkhave, bhikkhū mahiddhikā."
msgstr ""

msgctxt "sn14.15:2.8"
msgid ""
"Passatha no tumhe, bhikkhave, kassapaṃ sambahulehi bhikkhūhi saddhiṃ "
"caṅkamantan”ti?"
msgstr ""

msgctxt "sn14.15:2.9"
msgid "“Evaṃ, bhante”."
msgstr ""

msgctxt "sn14.15:2.10"
msgid "“Sabbe kho ete, bhikkhave, bhikkhū dhutavādā."
msgstr ""

msgctxt "sn14.15:2.11"
msgid ""
"Passatha no tumhe, bhikkhave, anuruddhaṃ sambahulehi bhikkhūhi saddhiṃ "
"caṅkamantan”ti?"
msgstr ""

msgctxt "sn14.15:2.12"
msgid "“Evaṃ, bhante”."
msgstr ""

msgctxt "sn14.15:2.13"
msgid "“Sabbe kho ete, bhikkhave, bhikkhū dibbacakkhukā."
msgstr ""

msgctxt "sn14.15:2.14"
msgid ""
"Passatha no tumhe, bhikkhave, puṇṇaṃ mantāniputtaṃ sambahulehi bhikkhūhi "
"saddhiṃ caṅkamantan”ti?"
msgstr ""

msgctxt "sn14.15:2.15"
msgid "“Evaṃ, bhante”."
msgstr ""

msgctxt "sn14.15:2.16"
msgid "“Sabbe kho ete, bhikkhave, bhikkhū dhammakathikā."
msgstr ""

msgctxt "sn14.15:2.17"
msgid ""
"Passatha no tumhe, bhikkhave, upāliṃ sambahulehi bhikkhūhi saddhiṃ "
"caṅkamantan”ti?"
msgstr ""

msgctxt "sn14.15:2.18"
msgid "“Evaṃ, bhante”."
msgstr ""

msgctxt "sn14.15:2.19"
msgid "“Sabbe kho ete, bhikkhave, bhikkhū vinayadharā."
msgstr ""

msgctxt "sn14.15:2.20"
msgid ""
"Passatha no tumhe, bhikkhave, ānandaṃ sambahulehi bhikkhūhi saddhiṃ "
"caṅkamantan”ti?"
msgstr ""

msgctxt "sn14.15:2.21"
msgid "“Evaṃ, bhante”."
msgstr ""

msgctxt "sn14.15:2.22"
msgid "“Sabbe kho ete, bhikkhave, bhikkhū bahussutā."
msgstr ""

msgctxt "sn14.15:2.23"
msgid ""
"Passatha no tumhe, bhikkhave, devadattaṃ sambahulehi bhikkhūhi saddhiṃ "
"caṅkamantan”ti?"
msgstr ""

msgctxt "sn14.15:2.24"
msgid "“Evaṃ, bhante”."
msgstr ""

msgctxt "sn14.15:2.25"
msgid "“Sabbe kho ete, bhikkhave, bhikkhū pāpicchā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn14.15:3.1"
msgid "Dhātusova, bhikkhave, sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.15:3.2"
msgid "Hīnādhimuttikā hīnādhimuttikehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.15:3.3"
msgid "kalyāṇādhimuttikā kalyāṇādhimuttikehi saddhiṃ saṃsandanti samenti."
msgstr ""

msgctxt "sn14.15:3.4"
msgid "Atītampi kho, bhikkhave, addhānaṃ dhātusova sattā saṃsandiṃsu samiṃsu."
msgstr ""

msgctxt "sn14.15:3.5"
msgid "Hīnādhimuttikā hīnādhimuttikehi saddhiṃ saṃsandiṃsu samiṃsu;"
msgstr ""

msgctxt "sn14.15:3.6"
msgid "kalyāṇādhimuttikā kalyāṇādhimuttikehi saddhiṃ saṃsandiṃsu samiṃsu."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn14.15:4.1"
msgid ""
"Anāgatampi kho, bhikkhave, addhānaṃ dhātusova sattā saṃsandissanti "
"samessanti."
msgstr ""

msgctxt "sn14.15:4.2"
msgid "Hīnādhimuttikā hīnādhimuttikehi saddhiṃ saṃsandissanti samessanti;"
msgstr ""

msgctxt "sn14.15:4.3"
msgid ""
"kalyāṇādhimuttikā kalyāṇādhimuttikehi saddhiṃ saṃsandissanti samessanti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.157"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn14.15:5.1"
msgid ""
"Etarahipi kho, bhikkhave, paccuppannaṃ addhānaṃ dhātusova sattā saṃsandanti "
"samenti."
msgstr ""

msgctxt "sn14.15:5.2"
msgid "Hīnādhimuttikā hīnādhimuttikehi saddhiṃ saṃsandanti samenti;"
msgstr ""

msgctxt "sn14.15:5.3"
msgid "kalyāṇādhimuttikā kalyāṇādhimuttikehi saddhiṃ saṃsandanti samentī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn14.15:5.4"
msgid "Pañcamaṃ."
msgstr ""
