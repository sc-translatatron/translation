#. <section class="sutta" id="sn11.6"><article><div class="hgroup"><p class="division">
msgctxt "sn11.6:0.1"
msgid "Saṃyutta Nikāya 11"
msgstr ""

#. </p><p>
msgctxt "sn11.6:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
msgctxt "sn11.6:0.3"
msgid "6. Kulāvakasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn11.6:1.1"
msgid "Sāvatthiyaṃ."
msgstr ""

msgctxt "sn11.6:1.2"
msgid "“Bhūtapubbaṃ, bhikkhave, devāsurasaṅgāmo samupabyūḷho ahosi."
msgstr ""

msgctxt "sn11.6:1.3"
msgid "Tasmiṃ kho pana, bhikkhave, saṅgāme asurā jiniṃsu, devā parājiniṃsu."
msgstr ""

msgctxt "sn11.6:1.4"
msgid ""
"Parājitā ca kho, bhikkhave, devā apāyaṃsveva uttarenamukhā, abhiyaṃsveva ne "
"asurā."
msgstr ""

msgctxt "sn11.6:1.5"
msgid ""
"Atha kho, bhikkhave, sakko devānamindo mātali saṅgāhakaṃ gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.484"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn11.6:2.1"
msgid "‘Kulāvakā mātali simbalismiṃ,"
msgstr ""

#. <br>
msgctxt "sn11.6:2.2"
msgid "Īsāmukhena parivajjayassu;"
msgstr ""

#. <br>
msgctxt "sn11.6:2.3"
msgid "Kāmaṃ cajāma asuresu pāṇaṃ,"
msgstr ""

#. <br>
#. VAR: vikulāvakā → vikulāvā (s1-3, km, mr)
msgctxt "sn11.6:2.4"
msgid "Māyime dijā vikulāvakā ahesun’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts1ed" id="pts1ed1.225"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn11.6:3.1"
msgid ""
"‘Evaṃ, bhaddantavā’ti kho, bhikkhave, mātali saṅgāhako sakkassa "
"devānamindassa paṭissutvā sahassayuttaṃ ājaññarathaṃ paccudāvattesi."
msgstr ""

msgctxt "sn11.6:3.2"
msgid "Atha kho, bhikkhave, asurānaṃ etadahosi:"
msgstr ""

msgctxt "sn11.6:3.3"
msgid ""
"‘paccudāvatto kho dāni sakkassa devānamindassa sahassayutto ājaññaratho."
msgstr ""

msgctxt "sn11.6:3.4"
msgid ""
"Dutiyampi kho devā asurehi saṅgāmessantī’ti bhītā asurapurameva pāvisiṃsu."
msgstr ""

msgctxt "sn11.6:3.5"
msgid "Iti kho, bhikkhave, sakkassa devānamindassa dhammena jayo ahosī”ti."
msgstr ""
