#. <section class="sutta" id="sn12.2"><article><div class="hgroup"><p class="division">
msgctxt "sn12.2:0.1"
msgid "Saṃyutta Nikāya 12"
msgstr ""

#. </p><p>
msgctxt "sn12.2:0.2"
msgid "1. Buddhavagga"
msgstr ""

#. </p><h1>
msgctxt "sn12.2:0.3"
msgid "2. Vibhaṅgasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn12.2:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn12.2:1.2"
msgid "“Paṭiccasamuppādaṃ vo, bhikkhave, desessāmi vibhajissāmi."
msgstr ""

msgctxt "sn12.2:1.3"
msgid "Taṃ suṇātha, sādhukaṃ manasi karotha, bhāsissāmī”ti."
msgstr ""

msgctxt "sn12.2:1.4"
msgid "“Evaṃ, bhante”ti kho te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "sn12.2:1.5"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn12.2:2.1"
msgid "“Katamo ca, bhikkhave, paṭiccasamuppādo?"
msgstr ""

msgctxt "sn12.2:2.2"
msgid "Avijjāpaccayā, bhikkhave, saṅkhārā;"
msgstr ""

msgctxt "sn12.2:2.3"
msgid "saṅkhārapaccayā viññāṇaṃ;"
msgstr ""

msgctxt "sn12.2:2.4"
msgid "viññāṇapaccayā nāmarūpaṃ;"
msgstr ""

msgctxt "sn12.2:2.5"
msgid "nāmarūpapaccayā saḷāyatanaṃ;"
msgstr ""

msgctxt "sn12.2:2.6"
msgid "saḷāyatanapaccayā phasso;"
msgstr ""

msgctxt "sn12.2:2.7"
msgid "phassapaccayā vedanā;"
msgstr ""

msgctxt "sn12.2:2.8"
msgid "vedanāpaccayā taṇhā;"
msgstr ""

msgctxt "sn12.2:2.9"
msgid "taṇhāpaccayā upādānaṃ;"
msgstr ""

msgctxt "sn12.2:2.10"
msgid "upādānapaccayā bhavo;"
msgstr ""

msgctxt "sn12.2:2.11"
msgid "bhavapaccayā jāti;"
msgstr ""

msgctxt "sn12.2:2.12"
msgid "jātipaccayā jarāmaraṇaṃ sokaparidevadukkhadomanassupāyāsā sambhavanti."
msgstr ""

msgctxt "sn12.2:2.13"
msgid "Evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.3"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn12.2:3.1"
msgid "Katamañca, bhikkhave, jarāmaraṇaṃ?"
msgstr ""

msgctxt "sn12.2:3.2"
msgid ""
"Yā tesaṃ tesaṃ sattānaṃ tamhi tamhi sattanikāye jarā jīraṇatā khaṇḍiccaṃ "
"pāliccaṃ valittacatā āyuno saṃhāni indriyānaṃ paripāko;"
msgstr ""

msgctxt "sn12.2:3.3"
msgid "ayaṃ vuccati jarā."
msgstr ""

#. VAR: () → (jīvitindriyassa upacchedo) (s1-3, km)
msgctxt "sn12.2:3.4"
msgid ""
"Yā tesaṃ tesaṃ sattānaṃ tamhā tamhā sattanikāyā cuti cavanatā bhedo "
"antaradhānaṃ maccu maraṇaṃ kālakiriyā khandhānaṃ bhedo kaḷevarassa nikkhepo "
"(), "
msgstr ""

msgctxt "sn12.2:3.5"
msgid "idaṃ vuccati maraṇaṃ."
msgstr ""

msgctxt "sn12.2:3.6"
msgid "Iti ayañca jarā, idañca maraṇaṃ."
msgstr ""

msgctxt "sn12.2:3.7"
msgid "Idaṃ vuccati, bhikkhave, jarāmaraṇaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn12.2:4.1"
msgid "Katamā ca, bhikkhave, jāti?"
msgstr ""

msgctxt "sn12.2:4.2"
msgid ""
"Yā tesaṃ tesaṃ sattānaṃ tamhi tamhi sattanikāye jāti sañjāti okkanti "
"abhinibbatti khandhānaṃ pātubhāvo āyatanānaṃ paṭilābho."
msgstr ""

msgctxt "sn12.2:4.3"
msgid "Ayaṃ vuccati, bhikkhave, jāti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn12.2:5.1"
msgid "Katamo ca, bhikkhave, bhavo?"
msgstr ""

msgctxt "sn12.2:5.2"
msgid "Tayome, bhikkhave, bhavā—"
msgstr ""

msgctxt "sn12.2:5.3"
msgid "kāmabhavo, rūpabhavo, arūpabhavo."
msgstr ""

msgctxt "sn12.2:5.4"
msgid "Ayaṃ vuccati, bhikkhave, bhavo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn12.2:6.1"
msgid "Katamañca, bhikkhave, upādānaṃ?"
msgstr ""

msgctxt "sn12.2:6.2"
msgid "Cattārimāni, bhikkhave, upādānāni—"
msgstr ""

msgctxt "sn12.2:6.3"
msgid "kāmupādānaṃ, diṭṭhupādānaṃ, sīlabbatupādānaṃ, attavādupādānaṃ."
msgstr ""

msgctxt "sn12.2:6.4"
msgid "Idaṃ vuccati, bhikkhave, upādānaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn12.2:7.1"
msgid "Katamā ca, bhikkhave, taṇhā?"
msgstr ""

msgctxt "sn12.2:7.2"
msgid "Chayime, bhikkhave, taṇhākāyā—"
msgstr ""

msgctxt "sn12.2:7.3"
msgid ""
"rūpataṇhā, saddataṇhā, gandhataṇhā, rasataṇhā, phoṭṭhabbataṇhā, dhammataṇhā."
msgstr ""

msgctxt "sn12.2:7.4"
msgid "Ayaṃ vuccati, bhikkhave, taṇhā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn12.2:8.1"
msgid "Katamā ca, bhikkhave, vedanā?"
msgstr ""

msgctxt "sn12.2:8.2"
msgid "Chayime, bhikkhave, vedanākāyā—"
msgstr ""

msgctxt "sn12.2:8.3"
msgid ""
"cakkhusamphassajā vedanā, sotasamphassajā vedanā, ghānasamphassajā vedanā, "
"jivhāsamphassajā vedanā, kāyasamphassajā vedanā, manosamphassajā vedanā."
msgstr ""

msgctxt "sn12.2:8.4"
msgid "Ayaṃ vuccati, bhikkhave, vedanā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "sn12.2:9.1"
msgid "Katamo ca, bhikkhave, phasso?"
msgstr ""

msgctxt "sn12.2:9.2"
msgid "Chayime, bhikkhave, phassakāyā—"
msgstr ""

msgctxt "sn12.2:9.3"
msgid ""
"cakkhusamphasso, sotasamphasso, ghānasamphasso, jivhāsamphasso, "
"kāyasamphasso, manosamphasso."
msgstr ""

msgctxt "sn12.2:9.4"
msgid "Ayaṃ vuccati, bhikkhave, phasso."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn12.2:10.1"
msgid "Katamañca, bhikkhave, saḷāyatanaṃ?"
msgstr ""

msgctxt "sn12.2:10.2"
msgid ""
"Cakkhāyatanaṃ, sotāyatanaṃ, ghānāyatanaṃ, jivhāyatanaṃ, kāyāyatanaṃ, "
"manāyatanaṃ—"
msgstr ""

msgctxt "sn12.2:10.3"
msgid "idaṃ vuccati, bhikkhave, saḷāyatanaṃ."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.4"></a>
#. <a class="sc" id="sc11"></a>
msgctxt "sn12.2:11.1"
msgid "Katamañca, bhikkhave, nāmarūpaṃ?"
msgstr ""

msgctxt "sn12.2:11.2"
msgid "Vedanā, saññā, cetanā, phasso, manasikāro—"
msgstr ""

msgctxt "sn12.2:11.3"
msgid "idaṃ vuccati nāmaṃ."
msgstr ""

msgctxt "sn12.2:11.4"
msgid "Cattāro ca mahābhūtā, catunnañca mahābhūtānaṃ upādāyarūpaṃ."
msgstr ""

msgctxt "sn12.2:11.5"
msgid "Idaṃ vuccati rūpaṃ."
msgstr ""

msgctxt "sn12.2:11.6"
msgid "Iti idañca nāmaṃ, idañca rūpaṃ."
msgstr ""

msgctxt "sn12.2:11.7"
msgid "Idaṃ vuccati, bhikkhave, nāmarūpaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn12.2:12.1"
msgid "Katamañca, bhikkhave, viññāṇaṃ?"
msgstr ""

msgctxt "sn12.2:12.2"
msgid "Chayime, bhikkhave, viññāṇakāyā—"
msgstr ""

msgctxt "sn12.2:12.3"
msgid ""
"cakkhuviññāṇaṃ, sotaviññāṇaṃ, ghānaviññāṇaṃ, jivhāviññāṇaṃ, kāyaviññāṇaṃ, "
"manoviññāṇaṃ."
msgstr ""

msgctxt "sn12.2:12.4"
msgid "Idaṃ vuccati, bhikkhave, viññāṇaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc13"></a>
msgctxt "sn12.2:13.1"
msgid "Katame ca, bhikkhave, saṅkhārā?"
msgstr ""

msgctxt "sn12.2:13.2"
msgid "Tayome, bhikkhave, saṅkhārā—"
msgstr ""

msgctxt "sn12.2:13.3"
msgid "kāyasaṅkhāro, vacīsaṅkhāro, cittasaṅkhāro."
msgstr ""

msgctxt "sn12.2:13.4"
msgid "Ime vuccanti, bhikkhave, saṅkhārā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc14"></a>
msgctxt "sn12.2:14.1"
msgid "Katamā ca, bhikkhave, avijjā?"
msgstr ""

msgctxt "sn12.2:14.2"
msgid ""
"Yaṃ kho, bhikkhave, dukkhe aññāṇaṃ, dukkhasamudaye aññāṇaṃ, dukkhanirodhe "
"aññāṇaṃ, dukkhanirodhagāminiyā paṭipadāya aññāṇaṃ."
msgstr ""

msgctxt "sn12.2:14.3"
msgid "Ayaṃ vuccati, bhikkhave, avijjā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc15"></a>
msgctxt "sn12.2:15.1"
msgid "Iti kho, bhikkhave, avijjāpaccayā saṅkhārā;"
msgstr ""

msgctxt "sn12.2:15.2"
msgid "saṅkhārapaccayā viññāṇaṃ … pe …"
msgstr ""

msgctxt "sn12.2:15.3"
msgid "evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

msgctxt "sn12.2:15.4"
msgid "Avijjāya tveva asesavirāganirodhā saṅkhāranirodho;"
msgstr ""

msgctxt "sn12.2:15.5"
msgid "saṅkhāranirodhā viññāṇanirodho … pe …"
msgstr ""

msgctxt "sn12.2:15.6"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.2:15.7"
msgid "Dutiyaṃ."
msgstr ""
