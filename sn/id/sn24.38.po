msgid "38. Arūpīattāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn24.38:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn24.38:1.2"
msgid ""
"“Kismiṃ nu kho, bhikkhave, sati, kiṃ upādāya, kiṃ abhinivissa evaṃ diṭṭhi "
"uppajjati:"
msgstr ""

msgctxt "sn24.38:1.3"
msgid "‘arūpī attā hoti arogo paraṃ maraṇā’”ti?"
msgstr ""

msgctxt "sn24.38:1.4"
msgid "(Peyyālo.)"
msgstr ""

#. </p><p class="endsection">
msgctxt "sn24.38:1.5"
msgid "Vīsatimaṃ."
msgstr ""
