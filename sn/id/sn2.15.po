#. <section class="sutta" id="sn2.15"><article><div class="hgroup"><p class="division">
msgctxt "sn2.15:0.1"
msgid "Saṃyutta Nikāya 2"
msgstr ""

#. </p><p>
msgctxt "sn2.15:0.2"
msgid "2. Anāthapiṇḍikavagga"
msgstr ""

#. </p><h1>
msgctxt "sn2.15:0.3"
msgid "15. Candanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn2.15:1.1"
msgid "Ekamantaṃ ṭhito kho candano devaputto bhagavantaṃ gāthāya ajjhabhāsi:"
msgstr ""

# See BB for readings, adopting variant here.
#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
#. VAR: Kathaṃsu → ko sū dha (bj, pts2)
msgctxt "sn2.15:2.1"
msgid "“Kathaṃsu tarati oghaṃ,"
msgstr ""

#. <br>
msgctxt "sn2.15:2.2"
msgid "rattindivamatandito;"
msgstr ""

#. <br>
msgctxt "sn2.15:2.3"
msgid "Appatiṭṭhe anālambe,"
msgstr ""

#. <br>
msgctxt "sn2.15:2.4"
msgid "ko gambhīre na sīdatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn2.15:3.1"
msgid "“Sabbadā sīlasampanno,"
msgstr ""

#. <br>
msgctxt "sn2.15:3.2"
msgid "Paññavā susamāhito;"
msgstr ""

#. <br>
msgctxt "sn2.15:3.3"
msgid "Āraddhavīriyo pahitatto,"
msgstr ""

#. <br>
msgctxt "sn2.15:3.4"
msgid "Oghaṃ tarati duttaraṃ."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.122"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn2.15:4.1"
msgid "Virato kāmasaññāya,"
msgstr ""

#. <br>
msgctxt "sn2.15:4.2"
msgid "rūpasaṃyojanātigo;"
msgstr ""

#. <br>
msgctxt "sn2.15:4.3"
msgid "Nandīrāgaparikkhīṇo,"
msgstr ""

#. <br>
msgctxt "sn2.15:4.4"
msgid "so gambhīre na sīdatī”ti."
msgstr ""
