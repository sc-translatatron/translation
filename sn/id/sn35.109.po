#. <section class="sutta" id="sn35.109"><article><div class="hgroup"><p class="division">
msgctxt "sn35.109:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.109:0.2"
msgid "11. Yogakkhemivagga"
msgstr ""

#. </p><h1>
#. VAR: 109. Saṃyojaniyasutta → saññojanasuttaṃ (bj) | saṃyojana (pts1)
msgctxt "sn35.109:0.3"
msgid "109. Saṃyojaniyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.89"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.109:1.1"
msgid "“Saṃyojaniye ca, bhikkhave, dhamme desessāmi saṃyojanañca."
msgstr ""

msgctxt "sn35.109:1.2"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn35.109:1.3"
msgid "Katame ca, bhikkhave, saṃyojaniyā dhammā, katamañca saṃyojanaṃ?"
msgstr ""

msgctxt "sn35.109:1.4"
msgid "Cakkhuṃ, bhikkhave, saṃyojaniyo dhammo."
msgstr ""

msgctxt "sn35.109:1.5"
msgid "Yo tattha chandarāgo, taṃ tattha saṃyojanaṃ … pe …"
msgstr ""

msgctxt "sn35.109:1.6"
msgid "jivhā saṃyojaniyo dhammo … pe …"
msgstr ""

msgctxt "sn35.109:1.7"
msgid "mano saṃyojaniyo dhammo."
msgstr ""

msgctxt "sn35.109:1.8"
msgid "Yo tattha chandarāgo, taṃ tattha saṃyojanaṃ."
msgstr ""

msgctxt "sn35.109:1.9"
msgid "Ime vuccanti, bhikkhave, saṃyojaniyā dhammā, idaṃ saṃyojanan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.109:1.10"
msgid "Chaṭṭhaṃ."
msgstr ""
