#. <section class="sutta" id="sn12.54"><article><div class="hgroup"><p class="division">
msgctxt "sn12.54:0.1"
msgid "Saṃyutta Nikāya 12"
msgstr ""

#. </p><p>
msgctxt "sn12.54:0.2"
msgid "6. Dukkhavagga"
msgstr ""

#. </p><h1>
msgctxt "sn12.54:0.3"
msgid "54. Dutiyasaṃyojanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.87"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn12.54:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn12.54:1.2"
msgid ""
"“Seyyathāpi, bhikkhave, telañca paṭicca vaṭṭiñca paṭicca telappadīpo "
"jhāyeyya."
msgstr ""

msgctxt "sn12.54:1.3"
msgid "Tatra puriso kālena kālaṃ telaṃ āsiñceyya vaṭṭiṃ upasaṃhareyya."
msgstr ""

msgctxt "sn12.54:1.4"
msgid ""
"Evañhi so, bhikkhave, telappadīpo tadāhāro tadupādāno ciraṃ dīghamaddhānaṃ "
"jaleyya."
msgstr ""

msgctxt "sn12.54:1.5"
msgid ""
"Evameva kho, bhikkhave, saṃyojaniyesu dhammesu assādānupassino viharato "
"taṇhā pavaḍḍhati."
msgstr ""

msgctxt "sn12.54:1.6"
msgid "Taṇhāpaccayā upādānaṃ … pe …"
msgstr ""

msgctxt "sn12.54:1.7"
msgid "evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn12.54:2.1"
msgid ""
"Seyyathāpi, bhikkhave, telañca paṭicca vaṭṭiñca paṭicca telappadīpo jhāyeyya."
msgstr ""

msgctxt "sn12.54:2.2"
msgid "Tatra puriso na kālena kālaṃ telaṃ āsiñceyya na vaṭṭiṃ upasaṃhareyya."
msgstr ""

msgctxt "sn12.54:2.3"
msgid ""
"Evañhi so, bhikkhave, telappadīpo purimassa ca upādānassa pariyādānā aññassa "
"ca anupahārā anāhāro nibbāyeyya."
msgstr ""

msgctxt "sn12.54:2.4"
msgid ""
"Evameva kho, bhikkhave, saṃyojaniyesu dhammesu ādīnavānupassino viharato "
"taṇhā nirujjhati."
msgstr ""

msgctxt "sn12.54:2.5"
msgid "Taṇhānirodhā upādānanirodho … pe …"
msgstr ""

msgctxt "sn12.54:2.6"
msgid "evametassa kevalassa dukkhakkhandhassa nirodho hotī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.54:2.7"
msgid "Catutthaṃ."
msgstr ""
