#. <section class="sutta" id="sn23.20"><article><div class="hgroup"><p class="division">
msgctxt "sn23.20:0.1"
msgid "Saṃyutta Nikāya 23"
msgstr ""

#. </p><p>
msgctxt "sn23.20:0.2"
msgid "2. Dutiyamāravagga"
msgstr ""

#. </p><h1>
#. VAR: 20. Vayadhammasutta → vaya (pts1)
msgctxt "sn23.20:0.3"
msgid "20. Vayadhammasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn23.20:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn23.20:1.2"
msgid "Ekamantaṃ nisinno kho āyasmā rādho bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn23.20:1.3"
msgid "“‘vayadhammo, vayadhammo’ti, bhante, vuccati."
msgstr ""

msgctxt "sn23.20:1.4"
msgid "Katamo nu kho, bhante, vayadhammo”ti?"
msgstr ""

msgctxt "sn23.20:1.5"
msgid ""
"“Rūpaṃ kho, rādha, vayadhammo, vedanā vayadhammo, saññā vayadhammo, saṅkhārā "
"vayadhammo, viññāṇaṃ vayadhammo."
msgstr ""

msgctxt "sn23.20:1.6"
msgid "Evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn23.20:1.7"
msgid "nāparaṃ itthattāyāti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn23.20:1.8"
msgid "Dasamaṃ."
msgstr ""
