#. <section class="sutta" id="sn3.4"><article><div class="hgroup"><p class="division">
msgctxt "sn3.4:0.1"
msgid "Saṃyutta Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "sn3.4:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
msgctxt "sn3.4:0.3"
msgid "4. Piyasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.72"></a>
#. <a class="pts2ed" id="pts2ed1.165"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn3.4:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn3.4:1.2"
msgid "Ekamantaṃ nisinno kho rājā pasenadi kosalo bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn3.4:1.3"
msgid ""
"“idha mayhaṃ, bhante, rahogatassa paṭisallīnassa evaṃ cetaso parivitakko "
"udapādi:"
msgstr ""

msgctxt "sn3.4:1.4"
msgid "‘kesaṃ nu kho piyo attā, kesaṃ appiyo attā’ti?"
msgstr ""

msgctxt "sn3.4:1.5"
msgid "Tassa mayhaṃ, bhante, etadahosi:"
msgstr ""

msgctxt "sn3.4:1.6"
msgid ""
"‘ye ca kho keci kāyena duccaritaṃ caranti, vācāya duccaritaṃ caranti, manasā "
"duccaritaṃ caranti;"
msgstr ""

msgctxt "sn3.4:1.7"
msgid "tesaṃ appiyo attā."
msgstr ""

msgctxt "sn3.4:1.8"
msgid "Kiñcāpi te evaṃ vadeyyuṃ:"
msgstr ""

# close quote incorrect in MS.
msgctxt "sn3.4:1.9"
msgid "“piyo no attā”ti, atha kho tesaṃ appiyo attā’."
msgstr ""

msgctxt "sn3.4:1.10"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "sn3.4:1.11"
msgid "Yañhi appiyo appiyassa kareyya, taṃ te attanāva attano karonti;"
msgstr ""

msgctxt "sn3.4:1.12"
msgid "tasmā tesaṃ appiyo attā."
msgstr ""

msgctxt "sn3.4:1.13"
msgid ""
"Ye ca kho keci kāyena sucaritaṃ caranti, vācāya sucaritaṃ caranti, manasā "
"sucaritaṃ caranti;"
msgstr ""

msgctxt "sn3.4:1.14"
msgid "tesaṃ piyo attā."
msgstr ""

msgctxt "sn3.4:1.15"
msgid "Kiñcāpi te evaṃ vadeyyuṃ:"
msgstr ""

msgctxt "sn3.4:1.16"
msgid "‘appiyo no attā’ti; atha kho tesaṃ piyo attā."
msgstr ""

msgctxt "sn3.4:1.17"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "sn3.4:1.18"
msgid "Yañhi piyo piyassa kareyya, taṃ te attanāva attano karonti;"
msgstr ""

msgctxt "sn3.4:1.19"
msgid "tasmā tesaṃ piyo attā”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn3.4:2.1"
msgid "“Evametaṃ, mahārāja, evametaṃ, mahārāja."
msgstr ""

msgctxt "sn3.4:2.2"
msgid ""
"Ye hi keci, mahārāja, kāyena duccaritaṃ caranti, vācāya duccaritaṃ caranti, "
"manasā duccaritaṃ caranti;"
msgstr ""

msgctxt "sn3.4:2.3"
msgid "tasmā tesaṃ appiyo attā."
msgstr ""

msgctxt "sn3.4:2.4"
msgid "Kiñcāpi te evaṃ vadeyyuṃ:"
msgstr ""

msgctxt "sn3.4:2.5"
msgid "‘piyo no attā’ti, atha kho tesaṃ appiyo attā."
msgstr ""

msgctxt "sn3.4:2.6"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "sn3.4:2.7"
msgid ""
"Yañhi, mahārāja, appiyo appiyassa kareyya, taṃ te attanāva attano karonti;"
msgstr ""

msgctxt "sn3.4:2.8"
msgid "tasmā tesaṃ appiyo attā."
msgstr ""

msgctxt "sn3.4:2.9"
msgid ""
"Ye ca kho keci, mahārāja, kāyena sucaritaṃ caranti, vācāya sucaritaṃ "
"caranti, manasā sucaritaṃ caranti;"
msgstr ""

msgctxt "sn3.4:2.10"
msgid "tesaṃ piyo attā."
msgstr ""

msgctxt "sn3.4:2.11"
msgid "Kiñcāpi te evaṃ vadeyyuṃ:"
msgstr ""

msgctxt "sn3.4:2.12"
msgid "‘appiyo no attā’ti;"
msgstr ""

msgctxt "sn3.4:2.13"
msgid "atha kho tesaṃ piyo attā."
msgstr ""

msgctxt "sn3.4:2.14"
msgid "Taṃ kissa hetu?"
msgstr ""

msgctxt "sn3.4:2.15"
msgid "Yañhi, mahārāja, piyo piyassa kareyya, taṃ te attanāva attano karonti;"
msgstr ""

msgctxt "sn3.4:2.16"
msgid "tasmā tesaṃ piyo attā”ti."
msgstr ""

msgctxt "sn3.4:2.17"
msgid "Idamavoca … pe …"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn3.4:3.1"
msgid "“Attānañce piyaṃ jaññā,"
msgstr ""

#. <br>
msgctxt "sn3.4:3.2"
msgid "na naṃ pāpena saṃyuje;"
msgstr ""

#. <br>
msgctxt "sn3.4:3.3"
msgid "Na hi taṃ sulabhaṃ hoti,"
msgstr ""

#. <br>
msgctxt "sn3.4:3.4"
msgid "sukhaṃ dukkaṭakārinā."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.166"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn3.4:4.1"
msgid "Antakenādhipannassa,"
msgstr ""

#. <br>
msgctxt "sn3.4:4.2"
msgid "jahato mānusaṃ bhavaṃ;"
msgstr ""

#. <br>
msgctxt "sn3.4:4.3"
msgid "Kiñhi tassa sakaṃ hoti,"
msgstr ""

#. <br>
msgctxt "sn3.4:4.4"
msgid "kiñca ādāya gacchati;"
msgstr ""

#. <br>
msgctxt "sn3.4:4.5"
msgid "Kiñcassa anugaṃ hoti,"
msgstr ""

#. <br>
#. VAR: anapāyinī → anupāyinī (s1-3, km, mr)
msgctxt "sn3.4:4.6"
msgid "chāyāva anapāyinī."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn3.4:5.1"
msgid "Ubho puññañca pāpañca,"
msgstr ""

#. <br>
msgctxt "sn3.4:5.2"
msgid "yaṃ macco kurute idha;"
msgstr ""

#. <br>
msgctxt "sn3.4:5.3"
msgid "Tañhi tassa sakaṃ hoti,"
msgstr ""

#. <br>
#. VAR: taṃva → tañca (sabbattha)
msgctxt "sn3.4:5.4"
msgid "taṃva ādāya gacchati;"
msgstr ""

#. <br>
#. VAR: Taṃvassa → tañcassa (bj, s1-3, km, pts2) | taṃ cassa (pts1)
msgctxt "sn3.4:5.5"
msgid "Taṃvassa anugaṃ hoti,"
msgstr ""

#. <br>
msgctxt "sn3.4:5.6"
msgid "chāyāva anapāyinī."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn3.4:6.1"
msgid "Tasmā kareyya kalyāṇaṃ,"
msgstr ""

#. <br>
msgctxt "sn3.4:6.2"
msgid "nicayaṃ samparāyikaṃ;"
msgstr ""

#. <br>
msgctxt "sn3.4:6.3"
msgid "Puññāni paralokasmiṃ,"
msgstr ""

#. <br>
msgctxt "sn3.4:6.4"
msgid "patiṭṭhā honti pāṇinan”ti."
msgstr ""
