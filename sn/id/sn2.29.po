#. <section class="sutta" id="sn2.29"><article><div class="hgroup"><p class="division">
msgctxt "sn2.29:0.1"
msgid "Saṃyutta Nikāya 2"
msgstr ""

#. </p><p>
msgctxt "sn2.29:0.2"
msgid "3. Nānātitthiyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn2.29:0.3"
msgid "29. Susimasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts2ed" id="pts2ed1.147"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn2.29:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn2.29:1.2"
msgid ""
"Atha kho āyasmā ānando yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavantaṃ "
"abhivādetvā ekamantaṃ nisīdi. Ekamantaṃ nisinnaṃ kho āyasmantaṃ ānandaṃ "
"bhagavā etadavoca:"
msgstr ""

# BB’s “approve of” seems weird to me: who says they “approve of” another human being?
msgctxt "sn2.29:1.3"
msgid "“tuyhampi no, ānanda, sāriputto ruccatī”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn2.29:2.1"
msgid ""
"“Kassa hi nāma, bhante, abālassa aduṭṭhassa amūḷhassa avipallatthacittassa "
"āyasmā sāriputto na rucceyya?"
msgstr ""

msgctxt "sn2.29:2.2"
msgid "Paṇḍito, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.3"
msgid "Mahāpañño, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.4"
msgid "Puthupañño, bhante, āyasmā sāriputto."
msgstr ""

#. VAR: Hāsapañño → hāsupañño (bj, pts2)
msgctxt "sn2.29:2.5"
msgid "Hāsapañño, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.6"
msgid "Javanapañño, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.7"
msgid "Tikkhapañño, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.8"
msgid "Nibbedhikapañño, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.9"
msgid "Appiccho, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.10"
msgid "Santuṭṭho, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.11"
msgid "Pavivitto, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.12"
msgid "Asaṃsaṭṭho, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.13"
msgid "Āraddhavīriyo, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.14"
msgid "Vattā, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.15"
msgid "Vacanakkhamo, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.16"
msgid "Codako, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.17"
msgid "Pāpagarahī, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:2.18"
msgid ""
"Kassa hi nāma, bhante, abālassa aduṭṭhassa amūḷhassa avipallatthacittassa "
"āyasmā sāriputto na rucceyyā”ti?"
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.64"></a>
#. <a class="pts2ed" id="pts2ed1.148"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn2.29:3.1"
msgid "“Evametaṃ, ānanda, evametaṃ, ānanda."
msgstr ""

msgctxt "sn2.29:3.2"
msgid ""
"Kassa hi nāma, ānanda, abālassa aduṭṭhassa amūḷhassa avipallatthacittassa "
"sāriputto na rucceyya?"
msgstr ""

msgctxt "sn2.29:3.3"
msgid "Paṇḍito, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.4"
msgid "Mahāpañño, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.5"
msgid "Puthupañño, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.6"
msgid "Hāsapañño, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.7"
msgid "Javanapañño, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.8"
msgid "Tikkhapañño, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.9"
msgid "Nibbedhikapañño, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.10"
msgid "Appiccho, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.11"
msgid "Santuṭṭho, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.12"
msgid "Pavivitto, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.13"
msgid "Asaṃsaṭṭho, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.14"
msgid "Āraddhavīriyo, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.15"
msgid "Vattā, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.16"
msgid "Vacanakkhamo, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.17"
msgid "Codako, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.18"
msgid "Pāpagarahī, ānanda, sāriputto."
msgstr ""

msgctxt "sn2.29:3.19"
msgid ""
"Kassa hi nāma, ānanda, abālassa aduṭṭhassa amūḷhassa avipallatthacittassa "
"sāriputto na rucceyyā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
#. VAR: susimo → susīmo (bj, pts2)
msgctxt "sn2.29:4.1"
msgid ""
"Atha kho susimo devaputto āyasmato sāriputtassa vaṇṇe bhaññamāne mahatiyā "
"devaputtaparisāya parivuto yena bhagavā tenupasaṅkami; upasaṅkamitvā "
"bhagavantaṃ abhivādetvā ekamantaṃ aṭṭhāsi. Ekamantaṃ ṭhito kho susimo "
"devaputto bhagavantaṃ etadavoca:"
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.149"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn2.29:5.1"
msgid "“Evametaṃ, bhagavā, evametaṃ, sugata."
msgstr ""

msgctxt "sn2.29:5.2"
msgid ""
"Kassa hi nāma, bhante, abālassa aduṭṭhassa amūḷhassa avipallatthacittassa "
"āyasmā sāriputto na rucceyya?"
msgstr ""

msgctxt "sn2.29:5.3"
msgid "Paṇḍito, bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:5.4"
msgid ""
"Mahāpañño, bhante, puthupañño, bhante, hāsapañño, bhante, javanapañño, "
"bhante, tikkhapañño, bhante, nibbedhikapañño, bhante, appiccho, bhante, "
"santuṭṭho, bhante, pavivitto, bhante, asaṃsaṭṭho, bhante, āraddhavīriyo, "
"bhante, vattā, bhante, vacanakkhamo, bhante, codako, bhante, pāpagarahī, "
"bhante, āyasmā sāriputto."
msgstr ""

msgctxt "sn2.29:5.5"
msgid ""
"Kassa hi nāma, bhante, abālassa aduṭṭhassa amūḷhassa avipallatthacittassa "
"āyasmā sāriputto na rucceyya?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn2.29:6.1"
msgid ""
"Ahampi hi, bhante, yaññadeva devaputtaparisaṃ upasaṅkamiṃ, etadeva bahulaṃ "
"saddaṃ suṇāmi:"
msgstr ""

msgctxt "sn2.29:6.2"
msgid "‘paṇḍito āyasmā sāriputto;"
msgstr ""

msgctxt "sn2.29:6.3"
msgid ""
"mahāpañño āyasmā, puthupañño āyasmā, hāsapañño āyasmā, javanapañño āyasmā, "
"tikkhapañño āyasmā, nibbedhikapañño āyasmā, appiccho āyasmā, santuṭṭho "
"āyasmā, pavivitto āyasmā, asaṃsaṭṭho āyasmā, āraddhavīriyo āyasmā, vattā "
"āyasmā, vacanakkhamo āyasmā, codako āyasmā, pāpagarahī āyasmā sāriputto’ti."
msgstr ""

msgctxt "sn2.29:6.4"
msgid ""
"Kassa hi nāma, bhante, abālassa aduṭṭhassa amūḷhassa avipallatthacittassa "
"āyasmā sāriputto na rucceyyā”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn2.29:7.1"
msgid ""
"Atha kho susimassa devaputtassa devaputtaparisā āyasmato sāriputtassa vaṇṇe "
"bhaññamāne attamanā pamuditā pītisomanassajātā uccāvacā vaṇṇanibhā "
"upadaṃseti."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.65"></a>
#. <a class="pts2ed" id="pts2ed1.150"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "sn2.29:8.1"
msgid ""
"Seyyathāpi nāma maṇi veḷuriyo subho jātimā aṭṭhaṃso suparikammakato "
"paṇḍukambale nikkhitto bhāsate ca tapate ca virocati ca;"
msgstr ""

msgctxt "sn2.29:8.2"
msgid ""
"evamevaṃ susimassa devaputtassa devaputtaparisā āyasmato sāriputtassa vaṇṇe "
"bhaññamāne attamanā pamuditā pītisomanassajātā uccāvacā vaṇṇanibhā "
"upadaṃseti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "sn2.29:9.1"
msgid ""
"Seyyathāpi nāma nikkhaṃ jambonadaṃ "
"dakkhakammāraputtaukkāmukhasukusalasampahaṭṭhaṃ paṇḍukambale nikkhittaṃ "
"bhāsate ca tapate ca virocati ca;"
msgstr ""

msgctxt "sn2.29:9.2"
msgid ""
"evamevaṃ susimassa devaputtassa devaputtaparisā āyasmato sāriputtassa vaṇṇe "
"bhaññamāne attamanā pamuditā pītisomanassajātā uccāvacā vaṇṇanibhā "
"upadaṃseti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn2.29:10.1"
msgid ""
"Seyyathāpi nāma saradasamaye viddhe vigatavalāhake deve rattiyā "
"paccūsasamayaṃ osadhitārakā bhāsate ca tapate ca virocati ca;"
msgstr ""

msgctxt "sn2.29:10.2"
msgid ""
"evamevaṃ susimassa devaputtassa devaputtaparisā āyasmato sāriputtassa vaṇṇe "
"bhaññamāne attamanā pamuditā pītisomanassajātā uccāvacā vaṇṇanibhā "
"upadaṃseti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.151"></a>
#. <a class="sc" id="sc11"></a>
msgctxt "sn2.29:11.1"
msgid ""
"Seyyathāpi nāma saradasamaye viddhe vigatavalāhake deve ādicco nabhaṃ "
"abbhussakkamāno sabbaṃ ākāsagataṃ tamagataṃ abhivihacca bhāsate ca tapate ca "
"virocati ca;"
msgstr ""

msgctxt "sn2.29:11.2"
msgid ""
"evamevaṃ susimassa devaputtassa devaputtaparisā āyasmato sāriputtassa vaṇṇe "
"bhaññamāne attamanā pamuditā pītisomanassajātā uccāvacā vaṇṇanibhā "
"upadaṃseti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn2.29:12.1"
msgid ""
"Atha kho susimo devaputto āyasmantaṃ sāriputtaṃ ārabbha bhagavato santike "
"imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc13"></a>
msgctxt "sn2.29:13.1"
msgid "“Paṇḍitoti samaññāto,"
msgstr ""

#. <br>
msgctxt "sn2.29:13.2"
msgid "sāriputto akodhano;"
msgstr ""

#. <br>
msgctxt "sn2.29:13.3"
msgid "Appiccho sorato danto,"
msgstr ""

#. <br>
msgctxt "sn2.29:13.4"
msgid "satthuvaṇṇābhato isī”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc14"></a>
msgctxt "sn2.29:14.1"
msgid ""
"Atha kho bhagavā āyasmantaṃ sāriputtaṃ ārabbha susimaṃ devaputtaṃ gāthāya "
"paccabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc15"></a>
msgctxt "sn2.29:15.1"
msgid "“Paṇḍitoti samaññāto,"
msgstr ""

#. <br>
msgctxt "sn2.29:15.2"
msgid "sāriputto akodhano;"
msgstr ""

#. <br>
msgctxt "sn2.29:15.3"
msgid "Appiccho sorato danto,"
msgstr ""

# See BB for readings.
#. <br>
#. VAR: kālaṃ kaṅkhati sudanto → kālaṃ kaṅkhati bhatako sudanto (si) | kālaṃ kaṅkhati bhāvito sudanto (s1-3, km) | kālaṃ kaṅkhati bhatiko sudanto (pts1) | kālaṃ kaṅkhati bhāvito (pts2)
msgctxt "sn2.29:15.4"
msgid "kālaṃ kaṅkhati sudanto”ti."
msgstr ""
