#. <section class="sutta" id="sn35.103"><article><div class="hgroup"><p class="division">
msgctxt "sn35.103:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.103:0.2"
msgid "10. Saḷavagga"
msgstr ""

#. </p><h1>
#. VAR: 103. Udakasutta → uddakasuttaṃ (bj) | uddako (pts1)
msgctxt "sn35.103:0.3"
msgid "103. Udakasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.83"></a>
#. <a class="sc" id="sc1"></a>
#. VAR: Udako → uddako (bj, pts1)
msgctxt "sn35.103:1.1"
msgid "“Udako sudaṃ, bhikkhave, rāmaputto evaṃ vācaṃ bhāsati:"
msgstr ""

# This should be marked up as verse.
#. VAR: sabbajī → pabbaji (s1-3) | sabbaji (pts1)
msgctxt "sn35.103:1.2"
msgid ""
"‘idaṃ jātu vedagū, idaṃ jātu sabbajī, idaṃ jātu apalikhataṃ gaṇḍamūlaṃ "
"palikhaṇin’ti."
msgstr ""

msgctxt "sn35.103:1.3"
msgid ""
"Taṃ kho panetaṃ, bhikkhave, udako rāmaputto avedagūyeva samāno ‘vedagūsmī’ti "
"bhāsati, asabbajīyeva samāno ‘sabbajīsmī’ti bhāsati, apalikhataṃyeva "
"gaṇḍamūlaṃ palikhataṃ me ‘gaṇḍamūlan’ti bhāsati."
msgstr ""

msgctxt "sn35.103:1.4"
msgid "Idha kho taṃ, bhikkhave, bhikkhu sammā vadamāno vadeyya:"
msgstr ""

msgctxt "sn35.103:1.5"
msgid ""
"‘idaṃ jātu vedagū, idaṃ jātu sabbajī, idaṃ jātu apalikhataṃ gaṇḍamūlaṃ "
"palikhaṇin’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.103:2.1"
msgid "Kathañca, bhikkhave, vedagū hoti?"
msgstr ""

msgctxt "sn35.103:2.2"
msgid ""
"Yato kho, bhikkhave, bhikkhu channaṃ phassāyatanānaṃ samudayañca "
"atthaṅgamañca assādañca ādīnavañca nissaraṇañca yathābhūtaṃ pajānāti;"
msgstr ""

msgctxt "sn35.103:2.3"
msgid "evaṃ kho, bhikkhave, bhikkhu vedagū hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn35.103:3.1"
msgid "Kathañca, bhikkhave, bhikkhu sabbajī hoti?"
msgstr ""

msgctxt "sn35.103:3.2"
msgid ""
"Yato kho, bhikkhave, bhikkhu channaṃ phassāyatanānaṃ samudayañca "
"atthaṅgamañca assādañca ādīnavañca nissaraṇañca yathābhūtaṃ viditvā "
"anupādāvimutto hoti;"
msgstr ""

msgctxt "sn35.103:3.3"
msgid "evaṃ kho, bhikkhave, bhikkhu sabbajī hoti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.84"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn35.103:4.1"
msgid "Kathañca, bhikkhave, bhikkhuno apalikhataṃ gaṇḍamūlaṃ palikhataṃ hoti?"
msgstr ""

msgctxt "sn35.103:4.2"
msgid ""
"Gaṇḍoti kho, bhikkhave, imassetaṃ cātumahābhūtikassa kāyassa adhivacanaṃ "
"mātāpettikasambhavassa odanakummāsūpacayassa "
"aniccucchādanaparimaddanabhedanaviddhaṃsanadhammassa."
msgstr ""

msgctxt "sn35.103:4.3"
msgid "Gaṇḍamūlanti kho, bhikkhave, taṇhāyetaṃ adhivacanaṃ."
msgstr ""

msgctxt "sn35.103:4.4"
msgid ""
"Yato kho, bhikkhave, bhikkhuno taṇhā pahīnā hoti ucchinnamūlā tālāvatthukatā "
"anabhāvaṅkatā āyatiṃ anuppādadhammā;"
msgstr ""

msgctxt "sn35.103:4.5"
msgid "evaṃ kho, bhikkhave, bhikkhuno apalikhataṃ gaṇḍamūlaṃ palikhataṃ hoti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn35.103:5.1"
msgid "Udako sudaṃ, bhikkhave, rāmaputto evaṃ vācaṃ bhāsati:"
msgstr ""

msgctxt "sn35.103:5.2"
msgid ""
"‘idaṃ jātu vedagū, idaṃ jātu sabbajī, idaṃ jātu apalikhataṃ gaṇḍamūlaṃ "
"palikhaṇin’ti."
msgstr ""

msgctxt "sn35.103:5.3"
msgid ""
"Taṃ kho panetaṃ, bhikkhave, udako rāmaputto avedagūyeva samāno ‘vedagūsmī’ti "
"bhāsati, asabbajīyeva samāno ‘sabbajīsmī’ti bhāsati;"
msgstr ""

msgctxt "sn35.103:5.4"
msgid "apalikhataṃyeva gaṇḍamūlaṃ ‘palikhataṃ me gaṇḍamūlan’ti bhāsati."
msgstr ""

msgctxt "sn35.103:5.5"
msgid "Idha kho taṃ, bhikkhave, bhikkhu sammā vadamāno vadeyya:"
msgstr ""

msgctxt "sn35.103:5.6"
msgid ""
"‘idaṃ jātu vedagū, idaṃ jātu sabbajī, idaṃ jātu apalikhataṃ gaṇḍamūlaṃ "
"palikhaṇin’”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.103:5.7"
msgid "Dasamaṃ."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.103:5.8"
msgid "Saḷavaggo pañcamo."
msgstr ""

#. </p><blockquote class="uddanagatha"><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn35.103:6.1"
msgid "Dve saṅgayhā parihānaṃ,"
msgstr ""

#. <br>
msgctxt "sn35.103:6.2"
msgid "Pamādavihārī ca saṃvaro;"
msgstr ""

#. <br>
msgctxt "sn35.103:6.3"
msgid "Samādhi paṭisallānaṃ,"
msgstr ""

#. <br>
msgctxt "sn35.103:6.4"
msgid "Dve natumhākena uddakoti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn35.103:6.5"
msgid "Saḷāyatanavagge dutiyapaṇṇāsako samatto."
msgstr ""

#. </p><blockquote class="vagguddanagatha"><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn35.103:7.1"
msgid "Avijjā migajālañca,"
msgstr ""

#. <br>
msgctxt "sn35.103:7.2"
msgid "Gilānaṃ channaṃ catutthakaṃ;"
msgstr ""

#. <br>
msgctxt "sn35.103:7.3"
msgid "Saḷavaggena paññāsaṃ,"
msgstr ""

#. <br>
msgctxt "sn35.103:7.4"
msgid "Dutiyo paṇṇāsako ayanti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn35.103:7.5"
msgid "Paṭhamasatakaṃ."
msgstr ""
