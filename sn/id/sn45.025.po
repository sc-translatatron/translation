#. <section class="sutta" id="sn45.25"><article><div class="hgroup"><p class="division">
msgctxt "sn45.25:0.1"
msgid "Saṃyutta Nikāya 45"
msgstr ""

#. </p><p>
msgctxt "sn45.25:0.2"
msgid "3. Micchattavagga"
msgstr ""

#. </p><h1>
msgctxt "sn45.25:0.3"
msgid "25. Paṭhamaasappurisasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn45.25:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn45.25:1.2"
msgid "“Asappurisañca vo, bhikkhave, desessāmi, sappurisañca."
msgstr ""

msgctxt "sn45.25:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn45.25:1.4"
msgid "Katamo ca, bhikkhave, asappuriso?"
msgstr ""

msgctxt "sn45.25:1.5"
msgid ""
"Idha, bhikkhave, ekacco micchādiṭṭhiko hoti, micchāsaṅkappo, micchāvāco, "
"micchākammanto, micchāājīvo, micchāvāyāmo, micchāsati, micchāsamādhi—"
msgstr ""

msgctxt "sn45.25:1.6"
msgid "ayaṃ vuccati, bhikkhave, asappuriso."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.20"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn45.25:2.1"
msgid "Katamo ca, bhikkhave, sappuriso?"
msgstr ""

msgctxt "sn45.25:2.2"
msgid ""
"Idha, bhikkhave, ekacco sammādiṭṭhiko hoti, sammāsaṅkappo, sammāvāco, "
"sammākammanto, sammāājīvo, sammāvāyāmo, sammāsati, sammāsamādhi—"
msgstr ""

msgctxt "sn45.25:2.3"
msgid "ayaṃ vuccati, bhikkhave, sappuriso”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn45.25:2.4"
msgid "Pañcamaṃ."
msgstr ""
