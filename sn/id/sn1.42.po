#. <section class="sutta" id="sn1.42"><article><div class="hgroup"><p class="division">
msgctxt "sn1.42:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.42:0.2"
msgid "5. Ādittavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.42:0.3"
msgid "42. Kiṃdadasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.42:1.1"
msgid "“Kiṃdado balado hoti,"
msgstr ""

#. <br>
msgctxt "sn1.42:1.2"
msgid "kiṃdado hoti vaṇṇado;"
msgstr ""

#. <br>
msgctxt "sn1.42:1.3"
msgid "Kiṃdado sukhado hoti,"
msgstr ""

#. <br>
msgctxt "sn1.42:1.4"
msgid "kiṃdado hoti cakkhudo;"
msgstr ""

#. <br>
msgctxt "sn1.42:1.5"
msgid "Ko ca sabbadado hoti,"
msgstr ""

#. <br>
msgctxt "sn1.42:1.6"
msgid "taṃ me akkhāhi pucchito”ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.67"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.42:2.1"
msgid "“Annado balado hoti,"
msgstr ""

#. <br>
msgctxt "sn1.42:2.2"
msgid "vatthado hoti vaṇṇado;"
msgstr ""

#. <br>
msgctxt "sn1.42:2.3"
msgid "Yānado sukhado hoti,"
msgstr ""

#. <br>
msgctxt "sn1.42:2.4"
msgid "dīpado hoti cakkhudo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.42:3.1"
msgid "So ca sabbadado hoti,"
msgstr ""

#. <br>
msgctxt "sn1.42:3.2"
msgid "yo dadāti upassayaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.42:3.3"
msgid "Amataṃ dado ca so hoti,"
msgstr ""

#. <br>
msgctxt "sn1.42:3.4"
msgid "yo dhammamanusāsatī”ti."
msgstr ""
