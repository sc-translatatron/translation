#. <section class="sutta" id="sn11.5"><article><div class="hgroup"><p class="division">
msgctxt "sn11.5:0.1"
msgid "Saṃyutta Nikāya 11"
msgstr ""

#. </p><p>
msgctxt "sn11.5:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
msgctxt "sn11.5:0.3"
msgid "5. Subhāsitajayasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.223"></a>
#. <a class="pts2ed" id="pts2ed1.480"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn11.5:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn11.5:1.2"
msgid "“Bhūtapubbaṃ, bhikkhave, devāsurasaṅgāmo samupabyūḷho ahosi."
msgstr ""

msgctxt "sn11.5:1.3"
msgid "Atha kho, bhikkhave, vepacitti asurindo sakkaṃ devānamindaṃ etadavoca:"
msgstr ""

msgctxt "sn11.5:1.4"
msgid "‘hotu, devānaminda, subhāsitena jayo’ti."
msgstr ""

msgctxt "sn11.5:1.5"
msgid "‘Hotu, vepacitti, subhāsitena jayo’ti."
msgstr ""

msgctxt "sn11.5:1.6"
msgid "Atha kho, bhikkhave, devā ca asurā ca pārisajje ṭhapesuṃ:"
msgstr ""

msgctxt "sn11.5:1.7"
msgid "‘ime no subhāsitadubbhāsitaṃ ājānissantī’ti."
msgstr ""

msgctxt "sn11.5:1.8"
msgid "Atha kho, bhikkhave, vepacitti asurindo sakkaṃ devānamindaṃ etadavoca:"
msgstr ""

msgctxt "sn11.5:1.9"
msgid "‘bhaṇa, devānaminda, gāthan’ti."
msgstr ""

msgctxt "sn11.5:1.10"
msgid ""
"Evaṃ vutte, bhikkhave, sakko devānamindo vepacittiṃ asurindaṃ etadavoca:"
msgstr ""

# A backhanded compliment?
msgctxt "sn11.5:1.11"
msgid "‘tumhe khvettha, vepacitti, pubbadevā."
msgstr ""

msgctxt "sn11.5:1.12"
msgid "Bhaṇa, vepacitti, gāthan’ti."
msgstr ""

msgctxt "sn11.5:1.13"
msgid "Evaṃ vutte, bhikkhave, vepacitti asurindo imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn11.5:2.1"
msgid "‘Bhiyyo bālā pabhijjeyyuṃ,"
msgstr ""

#. <br>
msgctxt "sn11.5:2.2"
msgid "no cassa paṭisedhako;"
msgstr ""

#. <br>
msgctxt "sn11.5:2.3"
msgid "Tasmā bhusena daṇḍena,"
msgstr ""

#. <br>
msgctxt "sn11.5:2.4"
msgid "dhīro bālaṃ nisedhaye’ti."
msgstr ""

# This suggests that anumodati meant something more exuberant than just “rejoicing”.
#. </p></blockquote><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn11.5:3.1"
msgid ""
"Bhāsitāya kho pana, bhikkhave, vepacittinā asurindena gāthāya asurā "
"anumodiṃsu, devā tuṇhī ahesuṃ."
msgstr ""

msgctxt "sn11.5:3.2"
msgid "Atha kho, bhikkhave, vepacitti asurindo sakkaṃ devānamindaṃ etadavoca:"
msgstr ""

msgctxt "sn11.5:3.3"
msgid "‘bhaṇa, devānaminda, gāthan’ti."
msgstr ""

msgctxt "sn11.5:3.4"
msgid "Evaṃ vutte, bhikkhave, sakko devānamindo imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.481"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn11.5:4.1"
msgid "‘Etadeva ahaṃ maññe,"
msgstr ""

#. <br>
msgctxt "sn11.5:4.2"
msgid "bālassa paṭisedhanaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.5:4.3"
msgid "Paraṃ saṅkupitaṃ ñatvā,"
msgstr ""

#. <br>
msgctxt "sn11.5:4.4"
msgid "yo sato upasammatī’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn11.5:5.1"
msgid ""
"Bhāsitāya kho pana, bhikkhave, sakkena devānamindena gāthāya, devā "
"anumodiṃsu, asurā tuṇhī ahesuṃ."
msgstr ""

msgctxt "sn11.5:5.2"
msgid "Atha kho, bhikkhave, sakko devānamindo vepacittiṃ asurindaṃ etadavoca:"
msgstr ""

msgctxt "sn11.5:5.3"
msgid "‘bhaṇa, vepacitti, gāthan’ti."
msgstr ""

msgctxt "sn11.5:5.4"
msgid "Evaṃ vutte, bhikkhave, vepacitti asurindo imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn11.5:6.1"
msgid "‘Etadeva titikkhāya,"
msgstr ""

#. <br>
msgctxt "sn11.5:6.2"
msgid "vajjaṃ passāmi vāsava;"
msgstr ""

#. <br>
msgctxt "sn11.5:6.3"
msgid "Yadā naṃ maññati bālo,"
msgstr ""

#. <br>
msgctxt "sn11.5:6.4"
msgid "bhayā myāyaṃ titikkhati;"
msgstr ""

#. <br>
msgctxt "sn11.5:6.5"
msgid "Ajjhāruhati dummedho,"
msgstr ""

#. <br>
msgctxt "sn11.5:6.6"
msgid "gova bhiyyo palāyinan’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn11.5:7.1"
msgid ""
"Bhāsitāya kho pana, bhikkhave, vepacittinā asurindena gāthāya asurā "
"anumodiṃsu, devā tuṇhī ahesuṃ."
msgstr ""

msgctxt "sn11.5:7.2"
msgid "Atha kho, bhikkhave, vepacitti asurindo sakkaṃ devānamindaṃ etadavoca:"
msgstr ""

msgctxt "sn11.5:7.3"
msgid "‘bhaṇa, devānaminda, gāthan’ti."
msgstr ""

msgctxt "sn11.5:7.4"
msgid "Evaṃ vutte, bhikkhave, sakko devānamindo imā gāthāyo abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn11.5:8.1"
msgid "‘Kāmaṃ maññatu vā mā vā,"
msgstr ""

#. <br>
msgctxt "sn11.5:8.2"
msgid "bhayā myāyaṃ titikkhati;"
msgstr ""

#. <br>
msgctxt "sn11.5:8.3"
msgid "Sadatthaparamā atthā,"
msgstr ""

#. <br>
msgctxt "sn11.5:8.4"
msgid "khantyā bhiyyo na vijjati."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.482"></a>
#. <a class="sc" id="sc9"></a>
msgctxt "sn11.5:9.1"
msgid "Yo have balavā santo,"
msgstr ""

#. <br>
msgctxt "sn11.5:9.2"
msgid "dubbalassa titikkhati;"
msgstr ""

#. <br>
msgctxt "sn11.5:9.3"
msgid "Tamāhu paramaṃ khantiṃ,"
msgstr ""

#. <br>
msgctxt "sn11.5:9.4"
msgid "niccaṃ khamati dubbalo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn11.5:10.1"
msgid "Abalaṃ taṃ balaṃ āhu,"
msgstr ""

#. <br>
msgctxt "sn11.5:10.2"
msgid "yassa bālabalaṃ balaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.5:10.3"
msgid "Balassa dhammaguttassa,"
msgstr ""

#. <br>
msgctxt "sn11.5:10.4"
msgid "paṭivattā na vijjati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn11.5:11.1"
msgid "Tasseva tena pāpiyo,"
msgstr ""

#. <br>
msgctxt "sn11.5:11.2"
msgid "yo kuddhaṃ paṭikujjhati;"
msgstr ""

#. <br>
msgctxt "sn11.5:11.3"
msgid "Kuddhaṃ appaṭikujjhanto,"
msgstr ""

#. <br>
msgctxt "sn11.5:11.4"
msgid "saṅgāmaṃ jeti dujjayaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn11.5:12.1"
msgid "Ubhinnamatthaṃ carati,"
msgstr ""

#. <br>
msgctxt "sn11.5:12.2"
msgid "attano ca parassa ca;"
msgstr ""

#. <br>
msgctxt "sn11.5:12.3"
msgid "Paraṃ saṅkupitaṃ ñatvā,"
msgstr ""

#. <br>
msgctxt "sn11.5:12.4"
msgid "yo sato upasammati."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.224"></a>
#. <a class="sc" id="sc13"></a>
msgctxt "sn11.5:13.1"
msgid "Ubhinnaṃ tikicchantānaṃ,"
msgstr ""

#. <br>
msgctxt "sn11.5:13.2"
msgid "attano ca parassa ca;"
msgstr ""

#. <br>
msgctxt "sn11.5:13.3"
msgid "Janā maññanti bāloti,"
msgstr ""

#. <br>
msgctxt "sn11.5:13.4"
msgid "ye dhammassa akovidā’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts2ed" id="pts2ed1.483"></a>
#. <a class="sc" id="sc14"></a>
msgctxt "sn11.5:14.1"
msgid ""
"Bhāsitāsu kho pana, bhikkhave, sakkena devānamindena gāthāsu, devā "
"anumodiṃsu, asurā tuṇhī ahesuṃ."
msgstr ""

msgctxt "sn11.5:14.2"
msgid "Atha kho, bhikkhave, devānañca asurānañca pārisajjā etadavocuṃ:"
msgstr ""

msgctxt "sn11.5:14.3"
msgid "‘bhāsitā kho vepacittinā asurindena gāthāyo."
msgstr ""

msgctxt "sn11.5:14.4"
msgid ""
"Tā ca kho sadaṇḍāvacarā sasatthāvacarā, iti bhaṇḍanaṃ iti viggaho iti kalaho."
msgstr ""

#. VAR: Bhāsitā kho → bhāsitā kho pana (bj, pts2)
msgctxt "sn11.5:14.5"
msgid "Bhāsitā kho sakkena devānamindena gāthāyo."
msgstr ""

msgctxt "sn11.5:14.6"
msgid ""
"Tā ca kho adaṇḍāvacarā asatthāvacarā, iti abhaṇḍanaṃ iti aviggaho iti "
"akalaho."
msgstr ""

msgctxt "sn11.5:14.7"
msgid "Sakkassa devānamindassa subhāsitena jayo’ti."
msgstr ""

msgctxt "sn11.5:14.8"
msgid "Iti kho, bhikkhave, sakkassa devānamindassa subhāsitena jayo ahosī”ti."
msgstr ""
