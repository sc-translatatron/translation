#. <section class="sutta" id="sn22.104"><article><div class="hgroup"><p class="division">
msgctxt "sn22.104:0.1"
msgid "Saṃyutta Nikāya 22"
msgstr ""

#. </p><p>
msgctxt "sn22.104:0.2"
msgid "11. Antavagga"
msgstr ""

#. </p><h1>
msgctxt "sn22.104:0.3"
msgid "104. Dukkhasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.159"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn22.104:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn22.104:1.2"
msgid ""
"“Dukkhañca vo, bhikkhave, desessāmi dukkhasamudayañca dukkhanirodhañca "
"dukkhanirodhagāminiñca paṭipadaṃ."
msgstr ""

msgctxt "sn22.104:1.3"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn22.104:1.4"
msgid "Katamañca, bhikkhave, dukkhaṃ?"
msgstr ""

msgctxt "sn22.104:1.5"
msgid "Pañcupādānakkhandhātissa vacanīyaṃ."
msgstr ""

msgctxt "sn22.104:1.6"
msgid "Katame pañca?"
msgstr ""

msgctxt "sn22.104:1.7"
msgid "Seyyathidaṃ—rūpupādānakkhandho … pe … viññāṇupādānakkhandho."
msgstr ""

msgctxt "sn22.104:1.8"
msgid "Idaṃ vuccati, bhikkhave, dukkhaṃ."
msgstr ""

msgctxt "sn22.104:1.9"
msgid "Katamo ca, bhikkhave, dukkhasamudayo?"
msgstr ""

msgctxt "sn22.104:1.10"
msgid "Yāyaṃ taṇhā ponobhavikā … pe … vibhavataṇhā—"
msgstr ""

msgctxt "sn22.104:1.11"
msgid "ayaṃ vuccati, bhikkhave, dukkhasamudayo."
msgstr ""

msgctxt "sn22.104:1.12"
msgid "Katamo ca, bhikkhave, dukkhanirodho?"
msgstr ""

msgctxt "sn22.104:1.13"
msgid ""
"Yo tassāyeva taṇhāya asesavirāganirodho cāgo paṭinissaggo mutti anālayo—"
msgstr ""

msgctxt "sn22.104:1.14"
msgid "ayaṃ vuccati, bhikkhave, dukkhanirodho."
msgstr ""

msgctxt "sn22.104:1.15"
msgid "Katamā ca, bhikkhave, dukkhanirodhagāminī paṭipadā?"
msgstr ""

msgctxt "sn22.104:1.16"
msgid "Ayameva ariyo aṭṭhaṅgiko maggo."
msgstr ""

msgctxt "sn22.104:1.17"
msgid "Seyyathidaṃ—sammādiṭṭhi … pe … sammāsamādhi."
msgstr ""

msgctxt "sn22.104:1.18"
msgid "Ayaṃ vuccati, bhikkhave, dukkhanirodhagāminī paṭipadā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn22.104:1.19"
msgid "Dutiyaṃ."
msgstr ""
