#. <section class="sutta" id="sn23.14"><article><div class="hgroup"><p class="division">
msgctxt "sn23.14:0.1"
msgid "Saṃyutta Nikāya 23"
msgstr ""

#. </p><p>
msgctxt "sn23.14:0.2"
msgid "2. Dutiyamāravagga"
msgstr ""

#. </p><h1>
#. VAR: 14. Aniccadhammasutta → anicca 2 (pts1)
msgctxt "sn23.14:0.3"
msgid "14. Aniccadhammasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli3.196"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn23.14:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn23.14:1.2"
msgid "Ekamantaṃ nisinno kho āyasmā rādho bhagavantaṃ etadavoca:"
msgstr ""

msgctxt "sn23.14:1.3"
msgid "“‘aniccadhammo, aniccadhammo’ti, bhante, vuccati."
msgstr ""

msgctxt "sn23.14:1.4"
msgid "Katamo nu kho, bhante, aniccadhammo”ti?"
msgstr ""

msgctxt "sn23.14:1.5"
msgid ""
"“Rūpaṃ kho, rādha, aniccadhammo, vedanā aniccadhammo, saññā aniccadhammo, "
"saṅkhārā aniccadhammo, viññāṇaṃ aniccadhammo."
msgstr ""

msgctxt "sn23.14:1.6"
msgid "Evaṃ passaṃ … pe …"
msgstr ""

msgctxt "sn23.14:1.7"
msgid "nāparaṃ itthattāyāti pajānātī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn23.14:1.8"
msgid "Catutthaṃ."
msgstr ""
