#. <section class="sutta" id="sn11.4"><article><div class="hgroup"><p class="division">
msgctxt "sn11.4:0.1"
msgid "Saṃyutta Nikāya 11"
msgstr ""

#. </p><p>
msgctxt "sn11.4:0.2"
msgid "1. Paṭhamavagga"
msgstr ""

#. </p><h1>
msgctxt "sn11.4:0.3"
msgid "4. Vepacittisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.221"></a>
#. <a class="pts2ed" id="pts2ed1.476"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn11.4:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn11.4:1.2"
msgid "“Bhūtapubbaṃ, bhikkhave, devāsurasaṅgāmo samupabyūḷho ahosi."
msgstr ""

msgctxt "sn11.4:1.3"
msgid "Atha kho, bhikkhave, vepacitti asurindo asure āmantesi:"
msgstr ""

#. VAR: parājineyyuṃ → parājeyyuṃ (bj, pts1-2)
msgctxt "sn11.4:1.4"
msgid ""
"‘sace, mārisā, devānaṃ asurasaṅgāme samupabyūḷhe asurā jineyyuṃ devā "
"parājineyyuṃ, yena naṃ sakkaṃ devānamindaṃ kaṇṭhapañcamehi bandhanehi "
"bandhitvā mama santike āneyyātha asurapuran’ti."
msgstr ""

msgctxt "sn11.4:1.5"
msgid "Sakkopi kho, bhikkhave, devānamindo deve tāvatiṃse āmantesi:"
msgstr ""

msgctxt "sn11.4:1.6"
msgid ""
"‘sace, mārisā, devānaṃ asurasaṅgāme samupabyūḷhe devā jineyyuṃ asurā "
"parājineyyuṃ, yena naṃ vepacittiṃ asurindaṃ kaṇṭhapañcamehi bandhanehi "
"bandhitvā mama santike āneyyātha sudhammasabhan’ti."
msgstr ""

#. VAR: parājiniṃsu → parājiṃsu (bj, pts1-2)
msgctxt "sn11.4:1.7"
msgid "Tasmiṃ kho pana, bhikkhave, saṅgāme devā jiniṃsu, asurā parājiniṃsu."
msgstr ""

msgctxt "sn11.4:1.8"
msgid ""
"Atha kho, bhikkhave, devā tāvatiṃsā vepacittiṃ asurindaṃ kaṇṭhapañcamehi "
"bandhanehi bandhitvā sakkassa devānamindassa santike ānesuṃ sudhammasabhaṃ."
msgstr ""

msgctxt "sn11.4:1.9"
msgid ""
"Tatra sudaṃ, bhikkhave, vepacitti asurindo kaṇṭhapañcamehi bandhanehi baddho "
"sakkaṃ devānamindaṃ sudhammasabhaṃ pavisantañca nikkhamantañca asabbhāhi "
"pharusāhi vācāhi akkosati paribhāsati."
msgstr ""

msgctxt "sn11.4:1.10"
msgid ""
"Atha kho, bhikkhave, mātali saṅgāhako sakkaṃ devānamindaṃ gāthāhi ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn11.4:2.1"
msgid "‘Bhayā nu maghavā sakka,"
msgstr ""

#. <br>
msgctxt "sn11.4:2.2"
msgid "dubbalyā no titikkhasi;"
msgstr ""

#. <br>
msgctxt "sn11.4:2.3"
msgid "Suṇanto pharusaṃ vācaṃ,"
msgstr ""

#. <br>
msgctxt "sn11.4:2.4"
msgid "sammukhā vepacittino’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn11.4:3.1"
msgid "‘Nāhaṃ bhayā na dubbalyā,"
msgstr ""

#. <br>
msgctxt "sn11.4:3.2"
msgid "khamāmi vepacittino;"
msgstr ""

#. <br>
msgctxt "sn11.4:3.3"
msgid "Kathañhi mādiso viññū,"
msgstr ""

#. <br>
msgctxt "sn11.4:3.4"
msgid "bālena paṭisaṃyuje’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn11.4:4.1"
msgid "‘Bhiyyo bālā pabhijjeyyuṃ,"
msgstr ""

#. <br>
msgctxt "sn11.4:4.2"
msgid "no cassa paṭisedhako;"
msgstr ""

#. <br>
msgctxt "sn11.4:4.3"
msgid "Tasmā bhusena daṇḍena,"
msgstr ""

#. <br>
msgctxt "sn11.4:4.4"
msgid "dhīro bālaṃ nisedhaye’ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn11.4:5.1"
msgid "‘Etadeva ahaṃ maññe,"
msgstr ""

#. <br>
msgctxt "sn11.4:5.2"
msgid "bālassa paṭisedhanaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.4:5.3"
msgid "Paraṃ saṅkupitaṃ ñatvā,"
msgstr ""

#. <br>
msgctxt "sn11.4:5.4"
msgid "yo sato upasammatī’ti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.478"></a>
#. <a class="sc" id="sc6"></a>
msgctxt "sn11.4:6.1"
msgid "‘Etadeva titikkhāya,"
msgstr ""

#. <br>
msgctxt "sn11.4:6.2"
msgid "vajjaṃ passāmi vāsava;"
msgstr ""

#. <br>
msgctxt "sn11.4:6.3"
msgid "Yadā naṃ maññati bālo,"
msgstr ""

#. <br>
msgctxt "sn11.4:6.4"
msgid "bhayā myāyaṃ titikkhati;"
msgstr ""

#. <br>
msgctxt "sn11.4:6.5"
msgid "Ajjhāruhati dummedho,"
msgstr ""

#. <br>
msgctxt "sn11.4:6.6"
msgid "gova bhiyyo palāyinan’ti."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.222"></a>
#. <a class="sc" id="sc7"></a>
msgctxt "sn11.4:7.1"
msgid "‘Kāmaṃ maññatu vā mā vā,"
msgstr ""

#. <br>
msgctxt "sn11.4:7.2"
msgid "bhayā myāyaṃ titikkhati;"
msgstr ""

# I follow BB’s interpretation here. I’m not very happy with it, but can’t fault it.
#. <br>
msgctxt "sn11.4:7.3"
msgid "Sadatthaparamā atthā,"
msgstr ""

#. <br>
msgctxt "sn11.4:7.4"
msgid "khantyā bhiyyo na vijjati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn11.4:8.1"
msgid "Yo have balavā santo,"
msgstr ""

#. <br>
msgctxt "sn11.4:8.2"
msgid "dubbalassa titikkhati;"
msgstr ""

#. <br>
msgctxt "sn11.4:8.3"
msgid "Tamāhu paramaṃ khantiṃ,"
msgstr ""

#. <br>
msgctxt "sn11.4:8.4"
msgid "niccaṃ khamati dubbalo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc9"></a>
msgctxt "sn11.4:9.1"
msgid "Abalaṃ taṃ balaṃ āhu,"
msgstr ""

#. <br>
msgctxt "sn11.4:9.2"
msgid "yassa bālabalaṃ balaṃ;"
msgstr ""

#. <br>
msgctxt "sn11.4:9.3"
msgid "Balassa dhammaguttassa,"
msgstr ""

#. <br>
msgctxt "sn11.4:9.4"
msgid "paṭivattā na vijjati."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.479"></a>
#. <a class="sc" id="sc10"></a>
msgctxt "sn11.4:10.1"
msgid "Tasseva tena pāpiyo,"
msgstr ""

#. <br>
msgctxt "sn11.4:10.2"
msgid "yo kuddhaṃ paṭikujjhati;"
msgstr ""

#. <br>
msgctxt "sn11.4:10.3"
msgid "Kuddhaṃ appaṭikujjhanto,"
msgstr ""

#. <br>
msgctxt "sn11.4:10.4"
msgid "saṅgāmaṃ jeti dujjayaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn11.4:11.1"
msgid "Ubhinnamatthaṃ carati,"
msgstr ""

#. <br>
msgctxt "sn11.4:11.2"
msgid "attano ca parassa ca;"
msgstr ""

#. <br>
msgctxt "sn11.4:11.3"
msgid "Paraṃ saṅkupitaṃ ñatvā,"
msgstr ""

#. <br>
msgctxt "sn11.4:11.4"
msgid "yo sato upasammati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn11.4:12.1"
msgid "Ubhinnaṃ tikicchantānaṃ,"
msgstr ""

#. <br>
msgctxt "sn11.4:12.2"
msgid "attano ca parassa ca;"
msgstr ""

#. <br>
msgctxt "sn11.4:12.3"
msgid "Janā maññanti bāloti,"
msgstr ""

#. <br>
msgctxt "sn11.4:12.4"
msgid "ye dhammassa akovidā’ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc13"></a>
msgctxt "sn11.4:13.1"
msgid ""
"So hi nāma, bhikkhave, sakko devānamindo sakaṃ puññaphalaṃ upajīvamāno "
"devānaṃ tāvatiṃsānaṃ issariyādhipaccaṃ rajjaṃ kārento khantisoraccassa "
"vaṇṇavādī bhavissati."
msgstr ""

msgctxt "sn11.4:13.2"
msgid ""
"Idha kho taṃ, bhikkhave, sobhetha yaṃ tumhe evaṃ svākkhāte dhammavinaye "
"pabbajitā samānā khamā ca bhaveyyātha soratā cā”ti."
msgstr ""
