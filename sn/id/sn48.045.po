#. <section class="sutta" id="sn48.45"><article><div class="hgroup"><p class="division">
msgctxt "sn48.45:0.1"
msgid "Saṃyutta Nikāya 48"
msgstr ""

#. </p><p>
msgctxt "sn48.45:0.2"
msgid "5. Jarāvagga"
msgstr ""

#. </p><h1>
msgctxt "sn48.45:0.3"
msgid "45. Paṭhamapubbārāmasutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn48.45:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn48.45:1.2"
msgid "ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati pubbārāme migāramātupāsāde."
msgstr ""

msgctxt "sn48.45:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "sn48.45:1.4"
msgid ""
"“katinaṃ nu kho, bhikkhave, indriyānaṃ bhāvitattā bahulīkatattā khīṇāsavo "
"bhikkhu aññaṃ byākaroti:"
msgstr ""

msgctxt "sn48.45:1.5"
msgid ""
"‘khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyā’ti "
"pajānāmī”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn48.45:2.1"
msgid "“Bhagavaṃmūlakā no, bhante, dhammā … pe …"
msgstr ""

msgctxt "sn48.45:2.2"
msgid ""
"“ekassa kho, bhikkhave, indriyassa bhāvitattā bahulīkatattā khīṇāsavo "
"bhikkhu aññaṃ byākaroti:"
msgstr ""

msgctxt "sn48.45:2.3"
msgid ""
"‘khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyā’ti "
"pajānāmīti."
msgstr ""

# Punctuation in MS is inadequate here.
msgctxt "sn48.45:2.4"
msgid ""
"Katamassa ekassa paññindriyassa paññavato, bhikkhave, ariyasāvakassa "
"tadanvayā saddhā saṇṭhāti, tadanvayaṃ vīriyaṃ saṇṭhāti, tadanvayā sati "
"saṇṭhāti, tadanvayo samādhi saṇṭhāti."
msgstr ""

msgctxt "sn48.45:2.5"
msgid ""
"Imassa kho, bhikkhave, ekassa indriyassa bhāvitattā bahulīkatattā khīṇāsavo "
"bhikkhu aññaṃ byākaroti:"
msgstr ""

msgctxt "sn48.45:2.6"
msgid ""
"‘khīṇā jāti, vusitaṃ brahmacariyaṃ, kataṃ karaṇīyaṃ, nāparaṃ itthattāyā’ti "
"pajānāmī”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn48.45:2.7"
msgid "Pañcamaṃ."
msgstr ""
