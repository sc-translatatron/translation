#. <section class="sutta" id="sn14.16"><article><div class="hgroup"><p class="division">
msgctxt "sn14.16:0.1"
msgid "Saṃyutta Nikāya 14"
msgstr ""

#. </p><p>
msgctxt "sn14.16:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
#. VAR: 16. Sagāthāsutta → sagāthasuttaṃ (bj, pts1-2)
msgctxt "sn14.16:0.3"
msgid "16. Sagāthāsutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn14.16:1.1"
msgid "Sāvatthiyaṃ viharati."
msgstr ""

msgctxt "sn14.16:1.2"
msgid "“Dhātusova, bhikkhave, sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.16:1.3"
msgid "Hīnādhimuttikā hīnādhimuttikehi saddhiṃ saṃsandanti samenti."
msgstr ""

msgctxt "sn14.16:1.4"
msgid "Atītampi kho, bhikkhave, addhānaṃ dhātusova sattā saṃsandiṃsu samiṃsu."
msgstr ""

msgctxt "sn14.16:1.5"
msgid "Hīnādhimuttikā hīnādhimuttikehi saddhiṃ saṃsandiṃsu samiṃsu."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn14.16:2.1"
msgid ""
"Anāgatampi kho, bhikkhave, addhānaṃ dhātusova sattā saṃsandissanti "
"samessanti."
msgstr ""

msgctxt "sn14.16:2.2"
msgid "Hīnādhimuttikā hīnādhimuttikehi saddhiṃ saṃsandissanti samessanti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn14.16:3.1"
msgid ""
"Etarahipi kho, bhikkhave, paccuppannaṃ addhānaṃ dhātusova sattā saṃsandanti "
"samenti."
msgstr ""

msgctxt "sn14.16:3.2"
msgid "Hīnādhimuttikā hīnādhimuttikehi saddhiṃ saṃsandanti samenti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn14.16:4.1"
msgid "Seyyathāpi, bhikkhave, gūtho gūthena saṃsandati sameti;"
msgstr ""

msgctxt "sn14.16:4.2"
msgid "muttaṃ muttena saṃsandati sameti;"
msgstr ""

msgctxt "sn14.16:4.3"
msgid "kheḷo kheḷena saṃsandati sameti;"
msgstr ""

msgctxt "sn14.16:4.4"
msgid "pubbo pubbena saṃsandati sameti;"
msgstr ""

msgctxt "sn14.16:4.5"
msgid "lohitaṃ lohitena saṃsandati sameti;"
msgstr ""

#. VAR: dhātusova → sabbatthapi evameva
msgctxt "sn14.16:4.6"
msgid "evameva kho, bhikkhave, dhātusova sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.16:4.7"
msgid "Hīnādhimuttikā hīnādhimuttikehi saddhiṃ saṃsandanti samenti."
msgstr ""

msgctxt "sn14.16:4.8"
msgid "Atītampi kho addhānaṃ … pe …"
msgstr ""

msgctxt "sn14.16:4.9"
msgid "anāgatampi kho addhānaṃ … pe …"
msgstr ""

msgctxt "sn14.16:4.10"
msgid ""
"etarahipi kho paccuppannaṃ addhānaṃ dhātusova sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.16:4.11"
msgid "Hīnādhimuttikā hīnādhimuttikehi saddhiṃ saṃsandanti samenti."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.158"></a>
#. <a class="sc" id="sc5"></a>
msgctxt "sn14.16:5.1"
msgid "Dhātusova, bhikkhave, sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.16:5.2"
msgid "Kalyāṇādhimuttikā kalyāṇādhimuttikehi saddhiṃ saṃsandanti samenti."
msgstr ""

msgctxt "sn14.16:5.3"
msgid "Atītampi kho, bhikkhave, addhānaṃ dhātusova sattā saṃsandiṃsu samiṃsu."
msgstr ""

msgctxt "sn14.16:5.4"
msgid "Kalyāṇādhimuttikā kalyāṇādhimuttikehi saddhiṃ saṃsandiṃsu samiṃsu."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn14.16:6.1"
msgid "Anāgatampi kho, bhikkhave, addhānaṃ … pe …"
msgstr ""

msgctxt "sn14.16:6.2"
msgid ""
"etarahipi kho, bhikkhave, paccuppannaṃ addhānaṃ dhātusova sattā saṃsandanti "
"samenti."
msgstr ""

msgctxt "sn14.16:6.3"
msgid "Kalyāṇādhimuttikā kalyāṇādhimuttikehi saddhiṃ saṃsandanti samenti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn14.16:7.1"
msgid "Seyyathāpi, bhikkhave, khīraṃ khīrena saṃsandati sameti;"
msgstr ""

msgctxt "sn14.16:7.2"
msgid "telaṃ telena saṃsandati sameti;"
msgstr ""

msgctxt "sn14.16:7.3"
msgid "sappi sappinā saṃsandati sameti;"
msgstr ""

msgctxt "sn14.16:7.4"
msgid "madhu madhunā saṃsandati sameti;"
msgstr ""

msgctxt "sn14.16:7.5"
msgid "phāṇitaṃ phāṇitena saṃsandati sameti;"
msgstr ""

msgctxt "sn14.16:7.6"
msgid "evameva kho, bhikkhave, dhātusova sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.16:7.7"
msgid "Kalyāṇādhimuttikā kalyāṇādhimuttikehi saddhiṃ saṃsandanti samenti."
msgstr ""

msgctxt "sn14.16:7.8"
msgid "Atītampi kho addhānaṃ …"
msgstr ""

msgctxt "sn14.16:7.9"
msgid "anāgatampi kho addhānaṃ …"
msgstr ""

msgctxt "sn14.16:7.10"
msgid ""
"etarahipi kho paccuppannaṃ addhānaṃ dhātusova sattā saṃsandanti samenti."
msgstr ""

msgctxt "sn14.16:7.11"
msgid "Kalyāṇādhimuttikā kalyāṇādhimuttikehi saddhiṃ saṃsandanti samentī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc8"></a>
msgctxt "sn14.16:8.1"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "sn14.16:8.2"
msgid "Idaṃ vatvāna sugato athāparaṃ etadavoca satthā:"
msgstr ""

# BB’s “woods of lust” is barely comprehensible, and suggests a crude and presumably unintended pun. Mostly, as here, vanatha is used in ways that don’t rely on the metaphorical basis, so there’s no need to resort to such means.<a class="sc" id="sc9"></a>
#. </p><blockquote class="gatha"><p>
msgctxt "sn14.16:8.3"
msgid "“Saṃsaggā vanatho jāto,"
msgstr ""

#. <br>
msgctxt "sn14.16:8.4"
msgid "asaṃsaggena chijjati;"
msgstr ""

#. <br>
msgctxt "sn14.16:8.5"
msgid "Parittaṃ dārumāruyha,"
msgstr ""

#. <br>
msgctxt "sn14.16:8.6"
msgid "yathā sīde mahaṇṇave."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn14.16:10.1"
msgid "Evaṃ kusītamāgamma,"
msgstr ""

#. <br>
msgctxt "sn14.16:10.2"
msgid "sādhujīvipi sīdati;"
msgstr ""

#. <br>
msgctxt "sn14.16:10.3"
msgid "Tasmā taṃ parivajjeyya,"
msgstr ""

#. <br>
msgctxt "sn14.16:10.4"
msgid "kusītaṃ hīnavīriyaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
msgctxt "sn14.16:11.1"
msgid "Pavivittehi ariyehi,"
msgstr ""

#. <br>
#. VAR: jhāyīhi → jhāyihi (bj) | jhāyibhi (s1-3, km)
msgctxt "sn14.16:11.2"
msgid "pahitattehi jhāyīhi;"
msgstr ""

#. <br>
msgctxt "sn14.16:11.3"
msgid "Niccaṃ āraddhavīriyehi,"
msgstr ""

#. <br>
msgctxt "sn14.16:11.4"
msgid "paṇḍitehi sahāvase”ti."
msgstr ""

#. </p></blockquote><p class="endsection">
msgctxt "sn14.16:11.5"
msgid "Chaṭṭhaṃ."
msgstr ""
