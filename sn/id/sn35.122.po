#. <section class="sutta" id="sn35.122"><article><div class="hgroup"><p class="division">
msgctxt "sn35.122:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.122:0.2"
msgid "12. Lokakāmaguṇavagga"
msgstr ""

#. </p><h1>
#. VAR: 122. Saṃyojaniyadhammasutta → saññojanasuttaṃ (bj) | saṃyojanaṃ (pts1)
msgctxt "sn35.122:0.3"
msgid "122. Saṃyojaniyadhammasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.108"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.122:1.1"
msgid "“Saṃyojaniye ca, bhikkhave, dhamme desessāmi saṃyojanañca."
msgstr ""

msgctxt "sn35.122:1.2"
msgid "Taṃ suṇātha."
msgstr ""

msgctxt "sn35.122:1.3"
msgid "Katame ca, bhikkhave, saṃyojaniyā dhammā, katamañca saṃyojanaṃ?"
msgstr ""

msgctxt "sn35.122:1.4"
msgid ""
"Santi, bhikkhave, cakkhuviññeyyā rūpā iṭṭhā kantā manāpā piyarūpā "
"kāmūpasaṃhitā rajanīyā."
msgstr ""

msgctxt "sn35.122:1.5"
msgid "Ime vuccanti, bhikkhave, saṃyojaniyā dhammā."
msgstr ""

msgctxt "sn35.122:1.6"
msgid "Yo tattha chandarāgo, taṃ tattha saṃyojanaṃ … pe …"
msgstr ""

msgctxt "sn35.122:1.7"
msgid "santi, bhikkhave, jivhāviññeyyā rasā … pe …"
msgstr ""

msgctxt "sn35.122:1.8"
msgid ""
"santi, bhikkhave, manoviññeyyā dhammā iṭṭhā kantā manāpā piyarūpā "
"kāmūpasaṃhitā rajanīyā."
msgstr ""

msgctxt "sn35.122:1.9"
msgid "Ime vuccanti, bhikkhave, saṃyojaniyā dhammā."
msgstr ""

msgctxt "sn35.122:1.10"
msgid "Yo tattha chandarāgo taṃ tattha saṃyojanan”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.122:1.11"
msgid "Navamaṃ."
msgstr ""
