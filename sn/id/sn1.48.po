#. <section class="sutta" id="sn1.48"><article><div class="hgroup"><p class="division">
msgctxt "sn1.48:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.48:0.2"
msgid "5. Ādittavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.48:0.3"
msgid "48. Jetavanasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.71"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.48:1.1"
msgid "“Idañhi taṃ jetavanaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.48:1.2"
msgid "isisaṃghanisevitaṃ;"
msgstr ""

#. <br>
#. VAR: Āvutthaṃ → āvuṭṭhaṃ (mr)
msgctxt "sn1.48:1.3"
msgid "Āvutthaṃ dhammarājena,"
msgstr ""

#. <br>
msgctxt "sn1.48:1.4"
msgid "pītisañjananaṃ mama."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.34"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.48:2.1"
msgid "Kammaṃ vijjā ca dhammo ca,"
msgstr ""

#. <br>
msgctxt "sn1.48:2.2"
msgid "sīlaṃ jīvitamuttamaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.48:2.3"
msgid "Etena maccā sujjhanti,"
msgstr ""

#. <br>
msgctxt "sn1.48:2.4"
msgid "na gottena dhanena vā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.48:3.1"
msgid "Tasmā hi paṇḍito poso,"
msgstr ""

#. <br>
msgctxt "sn1.48:3.2"
msgid "sampassaṃ atthamattano;"
msgstr ""

#. <br>
msgctxt "sn1.48:3.3"
msgid "Yoniso vicine dhammaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.48:3.4"
msgid "evaṃ tattha visujjhati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.48:4.1"
msgid "Sāriputtova paññāya,"
msgstr ""

#. <br>
msgctxt "sn1.48:4.2"
msgid "sīlena upasamena ca;"
msgstr ""

#. <br>
msgctxt "sn1.48:4.3"
msgid "Yopi pāraṅgato bhikkhu,"
msgstr ""

#. <br>
msgctxt "sn1.48:4.4"
msgid "etāvaparamo siyā”ti."
msgstr ""
