#. <section class="sutta" id="sn1.41"><article><div class="hgroup"><p class="division">
msgctxt "sn1.41:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.41:0.2"
msgid "5. Ādittavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.41:0.3"
msgid "41. Ādittasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts2ed" id="pts2ed1.65"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn1.41:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn1.41:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "sn1.41:1.3"
msgid ""
"Atha kho aññatarā devatā abhikkantāya rattiyā abhikkantavaṇṇā kevalakappaṃ "
"jetavanaṃ obhāsetvā yena bhagavā tenupasaṅkami; upasaṅkamitvā bhagavantaṃ "
"abhivādetvā ekamantaṃ aṭṭhāsi."
msgstr ""

msgctxt "sn1.41:1.4"
msgid "Ekamantaṃ ṭhitā kho sā devatā bhagavato santike imā gāthāyo abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.41:2.1"
msgid "“Ādittasmiṃ agārasmiṃ,"
msgstr ""

#. <br>
msgctxt "sn1.41:2.2"
msgid "yaṃ nīharati bhājanaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.41:2.3"
msgid "Taṃ tassa hoti atthāya,"
msgstr ""

#. <br>
msgctxt "sn1.41:2.4"
msgid "no ca yaṃ tattha ḍayhati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.41:3.1"
msgid "Evaṃ ādittako loko,"
msgstr ""

#. <br>
msgctxt "sn1.41:3.2"
msgid "jarāya maraṇena ca;"
msgstr ""

#. <br>
msgctxt "sn1.41:3.3"
msgid "Nīharetheva dānena,"
msgstr ""

#. <br>
msgctxt "sn1.41:3.4"
msgid "dinnaṃ hoti sunīhataṃ."
msgstr ""

#. </p><p>
#. <a class="pts1ed" id="pts1ed1.32"></a>
#. <a class="pts2ed" id="pts2ed1.66"></a>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.41:4.1"
msgid "Dinnaṃ sukhaphalaṃ hoti,"
msgstr ""

#. <br>
msgctxt "sn1.41:4.2"
msgid "nādinnaṃ hoti taṃ tathā;"
msgstr ""

#. <br>
msgctxt "sn1.41:4.3"
msgid "Corā haranti rājāno,"
msgstr ""

#. <br>
msgctxt "sn1.41:4.4"
msgid "aggi ḍahati nassati."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn1.41:5.1"
msgid "Atha antena jahati,"
msgstr ""

#. <br>
msgctxt "sn1.41:5.2"
msgid "Sarīraṃ sapariggahaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.41:5.3"
msgid "Etadaññāya medhāvī,"
msgstr ""

#. <br>
msgctxt "sn1.41:5.4"
msgid "Bhuñjetha ca dadetha ca;"
msgstr ""

#. <br>
msgctxt "sn1.41:5.5"
msgid "Datvā ca bhutvā ca yathānubhāvaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.41:5.6"
msgid "Anindito saggamupeti ṭhānan”ti."
msgstr ""
