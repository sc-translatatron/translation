#. <section class="sutta" id="sn35.15"><article><div class="hgroup"><p class="division">
msgctxt "sn35.15:0.1"
msgid "Saṃyutta Nikāya 35"
msgstr ""

#. </p><p>
msgctxt "sn35.15:0.2"
msgid "2. Yamakavagga"
msgstr ""

#. </p><h1>
#. VAR: 15. Paṭhamaassādapariyesanasutta → assādena 1 (pts1)
msgctxt "sn35.15:0.3"
msgid "15. Paṭhamaassādapariyesanasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli4.9"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn35.15:1.1"
msgid "“Cakkhussāhaṃ, bhikkhave, assādapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn35.15:1.2"
msgid "Yo cakkhussa assādo tadajjhagamaṃ."
msgstr ""

msgctxt "sn35.15:1.3"
msgid "Yāvatā cakkhussa assādo paññāya me so sudiṭṭho."
msgstr ""

msgctxt "sn35.15:1.4"
msgid "Cakkhussāhaṃ, bhikkhave, ādīnavapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn35.15:1.5"
msgid "Yo cakkhussa ādīnavo tadajjhagamaṃ."
msgstr ""

msgctxt "sn35.15:1.6"
msgid "Yāvatā cakkhussa ādīnavo paññāya me so sudiṭṭho."
msgstr ""

msgctxt "sn35.15:1.7"
msgid "Cakkhussāhaṃ, bhikkhave, nissaraṇapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn35.15:1.8"
msgid "Yaṃ cakkhussa nissaraṇaṃ tadajjhagamaṃ."
msgstr ""

msgctxt "sn35.15:1.9"
msgid "Yāvatā cakkhussa nissaraṇaṃ, paññāya me taṃ sudiṭṭhaṃ."
msgstr ""

msgctxt "sn35.15:1.10"
msgid "Sotassāhaṃ, bhikkhave …"
msgstr ""

msgctxt "sn35.15:1.11"
msgid "ghānassāhaṃ, bhikkhave …"
msgstr ""

msgctxt "sn35.15:1.12"
msgid "jivhāyāhaṃ bhikkhave, assādapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn35.15:1.13"
msgid "Yo jivhāya assādo tadajjhagamaṃ."
msgstr ""

msgctxt "sn35.15:1.14"
msgid "Yāvatā jivhāya assādo paññāya me so sudiṭṭho."
msgstr ""

msgctxt "sn35.15:1.15"
msgid "Jivhāyāhaṃ, bhikkhave, ādīnavapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn35.15:1.16"
msgid "Yo jivhāya ādīnavo tadajjhagamaṃ."
msgstr ""

msgctxt "sn35.15:1.17"
msgid "Yāvatā jivhāya ādīnavo paññāya me so sudiṭṭho."
msgstr ""

msgctxt "sn35.15:1.18"
msgid "Jivhāyāhaṃ, bhikkhave, nissaraṇapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn35.15:1.19"
msgid "Yaṃ jivhāya nissaraṇaṃ tadajjhagamaṃ."
msgstr ""

# kāya omitted by mistake, presumably.
msgctxt "sn35.15:1.20"
msgid "Yāvatā jivhāya nissaraṇaṃ, paññāya me taṃ sudiṭṭhaṃ … pe …"
msgstr ""

msgctxt "sn35.15:1.21"
msgid "manassāhaṃ, bhikkhave, assādapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn35.15:1.22"
msgid "Yo manassa assādo tadajjhagamaṃ."
msgstr ""

msgctxt "sn35.15:1.23"
msgid "Yāvatā manassa assādo paññāya me so sudiṭṭho."
msgstr ""

msgctxt "sn35.15:1.24"
msgid "Manassāhaṃ, bhikkhave, ādīnavapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn35.15:1.25"
msgid "Yo manassa ādīnavo tadajjhagamaṃ."
msgstr ""

msgctxt "sn35.15:1.26"
msgid "Yāvatā manassa ādīnavo paññāya me so sudiṭṭho."
msgstr ""

msgctxt "sn35.15:1.27"
msgid "Manassāhaṃ, bhikkhave, nissaraṇapariyesanaṃ acariṃ."
msgstr ""

msgctxt "sn35.15:1.28"
msgid "Yaṃ manassa nissaraṇaṃ tadajjhagamaṃ."
msgstr ""

msgctxt "sn35.15:1.29"
msgid "Yāvatā manassa nissaraṇaṃ, paññāya me taṃ sudiṭṭhaṃ."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn35.15:2.1"
msgid ""
"Yāvakīvañcāhaṃ, bhikkhave, imesaṃ channaṃ ajjhattikānaṃ āyatanānaṃ assādañca "
"assādato, ādīnavañca ādīnavato, nissaraṇañca nissaraṇato yathābhūtaṃ "
"nābbhaññāsiṃ … pe …"
msgstr ""

msgctxt "sn35.15:2.2"
msgid "paccaññāsiṃ."
msgstr ""

msgctxt "sn35.15:2.3"
msgid "Ñāṇañca pana me dassanaṃ udapādi:"
msgstr ""

msgctxt "sn35.15:2.4"
msgid "‘akuppā me vimutti, ayamantimā jāti, natthi dāni punabbhavo’”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn35.15:2.5"
msgid "Tatiyaṃ."
msgstr ""
