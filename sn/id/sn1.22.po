#. <section class="sutta" id="sn1.22"><article><div class="hgroup"><p class="division">
msgctxt "sn1.22:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.22:0.2"
msgid "3. Sattivagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.22:0.3"
msgid "22. Phusatisutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.28"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.22:1.1"
msgid "“Nāphusantaṃ phusati ca,"
msgstr ""

#. <br>
msgctxt "sn1.22:1.2"
msgid "phusantañca tato phuse;"
msgstr ""

#. <br>
msgctxt "sn1.22:1.3"
msgid "Tasmā phusantaṃ phusati,"
msgstr ""

#. <br>
msgctxt "sn1.22:1.4"
msgid "appaduṭṭhapadosinan”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.22:2.1"
msgid "“Yo appaduṭṭhassa narassa dussati,"
msgstr ""

#. <br>
msgctxt "sn1.22:2.2"
msgid "Suddhassa posassa anaṅgaṇassa;"
msgstr ""

#. <br>
msgctxt "sn1.22:2.3"
msgid "Tameva bālaṃ pacceti pāpaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.22:2.4"
msgid "Sukhumo rajo paṭivātaṃva khitto”ti."
msgstr ""
