#. <section class="sutta" id="sn3.14"><article><div class="hgroup"><p class="division">
msgctxt "sn3.14:0.1"
msgid "Saṃyutta Nikāya 3"
msgstr ""

#. </p><p>
msgctxt "sn3.14:0.2"
msgid "2. Dutiyavagga"
msgstr ""

#. </p><h1>
msgctxt "sn3.14:0.3"
msgid "14. Paṭhamasaṅgāmasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts1ed" id="pts1ed1.83"></a>
#. <a class="pts2ed" id="pts2ed1.188"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn3.14:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn3.14:1.2"
msgid ""
"Atha kho rājā māgadho ajātasattu vedehiputto caturaṅginiṃ senaṃ sannayhitvā "
"rājānaṃ pasenadiṃ kosalaṃ abbhuyyāsi yena kāsi."
msgstr ""

msgctxt "sn3.14:1.3"
msgid "Assosi kho rājā pasenadi kosalo:"
msgstr ""

msgctxt "sn3.14:1.4"
msgid ""
"“rājā kira māgadho ajātasattu vedehiputto caturaṅginiṃ senaṃ sannayhitvā "
"mamaṃ abbhuyyāto yena kāsī”ti."
msgstr ""

msgctxt "sn3.14:1.5"
msgid ""
"Atha kho rājā pasenadi kosalo caturaṅginiṃ senaṃ sannayhitvā rājānaṃ "
"māgadhaṃ ajātasattuṃ vedehiputtaṃ paccuyyāsi yena kāsi."
msgstr ""

msgctxt "sn3.14:1.6"
msgid ""
"Atha kho rājā ca māgadho ajātasattu vedehiputto rājā ca pasenadi kosalo "
"saṅgāmesuṃ."
msgstr ""

msgctxt "sn3.14:1.7"
msgid ""
"Tasmiṃ kho pana saṅgāme rājā māgadho ajātasattu vedehiputto rājānaṃ "
"pasenadiṃ kosalaṃ parājesi."
msgstr ""

#. VAR: sakameva → saṅgāmā (mr)
#. VAR: paccuyyāsi → pāyāsi (bj, pts1) | paccāyāsi (pts2)
msgctxt "sn3.14:1.8"
msgid ""
"Parājito ca rājā pasenadi kosalo sakameva rājadhāniṃ sāvatthiṃ paccuyyāsi."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn3.14:2.1"
msgid ""
"Atha kho sambahulā bhikkhū pubbaṇhasamayaṃ nivāsetvā pattacīvaramādāya "
"sāvatthiṃ piṇḍāya pavisiṃsu."
msgstr ""

msgctxt "sn3.14:2.2"
msgid ""
"Sāvatthiyaṃ piṇḍāya caritvā pacchābhattaṃ piṇḍapātapaṭikkantā yena bhagavā "
"tenupasaṅkamiṃsu; upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ nisīdiṃsu. "
"Ekamantaṃ nisinnā kho te bhikkhū bhagavantaṃ etadavocuṃ:"
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.189"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn3.14:3.1"
msgid ""
"“Idha, bhante, rājā māgadho ajātasattu vedehiputto caturaṅginiṃ senaṃ "
"sannayhitvā rājānaṃ pasenadiṃ kosalaṃ abbhuyyāsi yena kāsi."
msgstr ""

msgctxt "sn3.14:3.2"
msgid "Assosi kho, bhante, rājā pasenadi kosalo:"
msgstr ""

msgctxt "sn3.14:3.3"
msgid ""
"‘rājā kira māgadho ajātasattu vedehiputto caturaṅginiṃ senaṃ sannayhitvā "
"mamaṃ abbhuyyāto yena kāsī’ti."
msgstr ""

msgctxt "sn3.14:3.4"
msgid ""
"Atha kho, bhante, rājā pasenadi kosalo caturaṅginiṃ senaṃ sannayhitvā "
"rājānaṃ māgadhaṃ ajātasattuṃ vedehiputtaṃ paccuyyāsi yena kāsi."
msgstr ""

msgctxt "sn3.14:3.5"
msgid ""
"Atha kho, bhante, rājā ca māgadho ajātasattu vedehiputto rājā ca pasenadi "
"kosalo saṅgāmesuṃ."
msgstr ""

msgctxt "sn3.14:3.6"
msgid ""
"Tasmiṃ kho pana, bhante, saṅgāme rājā māgadho ajātasattu vedehiputto rājānaṃ "
"pasenadiṃ kosalaṃ parājesi."
msgstr ""

msgctxt "sn3.14:3.7"
msgid ""
"Parājito ca, bhante, rājā pasenadi kosalo sakameva rājadhāniṃ sāvatthiṃ "
"paccuyyāsī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn3.14:4.1"
msgid ""
"“Rājā, bhikkhave, māgadho ajātasattu vedehiputto pāpamitto pāpasahāyo "
"pāpasampavaṅko;"
msgstr ""

msgctxt "sn3.14:4.2"
msgid ""
"rājā ca kho, bhikkhave, pasenadi kosalo kalyāṇamitto kalyāṇasahāyo "
"kalyāṇasampavaṅko."
msgstr ""

#. VAR: Ajjeva → ajjatañca (bj, pts1) | ajjevaṃ (s1-3, km)
msgctxt "sn3.14:4.3"
msgid ""
"Ajjeva, bhikkhave, rājā pasenadi kosalo imaṃ rattiṃ dukkhaṃ seti parājito”ti."
msgstr ""

msgctxt "sn3.14:4.4"
msgid "Idamavoca … pe …"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn3.14:5.1"
msgid "“Jayaṃ veraṃ pasavati,"
msgstr ""

#. <br>
msgctxt "sn3.14:5.2"
msgid "dukkhaṃ seti parājito;"
msgstr ""

#. <br>
msgctxt "sn3.14:5.3"
msgid "Upasanto sukhaṃ seti,"
msgstr ""

#. <br>
msgctxt "sn3.14:5.4"
msgid "hitvā jayaparājayan”ti."
msgstr ""
