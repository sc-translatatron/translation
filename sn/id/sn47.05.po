#. <section class="sutta" id="sn47.5"><article><div class="hgroup"><p class="division">
msgctxt "sn47.5:0.1"
msgid "Saṃyutta Nikāya 47"
msgstr ""

#. </p><p>
msgctxt "sn47.5:0.2"
msgid "1. Ambapālivagga"
msgstr ""

#. </p><h1>
#. VAR: 5. Akusalarāsisutta → kusalarāsi (pts1)
msgctxt "sn47.5:0.3"
msgid "5. Akusalarāsisutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli5.146"></a>
#. <a class="sc" id="sc1"></a>
msgctxt "sn47.5:1.1"
msgid "Sāvatthinidānaṃ."
msgstr ""

msgctxt "sn47.5:1.2"
msgid "Tatra kho bhagavā etadavoca:"
msgstr ""

msgctxt "sn47.5:1.3"
msgid ""
"“‘akusalarāsī’ti, bhikkhave, vadamāno pañca nīvaraṇe sammā vadamāno vadeyya."
msgstr ""

msgctxt "sn47.5:1.4"
msgid "Kevalo hāyaṃ, bhikkhave, akusalarāsi, yadidaṃ—pañca nīvaraṇā."
msgstr ""

msgctxt "sn47.5:1.5"
msgid "Katame pañca?"
msgstr ""

msgctxt "sn47.5:1.6"
msgid ""
"Kāmacchandanīvaraṇaṃ, byāpādanīvaraṇaṃ, thinamiddhanīvaraṇaṃ, "
"uddhaccakukkuccanīvaraṇaṃ, vicikicchānīvaraṇaṃ."
msgstr ""

msgctxt "sn47.5:1.7"
msgid ""
"‘Akusalarāsī’ti, bhikkhave, vadamāno ime pañca nīvaraṇe sammā vadamāno "
"vadeyya."
msgstr ""

msgctxt "sn47.5:1.8"
msgid "Kevalo hāyaṃ, bhikkhave, akusalarāsi, yadidaṃ—pañca nīvaraṇā."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn47.5:2.1"
msgid ""
"‘Kusalarāsī’ti, bhikkhave, vadamāno cattāro satipaṭṭhāne sammā vadamāno "
"vadeyya."
msgstr ""

msgctxt "sn47.5:2.2"
msgid "Kevalo hāyaṃ, bhikkhave, kusalarāsi, yadidaṃ—cattāro satipaṭṭhānā."
msgstr ""

msgctxt "sn47.5:2.3"
msgid "Katame cattāro?"
msgstr ""

msgctxt "sn47.5:2.4"
msgid ""
"Idha, bhikkhave, bhikkhu kāye kāyānupassī viharati ātāpī sampajāno satimā, "
"vineyya loke abhijjhādomanassaṃ;"
msgstr ""

msgctxt "sn47.5:2.5"
msgid "vedanāsu … pe …"
msgstr ""

msgctxt "sn47.5:2.6"
msgid "citte … pe …"
msgstr ""

msgctxt "sn47.5:2.7"
msgid ""
"dhammesu dhammānupassī viharati ātāpī sampajāno satimā, vineyya loke "
"abhijjhādomanassaṃ."
msgstr ""

msgctxt "sn47.5:2.8"
msgid ""
"‘Kusalarāsī’ti, bhikkhave, vadamāno ime cattāro satipaṭṭhāne sammā vadamāno "
"vadeyya."
msgstr ""

msgctxt "sn47.5:2.9"
msgid "Kevalo hāyaṃ, bhikkhave, kusalarāsi, yadidaṃ—cattāro satipaṭṭhānā”ti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn47.5:2.10"
msgid "Pañcamaṃ."
msgstr ""
