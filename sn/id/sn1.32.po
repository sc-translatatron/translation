#. <section class="sutta" id="sn1.32"><article><div class="hgroup"><p class="division">
msgctxt "sn1.32:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.32:0.2"
msgid "4. Satullapakāyikavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.32:0.3"
msgid "32. Maccharisutta"
msgstr ""

#. </h1></div><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.32:1.1"
msgid ""
"Ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "sn1.32:1.2"
msgid ""
"Atha kho sambahulā satullapakāyikā devatāyo abhikkantāya rattiyā "
"abhikkantavaṇṇā kevalakappaṃ jetavanaṃ obhāsetvā yena bhagavā "
"tenupasaṅkamiṃsu; upasaṅkamitvā bhagavantaṃ abhivādetvā ekamantaṃ aṭṭhaṃsu."
msgstr ""

msgctxt "sn1.32:1.3"
msgid "Ekamantaṃ ṭhitā kho ekā devatā bhagavato santike imaṃ gāthaṃ abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.40"></a>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.32:2.1"
msgid "“Maccherā ca pamādā ca,"
msgstr ""

#. <br>
#. VAR: dīyati → diyyati (mr)
msgctxt "sn1.32:2.2"
msgid "evaṃ dānaṃ na dīyati;"
msgstr ""

#. <br>
msgctxt "sn1.32:2.3"
msgid "Puññaṃ ākaṅkhamānena,"
msgstr ""

#. <br>
msgctxt "sn1.32:2.4"
msgid "deyyaṃ hoti vijānatā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.32:3.1"
msgid "Atha kho aparā devatā bhagavato santike imā gāthāyo abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc4"></a>
msgctxt "sn1.32:4.1"
msgid "“Yasseva bhīto na dadāti maccharī,"
msgstr ""

#. <br>
msgctxt "sn1.32:4.2"
msgid "Tadevādadato bhayaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.32:4.3"
msgid "Jighacchā ca pipāsā ca,"
msgstr ""

#. <br>
msgctxt "sn1.32:4.4"
msgid "Yassa bhāyati maccharī;"
msgstr ""

#. <br>
msgctxt "sn1.32:4.5"
msgid "Tameva bālaṃ phusati,"
msgstr ""

#. <br>
msgctxt "sn1.32:4.6"
msgid "Asmiṃ loke paramhi ca."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc5"></a>
msgctxt "sn1.32:5.1"
msgid "Tasmā vineyya maccheraṃ,"
msgstr ""

#. <br>
msgctxt "sn1.32:5.2"
msgid "dajjā dānaṃ malābhibhū;"
msgstr ""

#. <br>
msgctxt "sn1.32:5.3"
msgid "Puññāni paralokasmiṃ,"
msgstr ""

#. <br>
msgctxt "sn1.32:5.4"
msgid "patiṭṭhā honti pāṇinan”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc6"></a>
msgctxt "sn1.32:6.1"
msgid "Atha kho aparā devatā bhagavato santike imā gāthāyo abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc7"></a>
msgctxt "sn1.32:7.1"
msgid "“Te matesu na mīyanti,"
msgstr ""

#. <br>
msgctxt "sn1.32:7.2"
msgid "panthānaṃva sahabbajaṃ;"
msgstr ""

#. <br>
msgctxt "sn1.32:7.3"
msgid "Appasmiṃ ye pavecchanti,"
msgstr ""

#. <br>
msgctxt "sn1.32:7.4"
msgid "esa dhammo sanantano."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.41"></a>
#. <a class="sc" id="sc8"></a>
msgctxt "sn1.32:8.1"
msgid "Appasmeke pavecchanti,"
msgstr ""

#. <br>
msgctxt "sn1.32:8.2"
msgid "bahuneke na dicchare;"
msgstr ""

#. <br>
msgctxt "sn1.32:8.3"
msgid "Appasmā dakkhiṇā dinnā,"
msgstr ""

# Cf. Mark 12:41-44
#. <br>
msgctxt "sn1.32:8.4"
msgid "sahassena samaṃ mitā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="pts1ed" id="pts1ed1.19"></a>
#. <a class="sc" id="sc9"></a>
msgctxt "sn1.32:9.1"
msgid "Atha kho aparā devatā bhagavato santike imā gāthāyo abhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc10"></a>
msgctxt "sn1.32:10.1"
msgid "“Duddadaṃ dadamānānaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.32:10.2"
msgid "dukkaraṃ kamma kubbataṃ;"
msgstr ""

#. <br>
msgctxt "sn1.32:10.3"
msgid "Asanto nānukubbanti,"
msgstr ""

#. <br>
#. VAR: duranvayo → durannayo (bj, pts1-2)
msgctxt "sn1.32:10.4"
msgid "sataṃ dhammo duranvayo."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc11"></a>
#. VAR: asataṃ → asatañca (bj, s1-3, km, pts1-2)
msgctxt "sn1.32:11.1"
msgid "Tasmā satañca asataṃ,"
msgstr ""

#. <br>
msgctxt "sn1.32:11.2"
msgid "nānā hoti ito gati;"
msgstr ""

#. <br>
msgctxt "sn1.32:11.3"
msgid "Asanto nirayaṃ yanti,"
msgstr ""

#. <br>
msgctxt "sn1.32:11.4"
msgid "santo saggaparāyanā”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc12"></a>
msgctxt "sn1.32:12.1"
msgid "Atha kho aparā devatā bhagavato santike etadavoca:"
msgstr ""

msgctxt "sn1.32:12.2"
msgid "“kassa nu kho, bhagavā, subhāsitan”ti?"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc13"></a>
msgctxt "sn1.32:13.1"
msgid "“Sabbāsaṃ vo subhāsitaṃ pariyāyena; api ca mamapi suṇātha—"
msgstr ""

# reading samuñchaka
#. </p><blockquote class="gatha"><p>
#. <a class="pts2ed" id="pts2ed1.42"></a>
#. <a class="sc" id="sc14"></a>
msgctxt "sn1.32:14.1"
msgid "Dhammaṃ care yopi samuñjakaṃ care,"
msgstr ""

#. <br>
msgctxt "sn1.32:14.2"
msgid "Dārañca posaṃ dadamappakasmiṃ;"
msgstr ""

#. <br>
msgctxt "sn1.32:14.3"
msgid "Sataṃ sahassānaṃ sahassayāginaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.32:14.4"
msgid "Kalampi nāgghanti tathāvidhassa te”ti."
msgstr ""

#. </p></blockquote><p>
#. <a class="sc" id="sc15"></a>
msgctxt "sn1.32:15.1"
msgid "Atha kho aparā devatā bhagavantaṃ gāthāya ajjhabhāsi:"
msgstr ""

#. </p><blockquote class="gatha"><p>
#. <a class="sc" id="sc16"></a>
msgctxt "sn1.32:16.1"
msgid "“Kenesa yañño vipulo mahaggato,"
msgstr ""

#. <br>
msgctxt "sn1.32:16.2"
msgid "Samena dinnassa na agghameti;"
msgstr ""

#. <br>
#. VAR: Kathaṃ → idaṃ padaṃ si, pts1-2 potthakesu
msgctxt "sn1.32:16.3"
msgid "Kathaṃ sataṃ sahassānaṃ sahassayāginaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.32:16.4"
msgid "Kalampi nāgghanti tathāvidhassa te”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc17"></a>
msgctxt "sn1.32:17.1"
msgid "“Dadanti heke visame niviṭṭhā,"
msgstr ""

#. <br>
msgctxt "sn1.32:17.2"
msgid "Chetvā vadhitvā atha socayitvā;"
msgstr ""

#. <br>
msgctxt "sn1.32:17.3"
msgid "Sā dakkhiṇā assumukhā sadaṇḍā,"
msgstr ""

#. <br>
msgctxt "sn1.32:17.4"
msgid "Samena dinnassa na agghameti."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.43"></a>
#. <a class="sc" id="sc18"></a>
msgctxt "sn1.32:18.1"
msgid "Evaṃ sataṃ sahassānaṃ sahassayāginaṃ,"
msgstr ""

#. <br>
msgctxt "sn1.32:18.2"
msgid "Kalampi nāgghanti tathāvidhassa te”ti."
msgstr ""
