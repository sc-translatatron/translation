#. <section class="sutta" id="sn12.1"><article><div class="hgroup"><p class="division">
msgctxt "sn12.1:0.1"
msgid "Saṃyutta Nikāya 12"
msgstr ""

#. </p><p>
#. VAR: 1. Buddhavagga → abhisamayasaṃyuttaṃ (s1-3)
msgctxt "sn12.1:0.2"
msgid "1. Buddhavagga"
msgstr ""

#. </p><h1>
#. VAR: 1. Paṭiccasamuppādasutta → desanā (pts1-2)
msgctxt "sn12.1:0.3"
msgid "1. Paṭiccasamuppādasutta"
msgstr ""

#. </h1></div><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.1"></a>
#. <a class="sc" id="sc1"></a>
#. <span class="evam">
msgctxt "sn12.1:1.1"
msgid "Evaṃ me sutaṃ—​"
msgstr ""

#. </span>
msgctxt "sn12.1:1.2"
msgid ""
"ekaṃ samayaṃ bhagavā sāvatthiyaṃ viharati jetavane anāthapiṇḍikassa ārāme."
msgstr ""

msgctxt "sn12.1:1.3"
msgid "Tatra kho bhagavā bhikkhū āmantesi:"
msgstr ""

msgctxt "sn12.1:1.4"
msgid "“bhikkhavo”ti."
msgstr ""

msgctxt "sn12.1:1.5"
msgid "“Bhadante”ti te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "sn12.1:1.6"
msgid "Bhagavā etadavoca:"
msgstr ""

msgctxt "sn12.1:1.7"
msgid "“paṭiccasamuppādaṃ vo, bhikkhave, desessāmi."
msgstr ""

msgctxt "sn12.1:1.8"
msgid "Taṃ suṇātha, sādhukaṃ manasi karotha, bhāsissāmī”ti."
msgstr ""

msgctxt "sn12.1:1.9"
msgid "“Evaṃ, bhante”ti kho te bhikkhū bhagavato paccassosuṃ."
msgstr ""

msgctxt "sn12.1:1.10"
msgid "Bhagavā etadavoca:"
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn12.1:2.1"
msgid "“Katamo ca, bhikkhave, paṭiccasamuppādo?"
msgstr ""

msgctxt "sn12.1:2.2"
msgid "Avijjāpaccayā, bhikkhave, saṅkhārā;"
msgstr ""

msgctxt "sn12.1:2.3"
msgid "saṅkhārapaccayā viññāṇaṃ;"
msgstr ""

msgctxt "sn12.1:2.4"
msgid "viññāṇapaccayā nāmarūpaṃ;"
msgstr ""

msgctxt "sn12.1:2.5"
msgid "nāmarūpapaccayā saḷāyatanaṃ;"
msgstr ""

msgctxt "sn12.1:2.6"
msgid "saḷāyatanapaccayā phasso;"
msgstr ""

msgctxt "sn12.1:2.7"
msgid "phassapaccayā vedanā;"
msgstr ""

msgctxt "sn12.1:2.8"
msgid "vedanāpaccayā taṇhā;"
msgstr ""

msgctxt "sn12.1:2.9"
msgid "taṇhāpaccayā upādānaṃ;"
msgstr ""

msgctxt "sn12.1:2.10"
msgid "upādānapaccayā bhavo;"
msgstr ""

msgctxt "sn12.1:2.11"
msgid "bhavapaccayā jāti;"
msgstr ""

msgctxt "sn12.1:2.12"
msgid "jātipaccayā jarāmaraṇaṃ sokaparidevadukkhadomanassupāyāsā sambhavanti."
msgstr ""

msgctxt "sn12.1:2.13"
msgid "Evametassa kevalassa dukkhakkhandhassa samudayo hoti."
msgstr ""

msgctxt "sn12.1:2.14"
msgid "Ayaṃ vuccati, bhikkhave, paṭiccasamuppādo."
msgstr ""

#. </p><p>
#. <a class="pts-vp-pli" id="pts-vp-pli2.2"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn12.1:3.1"
msgid "Avijjāya tveva asesavirāganirodhā saṅkhāranirodho;"
msgstr ""

msgctxt "sn12.1:3.2"
msgid "saṅkhāranirodhā viññāṇanirodho;"
msgstr ""

msgctxt "sn12.1:3.3"
msgid "viññāṇanirodhā nāmarūpanirodho;"
msgstr ""

msgctxt "sn12.1:3.4"
msgid "nāmarūpanirodhā saḷāyatananirodho;"
msgstr ""

msgctxt "sn12.1:3.5"
msgid "saḷāyatananirodhā phassanirodho;"
msgstr ""

msgctxt "sn12.1:3.6"
msgid "phassanirodhā vedanānirodho;"
msgstr ""

msgctxt "sn12.1:3.7"
msgid "vedanānirodhā taṇhānirodho;"
msgstr ""

msgctxt "sn12.1:3.8"
msgid "taṇhānirodhā upādānanirodho;"
msgstr ""

msgctxt "sn12.1:3.9"
msgid "upādānanirodhā bhavanirodho;"
msgstr ""

msgctxt "sn12.1:3.10"
msgid "bhavanirodhā jātinirodho;"
msgstr ""

msgctxt "sn12.1:3.11"
msgid "jātinirodhā jarāmaraṇaṃ sokaparidevadukkhadomanassupāyāsā nirujjhanti."
msgstr ""

msgctxt "sn12.1:3.12"
msgid "Evametassa kevalassa dukkhakkhandhassa nirodho hotī”ti."
msgstr ""

msgctxt "sn12.1:3.13"
msgid "Idamavoca bhagavā."
msgstr ""

msgctxt "sn12.1:3.14"
msgid "Attamanā te bhikkhū bhagavato bhāsitaṃ abhinandunti."
msgstr ""

#. </p><p class="endsection">
msgctxt "sn12.1:3.15"
msgid "Paṭhamaṃ."
msgstr ""
