#. <section class="sutta" id="sn1.43"><article><div class="hgroup"><p class="division">
msgctxt "sn1.43:0.1"
msgid "Saṃyutta Nikāya 1"
msgstr ""

#. </p><p>
msgctxt "sn1.43:0.2"
msgid "5. Ādittavagga"
msgstr ""

#. </p><h1>
msgctxt "sn1.43:0.3"
msgid "43. Annasutta"
msgstr ""

#. </h1></div><blockquote class="gatha"><p>
#. <a class="sc" id="sc1"></a>
msgctxt "sn1.43:1.1"
msgid "“Annamevābhinandanti,"
msgstr ""

#. <br>
msgctxt "sn1.43:1.2"
msgid "ubhaye devamānusā;"
msgstr ""

#. <br>
msgctxt "sn1.43:1.3"
msgid "Atha ko nāma so yakkho,"
msgstr ""

#. <br>
msgctxt "sn1.43:1.4"
msgid "yaṃ annaṃ nābhinandatī”ti."
msgstr ""

#. </p><p>
#. <a class="sc" id="sc2"></a>
msgctxt "sn1.43:2.1"
msgid "“Ye naṃ dadanti saddhāya,"
msgstr ""

#. <br>
msgctxt "sn1.43:2.2"
msgid "vippasannena cetasā;"
msgstr ""

#. <br>
msgctxt "sn1.43:2.3"
msgid "Tameva annaṃ bhajati,"
msgstr ""

#. <br>
msgctxt "sn1.43:2.4"
msgid "asmiṃ loke paramhi ca."
msgstr ""

#. </p><p>
#. <a class="pts2ed" id="pts2ed1.68"></a>
#. <a class="sc" id="sc3"></a>
msgctxt "sn1.43:3.1"
msgid "Tasmā vineyya maccheraṃ,"
msgstr ""

#. <br>
msgctxt "sn1.43:3.2"
msgid "dajjā dānaṃ malābhibhū;"
msgstr ""

#. <br>
msgctxt "sn1.43:3.3"
msgid "Puññāni paralokasmiṃ,"
msgstr ""

#. <br>
msgctxt "sn1.43:3.4"
msgid "patiṭṭhā honti pāṇinan”ti."
msgstr ""
